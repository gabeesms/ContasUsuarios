</h3>\r\n          <span>\r\n            <b>Alterando dados do perfil no sistema</b>\r\n          </span>\r\n        </v-col>\r\n        <v-row>\r\n          <v-col cols=\"12\" sm=\"12\">\r\n            <v-row style=\"margin-left: 5px; margin-top: -5px\">\r\n              <v-col cols=\"12\" sm=\"6\">\r\n                <v-text-field\r\n                  v-model=\"perfil.descricao\"\r\n                  outlined\r\n                  label=\"Descrição do Perfil *\"\r\n                  dense\r\n                  counter=\"15\"\r\n                  maxlength=\"15\"\r\n                  required\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"4\">\r\n                <v-row style=\"margin-top: -25px\">\r\n                  <label>\r\n                    <b>Módulos</b>\r\n                  </label>\r\n                </v-row>\r\n                <v-row style=\"margin-top: -5px\">\r\n                  <v-radio-group v-model=\"optMenu\" row>\r\n                    <v-radio label=\"Recursos Humanos\" value=\"1\"></v-radio>\r\n                    <v-radio label=\"Produção\" value=\"2\"></v-radio>\r\n                  </v-radio-group>\r\n                </v-row>\r\n              </v-col>\r\n            </v-row>\r\n            <v-row align=\"center\" justify=\"center\">\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <v-col cols=\"12\" sm=\"12\" v-show=\"optMenu == 1\">\r\n                  <k-data-list\r\n                    orientation=\"Horizontal\"\r\n                    mobileIconAdd=\"mdi-plus-circle\"\r\n                    mobileFunctionAdd=\"addSolicitacao\"\r\n                    @editarItem=\"editarItem\"\r\n                    @deleteItem=\"deleteItem\"\r\n                    @addSolicitacao=\"addSolicitacao\"\r\n                    :headers=\"headers\"\r\n                    :search=\"search\"\r\n                    :itens=\"itensRH\"\r\n                  >\r\n                    <template v-slot:perfilPermissao=\"{ item }\">\r\n                      <v-row>\r\n                        <v-checkbox v-model=\"item.perfilPermissao.visualizar\" label=\"Visualizar\"></v-checkbox>\r\n                        <v-spacer></v-spacer>\r\n                        <v-checkbox v-model=\"item.perfilPermissao.editar\" label=\"Editar\"></v-checkbox>\r\n                        <v-spacer></v-spacer>\r\n\r\n                        <v-checkbox v-model=\"item.perfilPermissao.aprovar\" label=\"Aprovar\"></v-checkbox>\r\n                      </v-row>\r\n                    </template>\r\n                  </k-data-list>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"12\" v-show=\"optMenu == 2\">\r\n                  <k-data-list\r\n                    orientation=\"Horizontal\"\r\n                    mobileIconAdd=\"mdi-plus-circle\"\r\n                    mobileFunctionAdd=\"addSolicitacao\"\r\n                    @editarItem=\"editarItem\"\r\n                    @deleteItem=\"deleteItem\"\r\n                    @addSolicitacao=\"addSolicitacao\"\r\n                    :headers=\"headers\"\r\n                    :search=\"search\"\r\n                    :itens=\"itensProd\"\r\n                  >\r\n                    <template v-slot:perfilPermissao=\"{ item }\">\r\n                      <v-row>\r\n                        <v-checkbox v-model=\"item.perfilPermissao.visualizar\" label=\"Visualizar\"></v-checkbox>\r\n                        <v-spacer></v-spacer>\r\n                        <v-checkbox v-model=\"item.perfilPermissao.editar\" label=\"Editar\"></v-checkbox>\r\n                        <v-spacer></v-spacer>\r\n                        <v-checkbox v-model=\"item.perfilPermissao.aprovar\" label=\"Aprovar\"></v-checkbox>\r\n                      </v-row>\r\n                    </template>\r\n                  </k-data-list>\r\n                </v-col>\r\n              </v-col>\r\n            </v-row>\r\n          </v-col>\r\n          <v-col cols=\"12\" sm=\"12\">\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn\r\n                color=\"orange lighten-1\"\r\n                @click=\"retornaListaPerfil()\"\r\n                large\r\n                dark\r\n                width=\"150\"\r\n              >Cancelar</v-btn>\r\n\r\n              <v-btn\r\n                v-if=\"!isMobile && idPerfil\"\r\n                color=\"orange darken-1\"\r\n                large\r\n                dark\r\n                width=\"150\"\r\n                @click=\"updatePerfil()\"\r\n              >Salvar</v-btn>\r\n\r\n              <v-btn\r\n                v-if=\"!isMobile && !idPerfil\"\r\n                color=\"orange darken-1\"\r\n                large\r\n                dark\r\n                width=\"150\"\r\n                @click=\"savePerfil()\"\r\n              >Salvar</v-btn>\r\n\r\n              <v-btn\r\n                v-else-if=\"isMobile && !idPerfil\"\r\n                color=\"orange darken-1\"\r\n                large\r\n                dark\r\n                width=\"150\"\r\n                @click=\"savePerfil()\"\r\n              >Salvar</v-btn>\r\n            </v-card-actions>\r\n          </v-col>\r\n        </v-row>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template> \r\n\r\n\r\n\r\n<script lang=\"ts\">\r\nvar validaNumeros = require(\"../../baseGenerica/validaNumeros.js\");\r\n\r\nimport Vue from \"vue\";\r\n\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  IPerfilFormView,\r\n  IPerfilFormController,\r\n} from \"../../contracts/Acessos/PerfilFormContract\";\r\nimport { PerfilFormController } from \"../../controllers/Acessos/PerfilFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class PerfilForm extends Vue implements IPerfilFormView {\r\n  _controller?: IPerfilFormController;\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  dialogDelete = false;\r\n  itemPerfilDelete: any;\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  optMenu = \"1\";\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  itens = [\r\n    {\r\n      modulo: \"\",\r\n      perfilPermissao: [\r\n        {\r\n          descricao: \"\",\r\n          aprovar: false,\r\n          visualizar: false,\r\n          editar: false,\r\n        },\r\n      ],\r\n    },\r\n  ];\r\n  itensRH = this.itens;\r\n  itensProd = this.itens;\r\n\r\n  perfil = {\r\n    id: \"\",\r\n    descricao: \"\",\r\n    menus: this.itens,\r\n  };\r\n\r\n  headers = new Headers([\r\n    new Header(\r\n      \"Descrição do Menu\",\r\n      \"descricao\",\r\n      false,\r\n      false,\r\n      \"left\",\r\n      false,\r\n      \"250\"\r\n    ),\r\n    new Header(\r\n      \"Opções de Acessos\",\r\n      \"perfilPermissao\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      false,\r\n      \"40\"\r\n    ),\r\n  ]);\r\n\r\n  idPerfil = \"\";\r\n  // checkboxRH = false;\r\n  // checkboxProd = falthis.itens = 1;\r\n  search = \"\";\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Perfis\",\r\n      disabled: false,\r\n      href: \"/perfils\",\r\n    },\r\n  ];\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemPerfilDelete = item;\r\n    this.dialogDelete = true;\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    //this.$router.push(\"/Perfil/\" + item.id + \"/\" + item.matricula);\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/perfils\");\r\n    }\r\n  }\r\n\r\n  retornaListaPerfil() {\r\n    this.$router.push(\"/perfils\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    console.log(\"TESTE MENSAGEM\");\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    if (this.perfil.descricao == \"\") {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"O campo Descrição do Perfil está vazio ou inválido.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  showPerfilForEditar(item: any): void {\r\n    this.itensRH = [];\r\n    this.itensProd = [];\r\n\r\n    this.perfil.descricao = item.descricao;\r\n\r\n    item.menus.forEach((element: any) => {\r\n      if (element.modulo == \"RH\") {\r\n        this.itensRH.push(element);\r\n      }\r\n\r\n      if (element.modulo == \"PRO\") {\r\n        this.itensProd.push(element);\r\n      }\r\n    });\r\n\r\n    //throw new Error(\"Method not implemented.\");\r\n  }\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/perfil\");\r\n  }\r\n\r\n  updatePerfil() {\r\n    if (this.validate()) {\r\n    } else {\r\n      this.perfil.menus = [];\r\n      this.perfil.id = this.idPerfil;\r\n      this.itensRH.forEach((element) => {\r\n        this.perfil.menus.push(element);\r\n      });\r\n\r\n      this.itensProd.forEach((element) => {\r\n        this.perfil.menus.push(element);\r\n      });\r\n\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.update(this.idPerfil, this.perfil);\r\n      //this.retornaListaPerfil();\r\n    }\r\n  }\r\n\r\n  savePerfil() {\r\n    if (this.validate()) {\r\n    } else {\r\n      this.perfil.menus = [];\r\n      this.itensRH.forEach((element) => {\r\n        this.perfil.menus.push(element);\r\n      });\r\n\r\n      this.itensProd.forEach((element) => {\r\n        this.perfil.menus.push(element);\r\n      });\r\n\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.salvar(this.perfil);\r\n      //this.retornaListaPerfil();\r\n    }\r\n  }\r\n\r\n  carregaMenus(item: any) {\r\n    this.itens = [];\r\n    this.itensRH = [];\r\n    this.itensProd = [];\r\n    this.itens = item;\r\n\r\n    this.itens.forEach((element: any) => {\r\n      element.perfilPermissao = {};\r\n      element.perfilPermissao.aprovar = false;\r\n      element.perfilPermissao.visualizar = false;\r\n      element.perfilPermissao.editar = false;\r\n      if (element.modulo == \"RH\") {\r\n        this.itensRH.push(element);\r\n      }\r\n      if (element.modulo == \"PRO\") {\r\n        this.itensProd.push(element);\r\n      }\r\n    });\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.idPerfil = this.$route.params.id;\r\n\r\n    if (this.$route.params.id != undefined) {\r\n    }\r\n  }\r\n\r\n  created() {\r\n    this.optMenu = \"1\";\r\n    this._controller = new PerfilFormController(this);\r\n    this.carregarDtos();\r\n    this._controller.carregarDtos(this.$route.params.id);\r\n\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n</script> "]}]}