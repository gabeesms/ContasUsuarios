{"remainingRequest":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\wamp64\\www\\smartpoint\\smartpoint\\src\\views\\OrdemProducao\\OrdemProducaoLista.vue?vue&type=script&lang=ts&","dependencies":[{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\src\\views\\OrdemProducao\\OrdemProducaoLista.vue","mtime":1610379832707},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\thread-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\babel-loader\\lib\\index.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\ts-loader\\index.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["OrdemProducaoLista.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuSA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;;AAEA;;AAEA;AACA;;AAEA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA","file":"OrdemProducaoLista.vue","sourceRoot":"src/views/OrdemProducao","sourcesContent":["<template>\r\n  <v-content style=\"margin: 10px\">\r\n    <!-- ADD EVENTO LINHA A ORDEM DE PRODUCAO-->\r\n    <v-dialog v-model=\"dialogAddLinha\" persistent max-width=\"800px\">\r\n      <v-card>\r\n        <v-card-title style=\"background-color: #313c42\">\r\n          <span style=\"color: white\" class=\"headline\">\r\n            Relacionar Linha à Ordem de Produção\r\n          </span>\r\n        </v-card-title>\r\n        <v-card-text>\r\n          <v-container>\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <span>\r\n                  <b>Dados da Ordem de Produção</b>\r\n                </span>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"3\">\r\n                <v-text-field\r\n                  disabled\r\n                  class=\"form-control\"\r\n                  outlined\r\n                  label=\"Nº Ordem de Produção\"\r\n                  v-model=\"itemSelecionado.numOrdemProducao\"\r\n                  dense\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"3\">\r\n                <v-text-field\r\n                  disabled\r\n                  class=\"form-control\"\r\n                  outlined\r\n                  label=\"Produto\"\r\n                  v-model=\"itemSelecionado.codigoProduto\"\r\n                  dense\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"3\">\r\n                <v-text-field\r\n                  disabled\r\n                  class=\"form-control\"\r\n                  outlined\r\n                  label=\"Quantidade\"\r\n                  v-model=\"itemSelecionado.quantidade\"\r\n                  dense\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"3\">\r\n                <v-text-field\r\n                  disabled\r\n                  class=\"form-control\"\r\n                  outlined\r\n                  label=\"Data de Emissão\"\r\n                  :value=\"\r\n                    new Date(itemSelecionado.dataEmissao).toLocaleDateString(\r\n                      'pt-BR'\r\n                    )\r\n                  \"\r\n                  dense\r\n                ></v-text-field>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <span>\r\n                  <b>Selecione da Linha</b>\r\n                </span>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"5\">\r\n                <v-select\r\n                  id=\"event-tipo\"\r\n                  :items=\"itemsLinha\"\r\n                  v-model=\"linhaSelecionada\"\r\n                  return-object\r\n                  label=\"Linha *\"\r\n                  autocomplete=\"off\"\r\n                  item-text=\"nome\"\r\n                  placeholder=\"Selecione a Linha para a Ordem de Produção\"\r\n                  outlined\r\n                  dense\r\n                  required\r\n                  :rules=\"[\r\n                    () =>\r\n                      (!!linhaSelecionada && linhaSelecionada.length <= 100) ||\r\n                      '',\r\n                  ]\"\r\n                ></v-select>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"4\">\r\n                <v-text-field\r\n                  disabled\r\n                  class=\"form-control\"\r\n                  outlined\r\n                  label=\"Tempo de Produção Total da Linha\"\r\n                  placeholder=\"Tempo de Produção Total da Linha\"\r\n                  v-model=\"linhaSelecionada.capacidadeProdutiva\"\r\n                  dense\r\n                ></v-text-field>\r\n              </v-col>\r\n\r\n              <v-col cols=\"12\" sm=\"3\">\r\n                <v-text-field\r\n                  disabled\r\n                  class=\"form-control\"\r\n                  outlined\r\n                  label=\"Qtde de Operadores\"\r\n                  placeholder=\"Qtde de Operadores\"\r\n                  v-model=\"linhaSelecionada.quantidadeOperador\"\r\n                  dense\r\n                ></v-text-field>\r\n              </v-col>\r\n            </v-row>\r\n          </v-container>\r\n        </v-card-text>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn\r\n            color=\"orange lighten-1\"\r\n            width=\"150\"\r\n            @click=\"cancelarSaveAddLinhaOrdemProducao\"\r\n            dark\r\n            >Cancelar</v-btn\r\n          >\r\n          <v-btn\r\n            color=\"orange darken-1\"\r\n            width=\"150\"\r\n            dark\r\n            @click=\"saveAddLinhaOrdemProducao\"\r\n            >Salvar</v-btn\r\n          >\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n\r\n    <k-loading title=\"Carregando\" :dialog=\"dialogLoading\"></k-loading>\r\n    <k-dialog\r\n      @fecharDialog=\"fecharDialogMessage\"\r\n      :dialog=\"dialogMessage\"\r\n      title=\"Ordem de Produção\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n    <k-dialog\r\n      :dialog=\"dialogDelete\"\r\n      title=\"cancelar Ordem de Produção?\"\r\n      text=\"A ordem de Produção selecionado será cancelada. Deseja Continuar?\"\r\n    >\r\n      <template v-slot:buttoes>\r\n        <v-btn\r\n          @click=\"dialogDelete = false\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"red darken-2\"\r\n          >Cancelar</v-btn\r\n        >\r\n        <v-btn\r\n          @click=\"deleteItemConfirmado\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"green darken-2\"\r\n          >Sim</v-btn\r\n        >\r\n      </template>\r\n    </k-dialog>\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n          <template v-slot:item=\"{ item }\">\r\n            <router-link :to=\"item.href\">\r\n              {{ item.text }}\r\n            </router-link>\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n\r\n      <v-row>\r\n        <v-col cols=\"10\">\r\n          <h3>\r\n            <b>Ordem de Produção ERP</b>\r\n          </h3>\r\n          <span>\r\n            <b>Crie Simulações Linhas, Produtos, Operadores e Produção ERP</b>\r\n          </span>\r\n        </v-col>\r\n        <v-spacer></v-spacer>\r\n\r\n        <v-col cols=\"1\">\r\n          <v-btn\r\n            v-if=\"!isMobile\"\r\n            color=\"primary\"\r\n            fab\r\n            dark\r\n            @click=\"erpOrdemProducaoCarregar\"\r\n            >ERP</v-btn\r\n          >\r\n        </v-col>\r\n      </v-row>\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-col cols=\"12\" sm=\"5\" style=\"float: right\">\r\n            <v-text-field\r\n              v-model=\"search\"\r\n              label=\"Pesquisa Ordem de Produção\"\r\n              class=\"mx-4\"\r\n              prepend-icon=\"mdi-magnify\"\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row style=\"margin-top: -30px\" align=\"center\" justify=\"center\">\r\n        <v-col cols=\"12\">\r\n          <v-data-table :headers=\"headers\" :items=\"itens\" :search=\"search\">\r\n            <template v-slot:item.deletado=\"{ item }\">\r\n              <v-chip\r\n                v-if=\"item.deletado == 'N'\"\r\n                small\r\n                class=\"px-7 py-3\"\r\n                color=\"#019A53\"\r\n                dark\r\n                >Ativo</v-chip\r\n              >\r\n              <v-chip\r\n                v-if=\"item.deletado == 'S'\"\r\n                small\r\n                class=\"px-6 py-3\"\r\n                color=\"#A80000\"\r\n                dark\r\n                >Inativo</v-chip\r\n              >\r\n            </template>\r\n            <template v-slot:item.acoes=\"{ item }\">\r\n              <v-btn icon>\r\n                <v-icon @click=\"relacionarLinha(item)\" small color=\"primary\"\r\n                  >mdi-file-tree</v-icon\r\n                >\r\n              </v-btn>\r\n\r\n              <v-btn icon>\r\n                <v-icon\r\n                 \r\n                  @click=\"editarItem(item)\"\r\n                  small\r\n                  color=\"primary\"\r\n                  :disabled=\"item.idLinha !== 0 ? false : true\"\r\n                  >mdi-file-document</v-icon\r\n                   \r\n                >\r\n          \r\n              </v-btn>\r\n\r\n               <v-btn icon>\r\n                <v-icon\r\n                  @click=\"viewPlano(item)\"\r\n                  small\r\n                  color=\"primary\"\r\n                  :disabled=\"item.idOrdemProducaoPlano != 0 ? false : true\"\r\n                  >mdi-eye</v-icon\r\n                >\r\n              </v-btn>\r\n             </template>\r\n\r\n            <template v-slot:item.nomeLinha=\"{ item }\">\r\n              <v-chip\r\n                v-if=\"item.nomeLinha !== null\"\r\n                class=\"ma-2\"\r\n                outlined\r\n                pill\r\n                color=\"red\"\r\n                small\r\n              >\r\n                {{ item.nomeLinha }}\r\n              </v-chip>\r\n              <!-- <b v-if=\"item.nomeLinha !== null\" style=\"color: #3F51B5\">{{item.nomeLinha}}</b> -->\r\n              <l class=\"ma-2\" v-else>-</l>\r\n            </template>\r\n          </v-data-table>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from \"vue\";\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\n// import KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\n\r\n//import { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\n//import { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  IOrdemProducaoListaView,\r\n  IOrdemProducaoListaController,\r\n} from \"../../contracts/OrdemProducao/OrdemProducaoListaContract\";\r\n\r\nimport { OrdemProducaoListaController } from \"../../controllers/OrdemProducao/OrdemProducaoListaController\";\r\n\r\nimport Component from \"vue-class-component\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    // KDataList,\r\n  },\r\n})\r\nexport default class OrdemProducaoLista\r\n  extends Vue\r\n  implements IOrdemProducaoListaView {\r\n  _controller?: IOrdemProducaoListaController;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  search = \"\";\r\n\r\n  dialogAddLinha = false;\r\n\r\n  dialogDelete = false;\r\n  itemFuncionarioDelete: any;\r\n\r\n  isMobile = false;\r\n\r\n  itens = [];\r\n\r\n  headers = [\r\n    { text: \"Status\", align: \"center\", filterable: true, value: \"deletado\" },\r\n    {\r\n      text: \"Nº O.P.\",\r\n      align: \"start\",\r\n      filterable: true,\r\n      value: \"numOrdemProducao\",\r\n    },\r\n    {\r\n      text: \"Produto\",\r\n      align: \"start\",\r\n      filterable: true,\r\n      value: \"codigoProduto\",\r\n    },\r\n    {\r\n      text: \"Produção\",\r\n      align: \"start\",\r\n      filterable: true,\r\n      value: \"quantidade\",\r\n    },\r\n    {\r\n      text: \"Data de Emissão\",\r\n      align: \"start\",\r\n      filterable: true,\r\n      value: \"dataEmissao\",\r\n    },\r\n    { text: \"Linha\", align: \"center\", filterable: true, value: \"nomeLinha\" },\r\n    { text: \"Ação\", align: \"center\", filterable: true, value: \"acoes\" },\r\n  ];\r\n\r\n  itemsLinha: any = [];\r\n  linhaSelecionada: any = {};\r\n  itemSelecionado: any = {};\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Nova Ordem de Produção\",\r\n      disabled: false,\r\n      href: \"/ordemProducao\",\r\n    },\r\n  ];\r\n\r\n  viewPlano(item: any){\r\n    if (item.produtoComCapacidade > 0 && item.produtoComCapacidade != null) {\r\n      //NOVO\r\n      // if (item.idOrdemProducaoPlano == 0) {\r\n        this.$router.push(\"/ordensProducaoListaSimulacao/\" + item.id);\r\n      // } else {\r\n      //   //EDITAR\r\n      //   this.$router.push(\r\n      //     \"/ordemProducao/\" +\r\n      //       item.idOrdemProducaoLinha +\r\n      //       \"/\" +\r\n      //       item.idOrdemProducaoPlano\r\n      //   );\r\n      // }\r\n    } else {\r\n      this.showMsg(\r\n        \"Produto \" +\r\n          item.codigoProduto.toString().trim() +\r\n          \" sem Capacidade de Linha cadastrada ou configurada. Antes de definir o plano de Ordem de Produção, configure os dados do Produto, utilizando a opção de Menu Cadastro -> Produto\"\r\n      );\r\n    }\r\n  }\r\n\r\n  relacionarLinha(item: any) {\r\n    this.dialogAddLinha = true;\r\n    this.itemSelecionado = item;\r\n    console.log(this.itemSelecionado);\r\n  }\r\n\r\n  saveAddLinhaOrdemProducao() {\r\n    //console.log(this.linhaSelecionada);\r\n\r\n    //console.log(this.itemSelecionado);\r\n\r\n    let ordemProducaoLinha = {\r\n      idLinha: parseInt(this.linhaSelecionada.id),\r\n      idOrdemProducao: parseInt(this.itemSelecionado.id),\r\n      dataCadastro: new Date(),\r\n      deletado: \"N\",\r\n    };\r\n\r\n    this._controller!.salvarOrdemProducaoLinha(ordemProducaoLinha);\r\n    this.dialogAddLinha = false;\r\n  }\r\n\r\n  cancelarSaveAddLinhaOrdemProducao() {\r\n    this.dialogAddLinha = false;\r\n    this.linhaSelecionada = {};\r\n    this.itemSelecionado = {};\r\n  }\r\n\r\n  erpOrdemProducaoCarregar() {\r\n    this.loading(true);\r\n    this.itens = [];\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new OrdemProducaoListaController(this);\r\n    this._controller.atualizaBaseOrdemProducaoERP();\r\n    this._controller.carregarItens();\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  showItens(list: any): void {\r\n    this.linhaSelecionada = {};\r\n    this.itemSelecionado = {};\r\n    this.itens = list;\r\n  }\r\n\r\n  carregaLinha(linhas: any): void {\r\n    this.itemsLinha = linhas;\r\n\r\n    console.log(this.itemsLinha);\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemFuncionarioDelete = item;\r\n    this.dialogDelete = true;\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    //this._controller!.deleteitem(this.itemFuncionarioDelete.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    if (item.produtoComCapacidade > 0 && item.produtoComCapacidade != null) {\r\n      //NOVO\r\n      // if (item.idOrdemProducaoPlano == 0) {\r\n        this.$router.push(\"/ordemProducao/\" + item.idOrdemProducaoLinha);\r\n      // } else {\r\n      //   //EDITAR\r\n      //   this.$router.push(\r\n      //     \"/ordemProducao/\" +\r\n      //       item.idOrdemProducaoLinha +\r\n      //       \"/\" +\r\n      //       item.idOrdemProducaoPlano\r\n      //   );\r\n      // }\r\n    } else {\r\n      this.showMsg(\r\n        \"Produto \" +\r\n          item.codigoProduto.toString().trim() +\r\n          \" sem Capacidade de Linha cadastrada ou configurada. Antes de definir o plano de Ordem de Produção, configure os dados do Produto, utilizando a opção de Menu Cadastro -> Produto\"\r\n      );\r\n    }\r\n  }\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/ordemProducao\");\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new OrdemProducaoListaController(this);\r\n\r\n    this._controller.carregarItens();\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.v-data-table th {\r\n  color: white !important;\r\n  background-color: #313c42 !important;\r\n}\r\n</style>>\r\n\r\n</style>"]}]}