=\"textError\"\r\n    ></k-dialog>\r\n    <k-dialog\r\n      :dialog=\"dialogDelete\"\r\n      title=\"Remover Funcionário?\"\r\n      text=\"O funcionário selecionado será removido. Deseja Continuar?\"\r\n    >\r\n      <template v-slot:buttoes>\r\n        <v-btn @click=\"dialogDelete = false\" width=\"100\" text dark color=\"red darken-2\">Cancelar</v-btn>\r\n        <v-btn @click=\"deleteItemConfirmado\" width=\"100\" text dark color=\"green darken-2\">Sim</v-btn>\r\n      </template>\r\n    </k-dialog>\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n\r\n      <v-row>\r\n        <v-col cols=\"10\">\r\n          <h3>\r\n            <b>Funcionários</b>\r\n          </h3>\r\n          <span>\r\n            <b>Lista de Funcionários cadastrados no sistema</b>\r\n          </span>\r\n        </v-col>\r\n        <v-spacer></v-spacer>\r\n        <v-col cols=\"1\">\r\n          <v-btn v-if=\"!isMobile\" color=\"primary\" fab dark @click=\"erpFuncionariosCarregar\">ERP</v-btn>\r\n\r\n          <!-- <v-icon v-if=\"!isMobile\" @click=\"addSolicitacao\" large color=\"primary\">mdi-plus-circle</v-icon> -->\r\n        </v-col>\r\n        <v-col cols=\"1\">\r\n          <v-btn v-if=\"!isMobile\" color=\"primary\" fab dark @click=\"addSolicitacao\">\r\n            <v-icon dark>mdi-plus</v-icon>\r\n          </v-btn>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-col cols=\"12\" sm=\"5\" style=\"float: right\">\r\n            <v-text-field\r\n              v-model=\"search\"\r\n              label=\"Pesquisa de Funcionários\"\r\n              class=\"mx-4\"\r\n              prepend-icon=\"mdi-magnify\"\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row style=\"margin-top: -30px\" align=\"center\" justify=\"center\">\r\n        <v-col cols=\"12\">\r\n          <k-data-list\r\n            orientation=\"Horizontal\"\r\n            mobileIconAdd=\"mdi-plus-circle\"\r\n            mobileFunctionAdd=\"addSolicitacao\"\r\n            @editarItem=\"editarItem\"\r\n            @deleteItem=\"deleteItem\"\r\n            @addSolicitacao=\"addSolicitacao\"\r\n            :headers=\"headers\"\r\n            :search=\"search\"\r\n            :itens=\"itens\"\r\n          >\r\n            <!-- <template v-slot:dtAdmissao=\"{ item }\">\r\n            <b>{{ \r\n              item.dtAdmissao\r\n            \r\n            }}</b>\r\n            </template>-->\r\n          </k-data-list>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from \"vue\";\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  IFuncionarioListaView,\r\n  IFuncionarioListaController,\r\n} from \"../../contracts/Cadastro/FuncionarioListaContract\";\r\n\r\nimport { FuncionarioListaController } from \"../../controllers/Cadastro/FuncionarioListaController\";\r\n\r\nimport Component from \"vue-class-component\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class FuncionarioLista extends Vue\r\n  implements IFuncionarioListaView {\r\n  _controller?: IFuncionarioListaController;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  search = \"\";\r\n\r\n  dialogDelete = false;\r\n  itemFuncionarioDelete: any;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Novo Funcionário\",\r\n      disabled: false,\r\n      href: \"/funcionario\",\r\n    },\r\n  ];\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n  showItens(list: any): void {\r\n    this.itens = list;\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemFuncionarioDelete = item;\r\n    this.dialogDelete = true;\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    this._controller!.deleteitem(this.itemFuncionarioDelete.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    this.$router.push(\"/funcionario/\" + item.id + \"/\" + item.matricula);\r\n  }\r\n\r\n  erpFuncionariosCarregar() {\r\n    this.loading(true);\r\n    this.itens = [];\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new FuncionarioListaController(this);\r\n    this._controller.atualizaBaseFuncionariosERP();\r\n    this._controller.carregarItens();\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  headers = new Headers([\r\n    new Header(\"Matrícula\", \"matricula\", false, false, \"center\", true, \"100\"),\r\n    new Header(\"Nome\", \"nome\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Cargo\", \"cargo\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Setor\", \"setor\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Situação\", \"situacao\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  itens = [];\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/funcionario\");\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new FuncionarioListaController(this);\r\n\r\n    this._controller.carregarItens();\r\n  }\r\n}\r\n</script>"]}]}