ialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  ICentroCustoListaView,\r\n  ICentroCustoListaController,\r\n} from \"../../contracts/Cadastro/CentroCustoListaContract\";\r\n\r\nimport { CentroCustoListaController } from \"../../controllers/Cadastro/CentroCustoListaController\";\r\n\r\nimport Component from \"vue-class-component\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class CentroCustoLista\r\n  extends Vue\r\n  implements ICentroCustoListaView {\r\n  _controller?: ICentroCustoListaController;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  search = \"\";\r\n\r\n  dialogDelete = false;\r\n  itemCentroCustoDelete: any;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Centro de Custos\",\r\n      disabled: false,\r\n      href: \"/centrosCustos\",\r\n    },\r\n  ];\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n  showItens(list: any): void {\r\n    //console.log(list);\r\n    this.itens = list;\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemCentroCustoDelete = item;\r\n    this.dialogDelete = true;\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    this.$router.push(\"/centroCusto/\" + item.codigo);\r\n  }\r\n\r\n  erpCentroCustosCarregar() {\r\n    this.loading(true);\r\n    this.itens = [];\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new CentroCustoListaController(this);\r\n    this._controller.carregarItens();\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  headers = new Headers([\r\n    new Header(\"Código\", \"codigo\", false, false, \"left\", false, \"200\"),\r\n    new Header(\"Descrição\", \"descricao\", false, false, \"left\", false, \"200\"),\r\n    new Header(\"Setor\", \"setor\", false, false, \"center\", false, \"200\"),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  itens = [];\r\n\r\n  // addSolicitacao() {\r\n  //   this.$router.push(\"/CentroCusto\");\r\n  // }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new CentroCustoListaController(this);\r\n\r\n    this._controller.carregarItens();\r\n  }\r\n}\r\n</script>"]}]}