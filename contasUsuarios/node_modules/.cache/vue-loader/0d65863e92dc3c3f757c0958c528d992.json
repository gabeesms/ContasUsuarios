       <template v-slot:expanded-item=\"{ headers, item }\">\r\n                    <td :colspan=\"headers.length\">\r\n                      <!-- TABLE INTERNA  -->\r\n                      <v-data-table\r\n                        :headers=\"headersNovaLinha\"\r\n                        :items=\"item.linhaCapacidadeHabilidades\"\r\n                        hide-default-footer\r\n                        style=\"margin-top: 15px\"\r\n                        class=\"elevation-1 mytable\"\r\n                      >\r\n                        <template v-slot:body=\"{ items }\">\r\n                          <tr v-for=\"(item2, index) in items\" :key=\"index\">\r\n                            <td class=\"text-center\">\r\n                              {{ item.processoDTOOperacao.nome }}\r\n                            </td>\r\n                            <td class=\"text-center\">\r\n                              {{ item2.processoDTOHabilidade.nome }}\r\n                            </td>\r\n                            <td>\r\n                              <v-select\r\n                                outlined\r\n                                placeholder=\"Selecione os Operadores\"\r\n                                :items=\"itensOperadores\"\r\n                                chips\r\n                                style=\"text-align: right; margin-top: 21px\"\r\n                                item-value=\"id\"\r\n                                item-text=\"nome\"\r\n                                dense\r\n                                required\r\n                                return-object\r\n                                multiple\r\n                                v-model=\"item2.operadores\"\r\n                              >\r\n                              </v-select>\r\n                            </td>\r\n                            <td class=\"text-center\">\r\n                              <v-col cols=\"12\" sm=\"11\">\r\n                                <v-text-field\r\n                                  placeholder=\"0\"\r\n                                  type=\"number\"\r\n                                  v-model=\"item2.qtdeOperadores\"\r\n                                  outlined\r\n                                  style=\"height: 43px\"\r\n                                  dense\r\n                                  disabled\r\n                                ></v-text-field>\r\n                              </v-col>\r\n                            </td>\r\n                            <td class=\"text-center\">\r\n                              <v-col cols=\"12\" sm=\"11\">\r\n                                <v-text-field\r\n                                  placeholder=\"0\"\r\n                                  type=\"time\"\r\n                                  :value=\"item2.time\"\r\n                                  outlined\r\n                                  style=\"height: 43px; border-width: 1px\"\r\n                                  step=\"1\"\r\n                                  dense\r\n                                  disabled\r\n                                ></v-text-field>\r\n                              </v-col>\r\n                            </td>\r\n                          </tr>\r\n                        </template>\r\n                      </v-data-table>\r\n\r\n                      <br />\r\n                    </td>\r\n                  </template>\r\n                </v-data-table>\r\n              </v-col>\r\n            </v-row>\r\n            <br />\r\n            <v-row>\r\n              <span><b> Recursos Técnicos</b></span>\r\n            </v-row>\r\n            <!-- TABLE CPACIDADE X HABILIDADE - RECURSO TECNICO-->\r\n            <v-row align=\"center\" justify=\"center\">\r\n              <v-col cols=\"12\">\r\n                <v-data-table\r\n                  :headers=\"headersLinha\"\r\n                  :items=\"itensRecursoTecnicoLinha\"\r\n                  :single-expand=\"singleExpand\"\r\n                  item-key=\"fase\"\r\n                  show-expand\r\n                  class=\"elevation-1\"\r\n                  hide-default-header\r\n                  hide-default-footer\r\n                >\r\n                  <template v-slot:expanded-item=\"{ headers, item }\">\r\n                    <td :colspan=\"headers.length\">\r\n                      <!-- TABLE INTERNA  -->\r\n                      <v-data-table\r\n                        :headers=\"headersNovaLinha\"\r\n                        :items=\"item.linhaRecursoTecnicoHabilidades\"\r\n                        hide-default-footer\r\n                        style=\"margin-top: 15px\"\r\n                        class=\"elevation-1 mytable\"\r\n                      >\r\n                        <template v-slot:body=\"{ items }\">\r\n                          <tr v-for=\"(item2, index) in items\" :key=\"index\">\r\n                            <td class=\"text-center\">\r\n                              {{ item.processoDTOOperacao.nome }}\r\n                            </td>\r\n                            <td class=\"text-center\">\r\n                              {{ item2.processoDTOHabilidade.nome }}\r\n                            </td>\r\n                            <td>\r\n                              <v-select\r\n                                outlined\r\n                                placeholder=\"Selecione os Operadores\"\r\n                                :items=\"itensOperadores\"\r\n                                chips\r\n                                style=\"text-align: right; margin-top: 21px\"\r\n                                item-value=\"id\"\r\n                                item-text=\"nome\"\r\n                                dense\r\n                                required\r\n                                return-object\r\n                                multiple\r\n                                v-model=\"item2.operadores\"\r\n                              >\r\n                              </v-select>\r\n                            </td>\r\n                            <td class=\"text-center\">\r\n                              <v-col cols=\"12\" sm=\"11\">\r\n                                <v-text-field\r\n                                  placeholder=\"0\"\r\n                                  type=\"number\"\r\n                                  v-model=\"item2.qtdeOperadores\"\r\n                                  outlined\r\n                                  style=\"height: 43px\"\r\n                                  dense\r\n                                  disabled\r\n                                ></v-text-field>\r\n                              </v-col>\r\n                            </td>\r\n                            <td class=\"text-center\">\r\n                              <v-col cols=\"12\" sm=\"11\">\r\n                                <v-text-field\r\n                                  placeholder=\"0\"\r\n                                  type=\"time\"\r\n                                  v-model=\"item2.time\"\r\n                                  outlined\r\n                                  style=\"height: 43px; border-width: 1px\"\r\n                                  step=\"1\"\r\n                                  dense\r\n                                  disabled\r\n                                ></v-text-field>\r\n                              </v-col>\r\n                            </td>\r\n                          </tr>\r\n                        </template>\r\n                      </v-data-table>\r\n\r\n                      <br />\r\n                    </td>\r\n                  </template>\r\n                </v-data-table>\r\n              </v-col>\r\n            </v-row>\r\n          </v-col>\r\n        </v-card-text>\r\n      </v-card>\r\n\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"12\"></v-col>\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn\r\n              color=\"orange lighten-1\"\r\n              @click=\"retornaListaOrdensProducao()\"\r\n              large\r\n              dark\r\n              width=\"150\"\r\n              >Cancelar</v-btn\r\n            >\r\n\r\n            <v-btn\r\n              v-if=\"!isMobile && idOrdemProducao\"\r\n              color=\"orange darken-1\"\r\n              large\r\n              dark\r\n              width=\"150\"\r\n              @click=\"updateOrdemProducao()\"\r\n              >EDITAR</v-btn\r\n            >\r\n\r\n            <v-btn\r\n              v-if=\"!isMobile && !idOrdemProducao\"\r\n              color=\"orange darken-1\"\r\n              large\r\n              dark\r\n              width=\"150\"\r\n              @click=\"saveOrdemProducao()\"\r\n              >Salvar</v-btn\r\n            >\r\n\r\n            <v-btn\r\n              v-else-if=\"isMobile && !idOrdemProducao\"\r\n              color=\"orange darken-1\"\r\n              large\r\n              dark\r\n              width=\"150\"\r\n              @click=\"saveOrdemProducao()\"\r\n              >Salvar</v-btn\r\n            >\r\n          </v-card-actions>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template> \r\n\r\n\r\n\r\n<script lang=\"ts\">\r\nvar validaNumeros = require(\"../../baseGenerica/validaNumeros.js\");\r\n\r\nimport Vue from \"vue\";\r\n\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  IOrdemProducaoFormView,\r\n  IOrdemProducaoFormController,\r\n} from \"../../contracts/OrdemProducao/OrdemProducaoFormContract\";\r\nimport { OrdemProducaoFormController } from \"../../controllers/OrdemProducao/OrdemProducaoFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class OrdemProducaoForm\r\n  extends Vue\r\n  implements IOrdemProducaoFormView {\r\n  _controller?: IOrdemProducaoFormController;\r\n\r\n  itensTurno: any = [];\r\n\r\n  expanded = [];\r\n  singleExpand = false;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Ordens de Produção\",\r\n      disabled: false,\r\n      href: \"/ordensProducao\",\r\n    },\r\n  ];\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  idOrdemProducao = \"\";\r\n\r\n  ordemProducao: any = {};\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  headersNovaLinha = [\r\n    {\r\n      text: \"Operação\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 100,\r\n      value: \"\",\r\n    },\r\n    {\r\n      text: \"Habilidade\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 100,\r\n      value: \"\",\r\n    },\r\n    {\r\n      text: \"Operadores\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 400,\r\n      value: \"\",\r\n    },\r\n    {\r\n      text: \"Qtde de Operadores\",\r\n      align: \"center\",\r\n      width: 100,\r\n      sortable: false,\r\n      value: \"\",\r\n    },\r\n    { text: \"Time\", align: \"center\", sortable: false, width: 100, value: \"\" },\r\n  ];\r\n\r\n  headersLinha = [\r\n    {\r\n      text: \"Nome\",\r\n      align: \"start\",\r\n      sortable: false,\r\n      value: \"fase\",\r\n    },\r\n    { text: \"Ação\", align: \"right\", value: \"acao\" },\r\n    { text: \"\", align: \"right\", value: \"data-table-expand\" },\r\n  ];\r\n\r\n  itensCapacidadeLinha: any = [];\r\n  itensRecursoTecnicoLinha: any = [];\r\n  itensOperadores: any = [];\r\n\r\n  customFilterOperador(item: any, queryText: any, itemText: any) {\r\n    const textOne = item.nome.toLowerCase();\r\n    const textTwo = item.nome.toLowerCase();\r\n    const searchText = queryText.toLowerCase();\r\n\r\n    return textOne.indexOf(searchText) > -1 || textTwo.indexOf(searchText) > -1;\r\n  }\r\n\r\n  carregaTurnos(itemsTurno: any): void {\r\n    this.itensTurno = itemsTurno;\r\n\r\n    this.itensTurno.forEach((element: any) => {\r\n      element.textoDescHora =\r\n        element.descricao + \" - HORAS TOTAL: \" + element.horaTotal;\r\n    });\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/ordensProducao\");\r\n    }\r\n  }\r\n\r\n  retornaListaOrdensProducao() {\r\n    this.$router.push(\"/ordensProducao\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    return false;\r\n  }\r\n\r\n  saveOrdemProducao() {\r\n    if (!this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.salvar(this.ordemProducao);\r\n      this.retornaListaOrdensProducao();\r\n    }\r\n  }\r\n\r\n  updateOrdemProducao() {\r\n    console.log(this.ordemProducao);\r\n    return;\r\n    if (!this.validate()) {\r\n    } else {\r\n      this._controller!.update(this.idOrdemProducao, this.ordemProducao);\r\n      this.retornaListaOrdensProducao();\r\n    }\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.idOrdemProducao = this.$route.params.idOrdemProducaoLinha;\r\n  }\r\n\r\n  showOrdemProducaoForEditar(item: any) {\r\n    this.ordemProducao = item;\r\n    this.itensCapacidadeLinha = this.ordemProducao.linha.linhaCapacidade;\r\n\r\n    this.itensRecursoTecnicoLinha = this.ordemProducao.linha.recursoTecnico;\r\n\r\n    //instanciando oeperadores em capacidade da linha\r\n    this.itensCapacidadeLinha.forEach((element: any) => {\r\n      element.linhaCapacidadeHabilidades.forEach((element2: any) => {\r\n        element2.operadores = null;\r\n      });\r\n    });\r\n\r\n    //instanciando oeperadores em recursos tecnico\r\n    this.itensRecursoTecnicoLinha.forEach((element: any) => {\r\n      element.linhaRecursoTecnicoHabilidades.forEach((element2: any) => {\r\n        element2.operadores = null;\r\n      });\r\n    });\r\n\r\n    //convertendo time para time imput\r\n    this.itensCapacidadeLinha.forEach((elementCapLinha: any) => {\r\n      elementCapLinha.linhaCapacidadeHabilidades.forEach(\r\n        (elementCapLinhaHabilidade: any) => {\r\n          elementCapLinhaHabilidade.time = new Date(\r\n            elementCapLinhaHabilidade.time\r\n          )\r\n            .toLocaleTimeString()\r\n            .toString();\r\n        }\r\n      );\r\n    });\r\n\r\n    //convertendo time para time imput\r\n    this.itensRecursoTecnicoLinha.forEach((elementRecursoTecnico: any) => {\r\n      elementRecursoTecnico.linhaRecursoTecnicoHabilidades.forEach(\r\n        (elementRecursoTecnicoHabilidade: any) => {\r\n          elementRecursoTecnicoHabilidade.time = new Date(\r\n            elementRecursoTecnicoHabilidade.time\r\n          )\r\n            .toLocaleTimeString()\r\n            .toString();\r\n        }\r\n      );\r\n    });\r\n  }\r\n\r\n  carregaOperadores(operadores: any): void {\r\n    this.itensOperadores = operadores;\r\n  }\r\n\r\n  created() {\r\n    this._controller = new OrdemProducaoFormController(this);\r\n    this._controller.carregarDtos(this.$route.params.idOrdemProducaoLinha);\r\n    this.carregarDtos();\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n</script> \r\n\r\n<style scoped>\r\n.centered-input >>> input {\r\n  text-align: right;\r\n}\r\n\r\n.mytable table td {\r\n  border-bottom: 1px solid rgba(0, 0, 0, 0.12);\r\n}\r\n</style>"]}]}