>\r\n    <v-snackbar\r\n      v-model=\"snackbar\"\r\n      :color=\"colorBorder\"\r\n      shaped\r\n      timeout=\"3000\"\r\n      :vertical=\"vertical\"\r\n    >\r\n      <b>{{ text }}</b>\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn v-bind=\"attrs\" @click=\"snackbar = false\" icon dark small>\r\n          <v-icon>mdi-close</v-icon>\r\n        </v-btn>\r\n        <!-- <v-btn icon v-bind=\"attrs\" @click=\"snackbar = false\">mdi-magnify</v-btn> -->\r\n      </template>\r\n    </v-snackbar>\r\n\r\n    <k-loading title=\"Carregando\" :dialog=\"dialogLoading\"></k-loading>\r\n\r\n    <k-dialog\r\n      @fecharDialog=\"fecharDialogMessage\"\r\n      :dialog=\"dialogMessage\"\r\n      title=\"Mensagem\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n          <template v-slot:item=\"{ item }\">\r\n            <router-link :to=\"item.href\">\r\n              {{ item.text }}\r\n            </router-link>\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n\r\n      <v-col cols=\"12\" v-if=\"!idOrdemProducao\">\r\n        <h3>\r\n          <b>Adicionar Linha á Ordem de Produção</b>\r\n        </h3>\r\n        <!-- <span>\r\n            <b>Editar Linha da Ordem de Produção</b>\r\n          </span> -->\r\n      </v-col>\r\n\r\n      <v-card flat>\r\n        <v-card-text>\r\n          <v-col cols=\"12\" sm=\"12\">\r\n            <v-row>\r\n              <span><b>Dados Gerais</b></span>\r\n            </v-row>\r\n\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"6\">\r\n                <v-text-field\r\n                  outlined\r\n                  label=\"Produto / Ordem de Produção\"\r\n                  placeholder=\"Produto / Ordem de Produção\"\r\n                  :value=\"\r\n                    ordemProducao.produto.codigo +\r\n                    ' - ' +\r\n                    ordemProducao.produto.descricao\r\n                  \"\r\n                  dense\r\n                  disabled\r\n                ></v-text-field>\r\n              </v-col>\r\n\r\n              <v-col cols=\"12\" sm=\"2\">\r\n                <v-text-field\r\n                  outlined\r\n                  label=\"Qtde Operadores Produto\"\r\n                  placeholder=\"Qtde Operadores Produto\"\r\n                  v-model=\"ordemProducao.produto.qtdeOperador\"\r\n                  dense\r\n                  disabled\r\n                ></v-text-field>\r\n              </v-col>\r\n\r\n              <v-col cols=\"12\" sm=\"2\">\r\n                <v-text-field\r\n                  outlined\r\n                  label=\"Meta Total\"\r\n                  placeholder=\"Meta Total\"\r\n                  v-model=\"ordemProducao.ordemProducao.quantidade\"\r\n                  dense\r\n                  disabled\r\n                ></v-text-field>\r\n              </v-col>\r\n\r\n              <v-col cols=\"12\" sm=\"2\">\r\n                <v-text-field\r\n                  outlined\r\n                  label=\"Meta Turno\"\r\n                  placeholder=\"Meta Turno\"\r\n                  v-model=\"ordemProducao.metaTurno\"\r\n                  dense\r\n                  @keydown=\"apenasNumeros\"\r\n                ></v-text-field>\r\n              </v-col>\r\n            </v-row>\r\n            <v-row style=\"margin-top: -20px\">\r\n              <v-col cols=\"12\" sm=\"8\">\r\n                <v-text-field\r\n                  outlined\r\n                  label=\"Nome da Linha\"\r\n                  placeholder=\"Nome da Linha\"\r\n                  dense\r\n                  disabled\r\n                  v-model=\"ordemProducao.linha.nome\"\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"2\">\r\n                <v-text-field\r\n                  outlined\r\n                  label=\"Qtde Operadores Linha\"\r\n                  placeholder=\"Qtde Operadores Linha\"\r\n                  dense\r\n                  disabled\r\n                  v-model=\"ordemProducao.linha.quantidadeOperador\"\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"2\">\r\n                <v-text-field\r\n                  outlined\r\n                  label=\"Capacidade Produtiva\"\r\n                  placeholder=\"Capacidade Produtiva\"\r\n                  dense\r\n                  disabled\r\n                  v-model=\"ordemProducao.linha.capacidadeProdutiva\"\r\n                ></v-text-field>\r\n              </v-col>\r\n            </v-row>\r\n            <v-row style=\"margin-top: -20px\">\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <v-select\r\n                  outlined\r\n                  label=\"Turnos\"\r\n                  placeholder=\"Turnos\"\r\n                  :items=\"itensTurno\"\r\n                  chips\r\n                  type=\"time\"\r\n                  style=\"text-align: right\"\r\n                  item-value=\"id\"\r\n                  item-text=\"textoDescHora\"\r\n                  dense\r\n                  required\r\n                  return-object\r\n                  @keypress=\"apenasNumeros\"\r\n                  multiple\r\n                >\r\n                </v-select>\r\n              </v-col>\r\n            </v-row>\r\n            <v-row style=\"margin-top: -20px\">\r\n              <span><b> Dias Da Semana</b></span>\r\n            </v-row>\r\n            <v-row style=\"margin-top: -20px\">\r\n              <v-col cols=\"12\" sm=\"6\">\r\n                <v-row>\r\n                  <v-checkbox\r\n                    v-model=\"ordemProducao.domingo\"\r\n                    label=\"Dom\"\r\n                  ></v-checkbox>\r\n                  <v-spacer></v-spacer