\r\n        <v-btn v-bind=\"attrs\" @click=\"snackbar = false\" icon dark small>\r\n          <v-icon>mdi-close</v-icon>\r\n        </v-btn>\r\n        <!-- <v-btn icon v-bind=\"attrs\" @click=\"snackbar = false\">mdi-magnify</v-btn> -->\r\n      </template>\r\n    </v-snackbar>\r\n\r\n    <k-loading title=\"Carregando\" :dialog=\"dialogLoading\"></k-loading>\r\n\r\n    <k-dialog\r\n      :dialog=\"dialogDelete\"\r\n      title=\"Remover Setor?\"\r\n      text=\"O setor selecionado será removido. Deseja Continuar?\"\r\n    >\r\n      <template v-slot:buttoes>\r\n        <v-btn\r\n          @click=\"cancelaRemoverSetor()\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"red darken-2\"\r\n          >Cancelar</v-btn\r\n        >\r\n        <v-btn\r\n          @click=\"removerSetor()\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"green darken-2\"\r\n          >Sim</v-btn\r\n        >\r\n      </template>\r\n    </k-dialog>\r\n\r\n    <k-dialog\r\n      @fecharDialog=\"fecharDialogMessage\"\r\n      :dialog=\"dialogMessage\"\r\n      title=\"Mensagem\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n          <template v-slot:item=\"{ item }\">\r\n            <router-link :to=\"item.href\">\r\n              {{ item.text }}\r\n            </router-link>\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n      <v-col cols=\"12\" v-if=\"linhaProducao.id == 0\">\r\n        <h3>\r\n          <b>Linha de Produção</b>\r\n        </h3>\r\n        <span>\r\n          <b>Adicionar Linha de Produção</b>\r\n        </span>\r\n      </v-col>\r\n\r\n      <v-col cols=\"12\" v-if=\"linhaProducao.id !== 0\">\r\n        <h3>\r\n          <b>Linha de Produção</b>\r\n        </h3>\r\n        <span>\r\n          <b>Editar Linha de Produção</b>\r\n        </span>\r\n      </v-col>\r\n\r\n      <v-row ref=\"form\">\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-card flat>\r\n            <!-- DADOS DA INSERÇÃO DE APROVCAÇÃO -->\r\n            <v-card-text>\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"4\">\r\n                  <span>\r\n                    <b>Dados Gerais</b>\r\n                  </span>\r\n                </v-col>\r\n              </v-row>\r\n\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"12\">\r\n                  <v-row>\r\n                    <v-col cols=\"12\" sm=\"8\">\r\n                      <v-text-field\r\n                        outlined\r\n                        v-model=\"linhaProducao.nome\"\r\n                        label=\"Nome\"\r\n                        placeholder=\"Nome da Linha\"\r\n                        dense\r\n                      ></v-text-field>\r\n                    </v-col>\r\n                    <v-col cols=\"12\" sm=\"4\">\r\n                      <v-text-field\r\n                        outlined\r\n                        v-model=\"linhaProducao.qtdeCapacidadeOperadoresLinha\"\r\n                        label=\"Capacidade de Operadores (Qtde)\"\r\n                        placeholder=\"0\"\r\n                        dense\r\n                        disabled\r\n                      ></v-text-field>\r\n                    </v-col>\r\n                  </v-row>\r\n                </v-col>\r\n              </v-row>\r\n              <v-row style=\"margin-top: -20px\">\r\n                <v-col cols=\"12\" sm=\"4\">\r\n                  <span>\r\n                    <b>Dados das Fases</b>\r\n                  </span>\r\n                </v-col>\r\n              </v-row>\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"6\">\r\n                  <v-autocomplete\r\n                    :items=\"itemsFases\"\r\n                    :filter=\"customFilterSetor\"\r\n                    return-object\r\n                    item-text=\"nome\"\r\n                    item-value=\"id\"\r\n                    placeholder=\"Selecione a Fase do Processo\"\r\n                    label=\"Fases do Processo\"\r\n                    outlined\r\n                    dense\r\n                  ></v-autocomplete>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"5\">\r\n                  <v-text-field\r\n                    outlined\r\n                    label=\"Qtde de Operadores por Fase (Capacidade)\"\r\n                    placeholder=\"0\"\r\n                    dense\r\n                    @keypress=\"apenasNumeros\"\r\n                    type=\"number\"\r\n                    class=\"centered-input text--darken-3 mt-3\" \r\n                  ></v-text-field>\r\n                </v-col>\r\n                <v-col cols=\"1\">\r\n                  <v-btn\r\n                    v-if=\"!isMobile\"\r\n                    color=\"#FB9514\"\r\n                    fab\r\n                    dark\r\n                    @click=\"addSetor()\"\r\n                  >\r\n                    <v-icon dark>mdi-plus</v-icon>\r\n                  </v-btn>\r\n                </v-col>\r\n              </v-row>\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"12\">\r\n                  <k-data-list\r\n                    orientation=\"Horizontal\"\r\n                    mobileIconAdd=\"mdi-plus-circle\"\r\n                    mobileFunctionAdd=\"add\"\r\n                    :headers=\"headers\"\r\n                    :itens=\"linhaProducao.fases\"\r\n                  >\r\n                    <template v-slot:acao=\"{ index }\">\r\n                      <v-btn\r\n                        style=\"float: right\"\r\n                        large\r\n                        v-if=\"!isMobile\"\r\n                        color=\"primary\"\r\n                        icon=\"dark\"\r\n                        @click=\"modalRemoverSetor(index)\"\r\n                      >\r\n                        <v-icon dark>mdi-delete</v-icon>\r\n                      </v-btn>\r\n                    </template>\r\n                  </k-data-list>\r\n                </v-col>\r\n              </v-row>\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"12\">\r\n                  <v-card-actions>\r\n                    <v-spacer></v-spacer>\r\n                    <v-btn\r\n                      color=\"orange lighten-1\"\r\n                      @click=\"retornaListaFuncionarios()\"\r\n                      large\r\n                      dark\r\n                      width=\"150\"\r\n                      >Cancelar</v-btn\r\n                    >\r\n\r\n                    <v-btn\r\n                      v-if=\"!isMobile && linhaProducao.id == 0\"\r\n                      color=\"orange darken-1\"\r\n                      large\r\n                      dark\r\n                      width=\"150\"\r\n                      @click=\"saveLinhaProducaoSetores()\"\r\n                      >Salvar</v-btn\r\n                    >\r\n\r\n                    <v-btn\r\n                      v-if=\"!isMobile && linhaProducao.id !== 0\"\r\n                      color=\"orange darken-1\"\r\n                      large\r\n                      dark\r\n                      width=\"150\"\r\n                      @click=\"updateLinhaProducaoSetores()\"\r\n                      >Salvar</v-btn\r\n                    >\r\n                  </v-card-actions>\r\n                </v-col>\r\n              </v-row>\r\n            </v-card-text>\r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template> \r\n\r\n\r\n\r\n<script lang=\"ts\">\r\nvar validaNumeros = require(\"../../baseGenerica/validaNumeros.js\");\r\n\r\nimport Vue from \"vue\";\r\n\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  ILinhaProducaoFormView,\r\n  ILinhaProducaoFormController,\r\n} from \"../../contracts/Cadastro/LinhaProducaoFormContract\";\r\nimport { LinhaProducaoFormController } from \"../../controllers/Cadastro/LinhaProducaoFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class LinhaProducaoForm\r\n  extends Vue\r\n  implements ILinhaProducaoFormView {\r\n  _controller?: ILinhaProducaoFormController;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Linha de Produção\",\r\n      disabled: false,\r\n      href: \"/centrosCustos\",\r\n    },\r\n  ];\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  dialogLoading = false;\r\n  dialogDelete = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  itemsFases: any = [];\r\n\r\n  idLinhaProducao = \"\";\r\n\r\n  fases: any = [];\r\n  linhaProducao: any = {\r\n    id: 0,\r\n    nome: \"\",\r\n    qtdeCapacidadeOperadoresLinha: \"\",\r\n    fases: [],\r\n    dataCadastro: new Date(),\r\n    deletado: \"N\",\r\n  };\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  headers = new Headers([\r\n    new Header(\"Sigla\", \"sigla\", false, false, \"left\", false, \"100\"),\r\n    new Header(\"Descrição\", \"descricao\", false, false, \"left\", false, \"200\"),\r\n    new Header(\"Ação\", \"acao\", false, false, \"center\", false, \"100\"),\r\n  ]);\r\n\r\n  indexRemoverSetor = 0;\r\n\r\n  removerSetor() {\r\n    for (const { item, index } of this.linhaProducao.fases.map(\r\n      (item: any, index: any) => ({\r\n        item,\r\n        index,\r\n      })\r\n    )) {\r\n      if (index == this.indexRemoverSetor) {\r\n        this.linhaProducao.fases.splice(index, 1);\r\n        this.indexRemoverSetor = -1;\r\n        this.dialogDelete = false;\r\n        return;\r\n      }\r\n    }\r\n  }\r\n\r\n  modalRemoverSetor(indexRemoveItem: string) {\r\n    this.indexRemoverSetor = parseInt(indexRemoveItem);\r\n    this.dialogDelete = true;\r\n  }\r\n\r\n  cancelaRemoverSetor() {\r\n    this.indexRemoverSetor = -1;\r\n    this.dialogDelete = false;\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/centrosCustos\");\r\n    }\r\n  }\r\n\r\n  retornaListaFuncionarios() {\r\n    this.$router.push(\"/centrosCustos\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    return false;\r\n  }\r\n\r\n  saveLinhaProducaoSetores() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.salvar(this.linhaProducao);\r\n    }\r\n  }\r\n\r\n  updateLinhaProducaoSetores() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.update(this.linhaProducao.id, this.linhaProducao);\r\n    }\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.idLinhaProducao = this.$route.params.id;\r\n  }\r\n\r\n  carregaFases(itemsFases: any): void {\r\n    this.itemsFases = itemsFases;\r\n  }\r\n\r\n  showLinhaProducaoForSave(item: any) {\r\n    this.linhaProducao.nome = item.nome;\r\n    this.linhaProducao.descricao = item.descricao;\r\n  }\r\n\r\n  showLinhaProducaoForEditar(item: any) {\r\n    this.linhaProducao = item;\r\n\r\n    item.forEach((element: any) => {\r\n      this.linhaProducao.fases.push(element.fasesDTO);\r\n    });\r\n  }\r\n\r\n  carregaSetores(item: any) {\r\n    //CARREGANDO DADOS DO FUNCIONARIO\r\n    this.itemsFases = item;\r\n  }\r\n\r\n  validateAddSetor() {\r\n    if (this.fases.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o setor para adicionar.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    } else {\r\n      let flag = false;\r\n      this.linhaProducao.fases.forEach((element: any) => {\r\n        if (element.id == this.fases.id) {\r\n          this.colorBorder = \"warning\";\r\n          this.color = \"warning\";\r\n          this.text = \"Setor já adicionado.\";\r\n          this.fases = [];\r\n          this.snackbar = true;\r\n          flag = true;\r\n        }\r\n      });\r\n\r\n      return flag;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/centroCusto\");\r\n  }\r\n\r\n  customFilterSetor(item: any, queryText: any, itemText: any) {\r\n    const textOne = item.sigla.toLowerCase();\r\n    const textTwo = item.descricao.toLowerCase();\r\n    const searchText = queryText.toLowerCase();\r\n\r\n    return textOne.indexOf(searchText) > -1 || textTwo.indexOf(searchText) > -1;\r\n  }\r\n\r\n  addSetor() {\r\n    if (this.validateAddSetor()) {\r\n    } else {\r\n      this.linhaProducao.fases.push(this.fases);\r\n      this.fases = [];\r\n    }\r\n  }\r\n\r\n  created() {\r\n    this._controller = new LinhaProducaoFormController(this);\r\n    this._controller.carregarDtos(this.$route.params.codigo);\r\n    this.carregarDtos();\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n</script> \r\n\r\n<style scoped>\r\n/deep/ .centered-input input {\r\n  text-align: center\r\n}\r\n</style>"]}]}