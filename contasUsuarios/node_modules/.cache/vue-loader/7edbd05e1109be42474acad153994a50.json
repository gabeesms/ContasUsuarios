00,\r\n      value: \"\",\r\n    },\r\n    {\r\n      text: \"Operadores\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 400,\r\n      value: \"\",\r\n    },\r\n    {\r\n      text: \"Qtde de Operadores\",\r\n      align: \"center\",\r\n      width: 100,\r\n      sortable: false,\r\n      value: \"\",\r\n    },\r\n    { text: \"Time\", align: \"center\", sortable: false, width: 100, value: \"\" },\r\n    {\r\n      text: \"Time Total\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 100,\r\n      value: \"\",\r\n    },\r\n  ];\r\n\r\n  headersLinha = [\r\n    {\r\n      text: \"Nome\",\r\n      align: \"start\",\r\n      sortable: false,\r\n      value: \"fase\",\r\n    },\r\n    { text: \"Ação\", align: \"right\", value: \"acao\" },\r\n    { text: \"\", align: \"right\", value: \"data-table-expand\" },\r\n  ];\r\n\r\n  itensCapacidadeLinha: any = [];\r\n  itensRecursoTecnicoLinha: any = [];\r\n  itensOperadores: any = [];\r\n\r\n  idOrdemProducaoLinha = \"\";\r\n  idOrdemProducaoPlano = \"\";\r\n\r\n  ordemProducaoResponse: any = {};\r\n\r\n  codigoDescricao = \"\";\r\n  qtdeOperador = 0;\r\n  quantidade = 0;\r\n  nome = \"\";\r\n  quantidadeOperador = 0;\r\n  capacidadeProdutiva = 0;\r\n  numOrdemProducao = \"-\";\r\n\r\n  ordemProducao = {\r\n    dataCadastro: new Date(),\r\n    status: \"S\",\r\n    segunda: false,\r\n    terca: false,\r\n    quarta: false,\r\n    quinta: false,\r\n    sexta: false,\r\n    sabado: false,\r\n    domingo: false,\r\n    producao: 0,\r\n    metaTurno: \"\",\r\n    linha: {\r\n      idLinha: 0,\r\n      capacidadeLinha: this.itensCapacidadeLinha,\r\n      recursoTecnicoLinha: this.itensRecursoTecnicoLinha,\r\n    },\r\n\r\n    idOrdemProducao: 0,\r\n    idOrdemProducaoLinha: 0,\r\n    idProduto: 0,\r\n    turnos: [],\r\n  };\r\n\r\n  valid = true;\r\n\r\n  customFilterOperador(item: any, queryText: any, itemText: any) {\r\n    const textOne = item.nome.toLowerCase();\r\n    const textTwo = item.nome.toLowerCase();\r\n    const searchText = queryText.toLowerCase();\r\n\r\n    return textOne.indexOf(searchText) > -1 || textTwo.indexOf(searchText) > -1;\r\n  }\r\n\r\n  carregaTurnos(itemsTurno: any): void {\r\n    this.itensTurno = itemsTurno;\r\n\r\n    this.itensTurno.forEach((element: any) => {\r\n      element.textoDescHora =\r\n        element.descricao + \" - HORAS TOTAL: \" + element.horaTotal;\r\n    });\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/ordensProducao\");\r\n    }\r\n  }\r\n\r\n  retornaListaOrdensProducao() {\r\n    this.$router.push(\"/ordensProducao\");\r\n  }\r\n\r\n  retornaListaOrdensProducaoSimulacao() {\r\n    this.$router.push(\"/ordensProducao\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    if (\r\n      this.ordemProducao.turnos == null ||\r\n      this.ordemProducao.turnos.length == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text =\r\n        \"Selecione o Turno em que deseja realizar a Ordem de Produção\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.ordemProducao.segunda == false &&\r\n      this.ordemProducao.terca == false &&\r\n      this.ordemProducao.quarta == false &&\r\n      this.ordemProducao.quinta == false &&\r\n      this.ordemProducao.sexta == false &&\r\n      this.ordemProducao.sabado == false &&\r\n      this.ordemProducao.domingo == false\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text =\r\n        \"Selecione ao menos um dia da semana para realizar a Ordem de Produção\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    //verifica se ha operadores para a capacidade de linha\r\n    for (let i = 0; i < this.itensCapacidadeLinha.length; i++) {\r\n      let verificador = false;\r\n      for (\r\n        let j = 0;\r\n        j < this.itensCapacidadeLinha[i].linhaCapacidadeHabilidades.length;\r\n        j++\r\n      ) {\r\n        if (\r\n          this.itensCapacidadeLinha[i].linhaCapacidadeHabilidades[j]\r\n            .operadores == null\r\n        ) {\r\n          this.colorBorder = \"error\";\r\n          this.color = \"error\";\r\n          this.text =\r\n            \"Selecione o(s) operador(es) da Capacidade de Linha: \" +\r\n            this.itensCapacidadeLinha[i].processoDTOOperacao.nome;\r\n          this.snackbar = true;\r\n          verificador = true;\r\n          break;\r\n        }\r\n      }\r\n\r\n      if (verificador == true) {\r\n        return true;\r\n        break;\r\n      }\r\n    }\r\n\r\n    //verifica operadores para a recurso tecnico\r\n    for (let i = 0; i < this.itensRecursoTecnicoLinha.length; i++) {\r\n      let verificador = false;\r\n      for (\r\n        let j = 0;\r\n        j <\r\n        this.itensRecursoTecnicoLinha[i].linhaRecursoTecnicoHabilidades.length;\r\n        j++\r\n      ) {\r\n        if (\r\n          this.itensRecursoTecnicoLinha[i].linhaRecursoTecnicoHabilidades[j]\r\n            .operadores == null\r\n        ) {\r\n          this.colorBorder = \"error\";\r\n          this.color = \"error\";\r\n          this.text =\r\n            \"Selecione o(s) operador(es) do Recurso Técnico: \" +\r\n            this.itensRecursoTecnicoLinha[i].fase;\r\n          this.snackbar = true;\r\n          verificador = true;\r\n          break;\r\n        }\r\n      }\r\n\r\n      if (verificador == true) {\r\n        return true;\r\n        break;\r\n      }\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  saveOrdemProducao(tipo: string) {\r\n    this.ordemProducao.linha.idLinha = this.ordemProducaoResponse.linha.id;\r\n    this.ordemProducao.linha.capacidadeLinha = this.itensCapacidadeLinha;\r\n    this.ordemProducao.linha.recursoTecnicoLinha = this.itensRecursoTecnicoLinha;\r\n    this.ordemProducao.idOrdemProducao = this.ordemProducaoResponse.ordemProducao.id;\r\n    this.ordemProducao.idOrdemProducaoLinha = this.ordemProducaoResponse.ordemProducaoLinha.id;\r\n    this.ordemProducao.idProduto = this.ordemProducaoResponse.produto.id;\r\n    this.ordemProducao.producao = this.ordemProducaoResponse.ordemProducao.quantidade;\r\n\r\n    this.ordemProducao.status = tipo == \"S\" ? \"S\" : \"P\";\r\n\r\n    if (!this.validate()) {\r\n      this._controller!.salvar(this.ordemProducao);\r\n      // this._controller!.update(this.idOrdemProducao, this.ordemProducao);\r\n      //   this.retornaListaOrdensProducao();\r\n    }\r\n  }\r\n\r\n  updateOrdemProducao() {\r\n    this.ordemProducao.linha.idLinha = this.ordemProducaoResponse.linha.id;\r\n    this.ordemProducao.linha.capacidadeLinha = this.itensCapacidadeLinha;\r\n    this.ordemProducao.linha.recursoTecnicoLinha = this.itensRecursoTecnicoLinha;\r\n    this.ordemProducao.idOrdemProducao = this.ordemProducaoResponse.ordemProducao.id;\r\n    this.ordemProducao.idOrdemProducaoLinha = this.ordemProducaoResponse.ordemProducaoLinha.id;\r\n    this.ordemProducao.idProduto = this.ordemProducaoResponse.produto.id;\r\n    this.ordemProducao.producao = this.ordemProducaoResponse.ordemProducao.quantidade;\r\n\r\n    // if (!this.validate()) {\r\n    // } else {\r\n    //this._controller!.salvar(this.ordemProducao);\r\n    //this._controller!.update(this.idOrdemProducao, this.ordemProducao);\r\n    //   this.retornaListaOrdensProducao();\r\n    // }\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.idOrdemProducaoLinha = this.$route.params.idOrdemProducaoLinha;\r\n    this.idOrdemProducaoPlano = this.$route.params.idOrdemProducaoPlano;\r\n\r\n    if (this.$route.params.idOrdemProducaoPlano) {\r\n      this.disabledCampos = true;\r\n    } else {\r\n      this.disabledCampos = false;\r\n    }\r\n  }\r\n\r\n  showOrdemProducaoForEditar(item: any) {\r\n    this.loading(true);\r\n    this.ordemProducaoResponse = item;\r\n\r\n    this.itensCapacidadeLinha = this.ordemProducaoResponse.linha.linhaCapacidade;\r\n\r\n    this.itensRecursoTecnicoLinha = this.ordemProducaoResponse.linha.recursoTecnico;\r\n\r\n    this.ordemProducao = this.ordemProducaoResponse.ordemProducaoPlano;\r\n\r\n    this.ordemProducao.turnos = this.ordemProducaoResponse.ordemProducaoTurnos[0];\r\n\r\n    this.codigoDescricao =\r\n      this.ordemProducaoResponse.produto.codigo.toString().trim() +\r\n      \" - \" +\r\n      this.ordemProducaoResponse.produto.descricao.toString().trim();\r\n    this.qtdeOperador = this.ordemProducaoResponse.produto.qtdeOperador;\r\n    this.quantidade = this.ordemProducaoResponse.ordemProducao.quantidade;\r\n    this.nome = this.ordemProducaoResponse.linha.nome;\r\n    this.quantidadeOperador = this.ordemProducaoResponse.linha.quantidadeOperador;\r\n    this.capacidadeProdutiva = this.ordemProducaoResponse.linha.capacidadeProdutiva;\r\n    this.numOrdemProducao = this.ordemProducaoResponse.ordemProducao.numOrdemProducao;\r\n\r\n    //instanciando oeperadores em capacidade da linha\r\n    this.itensCapacidadeLinha.forEach((element: any) => {\r\n      element.linhaCapacidadeHabilidades.forEach((element2: any) => {\r\n        element2.operadores = [];\r\n      });\r\n    });\r\n\r\n    //instanciando oeperadores em recursos tecnico\r\n    this.itensRecursoTecnicoLinha.forEach((element: any) => {\r\n      element.linhaRecursoTecnicoHabilidades.forEach((element2: any) => {\r\n        element2.operadores = [];\r\n      });\r\n    });\r\n\r\n    //convertendo time para time imput\r\n    this.itensCapacidadeLinha.forEach((elementCapLinha: any) => {\r\n      elementCapLinha.linhaCapacidadeHabilidades.forEach(\r\n        (elementCapLinhaHabilidade: any) => {\r\n          elementCapLinhaHabilidade.time = new Date(\r\n            elementCapLinhaHabilidade.time\r\n          )\r\n            .toLocaleTimeString()\r\n            .toString();\r\n\r\n          elementCapLinhaHabilidade.ordemProducaoOperadorLinhaCapacidade.forEach(\r\n            (element: any) => {\r\n              elementCapLinhaHabilidade.operadores.push(\r\n                element.funcionarioErpDTO\r\n              );\r\n            }\r\n          );\r\n        }\r\n      );\r\n    });\r\n\r\n    //convertendo time para time imput\r\n    this.itensRecursoTecnicoLinha.forEach((elementRecursoTecnico: any) => {\r\n      elementRecursoTecnico.linhaRecursoTecnicoHabilidades.forEach(\r\n        (elementRecursoTecnicoHabilidade: any) => {\r\n          elementRecursoTecnicoHabilidade.time = new Date(\r\n            elementRecursoTecnicoHabilidade.time\r\n          )\r\n            .toLocaleTimeString()\r\n            .toString();\r\n\r\n          elementRecursoTecnicoHabilidade.ordemProducaoOperadorRecursoTecnico.forEach(\r\n            (element: any) => {\r\n              elementRecursoTecnicoHabilidade.operadores.push(\r\n                element.funcionarioErpDTO\r\n              );\r\n            }\r\n          );\r\n        }\r\n      );\r\n    });\r\n\r\n    this.loading(false);\r\n  }\r\n\r\n  showOrdemProducaoForNovo(item: any) {\r\n    this.loading(true);\r\n    this.ordemProducaoResponse = item;\r\n\r\n    this.itensCapacidadeLinha = this.ordemProducaoResponse.linha.linhaCapacidade;\r\n\r\n    this.itensRecursoTecnicoLinha = this.ordemProducaoResponse.linha.recursoTecnico;\r\n\r\n    this.codigoDescricao =\r\n      this.ordemProducaoResponse.produto.codigo.toString().trim() +\r\n      \" - \" +\r\n      this.ordemProducaoResponse.produto.descricao.toString().trim();\r\n    this.qtdeOperador = this.ordemProducaoResponse.produto.qtdeOperador;\r\n    this.quantidade = this.ordemProducaoResponse.ordemProducao.quantidade;\r\n    this.nome = this.ordemProducaoResponse.linha.nome;\r\n    this.quantidadeOperador = this.ordemProducaoResponse.linha.quantidadeOperador;\r\n    this.capacidadeProdutiva = this.ordemProducaoResponse.linha.capacidadeProdutiva;\r\n    this.numOrdemProducao = this.ordemProducaoResponse.ordemProducao.numOrdemProducao;\r\n\r\n    //instanciando oeperadores em capacidade da linha\r\n    this.itensCapacidadeLinha.forEach((element: any) => {\r\n      element.linhaCapacidadeHabilidades.forEach((element2: any) => {\r\n        element2.operadores = null;\r\n      });\r\n    });\r\n\r\n    //instanciando oeperadores em recursos tecnico\r\n    this.itensRecursoTecnicoLinha.forEach((element: any) => {\r\n      element.linhaRecursoTecnicoHabilidades.forEach((element2: any) => {\r\n        element2.operadores = null;\r\n      });\r\n    });\r\n\r\n    //convertendo time para time imput\r\n    this.itensCapacidadeLinha.forEach((elementCapLinha: any) => {\r\n      elementCapLinha.linhaCapacidadeHabilidades.forEach(\r\n        (elementCapLinhaHabilidade: any) => {\r\n          elementCapLinhaHabilidade.time = new Date(\r\n            elementCapLinhaHabilidade.time\r\n          )\r\n            .toLocaleTimeString()\r\n            .toString();\r\n        }\r\n      );\r\n    });\r\n\r\n    //convertendo time para time imput\r\n    this.itensRecursoTecnicoLinha.forEach((elementRecursoTecnico: any) => {\r\n      elementRecursoTecnico.linhaRecursoTecnicoHabilidades.forEach(\r\n        (elementRecursoTecnicoHabilidade: any) => {\r\n          elementRecursoTecnicoHabilidade.time = new Date(\r\n            elementRecursoTecnicoHabilidade.time\r\n          )\r\n            .toLocaleTimeString()\r\n            .toString();\r\n        }\r\n      );\r\n    });\r\n\r\n    this.loading(false);\r\n  }\r\n\r\n  carregaOperadores(operadores: any): void {\r\n    this.itensOperadores = operadores;\r\n  }\r\n\r\n  created() {\r\n    this._controller = new OrdemProducaoFormController(this);\r\n    this._controller.carregarDtos(\r\n      this.$route.params.idOrdemProducaoLinha,\r\n      this.$route.params.idOrdemProducaoPlano\r\n    );\r\n    this.carregarDtos();\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n</script> \r\n\r\n<style scoped>\r\n.centered-input >>> input {\r\n  text-align: right;\r\n}\r\n\r\n.mytable table td {\r\n  border-bottom: 1px solid rgba(0, 0, 0, 0.12);\r\n}\r\n</style>"]}]}