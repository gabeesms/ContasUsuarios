d-color: #313c42\">\r\n          <span style=\"color: white\" class=\"headline\">\r\n            Inserir Observação\r\n          </span>\r\n        </v-card-title>\r\n        <v-card-text>\r\n          <v-container>\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <span>\r\n                  <b>Dados Gerais do Evento</b>\r\n                </span>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row style=\"margin-top: -10px\">\r\n              <v-col cols=\"12\" sm=\"6\">\r\n                <v-text-field\r\n                  outlined\r\n                  v-model=\"itemSelecionado.dataFormatada\"\r\n                  label=\"Data de Inconsistência \"\r\n                  dense\r\n                  required\r\n                  append-icon=\"mdi-calendar\"\r\n                  disabled\r\n                ></v-text-field>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row style=\"margin-top: -20px\">\r\n              <v-col cols=\"12\" sm=\"3\">\r\n                <v-text-field\r\n                  outlined\r\n                  v-model=\"itemSelecionado.priENT\"\r\n                  label=\"Primeira Entrada \"\r\n                  dense\r\n                  required\r\n                  disabled\r\n                ></v-text-field>\r\n              </v-col>\r\n\r\n              <v-col cols=\"12\" sm=\"3\">\r\n                <v-text-field\r\n                  outlined\r\n                  v-model=\"itemSelecionado.priSAI\"\r\n                  label=\"Primeira Saída \"\r\n                  dense\r\n                  required\r\n                  disabled\r\n                ></v-text-field>\r\n              </v-col>\r\n\r\n              <v-col cols=\"12\" sm=\"3\">\r\n                <v-text-field\r\n                  outlined\r\n                  v-model=\"itemSelecionado.segENT\"\r\n                  label=\"Segunda Entrada \"\r\n                  dense\r\n                  required\r\n                  disabled\r\n                ></v-text-field>\r\n              </v-col>\r\n\r\n              <v-col cols=\"12\" sm=\"3\">\r\n                <v-text-field\r\n                  outlined\r\n                  v-model=\"itemSelecionado.segSAI\"\r\n                  label=\"Segunda Saída \"\r\n                  dense\r\n                  required\r\n                  disabled\r\n                ></v-text-field>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row style=\"margin-top: -20px\">\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <v-text-field\r\n                  outlined\r\n                  v-model=\"itemSelecionado.motivo\"\r\n                  label=\"Motivo\"\r\n                  placeholder=\"Descreva o motivo\"\r\n                  dense\r\n                  required\r\n                ></v-text-field>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row style=\"margin-top: -20px\">\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <v-textarea\r\n                  outlined\r\n                  v-model=\"itemSelecionado.observacao\"\r\n                  label=\"Observação\"\r\n                  placeholder=\"Descreva a Observação\"\r\n                  dense\r\n                  required\r\n                ></v-textarea>\r\n              </v-col>\r\n            </v-row>\r\n          </v-container>\r\n        </v-card-text>\r\n        <v-card-actions style=\"margin-top: -50px\">\r\n          <v-spacer></v-spacer>\r\n          <v-btn\r\n            color=\"orange lighten-1\"\r\n            width=\"150\"\r\n            @click=\"cancelarInserirObservacao\"\r\n            dark\r\n            >Cancelar</v-btn\r\n          >\r\n          <v-btn\r\n            color=\"orange darken-1\"\r\n            width=\"150\"\r\n            dark\r\n            @click=\"saveEspelhoPonto\"\r\n            >Salvar</v-btn\r\n          >\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n          <template v-slot:item=\"{ item }\">\r\n            <router-link :to=\"item.href\">\r\n              {{ item.text }}\r\n            </router-link>\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n\r\n      <v-row ref=\"form\">\r\n        <v-col cols=\"12\" v-if=\"!idFuncionario\">\r\n          <h3>\r\n            <b>Espelho de Ponto</b>\r\n          </h3>\r\n        </v-col>\r\n\r\n        <v-col cols=\"12\" v-if=\"idFuncionario\">\r\n          <h3>\r\n            <b>Editar Perfil</b>\r\n          </h3>\r\n          <span>\r\n            <b>Alterando dados do perfil no sistema</b>\r\n          </span>\r\n        </v-col>\r\n\r\n        <v-col cols=\"12\">\r\n          <span>\r\n            <b>Funcionário</b>\r\n          </span>\r\n        </v-col>\r\n\r\n        <v-col cols=\"12\" sm=\"2\">\r\n          <v-text-field\r\n            outlined\r\n            v-model=\"espelhoPonto.funcionario.matricula\"\r\n            label=\"Matrícula\"\r\n            dense\r\n            required\r\n            disabled\r\n          ></v-text-field>\r\n        </v-col>\r\n\r\n        <v-col cols=\"12\" sm=\"10\">\r\n          <v-text-field\r\n            outlined\r\n            v-model=\"espelhoPonto.funcionario.nome\"\r\n            label=\"Nome Completo\"\r\n            dense\r\n            required\r\n            disabled\r\n          ></v-text-field>\r\n        </v-col>\r\n\r\n        <v-col cols=\"12\" sm=\"5\">\r\n          <v-text-field\r\n            outlined\r\n            v-model=\"espelhoPonto.funcionario.setor\"\r\n            label=\"Departamento\"\r\n            dense\r\n            required\r\n            disabled\r\n          ></v-text-field>\r\n        </v-col>\r\n\r\n        <v-col cols=\"12\" sm=\"3\">\r\n          <v-text-field\r\n            outlined\r\n            v-model=\"espelhoPonto.funcionario.sexo\"\r\n            label=\"Categoria\"\r\n            dense\r\n            required\r\n            disabled\r\n          ></v-text-field>\r\n        </v-col>\r\n\r\n        <v-col cols=\"12\" sm=\"4\">\r\n          <v-text-field\r\n            outlined\r\n            v-model=\"espelhoPonto.funcionario.cargo\"\r\n            label=\"Função\"\r\n            dense\r\n            required\r\n            disabled\r\n          ></v-text-field>\r\n        </v-col>\r\n\r\n        <!-- <v-row style=\"margin-top: -10px\" align=\"center\" justify=\"center\"> -->\r\n        <v-col cols=\"12\">\r\n          <v-data-table\r\n            :headers=\"headers\"\r\n            :items=\"itens\"\r\n            hide-default-footer\r\n            style=\"margin-top: 15px\"\r\n            class=\"elevation-1 mytable\"\r\n          >\r\n            <template v-slot:item.acao=\"{ item }\">\r\n              <v-icon\r\n                small\r\n                color=\"primary\"\r\n                title=\"Inserir Motivo Observação\"\r\n                @click=\"inserirObservacao(item)\"\r\n              >\r\n                mdi-pencil\r\n              </v-icon>\r\n            </template>\r\n          </v-data-table>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template> \r\n\r\n\r\n\r\n<script lang=\"ts\">\r\nvar validaNumeros = require(\"../../baseGenerica/validaNumeros.js\");\r\n\r\nimport Vue from \"vue\";\r\n\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport moment from \"moment\";\r\n\r\nimport {\r\n  IEspelhoPontoFormView,\r\n  IEspelhoPontoFormController,\r\n} from \"../../contracts/EspelhoPonto/EspelhoPontoFormContract\";\r\nimport { EspelhoPontoFormController } from \"../../controllers/EspelhoPonto/EspelhoPontoFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class EspelhoPontoForm\r\n  extends Vue\r\n  implements IEspelhoPontoFormView {\r\n  _controller?: IEspelhoPontoFormController;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Espelho de Ponto\",\r\n      disabled: false,\r\n      href: \"/espelhosPonto\",\r\n    },\r\n  ];\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n  itens: any = [];\r\n\r\n  //calendario de datas\r\n  //date = new Date().toISOString().substr(0, 10);\r\n  //date2 = new Date().toISOString().substr(0, 10);\r\n  //dateFormatted = this.formatDate(new Date().toISOString().substr(0, 10));\r\n  //dateFormatted2 = this.formatDate(new Date().toISOString().substr(0, 10));\r\n\r\n  //menu2 = false;\r\n  menu1 = false;\r\n  dataInconsistencia = this.formatDate(new Date().toISOString().substr(0, 10));\r\n  date = new Date().toISOString().substr(0, 10);\r\n\r\n  maskCPF = \"###.###.###-##\";\r\n  maskCEP = \"#####-###\";\r\n  maskCel = \"(92)#####-####\";\r\n  maskDatas = \"##/##/####\";\r\n  maskDatas2 = \"##/##/####\";\r\n  maskPIS = \"###.#####.##-#\";\r\n\r\n  //SITUAÇÃO FUNCIONARIO\r\n  itemsSitFuncionario = [];\r\n\r\n  //CARGO FUNCIONARIO\r\n  itemsCargoFuncionario = [];\r\n\r\n  //CARGO FUNCIONARIO\r\n  itemsSetorFuncionario = [];\r\n\r\n  //CATEGORIA FUNCIONARIO\r\n  itemsCategoriaFuncionario = [];\r\n\r\n  //TURNO FUNCIONARIO\r\n  itemsTurnoFuncionario = [];\r\n\r\n  dataVencimentoMenu = false;\r\n  dataNecessidadeMenu = false;\r\n  dataNecessidadeItemMenu = false;\r\n\r\n  isMobile = false;\r\n\r\n  dialogInserirObservacao = false;\r\n\r\n  isSucesso = false;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  matricula = \"\";\r\n\r\n  idFuncionario = \"\";\r\n\r\n  itemSelecionado: any = [];\r\n\r\n  espelhoPonto = {\r\n    funcionario: {\r\n      cargo: \"\",\r\n      cbo: \"\",\r\n      cpf: \"\",\r\n      ctps: \"\",\r\n      deficiente: \"\",\r\n      dtAdmissao: \"\",\r\n      dtDemissao: new Date().toISOString().substr(0, 10),\r\n      dtNascimento: new Date().toISOString().substr(0, 10),\r\n      dtVencimentoExperiencia: undefined,\r\n      estadoCivil: \"\",\r\n\r\n      idFuncao: \"\",\r\n      idSetor: \"\",\r\n      idTipoMaoObra: \"\",\r\n      idTurno: \"\",\r\n      matricula: \"\",\r\n      nome: \"\",\r\n      pis: \"\",\r\n      setor: \"\",\r\n      sexo: \"\",\r\n      situacao: \"\",\r\n      staff: \"\",\r\n      tipoMaoObra: \"\",\r\n      turno: \"\",\r\n      nomeMae: \"\",\r\n      cep: \"\",\r\n      endereco: \"\",\r\n      bairro: \"\",\r\n      municipio: \"\",\r\n      estado: \"\",\r\n      celular: \"\",\r\n      deletado: \"\",\r\n    },\r\n    listaEspelhoPontoErpDTO: [],\r\n  };\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  headers = [\r\n    {\r\n      text: \"Data\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 100,\r\n      value: \"dataFormatada\",\r\n    },\r\n    {\r\n      text: \"Dia\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 100,\r\n      value: \"diaSemana\",\r\n    },\r\n    {\r\n      text: \"1a E.\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 80,\r\n      value: \"priENT\",\r\n    },\r\n    {\r\n      text: \"1a S.\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 80,\r\n      value: \"priSAI\",\r\n    },\r\n    {\r\n      text: \"2a E.\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 80,\r\n      value: \"segENT\",\r\n    },\r\n    {\r\n      text: \"2a S.\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 80,\r\n      value: \"segSAI\",\r\n    },\r\n    {\r\n      text: \"Motivo\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 100,\r\n      value: \"motivo\",\r\n    },\r\n    {\r\n      text: \"Observação\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 100,\r\n      value: \"observacao\",\r\n    },\r\n    {\r\n      text: \"Ações\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 50,\r\n      value: \"acao\",\r\n    },\r\n  ];\r\n\r\n  limparCampos(): void {\r\n    this.itemSelecionado.motivo = \"\",\r\n    this.itemSelecionado.observacao = \"\";\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  formatDate(date: string) {\r\n    if (!date) return null;\r\n\r\n    const [year, month, day] = date.split(\"-\");\r\n    return `${day}/${month}/${year}`;\r\n  }\r\n\r\n  parseDate(date: string) {\r\n    if (!date) return null;\r\n    const [day, month, year] = date.split(\"/\");\r\n    return `${year}-${month.padStart(2, \"0\")}-${day.padStart(2, \"0\")}`;\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/espelhosPonto\");\r\n    }\r\n  }\r\n\r\n  retornaListaFuncionarios() {\r\n    this.$router.push(\"/espelhosPonto\");\r\n  }\r\n\r\n  cancelarInserirObservacao() {\r\n    this.limparCampos();\r\n    this.dialogInserirObservacao = false;\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    if (\r\n      this.itemSelecionado.motivo == \"\" ||\r\n      this.itemSelecionado.motivo.length > 50\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"O campo Motivo está vazio ou inválido.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.itemSelecionado.observacao == \"\" ||\r\n      this.itemSelecionado.observacao.length > 200\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"O campo Observação está vazio ou inválido.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n  }\r\n\r\n  saveEspelhoPonto() {\r\n    if (!this.validate()) {\r\n      this._controller!.salvar(this.itemSelecionado);\r\n    }\r\n  }\r\n\r\n  // updateFuncionario() {\r\n  //   if (!this.validate()) {\r\n  //   } else {\r\n  //     //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n  //     //this._controller!.update(this.idFuncionario, this.funcionario);\r\n  //     this.retornaListaFuncionarios();\r\n  //   }\r\n  // }\r\n\r\n  carregarDtos(): void {\r\n    //this.idFuncionario = this.$route.params.id;\r\n    this.matricula = this.$route.params.matricula;\r\n  }\r\n\r\n  showEspelhoPontoForEditar(item: any) {\r\n    //CARREGANDO DADOS DO FUNCIONARIO\r\n\r\n    this.espelhoPonto.funcionario = item.funcionarioErpDTO;\r\n    this.itens = item.listaEspelhoPontoErpDTO;\r\n\r\n  }\r\n\r\n  inserirObservacao(item: any) {\r\n    this.dialogInserirObservacao = true;\r\n    this.itemSelecionado = item;\r\n\r\n    // console.log(item);\r\n  }\r\n\r\n  created() {\r\n    this._controller = new EspelhoPontoFormController(this);\r\n    //this.idFuncionario = this.$route.params.id;\r\n    this._controller.carregarDtos(\r\n      this.$route.params.matricula,\r\n      this.$route.params.dtInicio,\r\n      this.$route.params.dtFinal\r\n    );\r\n    this.carregarDtos();\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n\r\n  @Watch(\"date\")\r\n  onDatasChanged(val: string) {\r\n    this.dataInconsistencia = this.formatDate(this.date);\r\n  }\r\n\r\n  // @Watch(\"date2\")\r\n  // onDatas2Changed(val: string) {\r\n  //   this.dateFormatted2 = this.formatDate(this.date2);\r\n  // }\r\n}\r\n</script> "]}]}