{"remainingRequest":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\src\\views\\Linha\\LinhaEmprestimoForm.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\src\\views\\Linha\\LinhaEmprestimoForm.vue","mtime":1615206708758},{"path":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["LinhaEmprestimoForm.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAggBA;AACA;AACA;AACA;AACA;AACA;AACA","file":"LinhaEmprestimoForm.vue","sourceRoot":"src/views/Linha","sourcesContent":["<template>\r\n  <v-content style=\"margin: 10px\">\r\n    <v-snackbar\r\n      v-model=\"snackbar\"\r\n      :color=\"colorBorder\"\r\n      shaped\r\n      timeout=\"4000\"\r\n      :vertical=\"vertical\"\r\n    >\r\n      <b>{{ text }}</b>\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn v-bind=\"attrs\" @click=\"snackbar = false\" icon dark small>\r\n          <v-icon>mdi-close</v-icon>\r\n        </v-btn>\r\n        <!-- <v-btn icon v-bind=\"attrs\" @click=\"snackbar = false\">mdi-magnify</v-btn> -->\r\n      </template>\r\n    </v-snackbar>\r\n\r\n    <k-loading title=\"Carregando\" :dialog=\"dialogLoading\"></k-loading>\r\n\r\n    <k-dialog\r\n      @fecharDialog=\"fecharDialogMessage\"\r\n      :dialog=\"dialogMessage\"\r\n      title=\"Mensagem\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n\r\n    <v-container>\r\n      <v-row ref=\"form\">\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-row>\r\n            <h3>\r\n              <b>Solicitações de Empréstimo</b>\r\n            </h3>\r\n          </v-row>\r\n\r\n          <br />\r\n\r\n          <v-row>\r\n            <!-- LINHA ATUAL -->\r\n            <v-col cols=\"12\" sm=\"12\">\r\n              <v-row Style=\"margin-top: -15px\">\r\n                <v-col cols=\"12\" sm=\"4\">\r\n                  <v-select\r\n                    :items=\"itemsLinha\"\r\n                    return-object\r\n                    v-model=\"linhaSelecionada\"\r\n                    @input=\"changeHabilidades()\"\r\n                    label=\"Linha *\"\r\n                    placeholder=\"Selecione a Linha *\"\r\n                    autocomplete=\"off\"\r\n                    item-text=\"nome\"\r\n                    item-value=\"id\"\r\n                    outlined\r\n                    dense\r\n                  ></v-select>\r\n                </v-col>\r\n\r\n                <v-col cols=\"12\" sm=\"4\">\r\n                  <v-select\r\n                    :items=\"itemsHabilidade\"\r\n                    return-object\r\n                    v-model=\"habilidadeSelecionada\"\r\n                    label=\"Habilidade *\"\r\n                    placeholder=\"Selecione a Habilidade *\"\r\n                    autocomplete=\"off\"\r\n                    item-text=\"nome\"\r\n                    item-value=\"id\"\r\n                    outlined\r\n                    dense\r\n                  ></v-select>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"3\">\r\n                  <v-text-field\r\n                    label=\"Quantidade de Operadores *\"\r\n                    placeholder=\"Informe a qtde de operadores\"\r\n                    v-model=\"qtdeOperadores\"\r\n                    outlined\r\n                    dense\r\n                    @keypress=\"apenasNumeros\"\r\n                  >\r\n                  </v-text-field>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"1\">\r\n                  <v-btn\r\n                    style=\"float: right\"\r\n                    small\r\n                    color=\"#FB9514\"\r\n                    fab\r\n                    dark\r\n                    @click=\"addSolicitacaoEmprestimo()\"\r\n                  >\r\n                    <v-icon dark>mdi-plus</v-icon>\r\n                  </v-btn>\r\n                </v-col>\r\n              </v-row>\r\n            </v-col>\r\n            <v-col cols=\"12\" sm=\"4\">\r\n              <v-menu\r\n                ref=\"menu1\"\r\n                v-model=\"menu1\"\r\n                :close-on-content-click=\"false\"\r\n                transition=\"scale-transition\"\r\n                offset-y\r\n                max-width=\"290px\"\r\n                min-width=\"290px\"\r\n              >\r\n                <template v-slot:activator=\"{ on, attrs }\">\r\n                  <v-text-field\r\n                    v-model=\"dateFormatted\"\r\n                    label=\"Período Início\"\r\n                    persistent-hint\r\n                    v-bind=\"attrs\"\r\n                    @blur=\"date = parseDate(dateFormatted)\"\r\n                    v-on=\"on\"\r\n                    outlined\r\n                    dense\r\n                    v-mask=\"maskDatas\"\r\n                  ></v-text-field>\r\n                </template>\r\n                <v-date-picker\r\n                  v-model=\"date\"\r\n                  no-title\r\n                  @input=\"menu1 = false\"\r\n                ></v-date-picker>\r\n              </v-menu>\r\n            </v-col>\r\n\r\n            <v-col cols=\"12\" sm=\"4\">\r\n              <v-menu\r\n                ref=\"menu2\"\r\n                v-model=\"menu2\"\r\n                :close-on-content-click=\"false\"\r\n                transition=\"scale-transition\"\r\n                offset-y\r\n                max-width=\"290px\"\r\n                min-width=\"290px\"\r\n              >\r\n                <template v-slot:activator=\"{ on, attrs }\">\r\n                  <v-text-field\r\n                    v-model=\"dateFormatted2\"\r\n                    label=\"Período Fim\"\r\n                    persistent-hint\r\n                    v-bind=\"attrs\"\r\n                    @blur=\"date2 = parseDate(dateFormatted2)\"\r\n                    v-on=\"on\"\r\n                    outlined\r\n                    dense\r\n                    v-mask=\"maskDatas2\"\r\n                  ></v-text-field>\r\n                </template>\r\n                <v-date-picker\r\n                  v-model=\"date2\"\r\n                  no-title\r\n                  @input=\"menu2 = false\"\r\n                ></v-date-picker>\r\n              </v-menu>\r\n            </v-col>\r\n          </v-row>\r\n\r\n          <v-divider></v-divider>\r\n          <v-row style=\"margin-top: -30px\" align=\"center\" justify=\"center\">\r\n            <v-col cols=\"12\">\r\n              <v-data-table\r\n                :headers=\"headers\"\r\n                :items=\"itensEmprestimo\"\r\n                :items-per-page=\"3\"\r\n              >\r\n                <!-- <template v-slot:item.acoes=\"{ item }\">\r\n                    <v-icon\r\n                      color=\"primary\"\r\n                      @click=\"removeItemHorarioImprodutivo(item)\"\r\n                      >mdi-delete</v-icon\r\n                    >\r\n                  </template> -->\r\n              </v-data-table>\r\n            </v-col>\r\n          </v-row>\r\n\r\n          <v-row>\r\n            <v-col cols=\"12\" sm=\"12\">\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn\r\n                  color=\"orange lighten-1\"\r\n                  @click=\"retornaListaLinhaControle()\"\r\n                  large\r\n                  dark\r\n                  width=\"150\"\r\n                  >Cancelar</v-btn\r\n                >\r\n\r\n                <v-btn\r\n                  v-if=\"!isMobile\"\r\n                  color=\"orange darken-1\"\r\n                  large\r\n                  dark\r\n                  width=\"150\"\r\n                  @click=\"saveSolicitacaoEmprestimo()\"\r\n                  ><v-icon left dark> mdi-account-clock </v-icon\r\n                  >EMPRESTAR</v-btn\r\n                >\r\n\r\n                <v-btn\r\n                  v-else-if=\"isMobile\"\r\n                  color=\"orange darken-1\"\r\n                  large\r\n                  dark\r\n                  width=\"150\"\r\n                  @click=\"saveSolicitacaoEmprestimo()\"\r\n                  ><v-icon left dark> mdi-account-clock </v-icon\r\n                  >EMPRESTAR</v-btn\r\n                >\r\n              </v-card-actions>\r\n            </v-col>\r\n          </v-row>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template> \r\n\r\n\r\n\r\n<script lang=\"ts\">\r\nvar validaNumeros = require(\"../../baseGenerica/validaNumeros.js\");\r\n\r\nimport Vue from \"vue\";\r\n\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  ILinhaEmprestimoFormView,\r\n  ILinhaEmprestimoFormController,\r\n} from \"../../contracts/Linha/LinhaEmprestimoFormContract\";\r\nimport { LinhaEmprestimoFormController } from \"../../controllers/Linha/LinhaEmprestimoFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class LinhaEmprestimoForm\r\n  extends Vue\r\n  implements ILinhaEmprestimoFormView {\r\n  _controller?: ILinhaEmprestimoFormController;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"RotaAqui\",\r\n      disabled: false,\r\n      href: \"/rota\",\r\n    },\r\n  ];\r\n\r\n  headers = [\r\n    {\r\n      text: \"Habilidade\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"habilidade\",\r\n    },\r\n    {\r\n      text: \"Quantidade\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"qtdeOperador\",\r\n    },\r\n  ];\r\n\r\n  itensEmprestimo: any = [];\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  //calendario de datas\r\n  date = new Date().toISOString().substr(0, 10);\r\n  date2 = new Date().toISOString().substr(0, 10);\r\n  dateFormatted = this.formatDate(new Date().toISOString().substr(0, 10));\r\n  dateFormatted2 = this.formatDate(new Date().toISOString().substr(0, 10));\r\n  menu1 = false;\r\n  menu2 = false;\r\n\r\n  //ABAS\r\n  tab = null;\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  // LINHAS ATUAL\r\n  itemsLinha: any = [];\r\n  linhaSelecionada: any = [];\r\n\r\n  qtdeOperadores = 0;\r\n\r\n  itemsHabilidade: any = [];\r\n  habilidadeSelecionada: any = [];\r\n\r\n  disableHabilidade = true;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  formatDate(date: string) {\r\n    if (!date) return null;\r\n\r\n    const [year, month, day] = date.split(\"-\");\r\n    return `${day}/${month}/${year}`;\r\n  }\r\n\r\n  parseDate(date: string) {\r\n    if (!date) return null;\r\n    const [day, month, year] = date.split(\"/\");\r\n    return `${year}-${month.padStart(2, \"0\")}-${day.padStart(2, \"0\")}`;\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/linhascontrole\");\r\n    }\r\n  }\r\n\r\n  retornaListaLinhaControle() {\r\n    this.$router.push(\"/linhascontrole\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    if (this.linhaSelecionada == \"\" || this.linhaSelecionada == null) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a linha.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.habilidadeSelecionada == \"\" ||\r\n      this.habilidadeSelecionada == null\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a habilidade.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.qtdeOperadores == 0 ||\r\n      this.qtdeOperadores == null ||\r\n      this.qtdeOperadores == undefined\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Infome a quantidade de operadores.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.date2 < this.date) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"A data final não pode ser inferior a data atual.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  saveSolicitacaoEmprestimo() {\r\n    if (this.validate()) {\r\n    } else {\r\n      let dadosSolicitacaiEmprestimo = {\r\n        idLinha: this.linhaSelecionada.id,\r\n        idHabilidade: this.habilidadeSelecionada.id,\r\n        qtdeOperador: this.qtdeOperadores,\r\n        dataCadastro: new Date(),\r\n        dataInicio: this.date,\r\n        dataFinal: this.date2,\r\n        status: \"AC\",\r\n      };\r\n\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.salvar(dadosSolicitacaiEmprestimo);\r\n      //this.retornaListaLinhaControle();\r\n    }\r\n  }\r\n\r\n  updateFuncionario() {\r\n    //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n    // this._controller!.update(this.idFuncionario, this.funcionario);\r\n    this.retornaListaLinhaControle();\r\n  }\r\n\r\n  carregarDtos(): void {}\r\n\r\n  showBaseForEditar(item: any) {}\r\n\r\n  //  carregando todas as linhas\r\n  carregaLinha(itemLinha: any): void {\r\n    this.itemsLinha = itemLinha;\r\n    this.itemsHabilidade = [];\r\n    this.disableHabilidade = true;\r\n  }\r\n\r\n  addSolicitacaoEmprestimo() {\r\n    let troca: any = {};\r\n\r\n    troca = {\r\n      idLinha: this.linhaSelecionada.id,\r\n      idHabilidade: this.habilidadeSelecionada.id,\r\n      habilidade: this.habilidadeSelecionada.nome,\r\n      qtdeOperador: this.qtdeOperadores,\r\n    };\r\n\r\n    if (!this.validate()) {\r\n      this.itensEmprestimo.push(troca);\r\n      this.linhaSelecionada = \"\";\r\n      this.habilidadeSelecionada = \"\";\r\n      this.qtdeOperadores = 0;\r\n    }\r\n  }\r\n\r\n  //carregando a Habilidades da Fase selecionada\r\n  changeHabilidades() {\r\n    this.itemsHabilidade = [];\r\n\r\n    //carregando dados da habilidade (Fase - Capacidade da Linha)\r\n    let habilidades: any = [];\r\n\r\n    this.itemsLinha.forEach((elementLinha: any) => {\r\n      elementLinha.linhaCapacidade.forEach((elementLinhaCapacidade: any) => {\r\n        elementLinhaCapacidade.linhaCapacidadeHabilidades.forEach(\r\n          (elementHabilidade: any) => {\r\n            if (\r\n              this.linhaSelecionada.id == elementLinha.id &&\r\n              this.linhaSelecionada.nome == elementLinha.nome\r\n            ) {\r\n              habilidades.push(elementHabilidade.processoHabilidade);\r\n            }\r\n          }\r\n        );\r\n      });\r\n    });\r\n\r\n    this.itemsHabilidade = habilidades;\r\n  }\r\n\r\n  created() {\r\n    this._controller = new LinhaEmprestimoFormController(this);\r\n    this._controller.carregarDtos(this.$route.params.id);\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n\r\n  @Watch(\"date\")\r\n  onDatasChanged(val: string) {\r\n    this.dateFormatted = this.formatDate(this.date);\r\n  }\r\n\r\n  @Watch(\"date2\")\r\n  onDatas2Changed(val: string) {\r\n    this.dateFormatted2 = this.formatDate(this.date2);\r\n  }\r\n}\r\n</script> \r\n\r\n<style>\r\n#titulo {\r\n  text-align: left;\r\n  font: normal normal bold 20px/24px Helvetica;\r\n  letter-spacing: 0px;\r\n  color: #000000;\r\n  opacity: 1;\r\n}\r\n</style>"]}]}