AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA","file":"CentroCustoForm.vue","sourceRoot":"src/views/Cadastro","sourcesContent":["<template>\r\n  <v-content style=\"margin: 10px;\">\r\n    <v-snackbar v-model=\"snackbar\" :color=\"colorBorder\" shaped timeout=\"3000\" :vertical=\"vertical\">\r\n      <b>{{ text }}</b>\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn v-bind=\"attrs\" @click=\"snackbar = false\" icon dark small>\r\n          <v-icon>mdi-close</v-icon>\r\n        </v-btn>\r\n        <!-- <v-btn icon v-bind=\"attrs\" @click=\"snackbar = false\">mdi-magnify</v-btn> -->\r\n      </template>\r\n    </v-snackbar>\r\n\r\n    <k-loading title=\"Carregando\" :dialog=\"dialogLoading\"></k-loading>\r\n\r\n    <k-dialog\r\n      :dialog=\"dialogDelete\"\r\n      title=\"Remover Setor?\"\r\n      text=\"O setor selecionado será removido. Deseja Continuar?\"\r\n    >\r\n      <template v-slot:buttoes>\r\n        <v-btn @click=\"cancelaRemoverSetor()\" width=\"100\" text dark color=\"red darken-2\">Cancelar</v-btn>\r\n        <v-btn @click=\"removerSetor()\" width=\"100\" text dark color=\"green darken-2\">Sim</v-btn>\r\n      </template>\r\n    </k-dialog>\r\n\r\n    <k-dialog\r\n      @fecharDialog=\"fecharDialogMessage\"\r\n      :dialog=\"dialogMessage\"\r\n      title=\"Mensagem\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n      <v-col cols=\"12\" v-if=\"centroCusto.id == 0\">\r\n        <h3>\r\n          <b>Centro de Custo e Setores</b>\r\n        </h3>\r\n        <span>\r\n          <b>Adicionar Setores ao Centro de Custo</b>\r\n        </span>\r\n      </v-col>\r\n\r\n      <v-col cols=\"12\" v-if=\"centroCusto.id !== 0\">\r\n        <h3>\r\n          <b>Centro de Custo e Setores</b>\r\n        </h3>\r\n        <span>\r\n          <b>Editar Setores do Centro de Custo</b>\r\n        </span>\r\n      </v-col>\r\n\r\n      <v-row ref=\"form\">\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-card flat>\r\n            <!-- DADOS DA INSERÇÃO DE APROVCAÇÃO -->\r\n            <v-card-text>\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"4\">\r\n                  <span>\r\n                    <b>Dados do Centro de Custo</b>\r\n                  </span>\r\n                </v-col>\r\n              </v-row>\r\n\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"12\">\r\n                  <v-row>\r\n                    <v-col cols=\"12\" sm=\"4\">\r\n                      <v-text-field\r\n                        outlined\r\n                        v-model=\"centroCusto.codigo\"\r\n                        label=\"Código\"\r\n                        dense\r\n                        disabled\r\n                      ></v-text-field>\r\n                    </v-col>\r\n                    <v-col cols=\"12\" sm=\"8\">\r\n                      <v-text-field\r\n                        outlined\r\n                        v-model=\"centroCusto.descricao\"\r\n                        label=\"Descrição do Centro de Custo\"\r\n                        dense\r\n                        disabled\r\n                      ></v-text-field>\r\n                    </v-col>\r\n                  </v-row>\r\n                </v-col>\r\n              </v-row>\r\n              <v-row style=\"margin-top: -20px\">\r\n                <v-col cols=\"12\" sm=\"4\">\r\n                  <span>\r\n                    <b>Dados dos Setores</b>\r\n                  </span>\r\n                </v-col>\r\n              </v-row>\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"4\">\r\n                  <v-autocomplete\r\n                    :items=\"itemsSetores\"\r\n                    :filter=\"customFilterSetor\"\r\n                    v-model=\"setor\"\r\n                    return-object\r\n                    item-text=\"descricao\"\r\n                    placeholder=\"Selecione o Setor\"\r\n                    label=\"Setor\"\r\n                    outlined\r\n                    dense\r\n                  ></v-autocomplete>\r\n                </v-col>\r\n                <v-col cols=\"1\">\r\n                  <v-btn v-if=\"!isMobile\" color=\"#FB9514\" fab dark @click=\"addSetor()\">\r\n                    <v-icon dark>mdi-plus</v-icon>\r\n                  </v-btn>\r\n                </v-col>\r\n              </v-row>\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"12\">\r\n                  <k-data-list\r\n                    orientation=\"Horizontal\"\r\n                    mobileIconAdd=\"mdi-plus-circle\"\r\n                    mobileFunctionAdd=\"add\"\r\n                    :headers=\"headers\"\r\n                    :itens=\"centroCusto.setores\"\r\n                  >\r\n                    <template v-slot:acao=\"{ index }\">\r\n                      <v-btn\r\n                        style=\"float: right\"\r\n                        large\r\n                        v-if=\"!isMobile\"\r\n                        color=\"primary\"\r\n                        icon=\"dark\"\r\n                        @click=\"modalRemoverSetor(index)\"\r\n                      >\r\n                        <v-icon dark>mdi-delete</v-icon>\r\n                      </v-btn>\r\n                    </template>\r\n                  </k-data-list>\r\n                </v-col>\r\n              </v-row>\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"12\">\r\n                  <v-card-actions>\r\n                    <v-spacer></v-spacer>\r\n                    <v-btn\r\n                      color=\"orange lighten-1\"\r\n                      @click=\"retornaListaFuncionarios()\"\r\n                      large\r\n                      dark\r\n                      width=\"150\"\r\n                    >Cancelar</v-btn>\r\n\r\n                    <v-btn\r\n                      v-if=\"!isMobile && centroCusto.id == 0\"\r\n                      color=\"orange darken-1\"\r\n                      large\r\n                      dark\r\n                      width=\"150\"\r\n                      @click=\"saveCentroCustoSetores()\"\r\n                    >Salvar</v-btn>\r\n\r\n                     <v-btn\r\n                      v-if=\"!isMobile && centroCusto.id !== 0\"\r\n                      color=\"orange darken-1\"\r\n                      large\r\n                      dark\r\n                      width=\"150\"\r\n                      @click=\"updateCentroCustoSetores()\"\r\n                    >Salvar</v-btn>\r\n                  </v-card-actions>\r\n                </v-col>\r\n              </v-row>\r\n            </v-card-text>\r\n          </v-card>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template> \r\n\r\n\r\n\r\n<script lang=\"ts\">\r\nvar validaNumeros = require(\"../../baseGenerica/validaNumeros.js\");\r\n\r\nimport Vue from \"vue\";\r\n\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  ICentroCustoFormView,\r\n  ICentroCustoFormController,\r\n} from \"../../contracts/Cadastro/CentroCustoFormContract\";\r\nimport { CentroCustoFormController } from \"../../controllers/Cadastro/CentroCustoFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class CentroCustoForm extends Vue\r\n  implements ICentroCustoFormView {\r\n  _controller?: ICentroCustoFormController;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"RotaAqui\",\r\n      disabled: false,\r\n      href: \"/rota\",\r\n    },\r\n  ];\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  dialogLoading = false;\r\n  dialogDelete = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  itemsSetores: any = [];\r\n\r\n  codigoCentroCusto = \"\";\r\n\r\n  setor: any = [];\r\n  centroCusto: any = {\r\n    id: 0,\r\n    codigo: \"\",\r\n    descricao: \"\",\r\n    setores: [],\r\n    dataCadastro: new Date(),\r\n  };\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  headers = new Headers([\r\n    new Header(\"Sigla\", \"sigla\", false, false, \"left\", false, \"100\"),\r\n    new Header(\"Descrição\", \"descricao\", false, false, \"left\", false, \"200\"),\r\n    new Header(\"Ação\", \"acao\", false, false, \"center\", false, \"100\"),\r\n  ]);\r\n\r\n  indexRemoverSetor = 0;\r\n\r\n  removerSetor() {\r\n    for (const { item, index } of this.centroCusto.setores.map(\r\n      (item: any, index: any) => ({\r\n        item,\r\n        index,\r\n      })\r\n    )) {\r\n      if (index == this.indexRemoverSetor) {\r\n        this.centroCusto.setores.splice(index, 1);\r\n        this.indexRemoverSetor = -1;\r\n        this.dialogDelete = false;\r\n        return;\r\n      }\r\n    }\r\n  }\r\n\r\n  modalRemoverSetor(indexRemoveItem: string) {\r\n    this.indexRemoverSetor = parseInt(indexRemoveItem);\r\n    this.dialogDelete = true;\r\n  }\r\n\r\n  cancelaRemoverSetor() {\r\n    this.indexRemoverSetor = -1;\r\n    this.dialogDelete = false;\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/centrosCustos\");\r\n    }\r\n  }\r\n\r\n  retornaListaFuncionarios() {\r\n    this.$router.push(\"/centrosCustos\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    return false;\r\n  }\r\n\r\n  saveCentroCustoSetores() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.salvar(this.centroCusto);\r\n    }\r\n  }\r\n\r\n    updateCentroCustoSetores() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.update(this.centroCusto.id, this.centroCusto);\r\n    }\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.codigoCentroCusto = this.$route.params.codigo;\r\n  }\r\n\r\n  showCentroCustoForSave(item: any) {\r\n    this.centroCusto.codigo = item.codigo;\r\n    this.centroCusto.descricao = item.descricao;\r\n  }\r\n\r\n  showCentroCustoForEditar(item: any) {\r\n    this.centroCusto.id = item[0].centroCustoDTO.id;\r\n    this.centroCusto.codigo = item[0].centroCustoDTO.codigo;\r\n    this.centroCusto.descricao = item[0].centroCustoDTO.descricao;\r\n\r\n    item.forEach((element: any) => {\r\n      this.centroCusto.setores.push(element.setorDTO);\r\n    });\r\n  }\r\n\r\n  carregaSetores(item: any) {\r\n    //CARREGANDO DADOS DO FUNCIONARIO\r\n    this.itemsSetores = item;\r\n  }\r\n\r\n  validateAddSetor() {\r\n    if (this.setor.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o setor para adicionar.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    } else {\r\n      let flag = false;\r\n      this.centroCusto.setores.forEach((element: any) => {\r\n        if (element.descricao == this.setor.descricao) {\r\n          this.colorBorder = \"warning\";\r\n          this.color = \"warning\";\r\n          this.text = \"Setor já adicionado.\";\r\n          this.setor = \"\";\r\n          this.snackbar = true;\r\n          flag = true;\r\n        }\r\n      });\r\n\r\n      return flag;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/centroCusto\");\r\n  }\r\n\r\n  customFilterSetor(item: any, queryText: any, itemText: any) {\r\n    const textOne = item.sigla.toLowerCase();\r\n    const textTwo = item.descricao.toLowerCase();\r\n    const searchText = queryText.toLowerCase();\r\n\r\n    return textOne.indexOf(searchText) > -1 || textTwo.indexOf(searchText) > -1;\r\n  }\r\n\r\n  addSetor() {\r\n    if (this.validateAddSetor()) {\r\n    } else {\r\n      this.centroCusto.setores.push(this.setor);\r\n      this.setor = \"\";\r\n    }\r\n  }\r\n\r\n  created() {\r\n    this._controller = new CentroCustoFormController(this);\r\n    this._controller.carregarDtos(this.$route.params.codigo);\r\n    this.carregarDtos();\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n</script> "]}]}