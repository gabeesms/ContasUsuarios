mente. Deseja Continuar?\"\r\n    >\r\n      <template v-slot:buttoes>\r\n        <v-btn\r\n          @click=\"dialogAtivar = false\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"red darken-2\"\r\n          >Cancelar</v-btn\r\n        >\r\n        <v-btn\r\n          @click=\"ativarItemConfirmado\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"green darken-2\"\r\n          >Sim</v-btn\r\n        >\r\n      </template>\r\n    </k-dialog>\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n          <template v-slot:item=\"{ item }\">\r\n            <router-link :to=\"item.href\">\r\n              {{ item.text }}\r\n            </router-link>\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n\r\n      <v-row>\r\n        <v-col cols=\"10\">\r\n          <h3>\r\n            <b>Processos</b>\r\n          </h3>\r\n        </v-col>\r\n        <v-spacer></v-spacer>\r\n\r\n        <!-- <v-col cols=\"1\">\r\n          <v-btn v-if=\"!isMobile\" color=\"primary\" fab dark @click=\"addSolicitacao\">\r\n            <v-icon dark>mdi-plus</v-icon>\r\n          </v-btn>\r\n        </v-col>-->\r\n      </v-row>\r\n      <v-row>\r\n        <v-col cols=\"10\">\r\n          <span>\r\n            <b>Dados Gerais</b>\r\n          </span>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"2\">\r\n          <v-text-field\r\n            v-model=\"processo.codigo\"\r\n            outlined\r\n            label=\"Código *\"\r\n            placeholder=\"Código do Processo\"\r\n            counter=\"5\"\r\n            dense\r\n            required\r\n            maxlength=\"5\"\r\n          ></v-text-field>\r\n        </v-col>\r\n        <v-col cols=\"12\" sm=\"5\">\r\n          <v-text-field\r\n            v-model=\"processo.nome\"\r\n            outlined\r\n            label=\"Nome do Processo *\"\r\n            placeholder=\"Nome do Processo\"\r\n            counter=\"30\"\r\n            dense\r\n            required\r\n            maxlength=\"30\"\r\n          ></v-text-field>\r\n        </v-col>\r\n        <v-col cols=\"12\" sm=\"3\">\r\n          <v-select\r\n            :items=\"itemsTipo\"\r\n            label=\"Tipo de Processo  *\"\r\n            autocomplete=\"off\"\r\n            v-model=\"processo.tipo\"\r\n            item-text=\"descricao\"\r\n            item-value=\"op\"\r\n            outlined\r\n            dense\r\n          ></v-select>\r\n        </v-col>\r\n        <v-col cols=\"12\" sm=\"2\" style=\"margin-top: -0.5vh\">\r\n          <v-btn\r\n            color=\"orange darken-1\"\r\n            large\r\n            dark\r\n            width=\"25vh\"\r\n            @click=\"saveProcesso()\"\r\n            >Adcionar</v-btn\r\n          >\r\n        </v-col>\r\n      </v-row>\r\n      <v-divider></v-divider>\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"5\">\r\n          <v-tabs\r\n            v-model=\"tab\"\r\n            background-color=\"transparent\"\r\n            color=\"orange\"\r\n            grow\r\n          >\r\n            <v-tab v-for=\"(item, index) in itemsTab\" :key=\"index\">{{\r\n              item\r\n            }}</v-tab>\r\n          </v-tabs>\r\n        </v-col>\r\n      </v-row>\r\n\r\n      <v-tabs-items v-model=\"tab\">\r\n        <v-tab-item v-for=\"(item, index) in itemsTab\" :key=\"index\">\r\n          <v-card flat v-if=\"index == 0\">\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <v-col cols=\"12\" sm=\"5\" style=\"float: right\">\r\n                  <v-text-field\r\n                    v-model=\"searchOperacoes\"\r\n                    label=\"Pesquisa Operações\"\r\n                    class=\"mx-4\"\r\n                    prepend-icon=\"mdi-magnify\"\r\n                  ></v-text-field>\r\n                </v-col>\r\n              </v-col>\r\n            </v-row>\r\n            <v-row style=\"margin-top: -30px\" align=\"center\" justify=\"center\">\r\n              <v-col cols=\"12\">\r\n                <k-data-list\r\n                  orientation=\"Horizontal\"\r\n                  mobileIconAdd=\"mdi-plus-circle\"\r\n                  mobileFunctionAdd=\"addSolicitacao\"\r\n                  @editarItem=\"editarItem\"\r\n                  @deleteItem=\"deleteItem\"\r\n                  @addSolicitacao=\"addSolicitacao\"\r\n                  :headers=\"headersOperacoes\"\r\n                  :search=\"search\"\r\n                  :itens=\"itensOperacoes\"\r\n                >\r\n                  <template v-slot:status=\"{ item }\">\r\n                    <v-chip v-if=\"item.status == 'A'\" color=\"#019A53\" dark\r\n                      >Ativo</v-chip\r\n                    >\r\n                    <v-chip v-if=\"item.status == 'I'\" color=\"#A80000\" dark\r\n                      >Inativo</v-chip\r\n                    >\r\n                  </template>\r\n                </k-data-list>\r\n              </v-col>\r\n            </v-row>\r\n          </v-card>\r\n          <v-card flat v-if=\"index == 1\">\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <v-col cols=\"12\" sm=\"5\" style=\"float: right\">\r\n                  <v-text-field\r\n                    v-model=\"search\"\r\n                    label=\"Pesquisa Habilidades\"\r\n                    class=\"mx-4\"\r\n                    prepend-icon=\"mdi-magnify\"\r\n                  ></v-text-field>\r\n                </v-col>\r\n              </v-col>\r\n            </v-row>\r\n            <v-row style=\"margin-top: -30px\" align=\"center\" justify=\"center\">\r\n              <v-col cols=\"12\">\r\n                <k-data-list\r\n                  orientation=\"Horizontal\"\r\n                  mobileIconAdd=\"mdi-plus-circle\"\r\n                  mobileFunctionAdd=\"addSolicitacao\"\r\n                  @editarItem=\"editarItem\"\r\n                  @deleteItem=\"deleteItem\"\r\n                  @addSolicitacao=\"addSolicitacao\"\r\n                  :headers=\"headers\"\r\n                  :search=\"search\"\r\n                  :itens=\"itens\"\r\n                >\r\n                  <template v-slot:status=\"{ item }\">\r\n                    <v-chip v-if=\"item.status == 'A'\" color=\"#019A53\" dark\r\n                      >Ativo</v-chip\r\n                    >\r\n                    <v-chip v-if=\"item.status == 'I'\" color=\"#A80000\" dark\r\n                      >Inativo</v-chip\r\n                    >\r\n                  </template>\r\n                </k-data-list>\r\n              </v-col>\r\n            </v-row>\r\n          </v-card>\r\n        </v-tab-item>\r\n      </v-tabs-items>\r\n    </v-container>\r\n  </v-content>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from \"vue\";\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport { Validation, ValidationSchema } from \"../../baseGenerica/validation\";\r\n\r\nimport {\r\n  IProcessoListaFormView,\r\n  IProcessoListaFormController,\r\n} from \"../../contracts/Cadastro/ProcessoListaFormContract\";\r\n\r\nimport { ProcessoListaFormController } from \"../../controllers/Cadastro/ProcessoListaFormController\";\r\n\r\nimport Component from \"vue-class-component\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class ProcessoListaForm\r\n  extends Vue\r\n  implements IProcessoListaFormView {\r\n  _controller?: IProcessoListaFormController;\r\n\r\n  //ABAS\r\n  itemsTab: any = [\"Operações\", \"Habilidades\"];\r\n  tab: any = null;\r\n\r\n  itemsTipo = [\r\n    { op: \"O\", descricao: \"OPERAÇÃO\" },\r\n    { op: \"H\", descricao: \"HABILIDADE\" },\r\n  ];\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  search = \"\";\r\n  searchOperacoes = \"\";\r\n\r\n  dialogDelete = false;\r\n  dialogAtivar = false;\r\n  itemProcesso: any;\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  processo = {\r\n    codigo: \"\",\r\n    nome: \"\",\r\n    status: \"A\", //STATUS: A: ATIVO / I: INATIVO\r\n    dataCadastro: new Date(),\r\n    tipo: \"\",\r\n  };\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Processos\",\r\n      disabled: false,\r\n      href: \"/processos\",\r\n    },\r\n  ];\r\n\r\n  validate() {\r\n    let schema: ValidationSchema[] = [];\r\n\r\n    schema.push(new ValidationSchema(\"codigo\", \"Código\", \"string\", true, 5));\r\n    schema.push(\r\n      new ValidationSchema(\"nome\", \"Nome do Processo\", \"string\", true, 30)\r\n    );\r\n\r\n    let result = new Validation().ValidSchema(schema, this.processo);\r\n    if (result.sucesso == false) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = result.mensagem;\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.processo.tipo == \"\") {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o Tipo de Processo.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  limparCampos(): void {\r\n    this.processo.codigo = \"\";\r\n    this.processo.nome = \"\";\r\n    this.processo.tipo = \"\";\r\n  }\r\n\r\n  saveProcesso() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      if (this.processo.tipo == \"H\") {\r\n        this.tab = 1;\r\n      } else {\r\n        this.tab = 0;\r\n      }\r\n\r\n      this._controller!.salvar(this.processo);\r\n    }\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  showItens(listOperacoes: any, listHabilidade: any): void {\r\n    console.log(listOperacoes);\r\n    console.log(listHabilidade);\r\n\r\n    this.itens = listHabilidade;\r\n    this.itensOperacoes = listOperacoes;\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemProcesso = item;\r\n    this.dialogDelete = true;\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    this._controller!.deleteitem(this.itemProcesso.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    console.log(\"jhfsdikfkl\");\r\n    this.itemProcesso = item;\r\n    this.dialogAtivar = true;\r\n  }\r\n\r\n  ativarItem(item: any) {\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  ativarItemConfirmado() {\r\n    this.dialogAtivar = false;\r\n    this._controller!.ativaritem(this.itemProcesso.id);\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  headers = new Headers([\r\n    new Header(\"Código\", \"codigo\", false, false, \"center\", true, \"100\"),\r\n    new Header(\r\n      \"Descrição da Habilidade\",\r\n      \"nome\",\r\n      false,\r\n      false,\r\n      \"left\",\r\n      true,\r\n      \"200\"\r\n    ),\r\n    new Header(\"Status\", \"status\", false, false, \"left\", true, \"100\"),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  headersOperacoes = new Headers([\r\n    new Header(\"Código\", \"codigo\", false, false, \"center\", true, \"100\"),\r\n    new Header(\r\n      \"Descrição da Operação\",\r\n      \"nome\",\r\n      false,\r\n      false,\r\n      \"left\",\r\n      true,\r\n      \"200\"\r\n    ),\r\n    new Header(\"Status\", \"status\", false, false, \"left\", true, \"100\"),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  itens = [];\r\n  itensOperacoes = [];\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/perfil\");\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new ProcessoListaFormController(this);\r\n\r\n    this._controller.carregarItens();\r\n  }\r\n}\r\n</script>"]}]}