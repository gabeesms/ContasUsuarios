{"remainingRequest":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\src\\views\\OrdemProducao\\OrdemProducaoLista.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\src\\views\\OrdemProducao\\OrdemProducaoLista.vue","mtime":1615206741710},{"path":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\ContasUsuarios_ModeloTeste\\smartpoint\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["OrdemProducaoLista.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2wBA;AACA;AACA;AACA","file":"OrdemProducaoLista.vue","sourceRoot":"src/views/OrdemProducao","sourcesContent":["<template>\r\n  <v-content style=\"margin: 10px\">\r\n    <v-snackbar\r\n      v-model=\"snackbar\"\r\n      :color=\"colorBorder\"\r\n      shaped\r\n      timeout=\"3000\"\r\n      :vertical=\"vertical\"\r\n    >\r\n      <b>{{ text }}</b>\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn v-bind=\"attrs\" @click=\"snackbar = false\" icon dark small>\r\n          <v-icon>mdi-close</v-icon>\r\n        </v-btn>\r\n        <!-- <v-btn icon v-bind=\"attrs\" @click=\"snackbar = false\">mdi-magnify</v-btn> -->\r\n      </template>\r\n    </v-snackbar>\r\n\r\n    <!-- ADD EVENTO LINHA A ORDEM DE PRODUCAO-->\r\n    <v-dialog\r\n      v-model=\"dialogAddLinha\"\r\n      scrollable\r\n      :fullscreen=\"fullscreenDialog\"\r\n      transition=\"dialog-bottom-transition\"\r\n    >\r\n      <v-card>\r\n        <v-card-title style=\"background-color: #313c42\">\r\n          <span style=\"color: white\" class=\"headline\">\r\n            Relacionar Linha à Ordem de Produção\r\n          </span>\r\n        </v-card-title>\r\n        <v-card-text>\r\n          <v-container>\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <span>\r\n                  <b>Dados da Ordem de Produção</b>\r\n                </span>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"4\">\r\n                <v-text-field\r\n                  disabled\r\n                  class=\"form-control\"\r\n                  outlined\r\n                  label=\"Nº Ordem de Produção\"\r\n                  v-model=\"itemSelecionado.numOrdemProducao\"\r\n                  dense\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"3\">\r\n                <v-text-field\r\n                  disabled\r\n                  class=\"form-control\"\r\n                  outlined\r\n                  label=\"Modelos (Produto)\"\r\n                  v-model=\"itemSelecionado.codigoProduto\"\r\n                  dense\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"3\">\r\n                <v-text-field\r\n                  disabled\r\n                  class=\"form-control\"\r\n                  outlined\r\n                  label=\"Quantidade\"\r\n                  v-model=\"itemSelecionado.quantidade\"\r\n                  dense\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col cols=\"12\" sm=\"2\">\r\n                <v-text-field\r\n                  disabled\r\n                  class=\"form-control\"\r\n                  outlined\r\n                  label=\"Data de Emissão\"\r\n                  :value=\"\r\n                    new Date(itemSelecionado.dataEmissao).toLocaleDateString(\r\n                      'pt-BR'\r\n                    )\r\n                  \"\r\n                  dense\r\n                ></v-text-field>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row style=\"margin-top: -15px\">\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <span>\r\n                  <b>Selecione da Linha</b>\r\n                </span>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"4\">\r\n                <v-select\r\n                  id=\"event-tipo\"\r\n                  :items=\"itemsLinha\"\r\n                  v-model=\"linhaSelecionada\"\r\n                  return-object\r\n                  label=\"Linha *\"\r\n                  autocomplete=\"off\"\r\n                  item-text=\"nome\"\r\n                  placeholder=\"Selecione a Linha para a Ordem de Produção\"\r\n                  outlined\r\n                  dense\r\n                  required\r\n                  @input=\"changeLinha()\"\r\n                  :rules=\"[\r\n                    () =>\r\n                      (!!linhaSelecionada && linhaSelecionada.length <= 100) ||\r\n                      '',\r\n                  ]\"\r\n                ></v-select>\r\n              </v-col>\r\n\r\n              <v-col cols=\"12\" sm=\"3\">\r\n                <v-text-field\r\n                  disabled\r\n                  class=\"form-control\"\r\n                  outlined\r\n                  label=\"Qtde de Operadores\"\r\n                  placeholder=\"Qtde de Operadores\"\r\n                  v-model=\"linhaSelecionada.qtdeCapacidadeOperadoresLinha\"\r\n                  dense\r\n                ></v-text-field>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row style=\"margin-top: -20px\">\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <v-card flat style=\"margin-left: -15px\">\r\n                  <v-card-text style=\"margin-top: -15px\">\r\n                    <span\r\n                      ><b>{{ mensagemTitulo }}</b></span\r\n                    >\r\n                    <br v-if=\"mensagemTitulo != ''\" />\r\n                    <br v-if=\"mensagemTitulo != ''\" />\r\n                    <v-alert\r\n                      v-for=\"(item, index) in itensFasesProdutosXLinhas\"\r\n                      :key=\"index\"\r\n                      :color=\"item.tipo == 1 ? '#FF5252' : '#FB9514'\"\r\n                      icon=\"mdi-alert-circle\"\r\n                      border=\"left\"\r\n                      colored-border\r\n                      \r\n                      text\r\n                    >\r\n                      <div>\r\n                        <div class=\"font-weight-normal\">\r\n                          <strong>{{ \"FASE: \" + item.nomeFase }}</strong>\r\n                        </div>\r\n\r\n                        <v-row>\r\n                          <v-col cols=\"12\" sm=\"12\" v-if=\"item.tipo == 2\"\r\n                            ><b>{{\r\n                              \"Capacidade da Linha: \" +\r\n                              item.qtdeOperadorLinhaProducao\r\n                            }}</b></v-col\r\n                          >\r\n                          <v-col cols=\"12\" sm=\"12\" v-else>{{\r\n                            \"Capacidade da Linha: \" +\r\n                            item.qtdeOperadorLinhaProducao\r\n                          }}</v-col>\r\n                        </v-row>\r\n                        <v-row style=\"margin-top: -15px\">\r\n                          <v-col cols=\"12\" sm=\"12\">{{\r\n                            \"Qtde Operadores Para Produção Modelo: \" +\r\n                            item.qtdeOperadorLinha\r\n                          }}</v-col>\r\n                        </v-row>\r\n                        <v-row style=\"margin-top: -15px\">\r\n                          <v-col cols=\"12\" sm=\"12\"><b>{{ item.mensagem }}</b></v-col>\r\n                        </v-row>\r\n                      </div>\r\n                    </v-alert>\r\n                    <!-- <v-timeline\r\n                      v-for=\"(item, index) in itensFasesProdutosXLinhas\"\r\n                      :key=\"index\"\r\n                      align-top\r\n                      dense\r\n                    >\r\n                      <v-timeline-item\r\n                        :color=\"item.tipo == 1 ? 'red' : '#FDD835'\"\r\n                        small\r\n                      >\r\n                        <div>\r\n                          <div class=\"font-weight-normal\">\r\n                            <strong>{{ \"FASE: \" + item.nomeFase }}</strong>\r\n                          </div>\r\n\r\n                          <v-row>\r\n                            <v-col cols=\"12\" sm=\"12\" v-if=\"item.tipo == 2\"\r\n                              ><b>{{\r\n                                \"Capacidade da Linha: \" +\r\n                                item.qtdeOperadorLinhaProducao\r\n                              }}</b></v-col\r\n                            >\r\n                            <v-col cols=\"12\" sm=\"12\" v-else>{{\r\n                              \"Capacidade da Linha: \" +\r\n                              item.qtdeOperadorLinhaProducao\r\n                            }}</v-col>\r\n                          </v-row>\r\n                          <v-row style=\"margin-top: -15px\">\r\n                            <v-col cols=\"12\" sm=\"12\">{{\r\n                              \"Qtde Operadores Para Produção Modelo: \" +\r\n                              item.qtdeOperadorLinha\r\n                            }}</v-col>\r\n                          </v-row>\r\n                          <v-row style=\"margin-top: -15px\">\r\n                            <v-col cols=\"12\" sm=\"12\"\r\n                              ><b>{{ item.mensagem }}</b></v-col\r\n                            >\r\n                          </v-row>\r\n                        </div>\r\n                      </v-timeline-item>\r\n                    </v-timeline> -->\r\n                  </v-card-text>\r\n                </v-card>\r\n              </v-col>\r\n            </v-row>\r\n          </v-container>\r\n        </v-card-text>\r\n        <v-divider></v-divider>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn\r\n            color=\"orange lighten-1\"\r\n            width=\"150\"\r\n            @click=\"cancelarSaveAddLinhaOrdemProducao\"\r\n            dark\r\n            large\r\n            >Cancelar</v-btn\r\n          >\r\n          <v-btn\r\n            color=\"orange darken-1\"\r\n            width=\"150\"\r\n            dark\r\n            large\r\n            @click=\"saveAddLinhaOrdemProducao\"\r\n            >RELACIONAR</v-btn\r\n          >\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n\r\n    <k-loading title=\"Carregando\" :dialog=\"dialogLoading\"></k-loading>\r\n    <k-dialog\r\n      @fecharDialog=\"fecharDialogMessage\"\r\n      :dialog=\"dialogMessage\"\r\n      title=\"Ordem de Produção\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n    <k-dialog\r\n      :dialog=\"dialogDelete\"\r\n      title=\"cancelar Ordem de Produção?\"\r\n      text=\"A ordem de Produção selecionado será cancelada. Deseja Continuar?\"\r\n    >\r\n      <template v-slot:buttoes>\r\n        <v-btn\r\n          @click=\"dialogDelete = false\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"red darken-2\"\r\n          >Cancelar</v-btn\r\n        >\r\n        <v-btn\r\n          @click=\"deleteItemConfirmado\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"green darken-2\"\r\n          >Sim</v-btn\r\n        >\r\n      </template>\r\n    </k-dialog>\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n          <template v-slot:item=\"{ item }\">\r\n            <router-link :to=\"item.href\">\r\n              {{ item.text }}\r\n            </router-link>\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n\r\n      <v-row>\r\n        <v-col cols=\"10\">\r\n          <h3>\r\n            <b>Ordem de Produção ERP</b>\r\n          </h3>\r\n          <span>\r\n            <b\r\n              >Crie Simulações para Linhas, Modelo (Produtos), Operadores e\r\n              Produção ERP</b\r\n            >\r\n          </span>\r\n        </v-col>\r\n        <v-spacer></v-spacer>\r\n\r\n        <v-col cols=\"1\">\r\n          <v-btn\r\n            v-if=\"!isMobile\"\r\n            color=\"primary\"\r\n            fab\r\n            dark\r\n            @click=\"erpOrdemProducaoCarregar\"\r\n            >ERP</v-btn\r\n          >\r\n        </v-col>\r\n      </v-row>\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-col cols=\"12\" sm=\"5\" style=\"float: right\">\r\n            <v-text-field\r\n              v-model=\"search\"\r\n              label=\"Pesquisa Ordem de Produção\"\r\n              class=\"mx-4\"\r\n              prepend-icon=\"mdi-magnify\"\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row style=\"margin-top: -30px\" align=\"center\" justify=\"center\">\r\n        <v-col cols=\"12\">\r\n          <v-data-table :headers=\"headers\" :items=\"itens\" :search=\"search\">\r\n            <template v-slot:item.deletado=\"{ item }\">\r\n              <v-chip\r\n                v-if=\"item.deletado == 'N'\"\r\n                small\r\n                class=\"px-7 py-3\"\r\n                color=\"#019A53\"\r\n                dark\r\n                >Ativo</v-chip\r\n              >\r\n              <v-chip\r\n                v-if=\"item.deletado == 'S'\"\r\n                small\r\n                class=\"px-6 py-3\"\r\n                color=\"#A80000\"\r\n                dark\r\n                >Inativo</v-chip\r\n              >\r\n            </template>\r\n            <template v-slot:item.acoes=\"{ item }\">\r\n              <v-btn icon>\r\n                <v-icon @click=\"relacionarLinha(item)\" small color=\"primary\"\r\n                  >mdi-file-tree</v-icon\r\n                >\r\n              </v-btn>\r\n\r\n              <v-btn icon>\r\n                <v-icon\r\n                  @click=\"editarItem(item)\"\r\n                  small\r\n                  color=\"primary\"\r\n                  :disabled=\"item.idLinha !== 0 ? false : true\"\r\n                  >mdi-file-document</v-icon\r\n                >\r\n              </v-btn>\r\n\r\n              <v-btn icon>\r\n                <v-icon\r\n                  @click=\"viewPlano(item)\"\r\n                  small\r\n                  color=\"primary\"\r\n                  :disabled=\"item.idOrdemProducaoPlano != 0 ? false : true\"\r\n                  >mdi-eye</v-icon\r\n                >\r\n              </v-btn>\r\n            </template>\r\n\r\n            <template v-slot:item.nomeLinha=\"{ item }\">\r\n              <v-chip\r\n                v-if=\"item.nomeLinha !== null\"\r\n                class=\"ma-2\"\r\n                outlined\r\n                pill\r\n                color=\"red\"\r\n                small\r\n              >\r\n                {{ item.nomeLinha }}\r\n              </v-chip>\r\n              <!-- <b v-if=\"item.nomeLinha !== null\" style=\"color: #3F51B5\">{{item.nomeLinha}}</b> -->\r\n              <l class=\"ma-2\" v-else>-</l>\r\n            </template>\r\n            <template v-slot:item.CapLinhaOperadorProduto=\"{ item }\">\r\n              <v-row>\r\n                <v-col\r\n                  cols=\"12\"\r\n                  sm=\"12\"\r\n                  v-if=\"\r\n                    parseInt(item.capacidadeOperadoresLinha) == 0 &&\r\n                    parseInt(item.qtdeOperadoresProduto) == 0\r\n                  \"\r\n                >\r\n                  -\r\n                </v-col>\r\n\r\n                <v-col cols=\"12\" sm=\"6\">\r\n                  <v-chip\r\n                    v-if=\"\r\n                      parseInt(item.capacidadeOperadoresLinha) > 0 ||\r\n                      parseInt(item.qtdeOperadoresProduto) > 0\r\n                    \"\r\n                    class=\"ma-2\"\r\n                    text-color=\"white\"\r\n                    pill\r\n                    :color=\"\r\n                      parseInt(item.capacidadeOperadoresLinha) <\r\n                      parseInt(item.qtdeOperadoresProduto)\r\n                        ? 'red'\r\n                        : parseInt(item.capacidadeOperadoresLinha) ==\r\n                          parseInt(item.qtdeOperadoresProduto)\r\n                        ? '#1867C0'\r\n                        : 'green'\r\n                    \"\r\n                    small\r\n                  >\r\n                    {{ item.capacidadeOperadoresLinha }}\r\n                  </v-chip>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"6\">\r\n                  <v-chip\r\n                    v-if=\"\r\n                      parseInt(item.qtdeOperadoresProduto) > 0 ||\r\n                      parseInt(item.capacidadeOperadoresLinha) > 0\r\n                    \"\r\n                    class=\"ma-2\"\r\n                    text-color=\"white\"\r\n                    pill\r\n                    :color=\"\r\n                      parseInt(item.capacidadeOperadoresLinha) <\r\n                      parseInt(item.qtdeOperadoresProduto)\r\n                        ? 'red'\r\n                        : parseInt(item.capacidadeOperadoresLinha) ==\r\n                          parseInt(item.qtdeOperadoresProduto)\r\n                        ? '#1867C0'\r\n                        : 'green'\r\n                    \"\r\n                    small\r\n                  >\r\n                    {{ item.qtdeOperadoresProduto }}\r\n                  </v-chip>\r\n                </v-col>\r\n              </v-row>\r\n            </template>\r\n          </v-data-table>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from \"vue\";\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\n// import KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\n\r\n//import { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\n//import { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  IOrdemProducaoListaView,\r\n  IOrdemProducaoListaController,\r\n} from \"../../contracts/OrdemProducao/OrdemProducaoListaContract\";\r\n\r\nimport { OrdemProducaoListaController } from \"../../controllers/OrdemProducao/OrdemProducaoListaController\";\r\n\r\nimport Component from \"vue-class-component\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    // KDataList,\r\n  },\r\n})\r\nexport default class OrdemProducaoLista\r\n  extends Vue\r\n  implements IOrdemProducaoListaView {\r\n  _controller?: IOrdemProducaoListaController;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  search = \"\";\r\n\r\n  fullscreenDialog = false;\r\n\r\n  dialogAddLinha = false;\r\n\r\n  dialogDelete = false;\r\n  itemFuncionarioDelete: any;\r\n\r\n  isMobile = false;\r\n\r\n  itens = [];\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  mensagemTitulo = \"\";\r\n\r\n  headers = [\r\n    {\r\n      text: \"Status\",\r\n      align: \"center\",\r\n      filterable: false,\r\n      value: \"deletado\",\r\n      width: \"100\",\r\n    },\r\n    {\r\n      text: \"Nº O.P.\",\r\n      align: \"center\",\r\n      filterable: false,\r\n      value: \"numOrdemProducao\",\r\n      width: \"200\",\r\n    },\r\n    {\r\n      text: \"Modelo (Produto)\",\r\n      align: \"center\",\r\n      filterable: false,\r\n      value: \"codigoProduto\",\r\n      width: \"200\",\r\n    },\r\n    {\r\n      text: \"Produção\",\r\n      align: \"center\",\r\n      filterable: false,\r\n      value: \"quantidade\",\r\n      width: \"100\",\r\n    },\r\n    {\r\n      text: \"Data de Emissão\",\r\n      align: \"center\",\r\n      filterable: false,\r\n      value: \"dataEmissaoFormat\",\r\n      width: \"80\",\r\n    },\r\n    {\r\n      text: \"Linha\",\r\n      align: \"center\",\r\n      filterable: false,\r\n      value: \"nomeLinha\",\r\n      width: \"150\",\r\n    },\r\n    {\r\n      text: \"Capacidade Linha x Qtde Operadores do Produto para Produção\",\r\n      align: \"center\",\r\n      filterable: false,\r\n      value: \"CapLinhaOperadorProduto\",\r\n      width: \"200\",\r\n    },\r\n    {\r\n      text: \"Ação\",\r\n      align: \"center\",\r\n      filterable: false,\r\n      value: \"acoes\",\r\n      width: \"150\",\r\n    },\r\n  ];\r\n\r\n  panel = [0, 1];\r\n  itensFasesProdutosXLinhas: any = [];\r\n\r\n  itemsLinha: any = [];\r\n  linhaSelecionada: any = {};\r\n  itemSelecionado: any = {};\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Nova Ordem de Produção\",\r\n      disabled: false,\r\n      href: \"/ordemProducao\",\r\n    },\r\n  ];\r\n\r\n  changeLinha() {\r\n    this.fullscreenDialog = false;\r\n    this.itensFasesProdutosXLinhas = [];\r\n    this.mensagemTitulo = \"\";\r\n\r\n    this.itemSelecionado.qtdeOperadorLinha;\r\n  }\r\n\r\n  viewPlano(item: any) {\r\n    if (item.qtdeOperadoresProduto > 0 && item.qtdeOperadoresProduto != null) {\r\n      //NOVO\r\n      // if (item.idOrdemProducaoPlano == 0) {\r\n      this.$router.push(\"/ordensProducaoListaSimulacao/\" + item.id);\r\n      // } else {\r\n      //   //EDITAR\r\n      //   this.$router.push(\r\n      //     \"/ordemProducao/\" +\r\n      //       item.idOrdemProducaoLinha +\r\n      //       \"/\" +\r\n      //       item.idOrdemProducaoPlano\r\n      //   );\r\n      // }\r\n    } else {\r\n      this.showMsg(\r\n        \"Modelo \" +\r\n          item.codigoProduto.toString().trim() +\r\n          \" sem Capacidade de Linha cadastrada ou configurada. Antes de definir o plano de Ordem de Produção, configure os dados do Modelo , utilizando a opção de Menu Cadastro -> Modelo\"\r\n      );\r\n    }\r\n  }\r\n\r\n  relacionarLinha(item: any) {\r\n    if (item.idLinha > 0) {\r\n      this.dialogAddLinha = true;\r\n      this.itemSelecionado = item;\r\n      //console.log(this.itemSelecionado);\r\n    } else {\r\n      this.showMsg(\r\n        \"Modelo \" +\r\n          item.codigoProduto.toString().trim() +\r\n          \" sem Capacidade de Linha cadastrada ou configurada. Antes de definir o plano de Ordem de Produção, configure os dados do Modelo, utilizando a opção de Menu Cadastro -> Modelo\"\r\n      );\r\n    }\r\n  }\r\n\r\n  validate() {\r\n    if (this.linhaSelecionada.id == null) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a Linha para a Ordem de Produção\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n    return false;\r\n  }\r\n\r\n  consultaFasesLinhaXFasesProduto(itens: any) {\r\n    this.fullscreenDialog = true;\r\n    this.itensFasesProdutosXLinhas = [];\r\n    this.itensFasesProdutosXLinhas = itens;\r\n\r\n    if (this.itensFasesProdutosXLinhas.length == 0) {\r\n      this.mensagemTitulo =\r\n        \"Fases do Processo da Linha correspondem as Fases do Processo de Produção do Modelo. \";\r\n      let ordemProducaoLinha = {\r\n        idLinhaProducao: parseInt(this.linhaSelecionada.id),\r\n        idLinha: parseInt(this.itemSelecionado.idLinha), //Produto\r\n        idOrdemProducao: parseInt(this.itemSelecionado.id),\r\n        dataCadastro: new Date(),\r\n        deletado: \"N\",\r\n      };\r\n\r\n      this._controller!.salvarOrdemProducaoLinha(ordemProducaoLinha);\r\n      this.dialogAddLinha = false;\r\n    } else {\r\n      this.mensagemTitulo =\r\n        \"Impedimentos do Relacionamento entre as Fases da Linha com as Fases do Processo de Produção do Modelo.\";\r\n    }\r\n  }\r\n\r\n  saveAddLinhaOrdemProducao() {\r\n    if (!this.validate()) {\r\n      this._controller!.consultarFasesLinhaXFasesProduto(\r\n        this.itemSelecionado.idLinha,\r\n        this.linhaSelecionada.id\r\n      );\r\n    }\r\n  }\r\n\r\n  cancelarSaveAddLinhaOrdemProducao() {\r\n    this.fullscreenDialog = false;\r\n    this.dialogAddLinha = false;\r\n    this.linhaSelecionada = {};\r\n    this.itemSelecionado = {};\r\n  }\r\n\r\n  erpOrdemProducaoCarregar() {\r\n    this.loading(true);\r\n    this.itens = [];\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new OrdemProducaoListaController(this);\r\n    this._controller.atualizaBaseOrdemProducaoERP();\r\n    this._controller.carregarItens();\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  showItens(list: any): void {\r\n    this.linhaSelecionada = {};\r\n    this.itemSelecionado = {};\r\n    this.itens = list;\r\n    this.mensagemTitulo = \"\";\r\n    this.fullscreenDialog = false;\r\n  }\r\n\r\n  carregaLinha(linhas: any): void {\r\n    this.itemsLinha = linhas;\r\n\r\n    //console.log(this.itemsLinha);\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemFuncionarioDelete = item;\r\n    this.dialogDelete = true;\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    //this._controller!.deleteitem(this.itemFuncionarioDelete.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    if (item.qtdeOperadoresProduto > 0 && item.capacidadeOperadoresLinha >0) {\r\n      //NOVO\r\n      // if (item.idOrdemProducaoPlano == 0) {\r\n      this.$router.push(\"/ordemProducao/\" + item.idOrdemProducaoLinha);\r\n      // } else {\r\n      //   //EDITAR\r\n      //   this.$router.push(\r\n      //     \"/ordemProducao/\" +\r\n      //       item.idOrdemProducaoLinha +\r\n      //       \"/\" +\r\n      //       item.idOrdemProducaoPlano\r\n      //   );\r\n      // }\r\n    } else {\r\n      this.showMsg(\r\n        \"O Modelo \" +\r\n          item.codigoProduto.toString().trim() +\r\n          \" sem Capacidade de Linha cadastrada ou configurada. Antes de definir o plano de Ordem de Produção, configure os dados do Modelo, utilizando a opção de Menu Cadastro -> Modelo\"\r\n      );\r\n    }\r\n  }\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/ordemProducao\");\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new OrdemProducaoListaController(this);\r\n\r\n    this._controller.carregarItens();\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.v-data-table th {\r\n  color: white !important;\r\n  background-color: #313c42 !important;\r\n}\r\n</style>>\r\n\r\n</style>"]}]}