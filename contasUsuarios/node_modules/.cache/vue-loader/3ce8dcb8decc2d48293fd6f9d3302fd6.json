isMobile\" color=\"#F79733\" fab dark @click=\"sair\">\r\n                <v-icon dark>mdi-login</v-icon>\r\n              </v-btn>\r\n            </v-col>\r\n          </v-row>\r\n\r\n          <v-row align=\"center\" justify=\"center\">\r\n            <v-col align=\"center\" justify=\"center\" cols=\"12\" sm=\"12\" md=\"12\">\r\n              <span id=\"clockData\">{{ \"Hoje: \" + dataAtual }}</span>\r\n            </v-col>\r\n          </v-row>\r\n\r\n          <v-row align=\"center\" justify=\"center\" style=\"margin-top: 0.5%\">\r\n            <v-col align=\"center\" justify=\"center\" cols=\"12\" sm=\"12\" md=\"12\">\r\n              <span id=\"clock\">{{ timeAtual }}</span>\r\n            </v-col>\r\n            <br />\r\n            <v-col cols=\"12\" sm=\"12\" md=\"4\">\r\n              <v-text-field\r\n                id=\"check\"\r\n                solo\r\n                placeholder=\"Informe o Check-in \"\r\n                v-model=\"frId\"\r\n                background-color=\"white\"\r\n                color=\"black\"\r\n                height=\"90\"\r\n                type=\"text\"\r\n                style=\"font-size: 40px\"\r\n                @change=\"validaChekingOperador\"\r\n              ></v-text-field>\r\n            </v-col>\r\n          </v-row>\r\n        </v-container>\r\n      </v-content>\r\n    </v-main>\r\n  </v-app>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from \"vue\";\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialogChekin from \"../../components/KDialogCheking.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\nimport moment from \"moment\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport {\r\n  ICheckinCatracaView,\r\n  ICheckinCatracaController,\r\n} from \"../../contracts/Linha/CheckinCatracaContract\";\r\n\r\nimport { CheckinCatracaController } from \"../../controllers/Linha/CheckinCatracaController\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialogChekin,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class CheckinCatraca extends Vue implements ICheckinCatracaView {\r\n  _controller?: ICheckinCatracaController;\r\n\r\n  resposta = \"Check-in realizado com sucesso.\";\r\n  dialogSelecionaLinha = true;\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  dialogMessageSeleciionaLinha = false;\r\n  dialogMessageSelecionaLinha = false;\r\n  textError = \"\";\r\n\r\n  cssTitulo = \"\";\r\n  cssTitulo2 = \"\";\r\n\r\n  dialogDadosOperador = false;\r\n\r\n  search = \"\";\r\n  dataAtual = moment(new Date()).format(\"DD/MM/YYYY\"); //\"\";\r\n  horaAtual = \"\";\r\n\r\n  dialogDelete = false;\r\n  itemFuncionarioDelete: any;\r\n\r\n  timeAtual: any = \"\";\r\n\r\n  frId = \"\";\r\n  funcionario: any = {};\r\n\r\n  tipo = \"E\";\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  registraCheckin() {\r\n    this.dialogDadosOperador = false;\r\n  }\r\n\r\n  chekinInvalido() {}\r\n\r\n  carregaDateTime() {\r\n    let dateTime = new Date();\r\n    this.dataAtual = \"Dia: \" + moment(dateTime).format(\"DD/MM/YYYY\");\r\n    this.horaAtual =\r\n      \"Hora: \" +\r\n      dateTime.getHours().toString() +\r\n      \":\" +\r\n      dateTime.getMinutes().toString();\r\n  }\r\n\r\n  sair() {\r\n    localStorage.clear();\r\n    this.$router.push(\"/Login\");\r\n  }\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  showItens(list: any): void {\r\n    this.itens = list;\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    this.setFocus(\"check\");\r\n  }\r\n\r\n  fecharDialogMessageSeecionaLinha() {\r\n    this.dialogMessageSelecionaLinha = false;\r\n    this.setFocus(\"check\");\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  itens = [];\r\n\r\n  validaDadosOperador(operador: any): void {\r\n    //if (!Storage.get(\"estacao_idLinha\")) {\r\n    //console.log(operador.length)\r\n    if (operador == null) {\r\n      this.showMsg(\r\n        \"Operador não Localizado, dirija-se ao R.H. para verificação dos seus dados de Check-in.\"\r\n      );\r\n    }\r\n\r\n    if (operador) {\r\n      this.dialogDadosOperador = true;\r\n      this.funcionario = operador;\r\n      this.carregaDateTime();\r\n\r\n      // if (operador.idLinha == this.idLinhaEstacao) {\r\n      //   this.cssTitulo = \"#019A53\";\r\n      //   this.resposta = \"OPERADOR ALOCADO PARA ESTA LINHA\";\r\n      //   checkin = true;\r\n      // } else {\r\n      //   this.cssTitulo = \"red\";\r\n      //   this.resposta =\r\n      //     \"OPERADOR NÃO ALOCADO PARA ESTA LINHA. OPERADOR DEVE REALIZAR CHECK-IN NA LINHA \" +\r\n      //     operador.nomeLinha;\r\n      // }\r\n\r\n      let d = new Date();\r\n      //d.toLocaleString('en-US', { timeZone: 'America/New_York' })\r\n\r\n      let dataConvert = new Date(d.valueOf() - d.getTimezoneOffset() * 60000);\r\n\r\n      let checkinOperadorLinha = {\r\n        idFuncionario: operador.id,\r\n\r\n        dataCheckin: dataConvert,\r\n        horaCheckin: dataConvert,\r\n        tipo: this.tipo,\r\n        observacao: \"\",\r\n      };\r\n\r\n      // console.log(checkinOperadorLinha);\r\n      // return;\r\n\r\n      this._controller!.salvarChekinOperador(checkinOperadorLinha);\r\n\r\n      //this.frId = \"\"\r\n    }\r\n    //}\r\n  }\r\n\r\n  validaChekingOperador() {\r\n    if (this.dialogDadosOperador == true) {\r\n      this.dialogDadosOperador = false;\r\n      // this.frId = \"\";\r\n      this.setFocus(\"check2\");\r\n    } else {\r\n      this._controller!.validaChekin(this.frId);\r\n      //  this.frId = \"\"\r\n      this.setFocus(\"check2\");\r\n    }\r\n  }\r\n\r\n  clockCheckin() {\r\n    setInterval(() => {\r\n      let time = new Date();\r\n\r\n      let hora = time.getHours().toString();\r\n      let minutos = time.getMinutes().toString();\r\n      let segundos = time.getSeconds().toString();\r\n\r\n      if (parseInt(hora) < 10) {\r\n        hora = \"0\" + time.getHours();\r\n      }\r\n\r\n      if (parseInt(minutos) < 10) {\r\n        minutos = \"0\" + time.getMinutes();\r\n      }\r\n\r\n      if (parseInt(segundos) < 10) {\r\n        segundos = \"0\" + time.getSeconds();\r\n      }\r\n\r\n      this.timeAtual = hora + \":\" + minutos + \":\" + segundos;\r\n\r\n      //this.timeAtual = time.getHours() + \":\" +min+ \":\"+time.getSeconds()\r\n    }, 1000);\r\n  }\r\n\r\n  setFocus(value: string) {\r\n    this.frId = \"\";\r\n    setTimeout(() => {\r\n      document.getElementById(value)!!.focus();\r\n    }, 500);\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new CheckinCatracaController(this);\r\n    this.clockCheckin();\r\n    this._controller.carregarItens();\r\n    // this.dialogSelecionaLinha = true;\r\n  }\r\n\r\n  mounted() {\r\n    this.setFocus(\"check\");\r\n  }\r\n\r\n  @Watch(\"dialogDadosOperador\")\r\n  dialogClose(val: string) {\r\n    //setTimeout(() => {\r\n    // if (this.dialogDadosOperador == true) {\r\n    //   this.dialogDadosOperador = false;\r\n    //this.frId = \"\";\r\n    this.setFocus(\"check\");\r\n    //}\r\n    //}, 3500);\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.loginFluid {\r\n  /* background-image: url(\"../assets/background-inventus6.png\"); */\r\n  background-size: cover;\r\n  height: 100%;\r\n}\r\n\r\n#inspire {\r\n  background: none;\r\n  height: 100%;\r\n}\r\n\r\n.Fundo {