{"remainingRequest":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\wamp64\\www\\smartpoint\\smartpoint\\src\\views\\Home.vue?vue&type=script&lang=ts&","dependencies":[{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\src\\views\\Home.vue","mtime":1615206741709},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\babel-loader\\lib\\index.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\ts-loader\\index.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Home.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;AAeA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Home.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div>\r\n    <k-loading title=\"Carregando\" :dialog=\"dialogLoading\"></k-loading>\r\n    <k-menu\r\n      @sair=\"sair\"\r\n      @changeMenu=\"createMenu\"\r\n      @changePass=\"changePass\"\r\n      :menu=\"menu\"\r\n    ></k-menu>\r\n\r\n    <router-view></router-view>\r\n  </div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from \"vue\";\r\nimport KLoading from \"../components/KLoading.vue\";\r\nimport KToolbarNavigationDrawer from \"../components/KToolbarNavigationDrawer.vue\";\r\nimport Component from \"vue-class-component\";\r\nimport { Menu, MenuItem } from \"../models/menu\";\r\n\r\nimport { IHomeView, IHomeController } from \"../contracts/HomeContract\";\r\n\r\nimport { Storage } from \"../configuration/storage\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    \"k-menu\": KToolbarNavigationDrawer,\r\n  },\r\n})\r\nexport default class Home extends Vue implements IHomeView {\r\n  itensMenu: MenuItem[] = [];\r\n  dialogLoading = false;\r\n\r\n  menu?: Menu;\r\n\r\n  usuario: any;\r\n  usuarioAcessos: any;\r\n\r\n  mostraErro(msg: string) {}\r\n\r\n  sair() {\r\n    Storage.salvar(\"Logado\", \"false\");\r\n    Storage.salvar(\"Session\", \"\");\r\n    Storage.salvar(\"User\", \"\");\r\n    Storage.salvar(\"Acessos\", \"\");\r\n    Storage.limpar();\r\n    this.$router.push(\"/Login\");\r\n  }\r\n\r\n  changePass() {\r\n    this.$router.push(\"/alteracaoSenha\");\r\n  }\r\n\r\n  async createMenu(modulo: String) {\r\n    this.menu = undefined;\r\n    this.itensMenu = [];\r\n\r\n    //verificar se é administrador\r\n    if (\r\n      this.usuarioAcessos.nomeUsuario == \"ADMINISTRADOR\" &&\r\n      this.usuarioAcessos.menus == null &&\r\n      this.usuarioAcessos.id == null &&\r\n      this.usuarioAcessos.descricaoPerfil == null\r\n    ) {\r\n      //MODULO RECURSOS HUMANOS\r\n      if (modulo == \"RH\") {\r\n        //CADASTRO\r\n        this.itensMenu.push(\r\n          new MenuItem(\"Cadastro\", \"\", true, [\r\n            new MenuItem(\"Funcionário\", \"/funcionarios\", false),\r\n            new MenuItem(\"Categorias\", \"/categorias\", false),\r\n            new MenuItem(\"Setor\", \"/setores\", false),\r\n            new MenuItem(\"Centro de Custo\", \"/centrosCustos\", false),\r\n            new MenuItem(\"Calendário\", \"/calendario\", false),\r\n          ])\r\n        );\r\n\r\n        //ACESSOS E PERFIS\r\n        this.itensMenu.push(\r\n          new MenuItem(\"Acessos e Perfis\", \"\", true, [\r\n            new MenuItem(\"Perfil\", \"/perfils\", false),\r\n            new MenuItem(\"Usuários\", \"/usuarios\", false),\r\n          ])\r\n        );\r\n\r\n        //FLUXO DE APROVAÇÃO\r\n        this.itensMenu.push(\r\n          new MenuItem(\"Fluxo de Aprovacão\", \"/fluxosAprovacao\", false)\r\n        );\r\n\r\n        //PONTOS\r\n        this.itensMenu.push(\r\n          new MenuItem(\"Pontos\", \"\", true, [\r\n            new MenuItem(\"Espelho de Ponto\", \"/espelhosPonto\", false),\r\n            new MenuItem(\"Saída Antecipada\", \"/saidaantecipada\", false),\r\n          ])\r\n        );\r\n\r\n        //ABSENTEISMO\r\n        this.itensMenu.push(new MenuItem(\"Absenteísmo\", \"/absenteismo\", false));\r\n      }\r\n\r\n      if (modulo == \"PRO\") {\r\n        //CADASTROS\r\n        this.itensMenu.push(\r\n          new MenuItem(\"Cadastro\", \"\", true, [\r\n            new MenuItem(\"Configurações\", \"/processos\", false),\r\n            new MenuItem(\"Turnos\", \"/turnos\", false),\r\n            //new MenuItem(\"Produto\", \"/produtos\", false),\r\n            new MenuItem(\"Modelos\", \"/linhas\", false),\r\n            new MenuItem(\"Linha\", \"/linhasProducao\", false),\r\n          ])\r\n        );\r\n\r\n        //ORDEM DE PRODUÇÃO\r\n        this.itensMenu.push(\r\n          new MenuItem(\"Ordem de Produção\", \"\", true, [\r\n            new MenuItem(\"Ordens de Produção ERP\", \"/ordensProducao\", false),\r\n            //new MenuItem(\"Simulações\", \"/ordensProducaoListaSimulacao\", false),\r\n          ])\r\n        );\r\n\r\n        //LINHAS\r\n        this.itensMenu.push(\r\n          new MenuItem(\"Linhas\", \"\", true, [\r\n            new MenuItem(\"Produção\", \"/linhascontrole\", false),\r\n          ])\r\n        );\r\n\r\n        //HABILIDADES DO OPERADOR\r\n        this.itensMenu.push(\r\n          new MenuItem(\"Habilidades do Operador\", \"/operadores\", false)\r\n        );\r\n\r\n        //CHECK-IN DE LINHA\r\n        this.itensMenu.push(\r\n          new MenuItem(\"Check-in de Linha\", \"/checkinLinha\", false)\r\n        );\r\n\r\n        //CATRACAS\r\n        this.itensMenu.push(\r\n          new MenuItem(\"Catracas\", \"\", true, [\r\n            new MenuItem(\"Check-in\", \"/checkinCatraca\", false),\r\n            new MenuItem(\"Logs Check-in e Check-out\", \"/log\", false),\r\n          ])\r\n        );\r\n      }\r\n    } else {\r\n      //usuarios do sistema\r\n      this.usuarioAcessos.menus.forEach((element: any) => {\r\n        //MODULO RECURSOS HUMANOS\r\n        if (modulo == \"RH\") {\r\n          if (element.modulo == \"RH\") {\r\n            //CADASTRO\r\n            if (element.descricao == \"CADASTROS\") {\r\n              this.itensMenu.push(\r\n                new MenuItem(\"Cadastro\", \"\", true, [\r\n                  new MenuItem(\"Funcionário\", \"/funcionarios\", false),\r\n                  new MenuItem(\"Categorias\", \"/categorias\", false),\r\n                  new MenuItem(\"Setor\", \"/setores\", false),\r\n                  new MenuItem(\"Centro de Custo\", \"/centrosCustos\", false),\r\n                  new MenuItem(\"Calendário\", \"/calendario\", false),\r\n                ])\r\n              );\r\n            }\r\n\r\n            //ACESSOS E PERFIS\r\n            if (element.descricao == \"ACESSOS E PERFIS\") {\r\n              this.itensMenu.push(\r\n                new MenuItem(\"Acessos e Perfis\", \"\", true, [\r\n                  new MenuItem(\"Perfil\", \"/perfils\", false),\r\n                  new MenuItem(\"Usuários\", \"/usuarios\", false),\r\n                ])\r\n              );\r\n            }\r\n\r\n            //FLUXO DE APROVAÇÃO\r\n            if (element.descricao == \"FLUXO DE APROVAÇÃO\") {\r\n              this.itensMenu.push(\r\n                new MenuItem(\"Fluxo de Aprovacão\", \"/fluxosAprovacao\", false)\r\n              );\r\n            }\r\n\r\n            //PONTOS\r\n            if (element.descricao == \"PONTOS\") {\r\n              this.itensMenu.push(\r\n                new MenuItem(\"Pontos\", \"\", true, [\r\n                  new MenuItem(\"Espelho de Ponto\", \"/espelhosPonto\", false),\r\n                  new MenuItem(\"Saída Antecipada\", \"/saidaantecipada\", false),\r\n                ])\r\n              );\r\n            }\r\n\r\n            //ABSENTEISMO\r\n            if (element.descricao == \"ABSENTEISMO\") {\r\n              this.itensMenu.push(\r\n                new MenuItem(\"Absenteísmo\", \"/absenteismo\", false)\r\n              );\r\n            }\r\n          }\r\n        }\r\n\r\n        //MODULO PRODUÇÃO\r\n        if (modulo == \"PRO\") {\r\n          if (element.modulo == \"PRO\") {\r\n            //CADASTROS\r\n            if (element.descricao == \"CADASTROS\") {\r\n              this.itensMenu.push(\r\n                new MenuItem(\"Cadastro\", \"\", true, [\r\n                  new MenuItem(\"Configurações\", \"/processos\", false),\r\n                  new MenuItem(\"Turnos\", \"/turnos\", false),\r\n                  //new MenuItem(\"Produto\", \"/produtos\", false),\r\n                  new MenuItem(\"Modelos\", \"/linhas\", false),\r\n                  new MenuItem(\"Linha\", \"/linhasProducao\", false),\r\n                ])\r\n              );\r\n            }\r\n\r\n            //ORDEM DE PRODUÇÃO\r\n            if (element.descricao == \"ORDEM DE PRODUÇÃO\") {\r\n              this.itensMenu.push(\r\n                new MenuItem(\"Ordem de Produção\", \"\", true, [\r\n                  new MenuItem(\r\n                    \"Ordens de Produção ERP\",\r\n                    \"/ordensProducao\",\r\n                    false\r\n                  ),\r\n                  //new MenuItem(\"Simulações\", \"/ordensProducaoListaSimulacao\", false),\r\n                ])\r\n              );\r\n            }\r\n\r\n            //LINHAS\r\n            if (element.descricao == \"LINHAS\") {\r\n              this.itensMenu.push(\r\n                new MenuItem(\"Linhas\", \"\", true, [\r\n                  new MenuItem(\"Produção\", \"/linhascontrole\", false),\r\n                ])\r\n              );\r\n            }\r\n\r\n            //HABILIDADES DO OPERADOR\r\n            if (element.descricao == \"HABILIDADES DO OPERADOR\") {\r\n              this.itensMenu.push(\r\n                new MenuItem(\"Habilidades do Operador\", \"/operadores\", false)\r\n              );\r\n            }\r\n\r\n            //CHECK-IN DE LINHA\r\n            if (element.descricao == \"CHECK-IN DE LINHA\") {\r\n              this.itensMenu.push(\r\n                new MenuItem(\"Check-in de Linha\", \"/checkinLinha\", false)\r\n              );\r\n            }\r\n\r\n            //CATRACAS\r\n            if (element.descricao == \"CATRACAS\") {\r\n              this.itensMenu.push(\r\n                new MenuItem(\"Catracas\", \"\", true, [\r\n                  new MenuItem(\"Check-in\", \"/checkinCatraca\", false),\r\n                  new MenuItem(\"Logs Check-in e Check-out\", \"/log\", false),\r\n                ])\r\n              );\r\n            }\r\n          }\r\n        }\r\n      });\r\n    }\r\n    let menu: Menu | undefined = undefined;\r\n\r\n    menu = new Menu(this.usuario.nome!, this.itensMenu);\r\n    menu.appBarStyle = \"background: white\";\r\n\r\n    //console.log(menu);\r\n\r\n    this.menu = menu;\r\n    this.$forceUpdate();\r\n  }\r\n\r\n  async usuarioPermissaoAcessos() {\r\n    let acessos: any = [];\r\n    acessos = JSON.parse(Storage.get(\"Acessos\")!!);\r\n    this.usuarioAcessos = acessos;\r\n\r\n    //console.log(this.usuarioAcessos);\r\n  }\r\n\r\n  async created() {\r\n    var logado = Storage.get(\"Logado\");\r\n    if (logado == \"true\") {\r\n      this.usuario = JSON.parse(Storage.get(\"Colaborador\")!!);\r\n      await this.usuarioPermissaoAcessos();\r\n      await this.createMenu(\"RH\");\r\n    } else {\r\n      this.sair();\r\n    }\r\n  }\r\n}\r\n</script>"]}]}