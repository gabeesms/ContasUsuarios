;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA","file":"LinhaParadaForm.vue","sourceRoot":"src/views/Linha","sourcesContent":["<template>\r\n  <v-content style=\"margin: 10px\">\r\n    <v-snackbar\r\n      v-model=\"snackbar\"\r\n      :color=\"colorBorder\"\r\n      shaped\r\n      timeout=\"3000\"\r\n      :vertical=\"vertical\"\r\n    >\r\n      <b>{{ text }}</b>\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn v-bind=\"attrs\" @click=\"snackbar = false\" icon dark small>\r\n          <v-icon>mdi-close</v-icon>\r\n        </v-btn>\r\n        <!-- <v-btn icon v-bind=\"attrs\" @click=\"snackbar = false\">mdi-magnify</v-btn> -->\r\n      </template>\r\n    </v-snackbar>\r\n\r\n    <k-loading title=\"Carregando\" :dialog=\"dialogLoading\"></k-loading>\r\n\r\n    <k-dialog\r\n      @fecharDialog=\"fecharDialogMessage\"\r\n      :dialog=\"dialogMessage\"\r\n      title=\"Mensagem\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n\r\n    <v-container>\r\n      <v-row ref=\"form\">\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-row>\r\n            <h3>\r\n              <b>Solicitação de Parada</b>\r\n            </h3>\r\n          </v-row>\r\n\r\n          <br />\r\n\r\n          <v-row>\r\n            <v-col cols=\"12\" sm=\"12\">\r\n              <v-row>\r\n                <span>\r\n                  <b id=\"titulo\">Linha</b>\r\n                </span>\r\n              </v-row>\r\n\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"6\">\r\n                  <v-select\r\n                    :items=\"itensLinhasProdutos\"\r\n                    v-model=\"linhaSolicitacaoParada.idLinha\"\r\n                    label=\"Linha\"\r\n                    placeholder=\"Selecione a Linha\"\r\n                    item-text=\"nomeLinha\"\r\n                    item-value=\"idLinha\"\r\n                    outlined\r\n                    dense\r\n                    @input=\"changeLinhaProdutoSelecionado()\"\r\n                  ></v-select>\r\n                </v-col>\r\n\r\n                <v-col cols=\"12\" sm=\"6\">\r\n                  <v-select\r\n                    :items=\"itensProdutos\"\r\n                    v-model=\"linhaSolicitacaoParada.idProduto\"\r\n                    label=\"Produto\"\r\n                    placeholder=\"Produto\"\r\n                    item-text=\"dadosProduto\"\r\n                    item-value=\"idProduto\"\r\n                    outlined\r\n                    dense\r\n                  >\r\n                  </v-select>\r\n                </v-col>\r\n              </v-row>\r\n\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"4\">\r\n                  <v-select\r\n                    v-model=\"linhaSolicitacaoParada.tipo\"\r\n                    :items=\"itemsTipoParada\"\r\n                    label=\"Tipo de Solicitação Parada\"\r\n                    placeholder=\"Selecione o tipo de Solicitação de Parada\"\r\n                    item-text=\"texto\"\r\n                    item-value=\"tipo\"\r\n                    outlined\r\n                    dense\r\n                  >\r\n                  </v-select>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"4\">\r\n                  <v-text-field\r\n                    label=\"Qtde Produzida\"\r\n                    placeholder=\"Qtde Produzida\"\r\n                    outlined\r\n                    dense\r\n                    v-model=\"qtdeProduzida\"\r\n                    @keypress=\"apenasNumeros\"\r\n                  >\r\n                  </v-text-field>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"4\">\r\n                  <v-text-field\r\n                    v-model=\"horario\"\r\n                    label=\"Hora\"\r\n                    placeholder=\"Hora da parada de linha\"\r\n                    type=\"time\"\r\n                    outlined\r\n                    style=\"height: 43px; border-width: 1px\"\r\n                    step=\"1\"\r\n                    dense\r\n                    @keypress=\"apenasNumeros\"\r\n                  ></v-text-field>\r\n                </v-col>\r\n              </v-row>\r\n\r\n              <v-row>\r\n                <v-col cols=\"12\" sm=\"12\">\r\n                  <v-text-field\r\n                    label=\"Justificativa\"\r\n                    placeholder=\"Justificativa da Parada de Linha\"\r\n                    outlined\r\n                    dense\r\n                    v-model=\"linhaSolicitacaoParada.justificativa\"\r\n                    maxlength=\"200\"\r\n                  >\r\n                  </v-text-field>\r\n                </v-col>\r\n              </v-row>\r\n            </v-col>\r\n          </v-row>\r\n\r\n          <v-row>\r\n            <v-col cols=\"12\" sm=\"12\"></v-col>\r\n            <v-col cols=\"12\" sm=\"12\">\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn\r\n                  color=\"orange lighten-1\"\r\n                  @click=\"retornaListaLinhaControle()\"\r\n                  large\r\n                  dark\r\n                  width=\"150\"\r\n                  >Cancelar</v-btn\r\n                >\r\n\r\n                <v-btn\r\n                  v-if=\"!isMobile\"\r\n                  color=\"orange darken-1\"\r\n                  large\r\n                  dark\r\n                  width=\"150\"\r\n                  @click=\"saveParadaLinha()\"\r\n                  >SOLICITAR</v-btn\r\n                >\r\n\r\n                <v-btn\r\n                  v-else-if=\"isMobile\"\r\n                  color=\"orange darken-1\"\r\n                  large\r\n                  dark\r\n                  width=\"150\"\r\n                  @click=\"saveParadaLinha()\"\r\n                  >SOLICITAR</v-btn\r\n                >\r\n              </v-card-actions>\r\n            </v-col>\r\n          </v-row>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template> \r\n\r\n\r\n\r\n<script lang=\"ts\">\r\nvar validaNumeros = require(\"../../baseGenerica/validaNumeros.js\");\r\n\r\nimport Vue from \"vue\";\r\n\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\nimport moment from \"moment\";\r\n\r\nimport {\r\n  ILinhaParadaFormView,\r\n  ILinhaParadaFormController,\r\n} from \"../../contracts/Linha/LinhaParadaFormContract\";\r\nimport { LinhaParadaFormController } from \"../../controllers/Linha/LinhaParadaFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class LinhaParadaForm\r\n  extends Vue\r\n  implements ILinhaParadaFormView {\r\n  _controller?: ILinhaParadaFormController;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"RotaAqui\",\r\n      disabled: false,\r\n      href: \"/rota\",\r\n    },\r\n  ];\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  qtdeProduzida = \"\";\r\n  horario = \"\";\r\n\r\n  /*PP - PARADA SOLICITADA\r\n  PNP - PARADA NÃO SOLICITADA*/\r\n  itemsTipoParada = [\r\n    {\r\n      tipo: \"PP\",\r\n      texto: \"PARADA PROGRAMADA\",\r\n    },\r\n    {\r\n      tipo: \"PNP\",\r\n      texto: \"PARADA NÃO PROGRAMADA\",\r\n    },\r\n  ];\r\n\r\n  //ESTRUTURA PARADA\r\n  /*status\r\n    NP - REPROVADO\r\n    AP - APROVADO\r\n    -AC - AGUARDANDO APROVACAO*/\r\n  linhaSolicitacaoParada = {\r\n    idLinha: 0,\r\n    idProduto: 0,\r\n    qtdeProduzida: 0,\r\n    horario: new Date(),\r\n    justificativa: \"\",\r\n    dataCadastro: new Date(),\r\n    status: \"AC\",\r\n    tipo: \"\",\r\n  };\r\n\r\n  itensProdutos: any = [];\r\n  itensLinhasProdutos: any = [];\r\n\r\n  changeLinhaProdutoSelecionado() {\r\n    //console.log(this.linhaSolicitacaoParada.idLinha);\r\n    this.itensProdutos = [];\r\n    this.itensLinhasProdutos.forEach((element: any) => {\r\n      if (element.idLinha == this.linhaSolicitacaoParada.idLinha) {\r\n        this.itensProdutos.push(element);\r\n      }\r\n    });\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/linhascontrole\");\r\n    }\r\n  }\r\n\r\n  retornaListaLinhaControle() {\r\n    this.$router.push(\"/linhascontrole\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    if (\r\n      this.linhaSolicitacaoParada.idLinha == null ||\r\n      this.linhaSolicitacaoParada.idLinha == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a linha.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.linhaSolicitacaoParada.idProduto == null ||\r\n      this.linhaSolicitacaoParada.idProduto == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a Produto.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.linhaSolicitacaoParada.tipo == null ||\r\n      this.linhaSolicitacaoParada.tipo == \"\"\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o tipo de solicitação de parada de linha.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.qtdeProduzida == null ||\r\n      this.qtdeProduzida == \"\" ||\r\n      this.qtdeProduzida.length == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Informe a quantidade produzida.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.horario == \"00:00:00\" ||\r\n      this.horario == null ||\r\n      this.horario == \"\" ||\r\n      this.horario.length == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Informe a hora da parada de linha.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.linhaSolicitacaoParada.justificativa == null ||\r\n      this.linhaSolicitacaoParada.justificativa == \"\" ||\r\n      this.linhaSolicitacaoParada.justificativa.length == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Informe a justificativa da parada de linha.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  saveParadaLinha() {\r\n    if (this.validate()) {\r\n    } else {\r\n      this.linhaSolicitacaoParada.qtdeProduzida = parseInt(this.qtdeProduzida);\r\n      this.linhaSolicitacaoParada.horario = new Date(\r\n        moment(this.horario, \"HH:mm:ss\").toDate().valueOf() -\r\n          moment(this.horario, \"HH:mm:ss\").toDate().getTimezoneOffset() * 60000\r\n      );\r\n\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.salvar(this.linhaSolicitacaoParada);\r\n    }\r\n  }\r\n\r\n  // carregaLinhas(itens: any): void{\r\n  //   this.itensLinhas = itens;\r\n  // }\r\n\r\n  carregaLinhasProdutos(itens: any): void {\r\n    this.itensLinhasProdutos = itens;\r\n  }\r\n\r\n  carregarDtos(): void {}\r\n\r\n  created() {\r\n    this._controller = new LinhaParadaFormController(this);\r\n    this._controller.carregarDtos(this.$route.params.id);\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n</script> "]}]}