{"remainingRequest":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\wamp64\\www\\smartpoint\\smartpoint\\src\\views\\EspelhoPonto\\EspelhoPontoLista.vue?vue&type=script&lang=ts&","dependencies":[{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\src\\views\\EspelhoPonto\\EspelhoPontoLista.vue","mtime":1609294096381},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\thread-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\babel-loader\\lib\\index.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\ts-loader\\index.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["EspelhoPontoLista.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwQA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;;AAEA;;AAEA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;;;AAIA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA","file":"EspelhoPontoLista.vue","sourceRoot":"src/views/EspelhoPonto","sourcesContent":["<template>\r\n  <v-content style=\"margin: 10px\">\r\n    <v-snackbar\r\n      v-model=\"snackbar\"\r\n      :color=\"colorBorder\"\r\n      shaped\r\n      timeout=\"3000\"\r\n      :vertical=\"vertical\"\r\n    >\r\n      <b>{{ text }}</b>\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn v-bind=\"attrs\" @click=\"snackbar = false\" icon dark small>\r\n          <v-icon>mdi-close</v-icon>\r\n        </v-btn>\r\n        <!-- <v-btn icon v-bind=\"attrs\" @click=\"snackbar = false\">mdi-magnify</v-btn> -->\r\n      </template>\r\n    </v-snackbar>\r\n\r\n    <k-loading title=\"Carregando\" :dialog=\"dialogLoading\"></k-loading>\r\n    <k-dialog\r\n      @fecharDialog=\"fecharDialogMessage\"\r\n      :dialog=\"dialogMessage\"\r\n      title=\"Error\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n    <k-dialog\r\n      :dialog=\"dialogDelete\"\r\n      title=\"Remover Funcionário?\"\r\n      text=\"O funcionário selecionado será removido. Deseja Continuar?\"\r\n    >\r\n      <template v-slot:buttoes>\r\n        <v-btn\r\n          @click=\"dialogDelete = false\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"red darken-2\"\r\n          >Cancelar</v-btn\r\n        >\r\n        <v-btn\r\n          @click=\"deleteItemConfirmado\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"green darken-2\"\r\n          >Sim</v-btn\r\n        >\r\n      </template>\r\n    </k-dialog>\r\n\r\n    <!-- MODAL ESPELHO DE PONTO -->\r\n\r\n    <v-dialog v-model=\"dialogEspelhoPonto\" persistent max-width=\"800px\">\r\n      <v-card>\r\n        <v-card-title style=\"background-color: #313c42\">\r\n          <span style=\"color: white\" class=\"headline\"> Espelho de Ponto </span>\r\n        </v-card-title>\r\n        <v-card-text>\r\n          <v-container>\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <span>\r\n                  <b>Parâmetros para o espelho de ponto</b>\r\n                </span>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row style=\"margin-top: -20px\">\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <v-select\r\n                  outlined\r\n                  label=\"Turno *\"\r\n                  placeholder=\"Selecione o Turno\"\r\n                  :items=\"itensTurno\"\r\n                  v-model=\"turnos\"\r\n                  style=\"text-align: right\"\r\n                  item-value=\"codigoGeneric\"\r\n                  item-text=\"campoGeneric\"\r\n                  dense\r\n                  required\r\n                  return-object\r\n                  multiple\r\n                >\r\n                </v-select>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"12\">\r\n                <span>\r\n                  <b>Informe o período para o espelho de ponto</b>\r\n                </span>\r\n              </v-col>\r\n            </v-row>\r\n\r\n            <v-row>\r\n              <v-col cols=\"12\" sm=\"4\">\r\n                <v-menu\r\n                  ref=\"menu1\"\r\n                  v-model=\"menu1\"\r\n                  :close-on-content-click=\"false\"\r\n                  transition=\"scale-transition\"\r\n                  offset-y\r\n                  max-width=\"290px\"\r\n                  min-width=\"290px\"\r\n                >\r\n                  <template v-slot:activator=\"{ on, attrs }\">\r\n                    <v-text-field\r\n                      v-model=\"dateFormatted\"\r\n                      label=\"Data Inicial *\"\r\n                      persistent-hint\r\n                      v-bind=\"attrs\"\r\n                      @blur=\"date = parseDate(dateFormatted)\"\r\n                      v-on=\"on\"\r\n                      outlined\r\n                      dense\r\n                      v-mask=\"maskDatas\"\r\n                      append-icon=\"mdi-calendar\"\r\n                    ></v-text-field>\r\n                  </template>\r\n                  <v-date-picker\r\n                    v-model=\"date\"\r\n                    no-title\r\n                    @input=\"menu1 = false\"\r\n                  ></v-date-picker>\r\n                </v-menu>\r\n              </v-col>\r\n\r\n              <v-col cols=\"12\" sm=\"4\">\r\n                <v-menu\r\n                  ref=\"menu2\"\r\n                  v-model=\"menu2\"\r\n                  :close-on-content-click=\"false\"\r\n                  transition=\"scale-transition\"\r\n                  offset-y\r\n                  max-width=\"290px\"\r\n                  min-width=\"290px\"\r\n                >\r\n                  <template v-slot:activator=\"{ on, attrs }\">\r\n                    <v-text-field\r\n                      v-model=\"dateFormatted2\"\r\n                      label=\"Data Final *\"\r\n                      persistent-hint\r\n                      v-bind=\"attrs\"\r\n                      @blur=\"date = parseDate(dateFormatted2)\"\r\n                      v-on=\"on\"\r\n                      outlined\r\n                      dense\r\n                      v-mask=\"maskDatas2\"\r\n                      append-icon=\"mdi-calendar\"\r\n                    ></v-text-field>\r\n                  </template>\r\n                  <v-date-picker\r\n                    v-model=\"date2\"\r\n                    no-title\r\n                    @input=\"menu2 = false\"\r\n                  ></v-date-picker>\r\n                </v-menu>\r\n              </v-col>\r\n            </v-row>\r\n          </v-container>\r\n        </v-card-text>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn\r\n            color=\"orange lighten-1\"\r\n            width=\"150\"\r\n            @click=\"cancelarBuscarEspelhoPonto\"\r\n            dark\r\n            >Cancelar</v-btn\r\n          >\r\n          <v-btn\r\n            color=\"orange darken-1\"\r\n            width=\"150\"\r\n            dark\r\n            @click=\"buscarEspelhoPonto\"\r\n            >Buscar</v-btn\r\n          >\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n          <template v-slot:item=\"{ item }\">\r\n            <router-link :to=\"item.href\">\r\n              {{ item.text }}\r\n            </router-link>\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n\r\n      <v-row>\r\n        <v-col cols=\"10\">\r\n          <h3>\r\n            <b>Espelho de Ponto</b>\r\n          </h3>\r\n          <!-- <span>\r\n          <b>Administre os perfis do sistema de maneira mais personalizada.</b>\r\n          </span>-->\r\n        </v-col>\r\n        <v-spacer></v-spacer>\r\n\r\n        <v-col cols=\"1\">\r\n          <v-btn\r\n            v-if=\"!isMobile\"\r\n            color=\"#F79D3E\"\r\n            fab\r\n            dark\r\n            @click=\"modalEspelhoPonto\"\r\n            ><v-icon title=\"Busca para espelho de ponto\"\r\n              >mdi-magnify</v-icon\r\n            ></v-btn\r\n          >\r\n        </v-col>\r\n\r\n        <!-- <v-col cols=\"1\">\r\n        <v-btn v-if=\"!isMobile\" color=\"primary\" fab dark @click=\"addSolicitacao\">\r\n          <v-icon dark>mdi-plus</v-icon>\r\n        </v-btn>\r\n        </v-col>-->\r\n      </v-row>\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-col cols=\"12\" sm=\"5\" style=\"float: right\">\r\n            <v-text-field\r\n              v-model=\"search\"\r\n              label=\"Pesquisa Funcionário\"\r\n              class=\"mx-4\"\r\n              prepend-icon=\"mdi-magnify\"\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-col>\r\n      </v-row>\r\n\r\n      <v-row style=\"margin-top: -10px\" align=\"center\" justify=\"center\">\r\n        <v-col cols=\"12\">\r\n          <k-data-list\r\n            orientation=\"Horizontal\"\r\n            mobileIconAdd=\"mdi-plus-circle\"\r\n            mobileFunctionAdd=\"addSolicitacao\"\r\n            @editarItem=\"editarItem\"\r\n            @deleteItem=\"deleteItem\"\r\n            @addSolicitacao=\"addSolicitacao\"\r\n            :headers=\"headers\"\r\n            :search=\"search\"\r\n            :itens=\"itens\"\r\n          >\r\n            <template v-slot:tipo=\"{ item }\">\r\n              <l v-if=\"item.tipo == 'EXT'\">EXTERNO</l>\r\n              <l v-if=\"item.tipo == 'ERP'\">ERP</l>\r\n            </template>\r\n          </k-data-list>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from \"vue\";\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  IEspelhoPontoListaView,\r\n  IEspelhoPontoListaController,\r\n} from \"../../contracts/EspelhoPonto/EspelhoPontoListaContract\";\r\n\r\nimport { EspelhoPontoListaController } from \"../../controllers/EspelhoPonto/EspelhoPontoListaController\";\r\n\r\nimport Component from \"vue-class-component\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\n\r\nexport default class EspelhoPontoLista\r\n  extends Vue\r\n  implements IEspelhoPontoListaView {\r\n  _controller?: IEspelhoPontoListaController;\r\n\r\n  itensTurno: any = [];\r\n  turnos: any = [];\r\n\r\n  // DATA INCIAL E FINAL PARA MODAL DE BUSCA DE ESPELHO DE PONTO\r\n  date = new Date().toISOString().substr(0, 10);\r\n  date2 = new Date().toISOString().substr(0, 10);\r\n  dateFormatted = this.formatDate(new Date().toISOString().substr(0, 10));\r\n  dateFormatted2 = this.formatDate(new Date().toISOString().substr(0, 10));\r\n\r\n  dtInicio: any;\r\n  dtFinal: any;\r\n\r\n  menu1 = false;\r\n  menu2 = false;\r\n  maskDatas = \"##/##/####\";\r\n  maskDatas2 = \"##/##/####\";\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  search = \"\";\r\n\r\n  dialogEspelhoPonto = false;\r\n\r\n  dialogDelete = false;\r\n  itemFuncionarioDelete: any;\r\n\r\n  itens = [];\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  limparCampos(): void {\r\n    this.dateFormatted = \"\",\r\n    this.dateFormatted2 = \"\",\r\n    this.turnos = \"\";\r\n\r\n  }\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Espelhos de Ponto\",\r\n      disabled: false,\r\n      href: \"/espelhosPonto\",\r\n    },\r\n  ];\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n  showItens(list: any): void {\r\n    this.itens = list;\r\n\r\n    this.dialogEspelhoPonto = false;\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemFuncionarioDelete = item;\r\n    this.dialogDelete = true;\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    //this._controller!.deleteitem(this.itemFuncionarioDelete.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    this.$router.push(\r\n      \"/espelhoponto/\" +\r\n        item.matricula +\r\n        \"/\" +\r\n        this.dtInicio +\r\n        \"/\" +\r\n        this.dtFinal\r\n    );\r\n  }\r\n\r\n  modalEspelhoPonto() {\r\n    this.limparCampos();\r\n    this.dialogEspelhoPonto = true;\r\n  }\r\n\r\n  cancelarBuscarEspelhoPonto() {\r\n    this.limparCampos();\r\n    this.dialogEspelhoPonto = false;\r\n  }\r\n\r\n  validateBuscaEspelhoPonto() {\r\n    if (\r\n      this.turnos.length == 0 ||\r\n      this.turnos == null ||\r\n      this.turnos == undefined\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o turno para a buscar o espelho de ponto.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.dateFormatted == \"\" ||\r\n      this.dateFormatted == null ||\r\n      this.dateFormatted == undefined\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text =\r\n        \"Informe a data inicial para buscar o período do espelho de ponto.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.dateFormatted2 == \"\" ||\r\n      this.dateFormatted2 == null ||\r\n      this.dateFormatted2 == undefined\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text =\r\n        \"Informe a data final para buscar o período do espelho de ponto.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n  }\r\n\r\n  buscarEspelhoPonto() {\r\n    if (!this.validateBuscaEspelhoPonto()) {\r\n      this.dtInicio = this.dateFormatted;\r\n      this.dtInicio = this.dtInicio.toString().split(\"/\").reverse().join(\"\");\r\n\r\n      this.dtFinal = this.dateFormatted2;\r\n      this.dtFinal = this.dtFinal.toString().split(\"/\").reverse().join(\"\");\r\n\r\n      this._controller!.buscarEspelhoPonto(\r\n        this.dtInicio,\r\n        this.dtFinal,\r\n        this.turnos\r\n      );\r\n    }\r\n  }\r\n\r\n  carregaTurnos(itemsTurno: any): void {\r\n    this.itensTurno = itemsTurno;\r\n\r\n    this.itensTurno.forEach((element: any) => {\r\n      element.textoDescHora =\r\n        element.descricao + \" - HORAS TOTAL: \" + element.horaTotal;\r\n    });\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  headers = new Headers([\r\n    new Header(\"Status\", \"\", false, false, \"center\", true, \"100\"),\r\n    new Header(\"Matrícula\", \"matricula\", false, false, \"left\", true, \"100\"),\r\n    new Header(\"Nome\", \"nome\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Tipo Funcionário\", \"tipo\", false, false, \"center\", true, \"150\"),\r\n    new Header(\"Turno\", \"turno\", false, false, \"left\", true, \"300\"),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  \r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/perfil\");\r\n  }\r\n\r\n  formatDate(date: string) {\r\n    if (!date) return null;\r\n\r\n    const [year, month, day] = date.split(\"-\");\r\n    return `${day}/${month}/${year}`;\r\n  }\r\n\r\n  parseDate(date: string) {\r\n    if (!date) return null;\r\n    const [day, month, year] = date.split(\"/\");\r\n    return `${year}-${month.padStart(2, \"0\")}-${day.padStart(2, \"0\")}`;\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new EspelhoPontoListaController(this);\r\n    this._controller.carregarItens();\r\n  }\r\n\r\n  @Watch(\"date\")\r\n  onDatasChanged(val: string) {\r\n    this.dateFormatted = this.formatDate(this.date);\r\n  }\r\n\r\n  @Watch(\"date2\")\r\n  onDatas2Changed(val: string) {\r\n    this.dateFormatted2 = this.formatDate(this.date2);\r\n  }\r\n}\r\n</script>"]}]}