{"remainingRequest":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\wamp64\\www\\smartpoint\\smartpoint\\src\\views\\Linha\\LinhaSolicitacaoEmprestimoForm.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\src\\views\\Linha\\LinhaSolicitacaoEmprestimoForm.vue","mtime":1615411745168},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\wamp64\\www\\smartpoint\\smartpoint\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["LinhaSolicitacaoEmprestimoForm.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAg1BA;AACA;AACA;AACA;AACA;AACA;AACA","file":"LinhaSolicitacaoEmprestimoForm.vue","sourceRoot":"src/views/Linha","sourcesContent":["<template>\r\n  <v-content style=\"margin: 10px\">\r\n    <v-snackbar\r\n      v-model=\"snackbar\"d\r\n      :color=\"colorBorder\"\r\n      shaped\r\n      timeout=\"4000\"\r\n      :vertical=\"vertical\"\r\n    >\r\n      <b>{{ text }}</b>\r\n      <template v-slot:action=\"{ attrs }\">\r\n        <v-btn v-bind=\"attrs\" @click=\"snackbar = false\" icon dark small>\r\n          <v-icon>mdi-close</v-icon>\r\n        </v-btn>\r\n        <!-- <v-btn icon v-bind=\"attrs\" @click=\"snackbar = false\">mdi-magnify</v-btn> -->\r\n      </template>\r\n    </v-snackbar>\r\n\r\n    <k-loading title=\"Carregando\" :dialog=\"dialogLoading\"></k-loading>\r\n\r\n    <k-dialog\r\n      @fecharDialog=\"fecharDialogMessage\"\r\n      :dialog=\"dialogMessage\"\r\n      title=\"Mensagem\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n\r\n    <v-container>\r\n      <v-row ref=\"form\">\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-row>\r\n            <h3>\r\n              <b>Empréstimo</b>\r\n            </h3>\r\n          </v-row>\r\n\r\n          <br />\r\n\r\n          <v-row>\r\n            <!-- LINHA ATUAL -->\r\n            <v-col cols=\"12\" sm=\"12\">\r\n              <v-row Style=\"margin-top: -15px\">\r\n                <v-col cols=\"12\" sm=\"4\">\r\n                  <v-select\r\n                    :items=\"itemsLinha\"\r\n                    return-object\r\n                    v-model=\"linhaSelecionada\"\r\n                    @input=\"changeOperacao()\"\r\n                    label=\"Linha *\"\r\n                    placeholder=\"Selecione a Linha *\"\r\n                    autocomplete=\"off\"\r\n                    item-text=\"nome\"\r\n                    item-value=\"id\"\r\n                    outlined\r\n                    dense\r\n                  ></v-select>\r\n                </v-col>\r\n\r\n                <v-col cols=\"12\" sm=\"4\">\r\n                  <v-select\r\n                    :items=\"itemsOperacao\"\r\n                    return-object\r\n                    v-model=\"operacaoSelecionada\"\r\n                    @input=\"changeHabilidades()\"\r\n                    label=\"Operação *\"\r\n                    placeholder=\"Selecione a Operação*\"\r\n                    autocomplete=\"off\"\r\n                    item-text=\"nome\"\r\n                    item-value=\"id\"\r\n                    outlined\r\n                    dense\r\n                  ></v-select>\r\n                </v-col>\r\n\r\n                <v-col cols=\"12\" sm=\"4\">\r\n                  <v-select\r\n                    :items=\"itemsHabilidade\"\r\n                    return-object\r\n                    v-model=\"habilidadeSelecionada\"\r\n                    label=\"Habilidade *\"\r\n                    placeholder=\"Selecione a Habilidade *\"\r\n                    autocomplete=\"off\"\r\n                    item-text=\"nome\"\r\n                    item-value=\"id\"\r\n                    outlined\r\n                    dense\r\n                  ></v-select>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"7\">\r\n                  <v-select\r\n                    :items=\"itemsOperador\"\r\n                    return-object\r\n                    v-model=\"operadorSelecionada\"\r\n                    label=\"Operadores *\"\r\n                    placeholder=\"Selecione os Operadores\"\r\n                    autocomplete=\"off\"\r\n                    item-text=\"nome\"\r\n                    item-value=\"id\"\r\n                    outlined\r\n                    dense\r\n                    multiple\r\n                  ></v-select>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"2\">\r\n                  <v-menu\r\n                    ref=\"menu1\"\r\n                    v-model=\"menu1\"\r\n                    :close-on-content-click=\"false\"\r\n                    transition=\"scale-transition\"\r\n                    offset-y\r\n                    max-width=\"290px\"\r\n                    min-width=\"290px\"\r\n                  >\r\n                    <template v-slot:activator=\"{ on, attrs }\">\r\n                      <v-text-field\r\n                        v-model=\"dateFormatted\"\r\n                        label=\"Período Início\"\r\n                        persistent-hint\r\n                        v-bind=\"attrs\"\r\n                        @blur=\"date = parseDate(dateFormatted)\"\r\n                        v-on=\"on\"\r\n                        outlined\r\n                        dense\r\n                        v-mask=\"maskDatas\"\r\n                      ></v-text-field>\r\n                    </template>\r\n                    <v-date-picker\r\n                      v-model=\"date\"\r\n                      no-title\r\n                      @input=\"menu1 = false\"\r\n                    ></v-date-picker>\r\n                  </v-menu>\r\n                </v-col>\r\n\r\n                <v-col cols=\"12\" sm=\"2\">\r\n                  <v-menu\r\n                    ref=\"menu2\"\r\n                    v-model=\"menu2\"\r\n                    :close-on-content-click=\"false\"\r\n                    transition=\"scale-transition\"\r\n                    offset-y\r\n                    max-width=\"290px\"\r\n                    min-width=\"290px\"\r\n                  >\r\n                    <template v-slot:activator=\"{ on, attrs }\">\r\n                      <v-text-field\r\n                        v-model=\"dateFormatted2\"\r\n                        label=\"Período Fim\"\r\n                        persistent-hint\r\n                        v-bind=\"attrs\"\r\n                        @blur=\"date2 = parseDate(dateFormatted2)\"\r\n                        v-on=\"on\"\r\n                        outlined\r\n                        dense\r\n                        v-mask=\"maskDatas2\"\r\n                      ></v-text-field>\r\n                    </template>\r\n                    <v-date-picker\r\n                      v-model=\"date2\"\r\n                      no-title\r\n                      @input=\"menu2 = false\"\r\n                    ></v-date-picker>\r\n                  </v-menu>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"1\">\r\n                  <v-btn\r\n                    style=\"float: right\"\r\n                    small\r\n                    color=\"#FB9514\"\r\n                    fab\r\n                    dark\r\n                    @click=\"addSolicitacaoTransferencia()\"\r\n                  >\r\n                    <v-icon dark>mdi-plus</v-icon>\r\n                  </v-btn>\r\n                </v-col>\r\n              </v-row>\r\n            </v-col>\r\n\r\n            <v-col align=\"center\" cols=\"12\" sm=\"1\">\r\n              <v-divider vertical></v-divider>\r\n            </v-col>\r\n          </v-row>\r\n\r\n          <v-divider></v-divider>\r\n          <v-row style=\"margin-top: -30px\" align=\"center\" justify=\"center\">\r\n            <v-col cols=\"12\">\r\n              <v-data-table\r\n                :headers=\"headers\"\r\n                :items=\"itensTransferencia\"\r\n                :items-per-page=\"3\"\r\n              >\r\n                <!-- <template v-slot:item.acoes=\"{ item }\">\r\n                    <v-icon\r\n                      color=\"primary\"\r\n                      @click=\"removeItemHorarioImprodutivo(item)\"\r\n                      >mdi-delete</v-icon\r\n                    >\r\n                  </template> -->\r\n              </v-data-table>\r\n            </v-col>\r\n          </v-row>\r\n\r\n          <v-row>\r\n            <v-col cols=\"12\" sm=\"12\">\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn\r\n                  color=\"orange lighten-1\"\r\n                  @click=\"retornaListaLinhaControle()\"\r\n                  large\r\n                  dark\r\n                  width=\"150\"\r\n                  >Cancelar</v-btn\r\n                >\r\n\r\n                <v-btn\r\n                  v-if=\"!isMobile\"\r\n                  color=\"orange darken-1\"\r\n                  large\r\n                  dark\r\n                  width=\"150\"\r\n                  @click=\"saveLinhaSolicitacaoTransferencia()\"\r\n                  ><v-icon left dark> mdi-account-arrow-right </v-icon\r\n                  >Emprestar</v-btn\r\n                >\r\n\r\n                <v-btn\r\n                  v-else-if=\"isMobile\"\r\n                  color=\"orange darken-1\"\r\n                  large\r\n                  dark\r\n                  width=\"150\"\r\n                  @click=\"saveLinhaSolicitacaoTransferencia()\"\r\n                  ><v-icon left dark> mdi-account-arrow-right </v-icon\r\n                  >Emprestar</v-btn\r\n                >\r\n              </v-card-actions>\r\n            </v-col>\r\n          </v-row>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template> \r\n\r\n\r\n\r\n<script lang=\"ts\">\r\nvar validaNumeros = require(\"../../baseGenerica/validaNumeros.js\");\r\n\r\nimport Vue from \"vue\";\r\n\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  ILinhaEmprestimoFormView,\r\n  ILinhaEmprestimoFormController,\r\n} from \"../../contracts/Linha/LinhaEmprestimoFormContract\";\r\nimport { LinhaEmprestimoFormController } from \"../../controllers/Linha/LinhaEmprestimoFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class LinhaEmprestimoForm\r\n  extends Vue\r\n  implements ILinhaEmprestimoFormView {\r\n  _controller?: ILinhaEmprestimoFormController;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"RotaAqui\",\r\n      disabled: false,\r\n      href: \"/rota\",\r\n    },\r\n  ];\r\n\r\n  headers = [\r\n    {\r\n      text: \"Linha\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"linha\",\r\n    },\r\n    {\r\n      text: \"Matricula\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"matricula\",\r\n    },\r\n    {\r\n      text: \"Nome\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"nome\",\r\n    },\r\n    {\r\n      text: \"Operação\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"operação\",\r\n    },\r\n    {\r\n      text: \"Habilidade\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"habilidade\",\r\n    },\r\n  ];\r\n\r\n  //calendario de datas\r\n  date = new Date().toISOString().substr(0, 10);\r\n  date2 = new Date().toISOString().substr(0, 10);\r\n  dateFormatted = this.formatDate(new Date().toISOString().substr(0, 10));\r\n  dateFormatted2 = this.formatDate(new Date().toISOString().substr(0, 10));\r\n  menu1 = false;\r\n  menu2 = false;\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  itemsLinha: any = [];\r\n  linhaSelecionada: any = [];\r\n\r\n  itemsLinhaOrdemProducaoPlano: any = [];\r\n  linhaOrdemProducaoPlanoSelecionada: any = [];\r\n  idOrdemProducaoPlanoTransfererir = 0;\r\n\r\n  itemsFase: any = [];\r\n  faseSelecionada: any = [];\r\n\r\n  itemsOperacao: any = [];\r\n  operacaoSelecionada: any = [];\r\n\r\n  itemsHabilidade: any = [];\r\n  habilidadeSelecionada: any = [];\r\n\r\n  itemsOperador: any = [];\r\n  operadorSelecionada: any = [];\r\n\r\n  // LINHAS A TRANSFERIR\r\n  itemsLinhaTransferir: any = [];\r\n  linhaSelecionadaTransferir: any = [];\r\n\r\n  itemsFaseTransferir: any = [];\r\n  faseSelecionadaTransferir: any = [];\r\n\r\n  itemsOperacaoTransferir: any = [];\r\n  operacaoSelecionadaTransferir: any = [];\r\n\r\n  itemsHabilidadeTransferir: any = [];\r\n  habilidadeSelecionadaTransferir: any = [];\r\n\r\n  disableFase = true;\r\n  disableOperacao = true;\r\n  disableHabilidade = true;\r\n  disableOperador = true;\r\n\r\n  disableFaseTransferir = true;\r\n  disableOperacaoTransferir = true;\r\n  disableHabilidadeTransferir = true;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  formatDate(date: string) {\r\n    if (!date) return null;\r\n\r\n    const [year, month, day] = date.split(\"-\");\r\n    return `${day}/${month}/${year}`;\r\n  }\r\n\r\n  parseDate(date: string) {\r\n    if (!date) return null;\r\n    const [day, month, year] = date.split(\"/\");\r\n    return `${year}-${month.padStart(2, \"0\")}-${day.padStart(2, \"0\")}`;\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/linhascontrole\");\r\n    }\r\n  }\r\n\r\n  retornaListaLinhaControle() {\r\n    this.$router.push(\"/linhascontrole\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    if (this.linhaSelecionada == \"\" || this.linhaSelecionada == null) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a linha atual.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.idOrdemProducaoPlanoTransfererir == 0 ||\r\n      this.idOrdemProducaoPlanoTransfererir == null\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text =\r\n        \"Selecione o Produto / Nº O.P. para onde deseja realizar a transferência do operador.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.faseSelecionada == \"\" || this.faseSelecionada == null) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a fase atual.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.operacaoSelecionada == \"\" || this.operacaoSelecionada == null) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a operação atual.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.habilidadeSelecionada == \"\" ||\r\n      this.habilidadeSelecionada == null\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a habilidade atual.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.operadorSelecionada == \"\" || this.operadorSelecionada == null) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o operador para solicitar a transferência.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.linhaSelecionadaTransferir == \"\" ||\r\n      this.linhaSelecionadaTransferir == null\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a linha a transferir.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.faseSelecionadaTransferir == \"\" ||\r\n      this.faseSelecionadaTransferir == null\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a fase a transferir.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.operacaoSelecionadaTransferir == \"\" ||\r\n      this.operacaoSelecionadaTransferir == null\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a operação a transferir.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.linhaSelecionada.id == this.linhaSelecionadaTransferir.id &&\r\n      (this.habilidadeSelecionada.id ==\r\n        this.habilidadeSelecionadaTransferir.id ||\r\n        this.habilidadeSelecionadaTransferir == \"\" ||\r\n        this.habilidadeSelecionadaTransferir == null)\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text =\r\n        \"Selecione a habilidade a transferir. A habilidade a transferir deve ser diferente da Habilidade atual.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  saveLinhaSolicitacaoTransferencia() {\r\n    if (this.validate()) {\r\n    } else {\r\n      let dadosSolicitacaiTransferencia = {\r\n        idLinha: this.linhaSelecionada.id,\r\n        idLinhaCapacidade: this.faseSelecionada.id,\r\n        idOperacao: this.operacaoSelecionada.id,\r\n        idHabilidade: this.habilidadeSelecionada.id,\r\n        idOperador: this.operadorSelecionada.idFuncionario,\r\n        idOrdemProducaoPlano: this.operadorSelecionada.idOrdemProducaoPlano,\r\n\r\n        idlinhaTransferir: this.linhaSelecionadaTransferir.id,\r\n        idLinhaCapacidadeTransferir: this.faseSelecionadaTransferir.id,\r\n        idOperacaoTransferir: this.operacaoSelecionadaTransferir.id,\r\n        idHabilidadeTransferir: this.habilidadeSelecionadaTransferir.id,\r\n        idOrdemProducaoPlanoTransferir: this.idOrdemProducaoPlanoTransfererir,\r\n\r\n        dataCadastro: new Date(),\r\n        status: \"AC\",\r\n      };\r\n\r\n      this._controller!.salvar(dadosSolicitacaiTransferencia);\r\n    }\r\n  }\r\n\r\n  updateFuncionario() {\r\n    if (!this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      // this._controller!.update(this.idFuncionario, this.funcionario);\r\n      this.retornaListaLinhaControle();\r\n    }\r\n  }\r\n\r\n  carregarDtos(): void {}\r\n\r\n  showBaseForEditar(item: any) {\r\n    //CARREGANDO DADOS DO FUNCIONARIO\r\n  }\r\n\r\n  //carregando todas as linhas\r\n  carregaLinha(itemLinha: any): void {\r\n    this.itemsLinha = itemLinha;\r\n    this.itemsFase = [];\r\n    this.itemsOperacao = [];\r\n    this.itemsHabilidade = [];\r\n    this.itemsOperador = [];\r\n    this.disableFase = false;\r\n    this.disableOperacao = true;\r\n    this.disableHabilidade = true;\r\n    this.disableOperador = true;\r\n  }\r\n\r\n  //carregando Ordem de Producao Plano para Transferncia de Linhas\r\n  carregaLinhaOrdemProducaoPlano(itemLinhaOrdemProducaoPlano: any): void {\r\n    this.itemsLinhaOrdemProducaoPlano = itemLinhaOrdemProducaoPlano;\r\n  }\r\n\r\n  changeLinhaOrdemProducaoPlano() {\r\n    this.linhaOrdemProducaoPlanoSelecionada = [];\r\n    this.itemsLinhaOrdemProducaoPlano.forEach((element: any) => {\r\n      if (element.idLinha == this.linhaSelecionadaTransferir.id) {\r\n        this.linhaOrdemProducaoPlanoSelecionada.push(element);\r\n      }\r\n    });\r\n  }\r\n\r\n  //carregando a fase da linha selecionada\r\n  changeFase() {\r\n    this.itemsFase = [];\r\n    this.itemsOperacao = [];\r\n    this.itemsHabilidade = [];\r\n    this.itemsOperador = [];\r\n    this.disableOperacao = false;\r\n    this.disableHabilidade = true;\r\n    this.disableOperador = true;\r\n\r\n    //carregando dados da fase (Capacidade da Linha)\r\n    let linhaCapacidade: any = [];\r\n\r\n    this.itemsLinha.forEach((element: any) => {\r\n      if (\r\n        this.linhaSelecionada.id == element.id &&\r\n        this.linhaSelecionada.nome == element.nome\r\n      ) {\r\n        linhaCapacidade.push(element.linhaCapacidade);\r\n      }\r\n    });\r\n\r\n    this.itemsFase = linhaCapacidade[0];\r\n  }\r\n\r\n  //carregando a Operacao da fase selecionada\r\n  changeOperacao() {\r\n    this.itemsOperacao = [];\r\n    this.itemsHabilidade = [];\r\n    this.itemsOperador = [];\r\n    this.disableHabilidade = false;\r\n    this.disableOperador = true;\r\n    //carregando dados da operacao (Fase - Capacidade da Linha)\r\n    let operacao: any = [];\r\n\r\n    this.itemsLinha.forEach((elementLinha: any) => {\r\n      elementLinha.linhaCapacidade.forEach((elementLinhaCapacidade: any) => {\r\n        if (\r\n          this.linhaSelecionada.id == elementLinha.id &&\r\n          this.linhaSelecionada.nome == elementLinha.nome &&\r\n          this.faseSelecionada.id == elementLinhaCapacidade.id &&\r\n          this.faseSelecionada.fase == elementLinhaCapacidade.fase\r\n        ) {\r\n          operacao.push(elementLinhaCapacidade.processoOperacao);\r\n        }\r\n      });\r\n    });\r\n\r\n    this.itemsOperacao = operacao;\r\n  }\r\n\r\n  //carregando a Habilidades da Fase selecionada\r\n  changeHabilidades() {\r\n    this.itemsHabilidade = [];\r\n    this.disableOperador = false;\r\n\r\n    //carregando dados da habilidade (Fase - Capacidade da Linha)\r\n    let habilidades: any = [];\r\n\r\n    this.itemsLinha.forEach((elementLinha: any) => {\r\n      elementLinha.linhaCapacidade.forEach((elementLinhaCapacidade: any) => {\r\n        elementLinhaCapacidade.linhaCapacidadeHabilidades.forEach(\r\n          (elementHabilidade: any) => {\r\n            if (\r\n              this.linhaSelecionada.id == elementLinha.id &&\r\n              this.linhaSelecionada.nome == elementLinha.nome &&\r\n              this.faseSelecionada.id == elementLinhaCapacidade.id &&\r\n              this.faseSelecionada.fase == elementLinhaCapacidade.fase &&\r\n              this.operacaoSelecionada.id ==\r\n                elementLinhaCapacidade.processoOperacao.id &&\r\n              this.operacaoSelecionada.nome ==\r\n                elementLinhaCapacidade.processoOperacao.nome\r\n            ) {\r\n              habilidades.push(elementHabilidade.processoHabilidade);\r\n            }\r\n          }\r\n        );\r\n      });\r\n    });\r\n\r\n    this.itemsHabilidade = habilidades;\r\n  }\r\n\r\n  //carregando os operadores com a Habilidades da Fase selecionada\r\n  changeOperadores() {\r\n    this.itemsOperador = [];\r\n    //carregando dados dos operadores da habilidade  selecionada(Fase - Capacidade da Linha)\r\n    let operadores: any = [];\r\n\r\n    this.itemsLinha.forEach((elementLinha: any) => {\r\n      elementLinha.linhaCapacidade.forEach((elementLinhaCapacidade: any) => {\r\n        elementLinhaCapacidade.linhaCapacidadeHabilidades.forEach(\r\n          (elementHabilidade: any) => {\r\n            elementHabilidade.operadores.forEach((elementOperadores: any) => {\r\n              if (\r\n                this.linhaSelecionada.id == elementLinha.id &&\r\n                this.linhaSelecionada.nome == elementLinha.nome &&\r\n                this.faseSelecionada.id == elementLinhaCapacidade.id &&\r\n                this.faseSelecionada.fase == elementLinhaCapacidade.fase &&\r\n                this.operacaoSelecionada.id ==\r\n                  elementLinhaCapacidade.processoOperacao.id &&\r\n                this.operacaoSelecionada.nome ==\r\n                  elementLinhaCapacidade.processoOperacao.nome &&\r\n                this.habilidadeSelecionada.id ==\r\n                  elementHabilidade.processoHabilidade.id &&\r\n                this.habilidadeSelecionada.nome ==\r\n                  elementHabilidade.processoHabilidade.nome\r\n              ) {\r\n                operadores.push(elementOperadores);\r\n              }\r\n            });\r\n          }\r\n        );\r\n      });\r\n    });\r\n    this.itemsOperador = operadores;\r\n  }\r\n\r\n  //carregando todas as linhas a transferir\r\n  carregaLinhaTransferir(itemLinha: any): void {\r\n    this.itemsLinhaTransferir = itemLinha;\r\n    this.itemsFaseTransferir = [];\r\n    this.itemsOperacaoTransferir = [];\r\n    this.itemsHabilidadeTransferir = [];\r\n    // this.itemsOperador = [];\r\n    this.disableFaseTransferir = false;\r\n    this.disableOperacaoTransferir = true;\r\n    this.disableHabilidadeTransferir = true;\r\n    // this.disableOperador = true;\r\n  }\r\n\r\n  //carregando a fase da linha selecionada a transferir\r\n  changeFaseTransferir() {\r\n    this.changeLinhaOrdemProducaoPlano();\r\n\r\n    this.itemsFaseTransferir = [];\r\n    this.itemsOperacaoTransferir = [];\r\n    this.itemsHabilidadeTransferir = [];\r\n    // this.itemsOperador = [];\r\n    this.disableOperacaoTransferir = false;\r\n    this.disableHabilidadeTransferir = true;\r\n    // this.disableOperador = true;\r\n\r\n    //carregando dados da fase (Capacidade da Linha)\r\n    let linhaCapacidade: any = [];\r\n\r\n    this.itemsLinhaTransferir.forEach((element: any) => {\r\n      if (\r\n        this.linhaSelecionadaTransferir.id == element.id &&\r\n        this.linhaSelecionadaTransferir.nome == element.nome\r\n      ) {\r\n        linhaCapacidade.push(element.linhaCapacidade);\r\n      }\r\n    });\r\n\r\n    this.itemsFaseTransferir = linhaCapacidade[0];\r\n  }\r\n\r\n  //carregando a Operacao da fase selecionada Transferir\r\n  changeOperacaoTransferir() {\r\n    this.itemsOperacaoTransferir = [];\r\n    this.itemsHabilidadeTransferir = [];\r\n    // this.itemsOperador = [];\r\n    this.disableHabilidadeTransferir = false;\r\n    // this.disableOperador = true;\r\n    //carregando dados da operacao (Fase - Capacidade da Linha)\r\n    let operacao: any = [];\r\n\r\n    this.itemsLinhaTransferir.forEach((elementLinha: any) => {\r\n      elementLinha.linhaCapacidade.forEach((elementLinhaCapacidade: any) => {\r\n        if (\r\n          this.linhaSelecionadaTransferir.id == elementLinha.id &&\r\n          this.linhaSelecionadaTransferir.nome == elementLinha.nome &&\r\n          this.faseSelecionadaTransferir.id == elementLinhaCapacidade.id &&\r\n          this.faseSelecionadaTransferir.fase == elementLinhaCapacidade.fase\r\n        ) {\r\n          operacao.push(elementLinhaCapacidade.processoOperacao);\r\n        }\r\n      });\r\n    });\r\n\r\n    this.itemsOperacaoTransferir = operacao;\r\n  }\r\n\r\n  //carregando a Habilidades da Fase selecionada a Transferir\r\n  changeHabilidadesTransferir() {\r\n    this.itemsHabilidadeTransferir = [];\r\n    // this.disableOperador = false;\r\n\r\n    //carregando dados da habilidade (Fase - Capacidade da Linha)\r\n    let habilidades: any = [];\r\n\r\n    this.itemsLinhaTransferir.forEach((elementLinha: any) => {\r\n      elementLinha.linhaCapacidade.forEach((elementLinhaCapacidade: any) => {\r\n        elementLinhaCapacidade.linhaCapacidadeHabilidades.forEach(\r\n          (elementHabilidade: any) => {\r\n            if (\r\n              this.linhaSelecionadaTransferir.id == elementLinha.id &&\r\n              this.linhaSelecionadaTransferir.nome == elementLinha.nome &&\r\n              this.faseSelecionadaTransferir.id == elementLinhaCapacidade.id &&\r\n              this.faseSelecionadaTransferir.fase ==\r\n                elementLinhaCapacidade.fase &&\r\n              this.operacaoSelecionadaTransferir.id ==\r\n                elementLinhaCapacidade.processoOperacao.id &&\r\n              this.operacaoSelecionadaTransferir.nome ==\r\n                elementLinhaCapacidade.processoOperacao.nome\r\n            ) {\r\n              habilidades.push(elementHabilidade.processoHabilidade);\r\n            }\r\n          }\r\n        );\r\n      });\r\n    });\r\n\r\n    this.itemsHabilidadeTransferir = habilidades;\r\n  }\r\n\r\n  created() {\r\n    //this._controller = new LinhaSolicitacaoTransferenciaFormController(this);\r\n    //this._controller.carregarDtos(this.$route.params.id);\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n\r\n  @Watch(\"date\")\r\n  onDatasChanged(val: string) {\r\n    this.dateFormatted = this.formatDate(this.date);\r\n  }\r\n\r\n  @Watch(\"date2\")\r\n  onDatas2Changed(val: string) {\r\n    this.dateFormatted2 = this.formatDate(this.date2);\r\n  }\r\n}\r\n</script> \r\n\r\n<style>\r\n#titulo {\r\n  text-align: left;\r\n  font: normal normal bold 20px/24px Helvetica;\r\n  letter-spacing: 0px;\r\n  color: #000000;\r\n  opacity: 1;\r\n}\r\n</style>"]}]}