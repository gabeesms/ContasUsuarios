holder=\"Selecione os responsáveis a quem deseja realizar a solicitação *\"\r\n                    autocomplete=\"off\"\r\n                    item-text=\"nome\"\r\n                    item-value=\"id\"\r\n                    outlined\r\n                    dense\r\n                    multiple\r\n                    chips\r\n                    :disabled=\"linhaSelecionada.length == 0  ? true : false\"\r\n                  ></v-select>\r\n                </v-col>\r\n               \r\n              </v-row>\r\n              <v-row Style=\"margin-top: -15px\">\r\n                 <v-col cols=\"12\" sm=\"8\">\r\n                  <v-select\r\n                    :items=\"itemsHabilidade\"\r\n                    return-object\r\n                    v-model=\"habilidadeSelecionada\"\r\n                    label=\"Habilidade *\"\r\n                    placeholder=\"Selecione a Habilidade *\"\r\n                    autocomplete=\"off\"\r\n                    item-text=\"nome\"\r\n                    item-value=\"id\"\r\n                    outlined\r\n                    dense\r\n                  ></v-select>\r\n                </v-col>\r\n                <v-col cols=\"12\" sm=\"3\">\r\n                  <v-text-field\r\n                    label=\"Quantidade de Operadores *\"\r\n                    placeholder=\"0\"\r\n                    v-model=\"qtdeOperadores\"\r\n                    outlined\r\n                    dense\r\n                    @keypress=\"apenasNumeros\"\r\n                  >\r\n                  </v-text-field>\r\n                </v-col>\r\n                \r\n                <v-col cols=\"12\" sm=\"1\">\r\n                  <v-btn\r\n                    style=\"float: right\"\r\n                    small\r\n                    color=\"#FB9514\"\r\n                    fab\r\n                    dark\r\n                    @click=\"addSolicitacaoTransferencia()\"\r\n                  >\r\n                    <v-icon dark>mdi-plus</v-icon>\r\n                  </v-btn>\r\n                </v-col>\r\n              </v-row>\r\n            </v-col>\r\n\r\n            <v-col align=\"center\" cols=\"12\" sm=\"1\">\r\n              <v-divider vertical></v-divider>\r\n            </v-col>\r\n          </v-row>\r\n          <v-divider></v-divider>\r\n          <v-row style=\"margin-top: -30px\" align=\"center\" justify=\"center\">\r\n            <v-col cols=\"12\">\r\n              <v-data-table\r\n                :headers=\"headers\"\r\n                :items=\"itensTransferencia\"\r\n                :items-per-page=\"3\"\r\n              >\r\n                <template v-slot:item.acao=\"{ item }\">\r\n                  <v-icon color=\"primary\" @click=\"removeHabilidade(item)\"\r\n                    >mdi-delete</v-icon\r\n                  >\r\n                </template>\r\n              </v-data-table>\r\n            </v-col>\r\n          </v-row>\r\n\r\n          <v-row>\r\n            <v-col cols=\"12\" sm=\"12\">\r\n              <v-card-actions>\r\n                <v-spacer></v-spacer>\r\n                <v-btn\r\n                  color=\"orange lighten-1\"\r\n                  @click=\"retornaListaLinhaControle()\"\r\n                  large\r\n                  dark\r\n                  width=\"150\"\r\n                  >Voltar</v-btn\r\n                >\r\n                <v-btn\r\n                  color=\"orange lighten-1\"\r\n                  @click=\"limparCampos()\"\r\n                  large\r\n                  dark\r\n                  width=\"150\"\r\n                  >Cancelar</v-btn\r\n                >\r\n\r\n                <v-btn\r\n                  v-if=\"!isMobile\"\r\n                  color=\"orange darken-1\"\r\n                  large\r\n                  dark\r\n                  width=\"150\"\r\n                  @click=\"saveLinhaSolicitacaoTransferencia()\"\r\n                  ><v-icon left dark> mdi-account-arrow-right </v-icon\r\n                  >Solicitar</v-btn\r\n                >\r\n\r\n                <v-btn\r\n                  v-else-if=\"isMobile\"\r\n                  color=\"orange darken-1\"\r\n                  large\r\n                  dark\r\n                  width=\"150\"\r\n                  @click=\"saveLinhaSolicitacaoTransferencia()\"\r\n                  ><v-icon left dark> mdi-account-arrow-right </v-icon\r\n                  >Transferir</v-btn\r\n                >\r\n              </v-card-actions>\r\n            </v-col>\r\n          </v-row>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template> \r\n\r\n\r\n\r\n<script lang=\"ts\">\r\nvar validaNumeros = require(\"../../baseGenerica/validaNumeros.js\");\r\n\r\nimport Vue from \"vue\";\r\n\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  ILinhaProducaoSolicitarTransferenciaFormView,\r\n  ILinhaProducaoSolicitarTransferenciaFormController,\r\n} from \"../../contracts/Linha/LinhaProducaoSolicitarTransferenciaFormContract\";\r\nimport { LinhaProducaoSolicitarTransferenciaFormController } from \"../../controllers/Linha/LinhaProducaoSolicitarTransferenciaFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class LinhaProducaoSolicitarTransferenciaForm\r\n  extends Vue\r\n  implements ILinhaProducaoSolicitarTransferenciaFormView {\r\n  _controller?: ILinhaProducaoSolicitarTransferenciaFormController;\r\n\r\n  itensTransferencia: any = [];\r\n\r\n  headers = [\r\n    {\r\n      text: \"Descrição da Habilidade\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"habilidade\",\r\n    },\r\n    {\r\n      text: \"Quantidade de Operadores Solicitada\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"qtdeOperador\",\r\n    },\r\n    {\r\n      text: \"Ações\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"acao\",\r\n    },\r\n  ];\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  itemsResponsaveis: any = [];\r\n  itemsLinha: any = [];\r\n  linhaSelecionada: any = [];\r\n  responsaveisSelecionados: any = [];\r\n\r\n  itemsHabilidade: any = [];\r\n  habilidadeSelecionada: any = [];\r\n\r\n  qtdeOperadores = \"\";\r\n  disableLinha = false;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  removeHabilidade(itemSelecionado: any) {\r\n    for (const { item, index } of this.itensTransferencia.map(\r\n      (item: any, index: any) => ({\r\n        item,\r\n        index,\r\n      })\r\n    )) {\r\n      if (item.idHabilidade == itemSelecionado.idHabilidade) {\r\n        this.itensTransferencia.splice(index, 1);\r\n      }\r\n    }\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/linhascontrole\");\r\n    }\r\n  }\r\n\r\n  limparCampos() {\r\n    this.disableLinha = false;\r\n    this.linhaSelecionada = [];\r\n    this.itensTransferencia = [];\r\n    this.habilidadeSelecionada = [];\r\n    this.responsaveisSelecionados = [];\r\n    this.itemsResponsaveis = [];\r\n  }\r\n\r\n  retornaListaLinhaControle() {\r\n    this.$router.push(\"/linhascontrole\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    if (this.linhaSelecionada == \"\" || this.linhaSelecionada == null) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a linha.\";\r\n      this.snackbar = true;\r\n      this.disableLinha = false;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.habilidadeSelecionada == \"\" ||\r\n      this.habilidadeSelecionada == null\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a habilidade.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      parseInt(this.qtdeOperadores) == 0 ||\r\n      this.qtdeOperadores == null ||\r\n      this.qtdeOperadores == undefined\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Infome a quantidade de operadores.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.itensTransferencia.length > 0) {\r\n      let retorno = false;\r\n      this.itensTransferencia.forEach((element: any) => {\r\n        if (this.habilidadeSelecionada.id == element.idHabilidade) {\r\n          this.colorBorder = \"error\";\r\n          this.color = \"error\";\r\n          this.text = \"Habilidade ja inserida à lista de solicitações.\";\r\n          this.snackbar = true;\r\n          retorno = true;\r\n        }\r\n      });\r\n      return retorno;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  saveLinhaSolicitacaoTransferencia() {\r\n    //if (this.validate()) {\r\n    //} else {\r\n      let dadosSolicitacaiTransferencia = {\r\n        idLinhaProducao: this.linhaSelecionada.id,\r\n        responsaveis: this.responsaveisSelecionados,\r\n        itensSolicitacaoTransferencia: this.itensTransferencia,\r\n        dataCadastro: new Date(),\r\n        status: \"AC\",\r\n      };\r\n\r\n      console.log(dadosSolicitacaiTransferencia)\r\n\r\n      this._controller!.salvar(dadosSolicitacaiTransferencia);\r\n   // }\r\n  }\r\n\r\n  updateFuncionario() {\r\n    if (!this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      // this._controller!.update(this.idFuncionario, this.funcionario);\r\n      this.retornaListaLinhaControle();\r\n    }\r\n  }\r\n\r\n  carregarDtos(): void {}\r\n\r\n  showBaseForEditar(item: any) {\r\n    //CARREGANDO DADOS DO FUNCIONARIO\r\n  }\r\n\r\n  changeCarregaResponsaveis() {\r\n    this.itemsResponsaveis = [];\r\n\r\n    this.itemsLinha.forEach((linha: any) => {\r\n      if (linha.id == this.linhaSelecionada.id) {\r\n        linha.linhaProducaoCategorias.forEach((categoria: any) => {\r\n          if (categoria.deletado == \"N\") {\r\n            categoria.linhaProducaoCategoriasFuncionarioLocal.forEach(\r\n              (funcionarios: any) => {\r\n                this.itemsResponsaveis.push(funcionarios.funcionarioErpDTO);\r\n              }\r\n            );\r\n          }\r\n        });\r\n      }\r\n    });\r\n\r\n    console.log(this.itemsResponsaveis);\r\n  }\r\n\r\n  //carregando todas as linhas\r\n  carregaLinha(itemLinha: any): void {\r\n    this.itemsLinha = itemLinha;\r\n    this.itemsHabilidade = [];\r\n  }\r\n\r\n  //carregando todas as Habilidades\r\n  carregaProcessoHabilidade(itemHabilidades: any): void {\r\n    this.itemsHabilidade = itemHabilidades;\r\n  }\r\n\r\n  addSolicitacaoTransferencia() {\r\n    this.disableLinha = true;\r\n    let transferencia: any = {};\r\n\r\n    transferencia = {\r\n      idHabilidade: this.habilidadeSelecionada.id,\r\n      habilidade: this.habilidadeSelecionada.nome,\r\n      qtdeOperador: parseInt(this.qtdeOperadores),\r\n    };\r\n\r\n    if (!this.validate()) {\r\n      this.itensTransferencia.push(transferencia);\r\n      this.habilidadeSelecionada = \"\";\r\n      this.qtdeOperadores = \"\";\r\n    }\r\n  }\r\n\r\n  created() {\r\n    this._controller = new LinhaProducaoSolicitarTransferenciaFormController(this);\r\n    this._controller.carregarDtos(this.$route.params.id);\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n</script> \r\n\r\n<style>\r\n#titulo {\r\n  text-align: left;\r\n  font: normal normal bold 20px/24px Helvetica;\r\n  letter-spacing: 0px;\r\n  color: #000000;\r\n  opacity: 1;\r\n}\r\n</style>"]}]}