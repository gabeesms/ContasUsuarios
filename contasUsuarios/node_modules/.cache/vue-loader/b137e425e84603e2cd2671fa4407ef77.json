                </v-col>\r\n                        </td>\r\n                        <td class=\"text-center\">\r\n                          <v-col cols=\"12\" sm=\"11\">\r\n                            <v-text-field\r\n                              placeholder=\"0\"\r\n                              v-model=\"item2.qtdePecaMinutoOperador\"\r\n                              outlined\r\n                              style=\"height: 43px; border-width: 1px\"\r\n                              dense\r\n                              disabled\r\n                            ></v-text-field>\r\n                          </v-col>\r\n                        </td>\r\n                        <td class=\"text-center\">\r\n                          <v-col cols=\"12\" sm=\"11\">\r\n                            <v-text-field\r\n                              placeholder=\"0\"\r\n                              v-model=\"item2.qtdePecaOperadorProducao\"\r\n                              outlined\r\n                              style=\"height: 43px; border-width: 1px\"\r\n                              dense\r\n                              disabled\r\n                            ></v-text-field>\r\n                          </v-col>\r\n                        </td>\r\n                        <td class=\"text-center\">\r\n                          <v-col cols=\"12\" sm=\"11\">\r\n                            <v-text-field\r\n                              placeholder=\"0\"\r\n                              type=\"time\"\r\n                              v-model=\"item2.operadorXtime\"\r\n                              outlined\r\n                              style=\"height: 43px; border-width: 1px\"\r\n                              step=\"1\"\r\n                              dense\r\n                              disabled\r\n                            ></v-text-field>\r\n                          </v-col>\r\n                        </td>\r\n                      </tr>\r\n                    </template>\r\n                  </v-data-table>\r\n\r\n                  <br />\r\n                </td>\r\n              </template>\r\n            </v-data-table>\r\n          </v-col>\r\n        </v-row>\r\n        <br />\r\n        <v-row>\r\n          <span><b> Suporte de Linha</b></span>\r\n        </v-row>\r\n        <!-- SUPORTE DE LINHA-->\r\n        <v-row align=\"center\" justify=\"center\">\r\n          <v-col cols=\"12\" sm=\"12\">\r\n            <k-data-list\r\n              orientation=\"Horizontal\"\r\n              mobileIconAdd=\"mdi-plus-circle\"\r\n              mobileFunctionAdd=\"add\"\r\n              :headers=\"headersSuporteLinha\"\r\n              :itens=\"itensSuporteLinha\"\r\n            >\r\n              <template v-slot:nomeCategoria=\"{ item }\">\r\n                {{ item.processoDTO.nome }}\r\n              </template>\r\n\r\n              <template v-slot:operadores=\"{ item }\">\r\n                <div\r\n                  v-for=\"(dadosOperador, index) in item.linhaProducaoCategoriasFuncionarioLocal\"\r\n                  :key=\"index\"\r\n                >\r\n                  <v-row>\r\n                    <v-col cols=\"12\" sm=\"2\">{{\r\n                      dadosOperador.funcionarioErpDTO.matricula + \": \"\r\n                    }}</v-col>\r\n                    <v-col cols=\"12\" sm=\"10\">{{ dadosOperador.funcionarioErpDTO.nome }}</v-col>\r\n                  </v-row>\r\n                </div>\r\n              </template>\r\n            </k-data-list>\r\n          </v-col>\r\n        </v-row>\r\n      </v-col>\r\n\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"12\"></v-col>\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn\r\n              v-if=\"!idOrdemProducaoPlano\"\r\n              color=\"orange lighten-1\"\r\n              @click=\"retornaListaOrdensProducao()\"\r\n              large\r\n              dark\r\n              width=\"150\"\r\n              >Cancelar</v-btn\r\n            >\r\n\r\n            <v-btn\r\n              v-if=\"idOrdemProducaoPlano\"\r\n              color=\"orange lighten-1\"\r\n              @click=\"retornaListaOrdensProducaoSimulacao()\"\r\n              large\r\n              dark\r\n              width=\"150\"\r\n              >Voltar</v-btn\r\n            >\r\n\r\n            <!-- <v-btn\r\n              v-if=\"!isMobile && idOrdemProducaoPlano\"\r\n              color=\"orange darken-1\"\r\n              large\r\n              dark\r\n              width=\"150\"\r\n              @click=\"updateOrdemProducao()\"\r\n              >EDITAR</v-btn\r\n            > -->\r\n\r\n            <v-btn\r\n              v-if=\"!isMobile && !idOrdemProducaoPlano\"\r\n              color=\"orange darken-1\"\r\n              large\r\n              dark\r\n              width=\"150\"\r\n              @click=\"saveOrdemProducao('S')\"\r\n              >Salvar</v-btn\r\n            >\r\n\r\n            <v-btn\r\n              v-else-if=\"isMobile && !idOrdemProducaoPlano\"\r\n              color=\"orange darken-1\"\r\n              large\r\n              dark\r\n              width=\"150\"\r\n              @click=\"saveOrdemProducao('S')\"\r\n              >Salvar</v-btn\r\n            >\r\n\r\n            <!-- <v-menu\r\n              top\r\n              large\r\n              rounded\r\n              offset-y\r\n              v-if=\"!isMobile && !idOrdemProducaoPlano\"\r\n              transition=\"scale-transition\"\r\n            >\r\n              <template v-slot:activator=\"{ attrs, on }\">\r\n                <v-btn\r\n                  style=\"margin-left: 5px\"\r\n                  color=\"orange darken-1\"\r\n                  large\r\n                  dark\r\n                  width=\"200\"\r\n                  v-bind=\"attrs\"\r\n                  v-on=\"on\"\r\n                  :disabled=\"!valid\"\r\n                >\r\n                  <v-icon left dark>mdi-content-save-settings</v-icon>\r\n                  {{ \"SALVAR\" }}\r\n                </v-btn>\r\n              </template>\r\n\r\n              <v-list>\r\n                <v-list-item link @click=\"saveOrdemProducao('S')\"\r\n                  ><v-icon>mdi-check-outline</v-icon>\r\n                  <v-list-item-title>Salvar Simulação</v-list-item-title>\r\n                </v-list-item>\r\n                <v-divider></v-divider>\r\n\r\n                <v-list-item link @click=\"saveOrdemProducao('P')\"\r\n                  ><v-icon>mdi-check-bold</v-icon>\r\n                  <v-list-item-title>Salvar em Produção </v-list-item-title>\r\n                </v-list-item>\r\n              </v-list>\r\n            </v-menu> -->\r\n          </v-card-actions>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template> \r\n\r\n\r\n\r\n<script lang=\"ts\">\r\nvar validaNumeros = require(\"../../baseGenerica/validaNumeros.js\");\r\nvar formatSegundos = require(\"../../baseGenerica/formatarSegundos.js\");\r\n\r\nimport Vue from \"vue\";\r\n\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  IOrdemProducaoFormView,\r\n  IOrdemProducaoFormController,\r\n} from \"../../contracts/OrdemProducao/OrdemProducaoFormContract\";\r\nimport { OrdemProducaoFormController } from \"../../controllers/OrdemProducao/OrdemProducaoFormController\";\r\n\r\nimport { Validation, ValidationSchema } from \"../../baseGenerica/validation\";\r\nimport moment from \"moment\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class OrdemProducaoForm\r\n  extends Vue\r\n  implements IOrdemProducaoFormView {\r\n  _controller?: IOrdemProducaoFormController;\r\n\r\n  itensTurno: any = [];\r\n\r\n  expanded = [];\r\n  singleExpand = false;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Ordens de Produção\",\r\n      disabled: false,\r\n      href: \"/ordensProducao\",\r\n    },\r\n  ];\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  disabledCampos = true;\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  dialogAddOperadorSemHabilidade = false;\r\n  operadoresSemHabilidade: any = [];\r\n\r\n  headersSuporteLinha = new Headers([\r\n    new Header(\r\n      \"Descrição da Categoria\",\r\n      \"nomeCategoria\",\r\n      false,\r\n      false,\r\n      \"left\",\r\n      false,\r\n      \"200\"\r\n    ),\r\n    new Header(\r\n      \"Operador(es) Responsáveis\",\r\n      \"operadores\",\r\n      false,\r\n      false,\r\n      \"left\",\r\n      false,\r\n      \"200\"\r\n    ),\r\n  ]);\r\n\r\n  headersNovaLinha = [\r\n    {\r\n      text: \"Operação\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 100,\r\n      value: \"\",\r\n    },\r\n    {\r\n      text: \"Habilidade\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 100,\r\n      value: \"\",\r\n    },\r\n    {\r\n      text: \"Operadores\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 400,\r\n      value: \"\",\r\n    },\r\n    {\r\n      //text: \"Operadores sem habilidade\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 20,\r\n      value: \"\",\r\n    },\r\n    {\r\n      text: \"Qtde de Operadores\",\r\n      align: \"center\",\r\n      width: 100,\r\n      sortable: false,\r\n      value: \"\",\r\n    },\r\n    {\r\n      text: \"Tempo Produção\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 150,\r\n      value: \"\",\r\n    },\r\n    {\r\n      text: \"Qtde Peças/Hora Por Operador\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 150,\r\n      value: \"\",\r\n    },\r\n    {\r\n      text: \"Qtde Peças/Hora Produção\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 150,\r\n      value: \"\",\r\n    },\r\n    {\r\n      text: \"Tempo Total\",\r\n      align: \"center\",\r\n      sortable: false,\r\n      width: 150,\r\n      value: \"\",\r\n    },\r\n  ];\r\n\r\n  headersLinha = [\r\n    {\r\n      text: \"Nome\",\r\n      align: \"start\",\r\n      sortable: false,\r\n      value: \"fase\",\r\n    },\r\n    { text: \"Ação\", align: \"right\", value: \"acao\" },\r\n    { text: \"\", align: \"right\", value: \"data-table-expand\" },\r\n  ];\r\n\r\n  itensCapacidadeLinha: any = [];\r\n  itensSuporteLinha: any = [];\r\n  itensOperadoresPorHabilidade: any = [];\r\n  itensOperadores: any = [];\r\n\r\n  idOrdemProducaoLinha = \"\";\r\n  idOrdemProducaoPlano = \"\";\r\n\r\n  ordemProducaoResponse: any = {};\r\n\r\n  codigoDescricao = \"\";\r\n  qtdeOperador = 0;\r\n  quantidade = 0;\r\n  nome = \"\";\r\n  quantidadeOperador = 0;\r\n  capacidadeProdutiva = 0;\r\n  capacidadeProdutivaReal = 0;\r\n  capacidadeProdutivaTurno = 0;\r\n  numOrdemProducao = \"-\";\r\n  producaoHoraHora = 0;\r\n  qtdeOperadores = 0;\r\n\r\n  turnoConvert: any = [];\r\n\r\n  ordemProducao = {\r\n    dataCadastro: new Date(),\r\n    status: \"S\",\r\n    segunda: false,\r\n    terca: false,\r\n    quarta: false,\r\n    quinta: false,\r\n    sexta: false,\r\n    sabado: false,\r\n    domingo: false,\r\n    producao: 0,\r\n    metaTurno: 0,\r\n    capacidadeProdutivaReal: 0,\r\n    capacidadeProdutivaTurno: 0,\r\n    qtdeOperadores: 0,\r\n    linha: {\r\n      idLinha: 0,\r\n      capacidadeLinha: this.itensCapacidadeLinha,\r\n      recursoTecnicoLinha: this.itensSuporteLinha,\r\n    },\r\n\r\n    idOrdemProducao: 0,\r\n    idOrdemProducaoLinha: 0,\r\n    idProduto: 0,\r\n    turnos: this.turnoConvert,\r\n  };\r\n\r\n  valid = true;\r\n\r\n  customFilterOperador(item: any, queryText: any, itemText: any) {\r\n    const textOne = item.nome.toLowerCase();\r\n    const textTwo = item.nome.toLowerCase();\r\n    const searchText = queryText.toLowerCase();\r\n\r\n    return textOne.indexOf(searchText) > -1 || textTwo.indexOf(searchText) > -1;\r\n  }\r\n\r\n  carregaTurnos(itemsTurno: any): void {\r\n    this.itensTurno = itemsTurno;\r\n\r\n    this.itensTurno.forEach((element: any) => {\r\n      element.textoDescHora =\r\n        element.descricao + \" - HORAS TOTAL: \" + element.horaTotal;\r\n    });\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/ordensProducao\");\r\n    }\r\n  }\r\n\r\n  retornaListaOrdensProducao() {\r\n    this.$router.push(\"/ordensProducao\");\r\n  }\r\n\r\n  retornaListaOrdensProducaoSimulacao() {\r\n    this.$router.push(\"/ordensProducao\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    if (\r\n      this.ordemProducao.turnos == null ||\r\n      this.ordemProducao.turnos.length == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text =\r\n        \"Selecione o Turno em que deseja realizar a Ordem de Produção\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.ordemProducao.segunda == false &&\r\n      this.ordemProducao.terca == false &&\r\n      this.ordemProducao.quarta == false &&\r\n      this.ordemProducao.quinta == false &&\r\n      this.ordemProducao.sexta == false &&\r\n      this.ordemProducao.sabado == false &&\r\n      this.ordemProducao.domingo == false\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text =\r\n        \"Selecione ao menos um dia da semana para realizar a Ordem de Produção\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    //verifica se ha operadores para a capacidade de linha\r\n    for (let i = 0; i < this.itensCapacidadeLinha.length; i++) {\r\n      let verificador = false;\r\n      for (\r\n        let j = 0;\r\n        j < this.itensCapacidadeLinha[i].linhaCapacidadeHabilidades.length;\r\n        j++\r\n      ) {\r\n        if (\r\n          this.itensCapacidadeLinha[i].linhaCapacidadeHabilidades[j]\r\n            .operadores == null\r\n        ) {\r\n          this.colorBorder = \"error\";\r\n          this.color = \"error\";\r\n          this.text =\r\n            \"Selecione o(s) operador(es) da Capacidade de Linha: \" +\r\n            this.itensCapacidadeLinha[i].processoDTOOperacao.nome;\r\n          this.snackbar = true;\r\n          verificador = true;\r\n          break;\r\n        }\r\n      }\r\n\r\n      if (verificador == true) {\r\n        return true;\r\n        break;\r\n      }\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  saveOrdemProducao(tipo: string) {\r\n    this.ordemProducao.linha.idLinha = this.ordemProducaoResponse.linha.id;\r\n    this.ordemProducao.linha.capacidadeLinha = this.itensCapacidadeLinha;\r\n    //this.ordemProducao.linha.recursoTecnicoLinha = this.itensSuporteLinha;\r\n    this.ordemProducao.idOrdemProducao = this.ordemProducaoResponse.ordemProducao.id;\r\n    this.ordemProducao.idOrdemProducaoLinha = this.ordemProducaoResponse.ordemProducaoLinha.id;\r\n    this.ordemProducao.idProduto = this.ordemProducaoResponse.produto.id;\r\n    this.ordemProducao.producao = this.ordemProducaoResponse.ordemProducao.quantidade;\r\n    this.ordemProducao.capacidadeProdutivaReal = this.capacidadeProdutivaReal;\r\n    this.ordemProducao.capacidadeProdutivaTurno = this.capacidadeProdutivaTurno;\r\n    this.ordemProducao.qtdeOperadores = this.qtdeOperadores;\r\n\r\n    this.ordemProducao.status = tipo == \"S\" ? \"S\" : \"P\";\r\n\r\n    if (!this.validate()) {\r\n      this._controller!.salvar(this.ordemProducao);\r\n      // this._controller!.update(this.idOrdemProducao, this.ordemProducao);\r\n      //   this.retornaListaOrdensProducao();\r\n    }\r\n  }\r\n\r\n  updateOrdemProducao() {\r\n    this.ordemProducao.linha.idLinha = this.ordemProducaoResponse.linha.id;\r\n    this.ordemProducao.linha.capacidadeLinha = this.itensCapacidadeLinha;\r\n    //this.ordemProducao.linha.recursoTecnicoLinha = this.itensSuporteLinha;\r\n    this.ordemProducao.idOrdemProducao = this.ordemProducaoResponse.ordemProducao.id;\r\n    this.ordemProducao.idOrdemProducaoLinha = this.ordemProducaoResponse.ordemProducaoLinha.id;\r\n    this.ordemProducao.idProduto = this.ordemProducaoResponse.produto.id;\r\n    this.ordemProducao.producao = this.ordemProducaoResponse.ordemProducao.quantidade;\r\n\r\n    // if (!this.validate()) {\r\n    // } else {\r\n    //this._controller!.salvar(this.ordemProducao);\r\n    //this._controller!.update(this.idOrdemProducao, this.ordemProducao);\r\n    //   this.retornaListaOrdensProducao();\r\n    // }\r\n  }\r\n\r\n  // calcularQtdOperadores(){\r\n\r\n  //   let horasSegundos = 3600;\r\n\r\n  //   let tempoProducaoPecas = this.producaoHoraHora == 0 ? 0 : horasSegundos/this.producaoHoraHora;\r\n\r\n  //   this.ordemProducao.qtdeOperadores = this.producaoHoraHora == 0 ? 0 : Math.ceil(this.producaoHoraHora / tempoProducaoPecas);\r\n\r\n  // }\r\n\r\n  calculaMetaTurno() {\r\n    // console.log(this.ordemProducao.turnos.horaTotal);\r\n\r\n    let calculoMetaTurno = 0;\r\n\r\n    let newTimeConvert = moment(\r\n      this.ordemProducao.turnos.horaTotal,\r\n      \"HH:mm:ss\"\r\n    );\r\n\r\n    let newTime = newTimeConvert.toDate();\r\n\r\n    calculoMetaTurno = this.formatarSegundos(\r\n      newTime.getHours(),\r\n      newTime.getMinutes(),\r\n      newTime.getSeconds()\r\n    );\r\n\r\n    let meta: any;\r\n\r\n    meta = (this.producaoHoraHora * (calculoMetaTurno / 60 / 60)).toFixed(2);\r\n\r\n    this.ordemProducao.metaTurno =\r\n      this.producaoHoraHora > 0 ? parseFloat(meta) : 0;\r\n\r\n    //CALCULO CAPACIDADE PRODUTIVA TURNO\r\n    let capacidadeProdutivaTurno = (\r\n      this.capacidadeProdutivaReal *\r\n      (calculoMetaTurno / 60 / 60)\r\n    ).toFixed(2);\r\n\r\n    this.capacidadeProdutivaTurno =\r\n      this.capacidadeProdutivaReal > 0\r\n        ? parseFloat(capacidadeProdutivaTurno)\r\n        : 0;\r\n  }\r\n\r\n  formatarSegundos(h: any, min: any, s: any) {\r\n    return formatSegundos(h, min, s);\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.idOrdemProducaoLinha = this.$route.params.idOrdemProducaoLinha;\r\n    this.idOrdemProducaoPlano = this.$route.params.idOrdemProducaoPlano;\r\n\r\n    if (this.$route.params.idOrdemProducaoPlano) {\r\n      this.disabledCampos = true;\r\n    } else {\r\n      this.disabledCampos = false;\r\n    }\r\n  }\r\n\r\n  showOrdemProducaoForEditar(item: any) {\r\n    this.loading(true);\r\n    this.ordemProducaoResponse = item;\r\n\r\n    this.itensCapacidadeLinha = this.ordemProducaoResponse.linha.linhaCapacidade;\r\n\r\n    let itensSuporteLinha = this.ordemProducaoResponse.linhaProducao.linhaProducaoCategorias;\r\n\r\n    this.ordemProducao = this.ordemProducaoResponse.ordemProducaoPlano;\r\n\r\n    this.ordemProducao.turnos = this.ordemProducaoResponse.ordemProducaoTurnos[0];\r\n\r\n    this.codigoDescricao =\r\n      this.ordemProducaoResponse.produto.codigo.toString().trim() +\r\n      \" - \" +\r\n      this.ordemProducaoResponse.produto.descricao.toString().trim();\r\n    this.qtdeOperador = this.ordemProducaoResponse.produto.qtdeOperador;\r\n    this.producaoHoraHora = this.ordemProducaoResponse.produto.producaoHoraHora;\r\n    this.quantidade = this.ordemProducaoResponse.ordemProducao.quantidade;\r\n    this.nome = this.ordemProducaoResponse.linhaProducao.nome;\r\n    this.quantidadeOperador = this.ordemProducaoResponse.linha.quantidadeOperador;\r\n    this.capacidadeProdutiva = this.ordemProducaoResponse.linha.capacidadeProdutiva;\r\n    this.numOrdemProducao = this.ordemProducaoResponse.ordemProducao.numOrdemProducao;\r\n    this.capacidadeProdutivaReal = this.ordemProducaoResponse.ordemProducaoPlano.capacidadeProdutivaReal;\r\n    this.qtdeOperadores = this.ordemProducaoResponse.ordemProducaoPlano.qtdeOperadores;\r\n    this.capacidadeProdutivaTurno = this.ordemProducaoResponse.ordemProducaoPlano.capacidadeProdutivaTurno;\r\n    //console.log(this.quantidade);\r\n    //console.log(this.producaoHoraHora);\r\n\r\n    //instanciando oeperadores em capacidade da linha\r\n    this.itensCapacidadeLinha.forEach((element: any) => {\r\n      element.linhaCapacidadeHabilidades.forEach((element2: any) => {\r\n        element2.operadores = [];\r\n      });\r\n    });\r\n\r\n    //convertendo time para time imput\r\n    this.itensCapacidadeLinha.forEach((elementCapLinha: any) => {\r\n      elementCapLinha.linhaCapacidadeHabilidades.forEach(\r\n        (elementCapLinhaHabilidade: any) => {\r\n          elementCapLinhaHabilidade.time = new Date(\r\n            elementCapLinhaHabilidade.time\r\n          )\r\n            .toLocaleTimeString()\r\n            .toString();\r\n\r\n          elementCapLinhaHabilidade.ordemProducaoOperadorLinhaCapacidade.forEach(\r\n            (element: any) => {\r\n              elementCapLinhaHabilidade.operadores.push(\r\n                element.funcionarioErpDTO\r\n              );\r\n            }\r\n          );\r\n        }\r\n      );\r\n    });\r\n\r\n    //Supore de Linha\r\n    itensSuporteLinha.forEach((elementSuporteLinha: any) => {\r\n      if(elementSuporteLinha.deletado == \"N\"){\r\n        this.itensSuporteLinha.push(elementSuporteLinha)\r\n      }\r\n    });\r\n\r\n    this.loading(false);\r\n  }\r\n\r\n  showOrdemProducaoForNovo(item: any) {\r\n    this.loading(true);\r\n    this.ordemProducaoResponse = item;\r\n\r\n    this.itensCapacidadeLinha = this.ordemProducaoResponse.linha.linhaCapacidade;\r\n\r\n    let itensSuporteLinha = this.ordemProducaoResponse.linhaProducao.linhaProducaoCategorias;\r\n\r\n    this.codigoDescricao =\r\n      this.ordemProducaoResponse.produto.codigo.toString().trim() +\r\n      \" - \" +\r\n      this.ordemProducaoResponse.produto.descricao.toString().trim();\r\n    this.qtdeOperador = this.ordemProducaoResponse.produto.qtdeOperador;\r\n    this.producaoHoraHora = this.ordemProducaoResponse.produto.producaoHoraHora;\r\n    this.quantidade = this.ordemProducaoResponse.ordemProducao.quantidade;\r\n    this.nome = this.ordemProducaoResponse.linhaProducao.nome;\r\n    this.quantidadeOperador = this.ordemProducaoResponse.linha.quantidadeOperador;\r\n    this.capacidadeProdutiva = this.ordemProducaoResponse.linha.capacidadeProdutiva;\r\n    this.numOrdemProducao = this.ordemProducaoResponse.ordemProducao.numOrdemProducao;\r\n\r\n    //CALCULO DA CAPACIDADE PRODUTIVA REAL/HORA\r\n    let horasSegundos = 3600;\r\n    let tempoProducaoPecas =\r\n      this.producaoHoraHora == 0 ? 0 : horasSegundos / this.producaoHoraHora;\r\n    let capacidadeProdutivaReal =\r\n      tempoProducaoPecas == 0\r\n        ? 0\r\n        : tempoProducaoPecas * this.quantidadeOperador;\r\n    this.capacidadeProdutivaReal = parseFloat(\r\n      capacidadeProdutivaReal.toFixed(2)\r\n    );\r\n\r\n    // CALCULO QTDE DE OPERADORES NECESSARIOS\r\n    this.qtdeOperadores =\r\n      this.producaoHoraHora == 0\r\n        ? 0\r\n        : Math.ceil(this.producaoHoraHora / tempoProducaoPecas);\r\n\r\n    //instanciando oeperadores em capacidade da linha\r\n    this.itensCapacidadeLinha.forEach((element: any) => {\r\n      element.linhaCapacidadeHabilidades.forEach((element2: any) => {\r\n        element2.operadores = null;\r\n      });\r\n    });\r\n\r\n    //convertendo time para time imput\r\n    this.itensCapacidadeLinha.forEach((elementCapLinha: any) => {\r\n      elementCapLinha.linhaCapacidadeHabilidades.forEach(\r\n        (elementCapLinhaHabilidade: any) => {\r\n          elementCapLinhaHabilidade.time = new Date(\r\n            elementCapLinhaHabilidade.time\r\n          )\r\n            .toLocaleTimeString()\r\n            .toString();\r\n        }\r\n      );\r\n    });\r\n\r\n    //retirando os suporte de linha com deletado == \"S\"\r\n    itensSuporteLinha.forEach((elementSuporteLinha: any) => {\r\n      if (elementSuporteLinha.deletado == \"N\"){\r\n        this.itensSuporteLinha.push(elementSuporteLinha);\r\n      }\r\n    });\r\n\r\n    //abatascendo operadores por habilidade\r\n    let i = 0;\r\n    this.itensCapacidadeLinha.forEach((elementCapLinha: any) => {\r\n      i = 0;\r\n      elementCapLinha.linhaCapacidadeHabilidades.forEach(\r\n        (elementCapLinhaHabilidade: any) => {\r\n          this.itensOperadoresPorHabilidade.forEach(\r\n            (elemOpPorHabilidade: any) => {\r\n              if (\r\n                elementCapLinhaHabilidade.idProcessoHabilidade !==\r\n                elemOpPorHabilidade.idProcessoHabilidade\r\n              ) {\r\n                this.itensOperadoresPorHabilidade.splice(i, 1);\r\n              }\r\n              i++;\r\n            }\r\n          );\r\n        }\r\n      );\r\n    });\r\n\r\n    this.loading(false);\r\n  }\r\n\r\n  carregaOperadoresPorHabilidade(operadores: any): void {\r\n    this.itensOperadoresPorHabilidade = operadores;\r\n  }\r\n\r\n  carregaOperadores(operadores: any): void {\r\n    this.itensOperadores = operadores;\r\n\r\n    if (this.$route.params.idOrdemProducaoPlano) {\r\n      this.itensOperadoresPorHabilidade = operadores;\r\n    }\r\n  }\r\n  modalOperadorSemHabilidade() {\r\n    this.operadoresSemHabilidade = [];\r\n    this.dialogAddOperadorSemHabilidade = true;\r\n  }\r\n  addOpeSemHabilidade() {\r\n    this.itensOperadoresPorHabilidade.forEach((element: any) => {\r\n      if (\r\n        element.id !== this.operadoresSemHabilidade.id &&\r\n        element.nome !== this.operadoresSemHabilidade.nome\r\n      ) {\r\n        this.itensOperadoresPorHabilidade.push(this.operadoresSemHabilidade);\r\n      }\r\n    });\r\n\r\n    this.dialogAddOperadorSemHabilidade = false;\r\n  }\r\n\r\n  created() {\r\n    this._controller = new OrdemProducaoFormController(this);\r\n    this._controller.carregarDtos(\r\n      this.$route.params.idOrdemProducaoLinha,\r\n      this.$route.params.idOrdemProducaoPlano\r\n    );\r\n    this.carregarDtos();\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.centered-input >>> input {\r\n  text-align: right;\r\n}\r\n\r\n.mytable table td {\r\n  border-bottom: 1px solid rgba(0, 0, 0, 0.12);\r\n}\r\n</style>"]}]}