    </v-list-item-title>\r\n            </v-list-item>\r\n\r\n            <v-list-item link v-for=\"itemSubMenu in itemMenu.itens\" :key=\"itemSubMenu.path\">\r\n              <v-list-item-content style=\"margin-left: -40px\">\r\n                <v-list-item-title>\r\n                  <router-link \r\n                    :to=\"itemSubMenu.path\"\r\n                  ><span class=\"white--text\">{{ itemSubMenu.title }}</span> </router-link>\r\n                </v-list-item-title>\r\n              </v-list-item-content>\r\n            </v-list-item>\r\n          </v-list-group>\r\n        </div>\r\n        <template v-if=\"isMobile\">\r\n          <v-list-item link @click=\"changePass\">\r\n            <v-list-item-icon>\r\n              <v-icon class=\"primary--text\">mdi-account-key</v-icon>\r\n            </v-list-item-icon>\r\n            <v-list-item-title>\r\n              <span class=\"primary--text\">Alterar Senha</span>\r\n            </v-list-item-title>\r\n          </v-list-item>\r\n          <v-list-item link @click=\"sair\">\r\n            <v-list-item-icon>\r\n              <v-icon class=\"primary--text\">mdi-exit-to-app</v-icon>\r\n            </v-list-item-icon>\r\n            <v-list-item-title>\r\n              <span class=\"primary--text\">Sair</span>\r\n            </v-list-item-title>\r\n          </v-list-item>\r\n        </template>\r\n      </v-list>\r\n    </v-navigation-drawer>\r\n  </div>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from \"vue\";\r\nimport { Menu, MenuItem } from \"../models/menu\";\r\n\r\nimport { Display } from \"../configuration/display\";\r\n\r\nexport default Vue.extend({\r\n  props: {\r\n    menu: Menu\r\n  },\r\n  data: () => ({\r\n   \r\n    isMobile: Display.isMobile(),\r\n    itemsNotificacao: [\r\n      { header: \"Notificações\" },\r\n      {\r\n        title: \"Notificação 1\",\r\n        subtitle:\r\n          \"<span class='text--primary'>Testando</span> &mdash; Dados da Notificação teste tamanho