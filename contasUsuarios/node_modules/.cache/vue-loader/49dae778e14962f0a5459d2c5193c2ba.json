\"\r\n    >\r\n      <template v-slot:buttoes>\r\n        <v-btn\r\n          @click=\"dialogAtivar = false\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"red darken-2\"\r\n          >Cancelar</v-btn\r\n        >\r\n        <v-btn\r\n          @click=\"ativarItemConfirmado\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"green darken-2\"\r\n          >Sim</v-btn\r\n        >\r\n      </template>\r\n    </k-dialog>\r\n\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n          <template v-slot:item=\"{ item }\">\r\n            <router-link :to=\"item.href\">\r\n              {{ item.text }}\r\n            </router-link>\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n\r\n      <v-row ref=\"form\">\r\n        <v-col cols=\"12\">\r\n          <h3>\r\n            <b>Configurações</b>\r\n          </h3>\r\n        </v-col>\r\n        <v-col cols=\"12\">\r\n          <span>\r\n            <b>Tipos de Absenteísmo</b>\r\n          </span>\r\n        </v-col>\r\n\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-row>\r\n            <v-col cols=\"12\" sm=\"3\">\r\n              <v-text-field\r\n                v-model=\"configuracao.sigla\"\r\n                outlined\r\n                label=\"Sigla *\"\r\n                placeholder=\"Sigla do tipo de absenteísmo\"\r\n                counter=\"20\"\r\n                dense\r\n                required\r\n                maxlength=\"20\"\r\n              ></v-text-field>\r\n            </v-col>\r\n            <v-col cols=\"12\" sm=\"7\">\r\n              <v-text-field\r\n                v-model=\"configuracao.descricao\"\r\n                outlined\r\n                label=\"Descrição *\"\r\n                placeholder=\"Descrição do tipo de absenteísmo\"\r\n                counter=\"50\"\r\n                dense\r\n                required\r\n                maxlength=\"50\"\r\n              ></v-text-field>\r\n            </v-col>\r\n            <v-col cols=\"12\" sm=\"2\" style=\"margin-top: -0.5vh\">\r\n              <v-btn\r\n                color=\"orange darken-1\"\r\n                large\r\n                dark\r\n                width=\"23vh\"\r\n                @click=\"saveConfiguracao()\"\r\n                >Adicionar</v-btn\r\n              >\r\n            </v-col>\r\n          </v-row>\r\n          <v-divider></v-divider>\r\n          <v-row>\r\n            <v-col cols=\"12\" sm=\"12\">\r\n              <k-data-list\r\n                orientation=\"Horizontal\"\r\n                mobileIconAdd=\"mdi-plus-circle\"\r\n                mobileFunctionAdd=\"addSolicitacao\"\r\n                @editarItem=\"editarItem\"\r\n                @deleteItem=\"deleteItem\"\r\n                :headers=\"headers\"\r\n                :search=\"search\"\r\n                :itens=\"itemsConfiguracoes\"\r\n              >\r\n                <template v-slot:status=\"{ item }\">\r\n                  <v-chip\r\n                    v-if=\"item.status == 'A'\"\r\n                    small\r\n                    class=\"px-7 py-3\"\r\n                    color=\"#019A53\"\r\n                    dark\r\n                    >Ativo</v-chip\r\n                  >\r\n                  <v-chip\r\n                    v-if=\"item.status == 'I'\"\r\n                    small\r\n                    class=\"px-6 py-3\"\r\n                    color=\"#A80000\"\r\n                    dark\r\n                    >Inativo</v-chip\r\n                  >\r\n                </template>\r\n              </k-data-list>\r\n            </v-col>\r\n          </v-row>\r\n        </v-col>\r\n\r\n        <v-row>\r\n          <v-col cols=\"12\" sm=\"12\">\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn\r\n                color=\"orange lighten-1\"\r\n                @click=\"retornaListaAbsenteismo()\"\r\n                large\r\n                dark\r\n                width=\"150\"\r\n                >Voltar</v-btn\r\n              >\r\n            </v-card-actions>\r\n          </v-col>\r\n        </v-row>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template> \r\n\r\n\r\n\r\n<script lang=\"ts\">\r\nvar validaNumeros = require(\"../../baseGenerica/validaNumeros.js\");\r\n\r\nimport Vue from \"vue\";\r\n\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport { Validation, ValidationSchema } from \"../../baseGenerica/validation\";\r\n\r\nimport {\r\n  IAbsenteismoConfiguracaoFormView,\r\n  IAbsenteismoConfiguracaoFormController,\r\n} from \"../../contracts/Absenteismo/AbsenteismoConfiguracaoFormContract\";\r\nimport { AbsenteismoConfiguracaoFormController } from \"../../controllers/Absenteismo/AbsenteismoConfiguracaoFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class AbsenteismoConfiguracaoForm\r\n  extends Vue\r\n  implements IAbsenteismoConfiguracaoFormView {\r\n  _controller?: IAbsenteismoConfiguracaoFormController;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Absenteísmo\",\r\n      disabled: false,\r\n      href: \"/absenteismo\",\r\n    },\r\n    {\r\n      text: \"Configurações\",\r\n      disabled: false,\r\n      href: \"/absenteismoConfiguracoes\",\r\n    },\r\n  ];\r\n  configuracao = {\r\n    sigla: \"\",\r\n    descricao: \"\",\r\n    status: \"A\", //STATUS: A: ATIVO / I: INATIVO\r\n    dataCadastro: new Date(),\r\n  };\r\n\r\n  itemsConfiguracoes = [];\r\n\r\n  search = \"\";\r\n\r\n  headers = new Headers([\r\n    new Header(\"Sigla\", \"sigla\", false, false, \"left\", false, \"100\"),\r\n    new Header(\"Descrição\", \"descricao\", false, false, \"left\", false, \"200\"),\r\n    new Header(\"Status\", \"status\", false, false, \"left\", true, \"100\"),\r\n    new Header(\"Ação\", \"acoes\", true, false, \"center\", false, \"100\"),\r\n  ]);\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  dialogDelete = false;\r\n  dialogAtivar = false;\r\n\r\n  itemTipoAbsenteismo: any;\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/absenteismoConfiguracoes\");\r\n    }\r\n  }\r\n\r\n  retornaListaAbsenteismo() {\r\n    this.$router.push(\"/absenteismo\");\r\n  }\r\n\r\n  limparCampos(): void {\r\n    this.configuracao.sigla = \"\";\r\n    this.configuracao.descricao = \"\";\r\n  }\r\n\r\n  validate() {\r\n    let schema: ValidationSchema[] = [];\r\n\r\n    schema.push(new ValidationSchema(\"sigla\", \"Sigla\", \"string\", true, 30));\r\n    schema.push(\r\n      new ValidationSchema(\"descricao\", \"Desrição\", \"string\", true, 50)\r\n    );\r\n\r\n    let result = new Validation().ValidSchema(schema, this.configuracao);\r\n    if (result.sucesso == false) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = result.mensagem;\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  saveConfiguracao() {\r\n    if (this.validate()) {\r\n    } else {\r\n      this._controller!.salvar(this.configuracao);\r\n      // this.retornaListaAbsenteismo();\r\n    }\r\n  }\r\n\r\n  showBaseForEditar(item: any) {}\r\n\r\n  showItens(list: any): void {\r\n    this.itemsConfiguracoes = list;\r\n  }\r\n\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemTipoAbsenteismo = item;\r\n    this.dialogDelete = true;\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    this._controller!.deleteitem(this.itemTipoAbsenteismo.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    this.itemTipoAbsenteismo = item;\r\n    this.dialogAtivar = true;\r\n  }\r\n\r\n  ativarItemConfirmado() {\r\n    this.dialogAtivar = false;\r\n    this._controller!.ativaritem(this.itemTipoAbsenteismo.id);\r\n  }\r\n\r\n  created() {\r\n    this._controller = new AbsenteismoConfiguracaoFormController(this);\r\n    this._controller.carregarItens();\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n</script> "]}]}