do\" :dialog=\"dialogLoading\"></k-loading>\r\n    <k-dialog\r\n      @fecharDialog=\"fecharDialogMessage\"\r\n      :dialog=\"dialogMessage\"\r\n      title=\"Error\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n    <k-dialog\r\n      :dialog=\"dialogDelete\"\r\n      title=\"Remover Operador?\"\r\n      text=\"O Operador selecionado serÃ¡ removido. Deseja Continuar?\"\r\n    >\r\n      <template v-slot:buttoes>\r\n        <v-btn\r\n          @click=\"dialogDelete = false\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"red darken-2\"\r\n          >Cancelar</v-btn\r\n        >\r\n        <v-btn\r\n          @click=\"deleteItemConfirmado\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"green darken-2\"\r\n          >Sim</v-btn\r\n        >\r\n      </template>\r\n    </k-dialog>\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n          <template v-slot:item=\"{ item }\">\r\n            <router-link :to=\"item.href\">\r\n              {{ item.text }}\r\n            </router-link>\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n\r\n      <v-row>\r\n        <v-col cols=\"10\">\r\n          <h3>\r\n            <b>Operadores</b>\r\n          </h3>\r\n          <span>\r\n            <b>Operadores Cadastrados</b>\r\n          </span>\r\n        </v-col>\r\n        <v-spacer></v-spacer>\r\n\r\n        <!-- <v-col cols=\"1\">\r\n          <v-btn v-if=\"!isMobile\" color=\"primary\" fab dark @click=\"addSolicitacao\">\r\n            <v-icon dark>mdi-plus</v-icon>\r\n          </v-btn>\r\n        </v-col>-->\r\n      </v-row>\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-col cols=\"12\" sm=\"5\" style=\"float: right\">\r\n            <v-text-field\r\n              v-model=\"search\"\r\n              label=\"Pesquisa Operadores\"\r\n              class=\"mx-4\"\r\n              prepend-icon=\"mdi-magnify\"\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row style=\"margin-top: -30px\">\r\n        <v-col cols=\"12\" sm=\"3\">\r\n          <v-icon small color=\"green\">mdi-circle</v-icon>\r\n          <span>Operadores com Habilidades</span>\r\n        </v-col>\r\n        <v-col cols=\"12\" sm=\"3\">\r\n          <v-icon small color=\"error\">mdi-circle</v-icon>\r\n          <span>Operadores sem Habilidade</span>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row style=\"margin-top: -10px\" align=\"center\" justify=\"center\">\r\n        <v-col cols=\"12\">\r\n          <k-data-list\r\n            orientation=\"Horizontal\"\r\n            mobileIconAdd=\"mdi-plus-circle\"\r\n            mobileFunctionAdd=\"addSolicitacao\"\r\n            @editarItem=\"editarItem\"\r\n            @deleteItem=\"deleteItem\"\r\n            @addSolicitacao=\"addSolicitacao\"\r\n            :headers=\"headers\"\r\n            :search=\"search\"\r\n            :itens=\"itens\"\r\n          >\r\n            <template v-slot:habilidadeOperadorVinculada=\"{ item }\">\r\n              <v-icon\r\n                small\r\n                v-if=\"item.habilidadeOperadorVinculada == 'S'\"\r\n                color=\"green\"\r\n                >mdi-circle</v-icon\r\n              >\r\n              <v-icon\r\n                small\r\n                v-if=\"item.habilidadeOperadorVinculada == 'N'\"\r\n                color=\"error\"\r\n                >mdi-circle</v-icon\r\n              >\r\n            </template>\r\n          </k-data-list>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from \"vue\";\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\n