 :dialog=\"dialogMessage\"\r\n      title=\"Mensagem\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n\r\n      <k-dialog\r\n        :dialog=\"dialogDelete\"\r\n        title=\"Remover Aprovador?\"\r\n        text=\"O Aprovador selecionado será removido. Deseja Continuar?\"\r\n      >\r\n        <template v-slot:buttoes>\r\n          <v-btn\r\n            @click=\"cancelaRemoverAprovadorResponsavel()\"\r\n            width=\"100\"\r\n            text\r\n            dark\r\n            color=\"red darken-2\"\r\n          >Cancelar</v-btn>\r\n          <v-btn\r\n            @click=\"removerAprovadorResponsavel()\"\r\n            width=\"100\"\r\n            text\r\n            dark\r\n            color=\"green darken-2\"\r\n          >Sim</v-btn>\r\n        </template>\r\n      </k-dialog>\r\n\r\n      <v-row ref=\"form\">\r\n        <v-col cols=\"12\" v-if=\"!idFluxoAprovaco\">\r\n          <h3>\r\n            <b>Novo Fluxo de Aprovação</b>\r\n          </h3>\r\n          <span>\r\n            <b>Adicione um novo fluxo de aprovação no sistema</b>\r\n          </span>\r\n        </v-col>\r\n\r\n        <v-col cols=\"12\" v-if=\"idFluxoAprovaco\">\r\n          <h3>\r\n            <b>Visualizar Fluxo de Aprovação</b>\r\n          </h3>\r\n          <span>\r\n            <b>Dados do fluxo de aprovação</b>\r\n          </span>\r\n        </v-col>\r\n\r\n        <v-row>\r\n          <v-col cols=\"12\" sm=\"12\">\r\n            <v-card flat>\r\n              <!-- DADOS DA INSERÇÃO DE APROVCAÇÃO -->\r\n              <v-card-text>\r\n                <v-row>\r\n                  <v-col cols=\"12\" sm=\"4\">\r\n                    <span>\r\n                      <b>Dados do Tipo de Fluxo de Aprovação</b>\r\n                    </span>\r\n                  </v-col>\r\n                </v-row>\r\n                <v-row style=\"margin-top: -15px\">\r\n                  <v-col cols=\"12\" sm=\"4\">\r\n                    <v-select\r\n                      v-if=\"idFluxoAprovaco == undefined\"\r\n                      :items=\"itemsFluxoAprovacao\"\r\n                      v-model=\"tipoFluxo\"\r\n                      return-object\r\n                      label=\"Tipo de Fluxo\"\r\n                      autocomplete=\"off\"\r\n                      item-text=\"descricao\"\r\n                      outlined\r\n                      dense\r\n                    ></v-select>\r\n                    <v-text-field\r\n                      v-else\r\n                      v-model=\"tipoFluxo.descricao\"\r\n                      outlined\r\n                      label=\"Tipo de Fluxo\"\r\n                      disabled\r\n                    ></v-text-field>\r\n                    \r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"2\" v-if=\"idFluxoAprovaco !== undefined\">\r\n                     <v-text-field\r\n                      v-model=\"tipoFluxo.versao\"\r\n                      outlined\r\n                      label=\"Versão do Fluxo\"\r\n                      disabled\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                   <v-col cols=\"12\" sm=\"2\" v-if=\"idFluxoAprovaco !== undefined\">\r\n                     <v-text-field\r\n                      v-model=\"tipoFluxo.status\"\r\n                      outlined\r\n                      label=\"Status Fluxo\"\r\n                      disabled\r\n                    ></v-text-field>\r\n                  </v-col>\r\n                </v-row>\r\n                <v-row style=\"margin-top: -20px\" v-if=\"idFluxoAprovaco == undefined\">\r\n                  <v-col cols=\"12\" sm=\"4\">\r\n                    <span>\r\n                      <b>Dados do Responsável da Nova Aprovação</b>\r\n                    </span>\r\n                  </v-col>\r\n                </v-row>\r\n\r\n                 <v-row style=\"margin-top: -20px\" v-if=\"idFluxoAprovaco !== undefined\">\r\n                  <v-col cols=\"12\" sm=\"4\">\r\n                    <span>\r\n                      <b>Responsáveis pela Aprovação</b>\r\n                    </span>\r\n                  </v-col>\r\n                </v-row>\r\n                <v-row style=\"margin-top: -15px\" v-if=\"idFluxoAprovaco == undefined\">\r\n                  <v-col cols=\"12\" sm=\"6\">\r\n                    <v-autocomplete\r\n                      :items=\"itemsCentroCusto\"\r\n                      :filter=\"customFilterCentroCusto\"\r\n                      v-model=\"dadosFluxo.centroCusto\"\r\n                      return-object\r\n                      @change=\"changeCentroCusto(dadosFluxo.centroCusto)\"\r\n                      item-text=\"dadoCentroCusto\"\r\n                      label=\"Código / Centro de Custo\"\r\n                      outlined\r\n                      dense\r\n                    ></v-autocomplete>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"6\">\r\n                    <v-select\r\n                      :items=\"itemsDepartamentoSelecionado\"\r\n                      v-model=\"dadosFluxo.departamento\"\r\n                      return-object\r\n                      label=\"Departamento\"\r\n                      autocomplete=\"off\"\r\n                      item-text=\"campoGeneric\"\r\n                      item-value=\"codigoGeneric\"\r\n                      outlined\r\n                      dense\r\n                      :disabled=\"disabledCentroCusto\"\r\n                    ></v-select>\r\n                  </v-col>\r\n                </v-row>\r\n                <v-row style=\"margin-top: -25px\" v-if=\"idFluxoAprovaco == undefined\">\r\n                  <v-col cols=\"12\" sm=\"6\">\r\n                    <v-autocomplete\r\n                      :disabled=\"disabledCentroCusto\"\r\n                      :items=\"itemsFuncionarioSelecionado\"\r\n                      v-model=\"dadosFluxo.funcionario\"\r\n                      return-object\r\n                      :filter=\"customFilter\"\r\n                      item-text=\"dadosFuncionario\"\r\n                      @change=\"changeFuncao(dadosFluxo.funcionario)\"\r\n                      label=\"Matrícula / Nome\"\r\n                      outlined\r\n                      dense\r\n                    ></v-autocomplete>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"5\">\r\n                    <v-text-field\r\n                      outlined\r\n                      v-model=\"dadosFluxo.cargo\"\r\n                      label=\"Função\"\r\n                      dense\r\n                      required\r\n                      disabled\r\n                    ></v-text-field>\r\n                  </v-col>\r\n\r\n                  <v-col cols=\"1\">\r\n                    <v-btn\r\n                      v-if=\"!isMobile\"\r\n                      color=\"#FB9514\"\r\n                      fab\r\n                      dark\r\n                      @click=\"addAprovadorResponsavel()\"\r\n                    >\r\n                      <v-icon dark>mdi-plus</v-icon>\r\n                    </v-btn>\r\n                  </v-col>\r\n                </v-row>\r\n\r\n                <!-- LISTAGEM DE APROVADORES -->\r\n                <v-row v-for=\"(item, index) in itemsAprovadores\" :key=\"index\">\r\n                  <v-col cols=\"12\" sm=\"12\">\r\n                    <v-card outlined>\r\n                      <v-list-item>\r\n                        <v-list-item-avatar color=\"primary\">\r\n                          <label class=\"white--text\">{{index + 1 + 'ª'}}</label>\r\n                        </v-list-item-avatar>\r\n                        <v-list-item-content>\r\n                          <v-list-item-title class=\"headline\">Aprovação</v-list-item-title>\r\n                        </v-list-item-content>\r\n                      </v-list-item>\r\n\r\n                      <!-- NOVO REGISTRO -->\r\n                      <v-card-text flat class=\"max-auto\"  v-if=\"idFluxoAprovaco == undefined\" >\r\n                        <v-row>\r\n                          <v-col cols=\"12\" sm=\"5\">\r\n                            <b>\r\n                              <v-text-field\r\n                                style=\"font-size: 13px\"\r\n                                disabled\r\n                                :hint=\"'CARGO: ' + item.funcionario.cargo\"\r\n                                :value=\"item.funcionario.dadosFuncionario\"\r\n                                :persistent-hint=\"true\"\r\n                                label=\"Responsável: Matricula / Nome\"\r\n                                readonly\r\n                              ></v-text-field>\r\n                            </b>\r\n                          </v-col>\r\n                          <v-col cols=\"12\" sm=\"4\">\r\n                            <b>\r\n                              <v-text-field\r\n                                style=\"font-size: 13px\"\r\n                                disabled\r\n                                :value=\"item.centroCusto.dadoCentroCusto\"\r\n                                label=\"Centro de Custo: Código / Descrição\"\r\n                                readonly\r\n                              ></v-text-field>\r\n                            </b>\r\n                          </v-col>\r\n                          <v-col cols=\"12\" sm=\"3\">\r\n                            <b>\r\n                              <v-text-field\r\n                                style=\"font-size: 13px\"\r\n                                disabled\r\n                                :value=\"item.departamento.campoGeneric\"\r\n                                label=\"Departamento\"\r\n                                readonly\r\n                              ></v-text-field>\r\n                            </b>\r\n                            <v-btn\r\n                              style=\"float: right\"\r\n                              large\r\n                              v-if=\"!isMobile\"\r\n                              color=\"primary\"\r\n                              icon=\"dark\"\r\n                              @click=\"modalRemoverAprovadorResponsavel(index)\"\r\n                            >\r\n                              <v-icon dark>mdi-delete</v-icon>\r\n                            </v-btn>\r\n                          </v-col>\r\n                        </v-row>\r\n                      </v-card-text>\r\n\r\n                      <!-- VISUALIZAR -->\r\n                      <v-card-text flat class=\"max-auto\" v-else>\r\n                        <v-row>\r\n                          <v-col cols=\"12\" sm=\"5\">\r\n                            <b>\r\n                              <v-text-field\r\n                                style=\"font-size: 13px\"\r\n                                disabled\r\n                                :hint=\"'CARGO: ' + item.cargo\"\r\n                                :value=\"item.funcionarioErpDTO.matricula + ': ' + item.funcionarioErpDTO.nome\"\r\n                                :persistent-hint=\"true\"\r\n                                label=\"Responsável: Matricula / Nome\"\r\n                                readonly\r\n                              ></v-text-field>\r\n                            </b>\r\n                          </v-col>\r\n                          <v-col cols=\"12\" sm=\"4\">\r\n                            <b>\r\n                              <v-text-field\r\n                                style=\"font-size: 13px\"\r\n                                disabled\r\n                                :value=\"item.idCentroCusto.trim() + ': ' + item.descricaoCentroCusto\"\r\n                                label=\"Centro de Custo: Código / Descrição\"\r\n                                readonly\r\n                              ></v-text-field>\r\n                            </b>\r\n                          </v-col>\r\n                          <v-col cols=\"12\" sm=\"3\">\r\n                            <b>\r\n                              <v-text-field\r\n                                style=\"font-size: 13px\"\r\n                                disabled\r\n                                :value=\"item.idSetor.trim() + ': ' + item.setor\"\r\n                                label=\"Departamento\"\r\n                                readonly\r\n                              ></v-text-field>\r\n                            </b>\r\n                            \r\n                          </v-col>\r\n                        </v-row>\r\n                      </v-card-text>\r\n                    </v-card>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-card-text>\r\n            </v-card>\r\n          </v-col>\r\n          <v-col cols=\"12\" sm=\"12\">\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn\r\n                v-if=\"idFluxoAprovaco == undefined\"\r\n                color=\"orange lighten-1\"\r\n                @click=\"retornaListaFluxosAprovacao()\"\r\n                large\r\n                dark\r\n                width=\"150\"\r\n              >Cancelar</v-btn>\r\n              <v-btn\r\n                v-if=\"idFluxoAprovaco !== undefined\"\r\n                color=\"orange lighten-1\"\r\n                @click=\"retornaListaFluxosAprovacao()\"\r\n                large\r\n                dark\r\n                width=\"150\"\r\n              >Voltar</v-btn>\r\n\r\n              <!-- <v-btn\r\n                v-if=\"!isMobile && idFluxoAprovaco\"\r\n                color=\"orange darken-1\"\r\n                large\r\n                dark\r\n                width=\"150\"\r\n                @click=\"updateFuncionario()\"\r\n              >Salvar</v-btn>-->\r\n\r\n              <v-btn\r\n                v-if=\"!isMobile && idFluxoAprovaco == undefined\"\r\n                color=\"orange darken-1\"\r\n                large\r\n                dark\r\n                width=\"150\"\r\n                @click=\"saveFluxoAprovacao()\"\r\n              >Salvar</v-btn>\r\n\r\n              <v-btn\r\n                v-else-if=\"isMobile && idFluxoAprovaco == undefined\"\r\n                color=\"orange darken-1\"\r\n                large\r\n                dark\r\n                width=\"150\"\r\n                @click=\"saveFluxoAprovacao()\"\r\n              >Salvar</v-btn>\r\n            </v-card-actions>\r\n          </v-col>\r\n        </v-row>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template> \r\n\r\n\r\n\r\n<script lang=\"ts\">\r\nvar validaNumeros = require(\"../../baseGenerica/validaNumeros.js\");\r\n\r\nimport Vue from \"vue\";\r\n\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  IFluxoAprovacaoFormView,\r\n  IFluxoAprovacaoFormController,\r\n} from \"../../contracts/FluxoAprovacao/FluxoAprovacaoFormContract\";\r\nimport { FluxoAprovacaoFormController } from \"../../controllers/FluxoAprovacao/FluxoAprovacaoFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class FluxoAprovacaoForm extends Vue\r\n  implements IFluxoAprovacaoFormView {\r\n  _controller?: IFluxoAprovacaoFormController;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Fluxo de Aprovação\",\r\n      disabled: false,\r\n      href: \"/fluxosAprovacao\",\r\n    },\r\n  ];\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  maskCPF = \"###.###.###-##\";\r\n\r\n  //FLUXO APROVAÇÃO TIPO\r\n  itemsFluxoAprovacao = [];\r\n\r\n  //FLUXO CENTRO DE CUSTO\r\n  itemsCentroCusto = [];\r\n\r\n  //FLUXO DEPARTAMENTO\r\n  itemsDepartamento = [];\r\n  itemsDepartamentoSelecionado: any = [];\r\n  disabledCentroCusto = true;\r\n\r\n  //FUNCIONARIOS\r\n  itemsFuncionario = [];\r\n  itemsFuncionarioSelecionado: any = [];\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  idFluxoAprovaco = \"\";\r\n\r\n  tipoFluxo: any = [];\r\n  tipoFluxoSelecionado: any = [];\r\n\r\n  dadosFluxo: any = {\r\n    centroCusto: \"\",\r\n    departamento: \"\",\r\n    funcionario: \"\",\r\n    cargo: \"\",\r\n    nivel: \"\",\r\n  };\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  //LISTAGEM DE AROVADORES\r\n  itemsAprovadores: any = [];\r\n  listaAprovadores: any = [];\r\n\r\n  dialogDelete = false;\r\n  indexRemoverAprovador = -1;\r\n\r\n  fluxoAprovacao = {\r\n    tipoFluxo: this.tipoFluxo,\r\n    aprovadores: this.itemsAprovadores,\r\n    dataCriacaoFluxo: new Date(),\r\n    versao: \"\",\r\n    idFluxoVersaoAnterior: 0,\r\n  };\r\n\r\n  versaoFluxo = 0;\r\n\r\n  modalRemoverAprovadorResponsavel(indexRemoveItem: string) {\r\n    this.indexRemoverAprovador = parseInt(indexRemoveItem);\r\n    this.dialogDelete = true;\r\n  }\r\n\r\n  removerAprovadorResponsavel() {\r\n    for (const { item, index } of this.itemsAprovadores.map(\r\n      (item: any, index: any) => ({\r\n        item,\r\n        index,\r\n      })\r\n    )) {\r\n      if (index == this.indexRemoverAprovador) {\r\n        this.itemsAprovadores.splice(index, 1);\r\n        this.indexRemoverAprovador = -1;\r\n        this.dialogDelete = false;\r\n        return;\r\n      }\r\n    }\r\n  }\r\n\r\n  cancelaRemoverAprovadorResponsavel() {\r\n    this.indexRemoverAprovador = -1;\r\n    this.dialogDelete = false;\r\n  }\r\n\r\n  addAprovadorResponsavel() {\r\n    if (this.validateAddResponsavel()) {\r\n    } else {\r\n      this.itemsAprovadores.push(this.dadosFluxo);\r\n      this.dadosFluxo = {\r\n        centroCusto: \"\",\r\n        departamento: \"\",\r\n        funcionario: \"\",\r\n        cargo: \"\",\r\n        nivel: \"\",\r\n        dataCriacaoFluxo: new Date(),\r\n      };\r\n      this.disabledCentroCusto = true;\r\n    }\r\n  }\r\n\r\n  formatDate(date: string) {\r\n    if (!date) return null;\r\n\r\n    const [year, month, day] = date.split(\"-\");\r\n    return `${day}/${month}/${year}`;\r\n  }\r\n\r\n  parseDate(date: string) {\r\n    if (!date) return null;\r\n    const [day, month, year] = date.split(\"/\");\r\n    return `${year}-${month.padStart(2, \"0\")}-${day.padStart(2, \"0\")}`;\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/fluxosAprovacao\");\r\n    }\r\n  }\r\n\r\n  retornaListaFluxosAprovacao() {\r\n    this.$router.push(\"/fluxosAprovacao\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validateAddResponsavel() {\r\n    if (this.dadosFluxo.centroCusto.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o Centro de Custo.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.dadosFluxo.departamento.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o Departamento.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.dadosFluxo.funcionario.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o Responsável pela aprovação.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    } else {\r\n      let flag = false;\r\n      this.itemsAprovadores.forEach((element: any) => {\r\n        if (\r\n          element.funcionario.matricula == this.dadosFluxo.funcionario.matricula\r\n        ) {\r\n          this.colorBorder = \"warning\";\r\n          this.color = \"warning\";\r\n          this.text = \"Responsável já adicionado como um aprovador.\";\r\n          this.snackbar = true;\r\n          flag = true;\r\n        }\r\n      });\r\n\r\n      return flag;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  validate() {\r\n    if (this.tipoFluxo.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o Tipo de Fluxo.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n    return false;\r\n  }\r\n\r\n  saveFluxoAprovacao() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      for (const { item, index } of this.itemsAprovadores.map(\r\n        (item: any, index: any) => ({\r\n          item,\r\n          index,\r\n        })\r\n      )) {\r\n        this.itemsAprovadores[index].nivel = index + 1;\r\n      }\r\n      this.salvarFluxoAprovacao();\r\n    }\r\n  }\r\n\r\n  showFluxoAprovacaoForView(item: any) {\r\n    //CARREGANDO DADOS DO FUNCIONARIO\r\n    this.tipoFluxo.descricao = item[0].fluxoAprovacaoTipoDTO.descricao;\r\n    this.tipoFluxo.versao = item[0].versao\r\n    \r\n    if (item[0].ativo == true){\r\n      this.tipoFluxo.status = \"ATIVO\";\r\n    }\r\n    \r\n    if  (item[0].ativo == false){\r\n      this.tipoFluxo.status = \"INATIVO\"\r\n    }\r\n\r\n    this.itemsAprovadores =\r\n      item[0].fluxoAprovacaoDadosFuncionariosAprovadoresDTO;\r\n\r\n    console.log(this.itemsAprovadores);\r\n  }\r\n\r\n  salvarFluxoAprovacao() {\r\n    this._controller!.consultarVersaoFluxoAprovacao(this.tipoFluxo.id);\r\n\r\n    //apos a execução dessa função o contrller chama a fnção dadosFluxoAprovacao\r\n    //para salvar os dados da tela\r\n    // PARA SALVAR DADOS\r\n  }\r\n\r\n  dadosFluxoAprovacao(item: any): void {\r\n    this.versaoFluxo = parseInt(item.versao) + 1;\r\n\r\n    this.fluxoAprovacao.tipoFluxo = this.tipoFluxo;\r\n    this.fluxoAprovacao.aprovadores = this.itemsAprovadores;\r\n    this.fluxoAprovacao.dataCriacaoFluxo = new Date();\r\n    this.fluxoAprovacao.versao = this.versaoFluxo.toString();\r\n    this.fluxoAprovacao.idFluxoVersaoAnterior = item.idFluxoVersaoAnterior\r\n\r\n    this._controller!.salvar(this.fluxoAprovacao);\r\n  }\r\n\r\n  carregaFluxoAprovacaoTipo(item: any): void {\r\n    if (this.idFluxoAprovaco == undefined) {\r\n      this.itemsFluxoAprovacao = item;\r\n    }\r\n  }\r\n\r\n  carregaCentroCusto(item: any): void {\r\n    this.itemsCentroCusto = item;\r\n  }\r\n\r\n  changeCentroCusto(centroCusto: any) {\r\n    const codigoCentroCusto: string = centroCusto.codigo.trim();\r\n\r\n    //DEPARTAMENTO POR CENTRO DE CUSTO\r\n    this.itemsDepartamentoSelecionado = [];\r\n    this.itemsDepartamento.forEach((element: any) => {\r\n      //campoGeneric2 = Centro de Custo (Bck)\r\n      const campoGeneric2: string = element.campoGeneric2.trim();\r\n      if (campoGeneric2 == codigoCentroCusto) {\r\n        this.itemsDepartamentoSelecionado.push(element);\r\n      }\r\n    });\r\n\r\n    // //FUNCIONARIO POR CENTRO DE CUSTO\r\n    this.itemsFuncionarioSelecionado = [];\r\n    this.itemsFuncionario.forEach((element: any) => {\r\n      const codigoCentroCustoFunc: string = element.codigoCentroCusto.trim();\r\n      if (codigoCentroCustoFunc == codigoCentroCusto) {\r\n        this.itemsFuncionarioSelecionado.push(element);\r\n      }\r\n    });\r\n\r\n    //caso nao esteja com nenhum funcioanrio por centro de cuto selecionado\r\n    //a aplicação deverá carregar todos os funcionarios\r\n    if (this.itemsFuncionarioSelecionado.length == 0) {\r\n      this.itemsFuncionarioSelecionado = this.itemsFuncionario;\r\n    }\r\n\r\n    this.disabledCentroCusto = false;\r\n  }\r\n\r\n  changeFuncao(item: any) {\r\n    this.dadosFluxo.cargo = \"\";\r\n    const matricula: string = item.matricula;\r\n    this.itemsFuncionarioSelecionado.forEach((element: any) => {\r\n      const eleMatricula: string = element.matricula;\r\n      if (eleMatricula.trim() == matricula.trim()) {\r\n        this.dadosFluxo.cargo = element.cargo;\r\n      }\r\n    });\r\n  }\r\n\r\n  carregaDepartamento(item: any): void {\r\n    this.itemsDepartamento = item;\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.idFluxoAprovaco = this.$route.params.id;\r\n  }\r\n\r\n  carregaFuncionariosPorCentroCusto(item: any): void {\r\n    this.itemsFuncionario = item;\r\n  }\r\n\r\n  customFilter(item: any, queryText: any, itemText: any) {\r\n    const textOne = item.matricula.toLowerCase();\r\n    const textTwo = item.nome.toLowerCase();\r\n    const searchText = queryText.toLowerCase();\r\n\r\n    return textOne.indexOf(searchText) > -1 || textTwo.indexOf(searchText) > -1;\r\n  }\r\n\r\n  customFilterCentroCusto(item: any, queryText: any, itemText: any) {\r\n    const textOne = item.campoGeneric.toLowerCase();\r\n    const textTwo = item.codigoGeneric.toLowerCase();\r\n    const searchText = queryText.toLowerCase();\r\n\r\n    return textOne.indexOf(searchText) > -1 || textTwo.indexOf(searchText) > -1;\r\n  }\r\n\r\n  created() {\r\n    this._controller = new FluxoAprovacaoFormController(this);\r\n    this._controller.carregarDtos(this.$route.params.id);\r\n    this.carregarDtos();\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n\r\n  // @Watch(\"date\")\r\n  // onDatasChanged(val: string) {\r\n  //   this.dateFormatted = this.formatDate(this.date);\r\n  // }\r\n}\r\n</script> "]}]}