mplate>\r\n  <v-content style=\"margin: 10px\">\r\n    <k-loading title=\"Carregando\" :dialog=\"dialogLoading\"></k-loading>\r\n    <k-dialog\r\n      @fecharDialog=\"fecharDialogMessage\"\r\n      :dialog=\"dialogMessage\"\r\n      title=\"Mensagem\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n          <template v-slot:item=\"{ item }\">\r\n            <router-link :to=\"item.href\">\r\n              {{ item.text }}\r\n            </router-link>\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n\r\n      <v-row>\r\n        <v-col cols=\"10\">\r\n          <h3>\r\n            <b>Produtos</b>\r\n          </h3>\r\n          <span>\r\n            <b>Produtos Acabados cadastrados no ERP.</b>\r\n          </span>\r\n        </v-col>\r\n        <v-spacer></v-spacer>\r\n\r\n        <v-col cols=\"1\">\r\n          <v-btn\r\n            v-if=\"!isMobile\"\r\n            color=\"primary\"\r\n            fab\r\n            dark\r\n            @click=\"erpProdutosCarregar\"\r\n            >ERP</v-btn\r\n          >\r\n        </v-col>\r\n      </v-row>\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-col cols=\"12\" sm=\"5\" style=\"float: right\">\r\n            <v-text-field\r\n              v-model=\"search\"\r\n              label=\"Pesquisa produtos\"\r\n              class=\"mx-4\"\r\n              prepend-icon=\"mdi-magnify\"\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row style=\"margin-top: -30px\" align=\"center\" justify=\"center\">\r\n        <v-col cols=\"12\">\r\n          <k-data-list\r\n            orientation=\"Horizontal\"\r\n            mobileIconAdd=\"mdi-plus-circle\"\r\n            mobileFunctionAdd=\"addSolicitacao\"\r\n            @editarItem=\"editarItem\"\r\n            @deleteItem=\"deleteItem\"\r\n            @addSolicitacao=\"addSolicitacao\"\r\n            :headers=\"headers\"\r\n            :search=\"search\"\r\n            :itens=\"itens\"\r\n          >\r\n            <template v-slot:qtdeOperador=\"{ item }\">\r\n             \r\n              <v-chip\r\n                v-if=\"item.qtdeOperador\"\r\n                class=\"ma-2\"\r\n                outlined\r\n                pill\r\n                color=\"red\"\r\n                small\r\n              >\r\n                {{ item.qtdeOperador }}\r\n              </v-chip>\r\n              <!-- <b v-if=\"item.nomeLinha !== null\" style=\"color: #3F51B5\">{{item.nomeLinha}}</b> -->\r\n              <l class=\"ma-2\" v-else>-</l>\r\n            </template>\r\n          </k-data-list>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from \"vue\";\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  IProdutoListaView,\r\n  IProdutoListaController,\r\n} from \"../../contracts/Cadastro/ProdutoListaContract\";\r\n\r\nimport { ProdutoListaController } from \"../../controllers/Cadastro/ProdutoListaController\";\r\n\r\nimport Component from \"vue-class-component\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class ProdutoLista extends Vue implements IProdutoListaView {\r\n  _controller?: IProdutoListaController;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  search = \"\";\r\n\r\n  dialogDelete = false;\r\n  itemProdutoDelete: any;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Produtos\",\r\n      disabled: false,\r\n      href: \"/produtos\",\r\n    },\r\n  ];\r\n\r\n  erpReProdutosCarregar() {\r\n    this.loading(true);\r\n    this.itens = [];\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new ProdutoListaController(this);\r\n    this._controller.carregarItens();\r\n    this._controller.carregarItens();\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n  showItens(list: any): void {\r\n    this.itens = list;\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemProdutoDelete = item;\r\n    this.dialogDelete = true;\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    //this._controller!.deleteitem(this.itemProdutoDelete.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    this.$router.push(\"/produto/\" + item.id);\r\n  }\r\n\r\n  erpProdutosCarregar() {\r\n    this.loading(true);\r\n    this.itens = [];\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new ProdutoListaController(this);\r\n\r\n    this._controller.importaProdutosErp();\r\n    this._controller.carregarItens();\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  headers = new Headers([\r\n    new Header(\"Codigo\", \"codigo\", false, false, \"center\", true, \"200\"),\r\n    new Header(\r\n      \"Descrição do Produto\",\r\n      \"descricao\",\r\n      false,\r\n      false,\r\n      \"left\",\r\n      true,\r\n      \"300\"\r\n    ),\r\n    new Header(\"Grupo\", \"grupo\", false, false, \"center\", true, \"200\"),\r\n    new Header(\r\n      \"Unidade de Medida\",\r\n      \"unidadeMedida\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      true,\r\n      \"100\"\r\n    ),\r\n    new Header(\"NCM\", \"ncm\", false, false, \"center\", true, \"100\"),\r\n    new Header(\r\n      \"Qtde de Operadores (Produção)\",\r\n      \"qtdeOperador\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      true,\r\n      \"200\"\r\n    ),\r\n\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"50\"),\r\n  ]);\r\n\r\n  itens = [];\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/produto\");\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new ProdutoListaController(this);\r\n\r\n    this._controller.carregarItens();\r\n  }\r\n}\r\n</script>"]}]}