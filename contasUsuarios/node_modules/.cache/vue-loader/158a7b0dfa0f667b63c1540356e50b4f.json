n    <k-loading title=\"Carregando\" :dialog=\"dialogLoading\"></k-loading>\r\n    <k-dialog\r\n      @fecharDialog=\"fecharDialogMessage\"\r\n      :dialog=\"dialogMessage\"\r\n      title=\"Mensagem\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n    <k-dialog\r\n      :dialog=\"dialogDelete\"\r\n      title=\"Remover Perfil do Usuário?\"\r\n      text=\"O Perfil do Usuário selecionado será removido. Deseja Continuar?\"\r\n    >\r\n      <template v-slot:buttoes>\r\n        <v-btn @click=\"dialogDelete = false\" width=\"100\" text dark color=\"red darken-2\">Cancelar</v-btn>\r\n        <v-btn @click=\"deleteItemConfirmado\" width=\"100\" text dark color=\"green darken-2\">Sim</v-btn>\r\n      </template>\r\n    </k-dialog>\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n\r\n      <v-row>\r\n        <v-col cols=\"10\">\r\n          <h3>\r\n            <b>Usuários</b>\r\n          </h3>\r\n          <span>\r\n            <b>Lista de Usuários credenciados no sistema.</b>\r\n          </span>\r\n        </v-col>\r\n        <v-spacer></v-spacer>\r\n\r\n        <!-- <v-col cols=\"1\">\r\n        <v-btn v-if=\"!isMobile\" color=\"primary\" fab dark @click=\"addSolicitacao\">\r\n          <v-icon dark>mdi-plus</v-icon>\r\n        </v-btn>\r\n        </v-col>-->\r\n      </v-row>\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-col cols=\"12\" sm=\"5\" style=\"float: right\">\r\n            <v-text-field\r\n              v-model=\"search\"\r\n              label=\"Pesquisa Usuário\"\r\n              class=\"mx-4\"\r\n              prepend-icon=\"mdi-magnify\"\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row style=\"margin-top: -30px\" align=\"center\" justify=\"center\">\r\n        <v-col cols=\"12\">\r\n          <k-data-list\r\n            orientation=\"Horizontal\"\r\n            mobileIconAdd=\"mdi-plus-circle\"\r\n            mobileFunctionAdd=\"addSolicitacao\"\r\n            @editarItem=\"editarItem\"\r\n            @deleteItem=\"deleteItem\"\r\n            @addSolicitacao=\"addSolicitacao\"\r\n            :headers=\"headers\"\r\n            :search=\"search\"\r\n            :itens=\"itens\"\r\n          >\r\n            <template v-slot:tipo=\"{ item }\">\r\n              <l v-if=\"item.tipo == 'EXT'\">EXTERNO</l>\r\n              <l v-if=\"item.tipo == 'ERP'\">ERP</l>\r\n            </template>\r\n            <template v-slot:usuarioPerfil=\"{ item }\">\r\n              <l v-if=\"item.usuarioPerfil != null\">{{item.usuarioPerfil.descricaoPerfil}}</l>\r\n              <l v-if=\"item.usuarioPerfil == null\">-</l>\r\n            </template>\r\n          </k-data-list>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from \"vue\";\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  IUsuarioListaView,\r\n  IUsuarioListaController,\r\n} from \"../../contracts/Acessos/UsuarioListaContract\";\r\n\r\nimport { UsuarioListaController } from \"../../controllers/Acessos/UsuarioListaController\";\r\n\r\nimport Component from \"vue-class-component\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class UsuarioLista extends Vue implements IUsuarioListaView {\r\n  _controller?: IUsuarioListaController;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  search = \"\";\r\n\r\n  dialogDelete = false;\r\n  itemUsuarioDelete: any;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Usuários\",\r\n      disabled: false,\r\n      href: \"/usuarios\",\r\n    },\r\n  ];\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  showItens(list: any): void {\r\n    console.log(\"TESTE\");\r\n    this.itens = [];\r\n\r\n    this.itens = list;\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    if (item.usuarioPerfil != null) {\r\n      this.itemUsuarioDelete = item.usuarioPerfil.id;\r\n      this.dialogDelete = true;\r\n    } else {\r\n      this.showMsg(\"Usuário não possui Perfil associado.\");\r\n    }\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    this._controller!.deleteitem(this.itemUsuarioDelete);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    if (item.usuarioPerfil == null) {\r\n      this.$router.push(\"/usuario/\" + item.id + \"/\" + item.matricula);\r\n    } else {\r\n      this.$router.push(\r\n        \"/usuario/\" +\r\n          item.id +\r\n          \"/\" +\r\n          item.matricula +\r\n          \"/\" +\r\n          item.usuarioPerfil.id\r\n      );\r\n    }\r\n  }\r\n\r\n  erpFuncionariosCarregar() {\r\n    this.loading(true);\r\n    this.itens = [];\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new UsuarioListaController(this);\r\n\r\n    //this._controller.carregarItens();\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  headers = new Headers([\r\n    new Header(\"Matrícula\", \"matricula\", false, false, \"center\", true, \"100\"),\r\n    new Header(\"Nome\", \"nome\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Cargo\", \"cargo\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Situação\", \"situacao\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Tipo\", \"tipo\", false, false, \"left\", true, \"100\"),\r\n    new Header(\r\n      \"Perfil Associado\",\r\n      \"usuarioPerfil\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      true,\r\n      \"200\"\r\n    ),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  itens = [];\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/perfil\");\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new UsuarioListaController(this);\r\n\r\n    this._controller.carregarItens();\r\n  }\r\n}\r\n</script>"]}]}