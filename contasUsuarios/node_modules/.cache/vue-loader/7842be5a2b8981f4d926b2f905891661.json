dialog\r\n      @fecharDialog=\"fecharDialogMessage\"\r\n      :dialog=\"dialogMessage\"\r\n      title=\"Mensagem\"\r\n      :text=\"textError\"\r\n    ></k-dialog>\r\n    <k-dialog\r\n      :dialog=\"dialogDelete\"\r\n      title=\"Remover Linha?\"\r\n      text=\"A Linha selecionada será removida. Deseja Continuar?\"\r\n    >\r\n      <template v-slot:buttoes>\r\n        <v-btn\r\n          @click=\"dialogDelete = false\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"red darken-2\"\r\n          >Cancelar</v-btn\r\n        >\r\n        <v-btn\r\n          @click=\"deleteItemConfirmado\"\r\n          width=\"100\"\r\n          text\r\n          dark\r\n          color=\"green darken-2\"\r\n          >Sim</v-btn\r\n        >\r\n      </template>\r\n    </k-dialog>\r\n    <v-container>\r\n      <v-row>\r\n        <v-breadcrumbs style=\"margin-left: -10px\" :items=\"caminho\">\r\n          <template v-slot:divider>\r\n            <!-- <v-icon>mdi-home</v-icon> -->\r\n          </template>\r\n          <template v-slot:item=\"{ item }\">\r\n            <router-link :to=\"item.href\">\r\n              {{ item.text }}\r\n            </router-link>\r\n          </template>\r\n        </v-breadcrumbs>\r\n      </v-row>\r\n\r\n      <v-row>\r\n        <v-col cols=\"10\">\r\n          <h3>\r\n            <b>Linhas de Produção</b>\r\n          </h3>\r\n          <span>\r\n            <b>Linha Cadastradas.</b>\r\n          </span>\r\n        </v-col>\r\n        <v-spacer></v-spacer>\r\n\r\n        <v-col cols=\"1\">\r\n          <v-btn\r\n            v-if=\"!isMobile\"\r\n            color=\"primary\"\r\n            fab\r\n            dark\r\n            @click=\"addSolicitacao\"\r\n          >\r\n            <v-icon dark>mdi-plus</v-icon>\r\n          </v-btn>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row>\r\n        <v-col cols=\"12\" sm=\"12\">\r\n          <v-col cols=\"12\" sm=\"5\" style=\"float: right\">\r\n            <v-text-field\r\n              v-model=\"search\"\r\n              label=\"Pesquisa Linha\"\r\n              class=\"mx-4\"\r\n              prepend-icon=\"mdi-magnify\"\r\n            ></v-text-field>\r\n          </v-col>\r\n        </v-col>\r\n      </v-row>\r\n      <v-row style=\"margin-top: -30px\" align=\"center\" justify=\"center\">\r\n        <v-col cols=\"12\">\r\n          <k-data-list\r\n            orientation=\"Horizontal\"\r\n            mobileIconAdd=\"mdi-plus-circle\"\r\n            mobileFunctionAdd=\"addSolicitacao\"\r\n            @editarItem=\"editarItem\"\r\n            @deleteItem=\"deleteItem\"\r\n            @addSolicitacao=\"addSolicitacao\"\r\n            :headers=\"headers\"\r\n            :search=\"search\"\r\n            :itens=\"itens\"\r\n          >\r\n            <template v-slot:deletado=\"{ item }\">\r\n              <v-chip\r\n                v-if=\"item.deletado == 'N'\"\r\n                small\r\n                class=\"px-7 py-3\"\r\n                color=\"#019A53\"\r\n                dark\r\n                >Ativo</v-chip\r\n              >\r\n              <v-chip\r\n                v-if=\"item.deletado == 'S'\"\r\n                small\r\n                class=\"px-6 py-3\"\r\n                color=\"#A80000\"\r\n                dark\r\n                >Inativo</v-chip\r\n              >\r\n            </template>\r\n\r\n            <!-- <template v-slot:deletado=\"{ item }\">\r\n              <v-chip  v-if=\"item.deletado == 'S'\" color=\"red\"\r\n                ><b style=\"color: white\">Inativo</b></v-chip\r\n              >\r\n              <v-chip  v-if=\"item.deletado == 'N'\" color=\"green\"\r\n                ><b style=\"color: white\">Ativo</b></v-chip\r\n              >\r\n            </template> -->\r\n          </k-data-list>\r\n        </v-col>\r\n      </v-row>\r\n    </v-container>\r\n  </v-content>\r\n</template>\r\n\r\n<script lang=\"ts\">\r\nimport Vue from \"vue\";\r\nimport KLoading from \"../../components/KLoading.vue\";\r\nimport KDialog from \"../../components/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  ILinhaListaView,\r\n  ILinhaListaController,\r\n} from \"../../contracts/Cadastro/LinhaListaContract\";\r\n\r\nimport { LinhaListaController } from \"../../controllers/Cadastro/LinhaListaController\";\r\n\r\nimport Component from \"vue-class-component\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class LinhaLista extends Vue implements ILinhaListaView {\r\n  _controller?: ILinhaListaController;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  search = \"\";\r\n\r\n  dialogDelete = false;\r\n  itemLinhaDelete: any;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Linha\",\r\n      disabled: false,\r\n      href: \"/linhas\",\r\n    },\r\n    {\r\n      text: \"Nova Linha\",\r\n      disabled: false,\r\n      href: \"/linha\",\r\n    },\r\n  ];\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n  showItens(list: any): void {\r\n    this.itens = list;\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemLinhaDelete = item;\r\n    this.dialogDelete = true;\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    this._controller!.deleteitem(this.itemLinhaDelete.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    this.$router.push(\"/Linha/\" + item.id);\r\n  }\r\n\r\n  erpLinhasCarregar() {\r\n    this.loading(true);\r\n    this.itens = [];\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new LinhaListaController(this);\r\n\r\n    this._controller.carregarItens();\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  headers = new Headers([\r\n    new Header(\"Status\", \"deletado\", false, false, \"center\", false, \"100\"),\r\n    new Header(\"Código\", \"id\", false, false, \"center\", false, \"100\"),\r\n    new Header(\"Nome da Linha\", \"nome\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  itens = [];\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/Linha\");\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new LinhaListaController(this);\r\n    this._controller.carregarItens();\r\n  }\r\n}\r\n</script>"]}]}