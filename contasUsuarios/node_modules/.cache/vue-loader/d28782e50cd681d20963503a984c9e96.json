ataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport { Validation, ValidationSchema } from \"../../baseGenerica/validation\";\r\n\r\nimport {\r\n  ILinhaFormView,\r\n  ILinhaFormController,\r\n} from \"../../contracts/Cadastro/LinhaFormContract\";\r\nimport { LinhaFormController } from \"../../controllers/Cadastro/LinhaFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class LinhaForm extends Vue implements ILinhaFormView {\r\n  _controller?: ILinhaFormController;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Linhas\",\r\n      disabled: false,\r\n      href: \"/linhas\",\r\n    },\r\n  ];\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n\r\n  //ADD CAPACIDADE LINHA\r\n  dialog = false;\r\n  notifications = false;\r\n  sound = true;\r\n  widgets = false;\r\n\r\n  textError = \"\";\r\n\r\n  idLinha = \"\";\r\n\r\n  linha = {\r\n    //dados gerais\r\n    nome: \"\",\r\n    qtdeOperador: 0,\r\n    capacidadeProdutiva: \"00:00\",\r\n    deletado: \"N\",\r\n  };\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  expanded = [];\r\n  singleExpand = false;\r\n\r\n  //CRUD DADOS CAPACIDADE DE LINHA\r\n  headersCadCapacidadeLinha = new Headers([\r\n    new Header(\r\n      \"Opereração\",\r\n      \"nomeOperacao\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      false,\r\n      \"150\"\r\n    ),\r\n    new Header(\r\n      \"Habilidade\",\r\n      \"nomeHabilidade\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      false,\r\n      \"150\"\r\n    ),\r\n    new Header(\r\n      \"Qtde. Operadores\",\r\n      \"qtdeOperador\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      false,\r\n      \"50\"\r\n    ),\r\n    new Header(\"Time\", \"time\", false, false, \"center\", false, \"50\"),\r\n  ]);\r\n\r\n  itensCadCapacidadeLinha: any = [];\r\n\r\n  itensOperacaoes: any = [];\r\n  itensHabilidades: any = [];\r\n  cadCapacidadeLinha = {\r\n    fase: \"\",\r\n    operacao: this.itensOperacaoes,\r\n    habilidades: this.itensHabilidades,\r\n  };\r\n\r\n  time = null;\r\n  menu2 = false;\r\n  modal2 = false;\r\n\r\n  //--------------------------------------\r\n\r\n  headersLinha = [\r\n    {\r\n      text: \"Nome\",\r\n      align: \"start\",\r\n      sortable: false,\r\n      value: \"name\",\r\n    },\r\n\r\n    { text: \"\", align: \"right\", value: \"data-table-expand\" },\r\n  ];\r\n\r\n  itensCapacidadeLinha = [\r\n    {\r\n      name: \"Pré-Forma\",\r\n    },\r\n    {\r\n      name: \"Manual\",\r\n    },\r\n  ];\r\n\r\n  subitensCapacidadeLinha = [\r\n    { id: 1, nome: \"MONIQUE\" },\r\n    { id: 2, nome: \"eranio\" },\r\n  ];\r\n\r\n  headersNovaLinha = new Headers([\r\n    new Header(\r\n      \"Operações\",\r\n      \"descOperacao\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      false,\r\n      \"200\"\r\n    ),\r\n    new Header(\r\n      \"Habilidade\",\r\n      \"descHabilidade\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      false,\r\n      \"200\"\r\n    ),\r\n    new Header(\r\n      \"Qtde de Operadores\",\r\n      \"qtdeOperadores\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      false,\r\n      \"100\"\r\n    ),\r\n    new Header(\"Timer\", \"timer\", false, false, \"center\", false, \"100\"),\r\n  ]);\r\n\r\n  cancelaSnack() {\r\n    this.snackbar = false;\r\n  }\r\n\r\n  cancelaSaveCapacidadeLinha() {\r\n    this.dialog = false;\r\n    this.itensCadCapacidadeLinha = [];\r\n    this.cadCapacidadeLinha = {\r\n      fase: \"\",\r\n      operacao: [],\r\n      habilidades: [],\r\n    };\r\n  }\r\n\r\n  saveCapacidadeLinha() {\r\n    if (!this.validateSaveCapacidadeLinha()) {\r\n      console.log(this.itensCadCapacidadeLinha);\r\n    }\r\n  }\r\n\r\n  addCadHabilidadesLinha() {\r\n    if (!this.validateCapacidadeLinha()) {\r\n      this.itensCadCapacidadeLinha = [];\r\n      let cont = 0;\r\n      this.cadCapacidadeLinha.habilidades.forEach((element: any) => {\r\n        let capacidadeLinha: any = [];\r\n        cont++;\r\n        capacidadeLinha.idOperacao = this.cadCapacidadeLinha.operacao.id;\r\n        capacidadeLinha.nomeOperacao = this.cadCapacidadeLinha.operacao.nome;\r\n        capacidadeLinha.idHabilidade = element.id;\r\n        capacidadeLinha.nomeHabilidade = element.nome;\r\n        capacidadeLinha.qtdeOperador = \"\";\r\n        capacidadeLinha.nomeRef = \"menuRef\" + cont;\r\n        capacidadeLinha.time = null;\r\n        capacidadeLinha.menu = false;\r\n        capacidadeLinha.menu2 = false;\r\n        capacidadeLinha.modal = false;\r\n\r\n        this.itensCadCapacidadeLinha.push(capacidadeLinha);\r\n      });\r\n    }\r\n\r\n    this.cadCapacidadeLinha = {\r\n      fase: \"\",\r\n      operacao: [],\r\n      habilidades: [],\r\n    };\r\n  }\r\n\r\n  carregaProcessos(itemsOperacao: any, itemsHabilidade: any): void {\r\n    this.itensOperacaoes = itemsOperacao;\r\n    this.itensHabilidades = itemsHabilidade;\r\n  }\r\n\r\n  addCapacidadeLinha() {\r\n    this.itensCadCapacidadeLinha = [];\r\n    this.cadCapacidadeLinha = {\r\n      fase: \"\",\r\n      operacao: [],\r\n      habilidades: [],\r\n    };\r\n    this.dialog = true;\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/Linhas\");\r\n    }\r\n  }\r\n\r\n  retornaListaLinhas() {\r\n    this.$router.push(\"/Linhas\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validateSaveCapacidadeLinha() {\r\n    let retorno = false;\r\n\r\n    if (this.itensCadCapacidadeLinha.length == 0){\r\n      \r\n       this.colorBorder = \"error\";\r\n        this.color = \"error\";\r\n        this.text =\r\n          \"Sem Operações e Habilidades selecionadas para salvar dados.\";\r\n        this.snackbar = true;\r\n        retorno = true;\r\n    }\r\n\r\n    this.itensCadCapacidadeLinha.forEach((element: any) => {\r\n      if (\r\n        parseInt(element.qtdeOperador) == 0 ||\r\n        parseInt(element.qtdeOperador) == null ||\r\n        parseInt(element.qtdeOperador) < 0 ||\r\n        element.qtdeOperador == \"\"\r\n      ) {\r\n        this.colorBorder = \"error\";\r\n        this.color = \"error\";\r\n        this.text =\r\n          \"O campo quantidade de operadores da habilidade \" +\r\n          element.nomeHabilidade +\r\n          \" não pode ser 0 (zero) ou inferior a 0 (zero).\";\r\n        this.snackbar = true;\r\n        retorno = true;\r\n      }\r\n\r\n      if (element.time == \"00:00\" || element.time == null) {\r\n        this.colorBorder = \"error\";\r\n        this.color = \"error\";\r\n        this.text =\r\n          \"O campo Time da habilidade \" +\r\n          element.nomeHabilidade +\r\n          \" não pode ser 00:00\";\r\n        this.snackbar = true;\r\n        retorno = true;\r\n      }\r\n    });\r\n    return retorno;\r\n  }\r\n\r\n  validateCapacidadeLinha() {\r\n    let schema: ValidationSchema[] = [];\r\n\r\n    schema.push(new ValidationSchema(\"fase\", \"Fase\", \"string\", true, 30));\r\n\r\n    let result = new Validation().ValidSchema(schema, this.cadCapacidadeLinha);\r\n    if (result.sucesso == false) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = result.mensagem;\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.cadCapacidadeLinha.operacao.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a Operação.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.cadCapacidadeLinha.habilidades.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a/as Habilidade(s)\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  validate() {\r\n    let schema: ValidationSchema[] = [];\r\n\r\n    schema.push(new ValidationSchema(\"nome\", \"Nome\", \"string\", true, 30));\r\n    schema.push(\r\n      new ValidationSchema(\r\n        \"capacidadeOperador\",\r\n        \"Capacidade do Operador\",\r\n        \"string\",\r\n        true,\r\n        15\r\n      )\r\n    );\r\n\r\n    let result = new Validation().ValidSchema(schema, this.linha);\r\n    if (result.sucesso == false) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = result.mensagem;\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  saveLinha() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.salvar(this.linha);\r\n    }\r\n  }\r\n\r\n  updateLinha() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.update(this.idLinha, this.linha);\r\n    }\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.idLinha = this.$route.params.id;\r\n  }\r\n\r\n  showLinhaForEditar(item: any) {\r\n    //CARREGANDO DADOS DO FUNCIONARIO\r\n    this.linha = item;\r\n  }\r\n\r\n  created() {\r\n    this._controller = new LinhaFormController(this);\r\n    this._controller.carregarDtos(this.$route.params.id);\r\n    this.carregarDtos();\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.centered-input >>> input {\r\n  text-align: right;\r\n}\r\n</style>\r\n"]}]}