ACH,OAdL;AAeH;AArLL;;AAAA;AAAA","sourcesContent":["import { ILinhaAprovacoesListaView, ILinhaAprovacoesListaController } from \"../../contracts/Linha/LinhaAprovacoesListaContract\";\r\nimport axios from \"../../plugins/axios\"\r\n\r\nimport { Storage } from \"../../configuration/storage\"\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nexport class LinhaAprovacoesListaController implements ILinhaAprovacoesListaController {\r\n\r\n    _view: ILinhaAprovacoesListaView\r\n\r\n    constructor(view: ILinhaAprovacoesListaView) {\r\n        this._view = view\r\n    }\r\n\r\n    async carregarItens(){\r\n        this._view.loading(true)\r\n        \r\n        let transferencias: any = [];\r\n        let trocas: any = [];\r\n        let emprestimos: any = [];\r\n        let paradas: any = [];\r\n\r\n        await axios\r\n            .get('LinhaSolicitacaoAprovacoes/transferencia')\r\n            .then((response: any) => {\r\n\r\n                // response.data.forEach((element: { acoes: HeaderAction[]; }) => {\r\n                //     element.acoes = [\r\n                //         new HeaderAction(\"mdi-checkbox-marked-circle\", \"primary\", \"Teste\", \"editarItem\", \"200px\"),\r\n                //         new HeaderAction(\"mdi-close-circle\", \"primary\", \"Teste\", \"deleteItem\", \"200px\")\r\n                //     ]\r\n                // });\r\n\r\n                transferencias = response.data;\r\n                //this._view.loading(false)\r\n                //this._view.showItens(response.data)\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n\r\n        await axios\r\n            .get('LinhaSolicitacaoAprovacoes/emprestimo')\r\n            .then((response: any) => {\r\n\r\n                response.data.forEach((element: { acoes: HeaderAction[]; }) => {\r\n                    element.acoes = [\r\n                        new HeaderAction(\"mdi-checkbox-marked-circle\", \"primary\", \"Teste\", \"editarItem\", \"200px\"),\r\n                        new HeaderAction(\"mdi-close-circle\", \"primary\", \"Teste\", \"deleteItem\", \"200px\")\r\n                    ]\r\n                });\r\n\r\n                emprestimos = response.data;\r\n                //this._view.loading(false)\r\n                //this._view.showItens(response.data)\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n\r\n        await axios\r\n            .get('LinhaSolicitacaoAprovacoes/parada')\r\n            .then((response: any) => {\r\n\r\n                response.data.forEach((element: { acoes: HeaderAction[]; }) => {\r\n                    element.acoes = [\r\n                        new HeaderAction(\"mdi-checkbox-marked-circle\", \"primary\", \"Teste\", \"editarItem\", \"200px\"),\r\n                        new HeaderAction(\"mdi-close-circle\", \"primary\", \"Teste\", \"deleteItem\", \"200px\")\r\n                    ]\r\n                });\r\n\r\n                paradas = response.data;\r\n                //this._view.loading(false)\r\n                //this._view.showItens(response.data)\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n\r\n        await axios\r\n            .get('LinhaSolicitacaoAprovacoes/troca')\r\n            .then((response: any) => {\r\n\r\n                response.data.forEach((element: { acoes: HeaderAction[]; }) => {\r\n                    element.acoes = [\r\n                        new HeaderAction(\"mdi-checkbox-marked-circle\", \"primary\", \"Teste\", \"editarItem\", \"200px\"),\r\n                        new HeaderAction(\"mdi-close-circle\", \"primary\", \"Teste\", \"deleteItem\", \"200px\")\r\n                    ]\r\n                });\r\n\r\n                trocas = response.data;\r\n                \r\n                let arrayG: any = {};\r\n\r\n                arrayG = {\r\n                    transferencia: transferencias,\r\n                    troca: trocas,\r\n                    emprestimo: emprestimos,\r\n                    parada: paradas\r\n                }\r\n                this._view.loading(false);\r\n                this._view.showItens(arrayG)\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n    }\r\n\r\n\r\n    aprovarSolicitacaoParadaAprovarReprovar(solicitacaoParadaAprovar: any): void {\r\n        this._view.loading(true)\r\n\r\n        axios\r\n            .post('LinhaSolicitacaoParada/aprovarReprovarParada', solicitacaoParadaAprovar)\r\n            .then((response: any) => {\r\n                this._view.loading(false)\r\n                if (response.data.sucesso) {\r\n                    // this._view.showMsg(\"Salvo com sucesso.\")\r\n                    this._view.showMsg(response.data.message)\r\n                    this.carregarItens()\r\n                } else {\r\n                    this._view.showMsg(response.data.message)\r\n                }\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n    }\r\n\r\n    aprovarSolicitacaoTransferenciaAprovarReprovar(solicitacaoTransferenciaAprovar: any): void {\r\n        this._view.loading(true)\r\n\r\n        axios\r\n            .post('LinhaSolicitacaoTransferencia/aprovarReprorarTransferencia', solicitacaoTransferenciaAprovar)\r\n            .then((response: any) => {\r\n                this._view.loading(false)\r\n                if (response.data.sucesso) {\r\n                    // this._view.showMsg(\"Salvo com sucesso.\")\r\n                    this._view.showMsg(response.data.message)\r\n                    this.carregarItens()\r\n                } else {\r\n                    this._view.showMsg(response.data.message)\r\n                }\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n    }\r\n\r\n    aprovarSolicitacaoTrocaAprovarReprovar(solicitacaoTrocaAprovar: any): void {\r\n        this._view.loading(true)\r\n\r\n        axios\r\n            .post('LinhaSolicitacaoTroca/aprovarReprorarTroca', solicitacaoTrocaAprovar)\r\n            .then((response: any) => {\r\n                this._view.loading(false)\r\n                if (response.data.sucesso) {\r\n                    // this._view.showMsg(\"Salvo com sucesso.\")\r\n                    this._view.showMsg(response.data.message)\r\n                    this.carregarItens()\r\n                } else {\r\n                    this._view.showMsg(response.data.message)\r\n                }\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n    }\r\n\r\n    aprovarSolicitacaoEmprestimoAprovarReprovar(solicitacaoEmprestimoAprovar: any): void {\r\n        this._view.loading(true)\r\n\r\n        axios\r\n            .post('LinhaSolicitacaoEmprestimo/aprovarReprorarEmprestimo', solicitacaoEmprestimoAprovar)\r\n            .then((response: any) => {\r\n                this._view.loading(false)\r\n                if (response.data.sucesso) {\r\n                    // this._view.showMsg(\"Salvo com sucesso.\")\r\n                    this._view.showMsg(response.data.message)\r\n                    this.carregarItens()\r\n                } else {\r\n                    this._view.showMsg(response.data.message)\r\n                }\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n    }\r\n\r\n}"],"sourceRoot":""}]}