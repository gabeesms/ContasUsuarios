"\";\r\n\r\n  dialogDadosOperador = false;\r\n\r\n  search = \"\";\r\n  dataAtual = \"\";\r\n  horaAtual = \"\";\r\n\r\n  dialogDelete = false;\r\n  itemFuncionarioDelete: any;\r\n\r\n  timeAtual: any = \"\";\r\n\r\n  frId = \"\";\r\n  funcionario: any = {};\r\n\r\n  itemsLinha: any = [];\r\n  linhaSelecionada: any = [];\r\n  itemsProduto: any = [];\r\n  produtoSelecionado: any = [];\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"RotaAqui\",\r\n      disabled: false,\r\n      href: \"/rota\",\r\n    },\r\n  ];\r\n\r\n  itemsLinhaProducaoProduto: any = [];\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  idLinhaProducoaEstacao = \"\";\r\n  idLinhaCheckin = 0;\r\n\r\n  changeLinhaProduto() {\r\n    this.itemsProduto = [];\r\n\r\n    this.itemsLinhaProducaoProduto.forEach((element2: any) => {\r\n      if (this.linhaSelecionada.id == element2.idLinhaProducao) {\r\n        this.itemsProduto.push(element2.produto);\r\n      }\r\n    });\r\n  }\r\n\r\n  cancelaSelecaoLinha() {\r\n    Storage.removeLinhaSelecionada(\"estacao_idLinhaProducao\");\r\n\r\n    Storage.removeLinhaSelecionada(\"estacao_nomeLinha\");\r\n    this.$router.push(\"/\");\r\n  }\r\n\r\n  validate() {\r\n    //console.log(this.linhaSelecionada);\r\n    if (this.linhaSelecionada.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a Linha para a abertura de Check-in.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    // if (this.produtoSelecionado.length == 0) {\r\n    //   this.colorBorder = \"error\";\r\n    //   this.color = \"error\";\r\n    //   this.text = \"Selecione o Modelo (Produto) para a abertura de Check-in.\";\r\n    //   this.snackbar = true;\r\n    //   return true;\r\n    // }\r\n\r\n    return false;\r\n  }\r\n\r\n  selecionaLinha() {\r\n    if (this.validate()) {\r\n    } else {\r\n      Storage.removeLinhaSelecionada(\"estacao_idLinhaProducao\");\r\n\r\n      Storage.removeLinhaSelecionada(\"estacao_nomeLinha\");\r\n\r\n      Storage.selecionaLinha(\r\n        \"estacao_idLinhaProducao\",\r\n        this.linhaSelecionada.id\r\n      );\r\n      Storage.selecionaLinha(\"estacao_nomeLinha\", this.linhaSelecionada.nome);\r\n\r\n      this.idLinhaProducoaEstacao = this.linhaSelecionada.id;\r\n\r\n      let dadosLinha = {\r\n        idLinhaProducao: this.linhaSelecionada.id,\r\n        dataHoraAberturaLinha: new Date(),\r\n        //idProdutoLocal: this.itemsProduto.id\r\n      };\r\n\r\n      this._controller!.salvar(dadosLinha);\r\n      this.dialogSelecionaLinha = false;\r\n    }\r\n  }\r\n\r\n  carregaLinhaProducaoProdutos(linhaProducaoProdutos: any): void {\r\n    this.itemsLinhaProducaoProduto = linhaProducaoProdutos;\r\n  }\r\n\r\n  carregaLinha(linhas: any): void {\r\n    this.itemsLinha = linhas;\r\n  }\r\n\r\n  registraCheckin() {\r\n    this.dialogDadosOperador = false;\r\n  }\r\n\r\n  chekinInvalido() {}\r\n\r\n  carregaDateTime() {\r\n    let dateTime = new Date();\r\n    this.dataAtual = \"Dia: \" + moment(dateTime).format(\"DD/MM/YYYY\");\r\n    this.horaAtual =\r\n      \"Hora: \" +\r\n      dateTime.getHours().toString() +\r\n      \":\" +\r\n      dateTime.getMinutes().toString();\r\n  }\r\n\r\n  sair() {\r\n    localStorage.clear();\r\n    this.$router.push(\"/Login\");\r\n  }\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  retornoLinhaCheckin(KRetorno: any): void {\r\n    this.showMsgSelecionaLinha(KRetorno.message);\r\n\r\n    this.idLinhaCheckin = KRetorno.tRetorno.id;\r\n  }\r\n\r\n  showMsgSelecionaLinha(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessageSelecionaLinha = true;\r\n  }\r\n\r\n  showItens(list: any): void {\r\n    this.itens = list;\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    this.setFocus(\"check\");\r\n  }\r\n\r\n  fecharDialogMessageSeecionaLinha() {\r\n    this.dialogMessageSelecionaLinha = false;\r\n    this.setFocus(\"check\");\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  itens = [];\r\n\r\n  validaDadosOperador(operador: any): void {\r\n    //if (!Storage.get(\"estacao_idLinha\")) {\r\n    //console.log(operador.length)\r\n    if (operador == null) {\r\n      this.showMsg(\r\n        \"Operador não Localizado, dirija-se ao R.H. para verificação dos seus dados de Check-in.\"\r\n      );\r\n    }\r\n\r\n    if (operador) {\r\n      if (operador.idLinhaProducao == 0) {\r\n        this.showMsg(\"Operador não está Alocado em nenhuma linha.\");\r\n        return;\r\n      }\r\n\r\n      this.dialogDadosOperador = true;\r\n      this.funcionario = operador;\r\n      this.carregaDateTime();\r\n\r\n      let checkin = false;\r\n\r\n      if (operador.idLinha == this.idLinhaProducoaEstacao) {\r\n        this.cssTitulo = \"#019A53\";\r\n        this.resposta = \"OPERADOR ALOCADO PARA ESTA LINHA\";\r\n        checkin = true;\r\n      } else {\r\n        this.cssTitulo = \"red\";\r\n        this.resposta =\r\n          \"OPERADOR NÃO ALOCADO PARA ESTA LINHA. OPERADOR DEVE REALIZAR CHECK-IN NA LINHA \" +\r\n          operador.nomeLinha;\r\n      }\r\n\r\n      let checkinOperadorLinha = {\r\n        idOrdemProducaoPlano: operador.idOrdemProducaoPlano,\r\n        idFuncionario: operador.idFuncionario,\r\n        dataCheckin: new Date(),\r\n        checkin: checkin,\r\n        idLinhaCheckin: this.idLinhaCheckin,\r\n        mensagem: this.resposta,\r\n        idProdutoLocal: operador.idProdutoLocal\r\n      };\r\n\r\n      this._controller!.salvarChekinOperador(checkinOperadorLinha);\r\n\r\n      //this.frId = \"\"\r\n    }\r\n    //}\r\n  }\r\n\r\n  validaChekingOperador() {\r\n    if (this.dialogDadosOperador == true) {\r\n      this.dialogDadosOperador = false;\r\n      // this.frId = \"\";\r\n      this.setFocus(\"check2\");\r\n    } else {\r\n      this._controller!.validaChekin(this.frId);\r\n      //  this.frId = \"\"\r\n      this.setFocus(\"check2\");\r\n    }\r\n  }\r\n\r\n  clockCheckin() {\r\n    setInterval(() => {\r\n      let time = new Date();\r\n\r\n      let hora = time.getHours().toString();\r\n      let minutos = time.getMinutes().toString();\r\n      let segundos = time.getSeconds().toString();\r\n\r\n      if (parseInt(hora) < 10) {\r\n        hora = \"0\" + time.getHours();\r\n      }\r\n\r\n      if (parseInt(minutos) < 10) {\r\n        minutos = \"0\" + time.getMinutes();\r\n      }\r\n\r\n      if (parseInt(segundos) < 10) {\r\n        segundos = \"0\" + time.getSeconds();\r\n      }\r\n\r\n      this.timeAtual = hora + \":\" + minutos + \":\" + segundos;\r\n\r\n      //this.timeAtual = time.getHours() + \":\" +min+ \":\"+time.getSeconds()\r\n    }, 1000);\r\n  }\r\n\r\n  setFocus(value: string) {\r\n    this.frId = \"\";\r\n    setTimeout(() => {\r\n      document.getElementById(value)!!.focus();\r\n    }, 500);\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new CheckinLinhaController(this);\r\n    this.clockCheckin();\r\n    this._controller.carregarItens();\r\n    this.dialogSelecionaLinha = true;\r\n  }\r\n\r\n  mounted() {\r\n    this.setFocus(\"check\");\r\n  }\r\n\r\n  @Watch(\"dialogDadosOperador\")\r\n  dialogClose(val: string) {\r\n    //setTimeout(() => {\r\n    // if (this.dialogDadosOperador == true) {\r\n    //   this.dialogDadosOperador = false;\r\n    //this.frId = \"\";\r\n    this.setFocus(\"check\");\r\n    //}\r\n    //}, 3500);\r\n  }\r\n}\r\n"],"sourceRoot":""}]}