AA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,mCAAnB;AAEH;AACJ,OAXL,EAYK,KAZL,CAYW,UAAC,CAAD,EAAW;AACd,QAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;;AACA,QAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,CAAnB;AACH,OAfL;AAgBH;AApGL;AAAA;AAAA;AAAA,2HAsG8C,eAtG9C,EAsGuE,oBAtGvE;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAwGc,KAAK,CACV,GADK,CACD,2DAA2D,eAA3D,GAA6E,GAA7E,GAAmF,oBADlF,EAEL,IAFK,CAEA,UAAC,QAAD,EAAkB;AAGpB,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;;AACA,kBAAA,MAAI,CAAC,KAAL,CAAW,gBAAX,CAA4B,QAAQ,CAAC,IAArC;AACH,iBAPK,EAOH,KAPG,CAOG,UAAC,CAAD,EAAW;AAChB,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;;AACA,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,CAAnB;AACH,iBAVK,CAxGd;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,QAqHI;;AArHJ;AAAA;AAAA;AAAA,iHAsHoC,oBAtHpC,EAsHkE,IAtHlE,EAsHgF,eAtHhF;AAAA;;AAAA;AAAA;AAAA;AAAA;AAwHQ,qBAAK,KAAL,CAAW,OAAX,CAAmB,IAAnB;;AAxHR;AAAA,uBA0Hc,KAAK,CACN,GADC,CACG,8CAA8C,oBAA9C,GAAqE,GAArE,GAA2E,IAD9E,EAED,IAFC,CAEI,UAAC,QAAD,EAAkB;AACpB,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;;AACA,sBAAI,QAAQ,CAAC,IAAT,CAAc,OAAlB,EAA2B;AACvB,oBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,QAAQ,CAAC,IAAT,CAAc,OAAjC;;AACA,oBAAA,MAAI,CAAC,aAAL,CAAmB,eAAnB,EAFuB,CAGvB;;AACH,mBAJD,MAIO;AACH,oBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,mCAAnB;AAEH;AACJ,iBAZC,EAaD,KAbC,CAaK,UAAC,CAAD,EAAW;AACd,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;;AACA,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,CAAnB;AACH,iBAhBC,CA1Hd;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA","sourcesContent":["import { IOrdemProducaoListaSimulacaoView, IOrdemProducaoListaSimulacaoController } from \"../../contracts/OrdemProducao/OrdemProducaoListaSimulacaoContract\";\r\nimport axios from \"../../plugins/axios\"\r\n\r\nimport { Storage } from \"../../configuration/storage\"\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nexport class OrdemProducaoListaSimulacaoController implements IOrdemProducaoListaSimulacaoController {\r\n\r\n    _view: IOrdemProducaoListaSimulacaoView\r\n\r\n    constructor(view: IOrdemProducaoListaSimulacaoView) {\r\n        this._view = view\r\n    }\r\n\r\n    async carregarItens(idOrdemProducao: string) {\r\n        this._view.loading(true)\r\n\r\n        await this.carregarLinhas();\r\n\r\n        await axios\r\n            .get('OrdemProducaoPlano/listaOrdemProducaoPlanoSimulacao/' + idOrdemProducao)\r\n            .then((response: any) => {\r\n\r\n                response.data.forEach((element: { acoes: HeaderAction[]; }) => {\r\n                    element.acoes = [\r\n                        new HeaderAction(\"mdi-eye\", \"primary\", \"Teste\", \"editarItem\", \"200px\"),\r\n                        new HeaderAction(\"mdi-check-bold\", \"primary\", \"Teste\", \"deleteItem\", \"200px\")\r\n                    ]\r\n                });\r\n\r\n                this._view.loading(false)\r\n                this._view.showItens(response.data)\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n    }\r\n\r\n\r\n    deleteitem(id: number): void {\r\n        this._view.loading(true)\r\n\r\n        axios.delete('FuncionarioLocal/' + id).then((response: any) => {\r\n            this._view.loading(false)\r\n            if (response.data.sucesso) {\r\n                // this._view.showMsg(\"Salvo com sucesso.\")\r\n                this._view.showMsg(response.data.message)\r\n                //this.carregarItens()\r\n            } else {\r\n                this._view.showMsg(response.data.message)\r\n            }\r\n        }).catch((e: any) => {\r\n            this._view.loading(false)\r\n            this._view.showMsg(e)\r\n        })\r\n    }\r\n\r\n    atualizaBaseOrdemProducaoERP(): void {\r\n        this._view.loading(true)\r\n\r\n        axios\r\n            .get('OrdemProducaoErp/getImpostacaoOrdemProducaoErp')\r\n            .then((response: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(response.data.message)\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n    }\r\n\r\n\r\n    async carregarLinhas() {\r\n        this._view.loading(true)\r\n\r\n        axios\r\n            .get('Linha')\r\n            .then((response: any) => {\r\n                this._view.loading(false)\r\n                this._view.carregaLinha(response.data)\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n    }\r\n\r\n    salvarOrdemProducaoLinha(ordemProducaoLinha: any): void {\r\n\r\n        this._view.loading(true);\r\n\r\n        axios\r\n            .post(\"OrdemProducaoLinha\", ordemProducaoLinha)\r\n            .then((response: any) => {\r\n                this._view.loading(false);\r\n                if (response.data.sucesso) {\r\n                    this._view.showMsg(response.data.message);\r\n                    //this.carregarItens();\r\n                } else {\r\n                    this._view.showMsg(\"Não foi possível salvar os dados.\");\r\n\r\n                }\r\n            })\r\n            .catch((e: any) => {\r\n                this._view.loading(false);\r\n                this._view.showMsg(e);\r\n            });\r\n    }\r\n\r\n    async verificarOrdemProducaoPlanoProducao(idOrdemProducao: string, idOrdemProducaoPlano: string) {\r\n\r\n        await axios\r\n        .get('OrdemProducaoPlano/verificaOrdemProducaoPlanoProducao/' + idOrdemProducao + \"/\" + idOrdemProducaoPlano)\r\n        .then((response: any) => {\r\n\r\n            \r\n            this._view.loading(false)\r\n            this._view.statusOPProducao(response.data)\r\n        }).catch((e: any) => {\r\n            this._view.loading(false)\r\n            this._view.showMsg(e)\r\n        })\r\n    }\r\n\r\n    //TIPO: 1 - PRODUÇÃO / 2 - SIMULAÇÃO\r\n    async salvarStatusOrdemProducao(idOrdemProducaoPlano: string, tipo: string, idOrdemProducao: string) {\r\n\r\n        this._view.loading(true);\r\n\r\n        await axios\r\n            .put(\"OrdemProducaoPlano/ordemProducaoAlocacao/\" + idOrdemProducaoPlano + \"/\" + tipo)\r\n            .then((response: any) => {\r\n                this._view.loading(false);\r\n                if (response.data.sucesso) {\r\n                    this._view.showMsg(response.data.message);\r\n                    this.carregarItens(idOrdemProducao);\r\n                    //this.carregarItens();\r\n                } else {\r\n                    this._view.showMsg(\"Não foi possível salvar os dados.\");\r\n\r\n                }\r\n            })\r\n            .catch((e: any) => {\r\n                this._view.loading(false);\r\n                this._view.showMsg(e);\r\n            });\r\n    }\r\n\r\n}"],"sourceRoot":""}]}