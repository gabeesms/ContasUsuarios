nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  ILinhaControleFormView,\r\n  ILinhaControleFormController,\r\n} from \"../../contracts/Linha/LinhaControleFormContract\";\r\nimport { LinhaControleFormController } from \"../../controllers/Linha/LinhaControleFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class LinhaControleForm\r\n  extends Vue\r\n  implements ILinhaControleFormView {\r\n  _controller?: ILinhaControleFormController;\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  //calendario de datas\r\n\r\n  menu1 = false;\r\n  menu2 = false;\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  idOrdemProducaoPlano = \"\";\r\n  dataCheckin = \"\";\r\n\r\n  dadosLinhaControle: any = [];\r\n  nomeLinha = \"\";\r\n  linhaCapacidadeProdutiva: any = [];\r\n  ordemProducao: any = [];\r\n  ordemProducaoLinha: any = [];\r\n  ordemProducaoPlano: any = [];\r\n  produto: any = [];\r\n  panel: any = [];\r\n\r\n  itensSuporteLinha: any = [];\r\n  headersSuporteLinha = new Headers([\r\n    new Header(\r\n      \"Descrição da Categoria\",\r\n      \"nomeCategoria\",\r\n      false,\r\n      false,\r\n      \"left\",\r\n      false,\r\n      \"200\"\r\n    ),\r\n    new Header(\r\n      \"Operador(es) Responsáveis\",\r\n      \"operadores\",\r\n      false,\r\n      false,\r\n      \"left\",\r\n      false,\r\n      \"200\"\r\n    ),\r\n  ]);\r\n\r\n  showBaseForViewLinhaControle(item: any): void {\r\n    this.dadosLinhaControle = item;\r\n\r\n    this.nomeLinha = this.dadosLinhaControle.linhaProducao.nome;\r\n    this.linhaCapacidadeProdutiva = this.dadosLinhaControle.linha.linhaCapacidade;\r\n    this.ordemProducao = this.dadosLinhaControle.ordemProducao;\r\n    this.ordemProducaoLinha = this.dadosLinhaControle.ordemProducaoLinha;\r\n    this.ordemProducaoPlano = this.dadosLinhaControle.ordemProducaoPlano;\r\n    this.produto = this.dadosLinhaControle.produto;\r\n\r\n    //suporte de linha\r\n    //this.itensSuporteLinha = this.dadosLinhaControle.linhaProducao.linhaProducaoCategorias;\r\n    this.dadosLinhaControle.linhaProducao.linhaProducaoCategorias.forEach(\r\n      (element: any) => {\r\n        if (element.deletado == \"N\") {\r\n          let itemFound: any = this.itensSuporteLinha.filter((t: any) => {\r\n            return t.idProcessoCategoria == element.idProcessoCategoria;\r\n          });\r\n\r\n          if (itemFound.length == 0) {\r\n            itemFound = {\r\n              idProcessoCategoria: element.idProcessoCategoria,\r\n              processoDTO: element.processoDTO,\r\n              funcionarios: [],\r\n            };\r\n            this.itensSuporteLinha.push(itemFound);\r\n          } else {\r\n            itemFound = itemFound[0];\r\n          }\r\n\r\n          element.linhaProducaoCategoriasFuncionarioLocal.forEach((t2: any) => {\r\n            itemFound.funcionarios.push(t2);\r\n          });\r\n        }\r\n      }\r\n    );\r\n\r\n    //console.log(this.itensSuporteLinha);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/funcionarios\");\r\n    }\r\n  }\r\n\r\n  retornaListaLinhaControle() {\r\n    this.$router.push(\"/linhascontrole\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.idOrdemProducaoPlano = this.$route.params.idOrdemProducaoPlano;\r\n    this.dataCheckin =\r\n      this.$route.params.dataCheckin.substring(6, 8) +\r\n      \"/\" +\r\n      this.$route.params.dataCheckin.substring(4, 6);\r\n\r\n    //console.log(this.dataCheckin)\r\n    // this.matricula = this.$route.params.matricula;\r\n  }\r\n\r\n  showLinhaControleForEditar(item: any) {\r\n    //CARREGANDO DADOS DO FUNCIONARIO\r\n    // this.funcionario = item;\r\n  }\r\n\r\n  created() {\r\n    this._controller = new LinhaControleFormController(this);\r\n    this._controller.carregarDtos(\r\n      this.$route.params.idOrdemProducaoPlano,\r\n      this.$route.params.dataCheckin\r\n    );\r\n    this.carregarDtos();\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n"],"sourceRoot":""}]}