his._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaDocumentos(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  carregarSequenciaMatriculaExterno(): void {\r\n    axios\r\n      .get(\"FuncionarioLocal/getmatriculasequencia\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaSequenciaMatriculaExterno(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  carregarSituacaoFuncionario(): void {\r\n    axios\r\n      .get(\"ConsultasBaseERPController/funcionarioerpsituacao\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaSituacaoFuncionario(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  carregarCargoFuncionario(): void {\r\n    axios\r\n      .get(\"ConsultasBaseERPController/funcionarioerpcargo\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaCargoFuncionario(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  carregarSetorFuncionario(): void {\r\n    axios\r\n      .get(\"ConsultasBaseERPController/funcionarioerpsetor\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaSetorFuncionario(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  carregarCategoriaFuncionario(): void {\r\n    axios\r\n      .get(\"ConsultasBaseERPController/funcionarioerptipomaoobra\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaCategoriaFuncionario(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  carregarTurnoFuncionario(): void {\r\n    axios\r\n      .get(\"ConsultasBaseERPController/funcionarioerpturno\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaTurnoFuncionario(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  carregarPastas(): void {\r\n    axios\r\n      .get(\"Pasta\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaPasta(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  carregarPastasDocumentos(itensPasta: any): void {\r\n    axios\r\n      .get(\"Pasta\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaPastaDocumentos(itensPasta);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n\r\n  salvar(funcionario: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"FuncionarioLocal\", funcionario)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Funcionário\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  salvarPasta(pasta: any, itensPasta: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"Pasta\", pasta)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 0);\r\n          this.carregarPastas();\r\n          this.carregarPastasDocumentos(itensPasta)\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados da Pasta\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n\r\n  salvarDocumento(documento: any, idFuncionario: string): void {\r\n\r\n    this._view.loading(true);\r\n    axios\r\n      .post(\"Documento\", documento)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        \r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 0);\r\n          this.carregarDocumentos(idFuncionario);\r\n          \r\n         \r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados da Pasta\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  deleteDocumento(id: string): void {\r\n\r\n    this._view.loading(true);\r\n    axios\r\n      .delete(\"Documento/\" +  id)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        \r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 0);\r\n          \r\n        } else {\r\n          this._view.showMsg(response.data.message, 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n\r\n  update(idFuncionario: string, funcionario: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .put(\"FuncionarioLocal/\" + idFuncionario, funcionario)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n        } else {\r\n          this._view.showMsg(response.data.message, 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n}\r\n"],"sourceRoot":""}]}