sSucesso = false;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  qtdeProduzida = \"\";\r\n  horario = \"\";\r\n\r\n  /*PP - PARADA SOLICITADA\r\n  PNP - PARADA NÃO SOLICITADA*/\r\n  itemsTipoParada = [\r\n    {\r\n      tipo: \"PP\",\r\n      texto: \"PARADA PROGRAMADA\",\r\n    },\r\n    {\r\n      tipo: \"PNP\",\r\n      texto: \"PARADA NÃO PROGRAMADA\",\r\n    },\r\n  ];\r\n\r\n  //ESTRUTURA PARADA\r\n  /*status\r\n    NP - REPROVADO\r\n    AP - APROVADO\r\n    -AC - AGUARDANDO APROVACAO*/\r\n  linhaSolicitacaoParada = {\r\n    idLinha: 0,\r\n    idProduto: 0,\r\n    qtdeProduzida: 0,\r\n    horario: new Date(),\r\n    justificativa: \"\",\r\n    dataCadastro: new Date(),\r\n    status: \"AC\",\r\n    tipo: \"\",\r\n  };\r\n\r\n  itensProdutos: any = [];\r\n  itensLinhasProdutos: any = [];\r\n\r\n  changeLinhaProdutoSelecionado() {\r\n    //console.log(this.linhaSolicitacaoParada.idLinha);\r\n    this.itensProdutos = [];\r\n    this.itensLinhasProdutos.forEach((element: any) => {\r\n      if (element.idLinha == this.linhaSolicitacaoParada.idLinha) {\r\n        this.itensProdutos.push(element);\r\n      }\r\n    });\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/linhascontrole\");\r\n    }\r\n  }\r\n\r\n  retornaListaLinhaControle() {\r\n    this.$router.push(\"/linhascontrole\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    if (\r\n      this.linhaSolicitacaoParada.idLinha == null ||\r\n      this.linhaSolicitacaoParada.idLinha == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a linha.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.linhaSolicitacaoParada.idProduto == null ||\r\n      this.linhaSolicitacaoParada.idProduto == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a Produto.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.linhaSolicitacaoParada.tipo == null ||\r\n      this.linhaSolicitacaoParada.tipo == \"\"\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o tipo de solicitação de parada de linha.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.qtdeProduzida == null ||\r\n      this.qtdeProduzida == \"\" ||\r\n      this.qtdeProduzida.length == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Informe a quantidade produzida.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.horario == \"00:00:00\" ||\r\n      this.horario == null ||\r\n      this.horario == \"\" ||\r\n      this.horario.length == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Informe a hora da parada de linha.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.linhaSolicitacaoParada.justificativa == null ||\r\n      this.linhaSolicitacaoParada.justificativa == \"\" ||\r\n      this.linhaSolicitacaoParada.justificativa.length == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Informe a justificativa da parada de linha.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  saveParadaLinha() {\r\n    if (this.validate()) {\r\n    } else {\r\n      this.linhaSolicitacaoParada.qtdeProduzida = parseInt(this.qtdeProduzida);\r\n      this.linhaSolicitacaoParada.horario = new Date(\r\n        moment(this.horario, \"HH:mm:ss\").toDate().valueOf() -\r\n          moment(this.horario, \"HH:mm:ss\").toDate().getTimezoneOffset() * 60000\r\n      );\r\n\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.salvar(this.linhaSolicitacaoParada);\r\n    }\r\n  }\r\n\r\n  // carregaLinhas(itens: any): void{\r\n  //   this.itensLinhas = itens;\r\n  // }\r\n\r\n  carregaLinhasProdutos(itens: any): void {\r\n    this.itensLinhasProdutos = itens;\r\n  }\r\n\r\n  carregarDtos(): void {}\r\n\r\n  created() {\r\n    this._controller = new LinhaParadaFormController(this);\r\n    this._controller.carregarDtos(this.$route.params.id);\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n"],"sourceRoot":""}]}