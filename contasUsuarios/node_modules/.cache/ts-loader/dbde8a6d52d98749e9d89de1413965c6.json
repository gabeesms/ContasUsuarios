this.dateFormatted2 = \"\",\r\n    this.turnos = \"\";\r\n\r\n  }\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Espelhos de Ponto\",\r\n      disabled: false,\r\n      href: \"/espelhosPonto\",\r\n    },\r\n  ];\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n  showItens(list: any): void {\r\n    this.itens = list;\r\n\r\n    this.dialogEspelhoPonto = false;\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemFuncionarioDelete = item;\r\n    this.dialogDelete = true;\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    //this._controller!.deleteitem(this.itemFuncionarioDelete.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    this.$router.push(\r\n      \"/espelhoponto/\" +\r\n        item.matricula +\r\n        \"/\" +\r\n        this.dtInicio +\r\n        \"/\" +\r\n        this.dtFinal\r\n    );\r\n  }\r\n\r\n  modalEspelhoPonto() {\r\n    this.limparCampos();\r\n    this.dialogEspelhoPonto = true;\r\n  }\r\n\r\n  cancelarBuscarEspelhoPonto() {\r\n    this.limparCampos();\r\n    this.dialogEspelhoPonto = false;\r\n  }\r\n\r\n  validateBuscaEspelhoPonto() {\r\n    if (\r\n      this.turnos.length == 0 ||\r\n      this.turnos == null ||\r\n      this.turnos == undefined\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o turno para a buscar o espelho de ponto.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.dateFormatted == \"\" ||\r\n      this.dateFormatted == null ||\r\n      this.dateFormatted == undefined\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text =\r\n        \"Informe a data inicial para buscar o período do espelho de ponto.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.dateFormatted2 == \"\" ||\r\n      this.dateFormatted2 == null ||\r\n      this.dateFormatted2 == undefined\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text =\r\n        \"Informe a data final para buscar o período do espelho de ponto.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n  }\r\n\r\n  buscarEspelhoPonto() {\r\n    if (!this.validateBuscaEspelhoPonto()) {\r\n      this.dtInicio = this.dateFormatted;\r\n      this.dtInicio = this.dtInicio.toString().split(\"/\").reverse().join(\"\");\r\n\r\n      this.dtFinal = this.dateFormatted2;\r\n      this.dtFinal = this.dtFinal.toString().split(\"/\").reverse().join(\"\");\r\n\r\n      this._controller!.buscarEspelhoPonto(\r\n        this.dtInicio,\r\n        this.dtFinal,\r\n        this.turnos\r\n      );\r\n    }\r\n  }\r\n\r\n  carregaTurnos(itemsTurno: any): void {\r\n    this.itensTurno = itemsTurno;\r\n\r\n    this.itensTurno.forEach((element: any) => {\r\n      element.textoDescHora =\r\n        element.descricao + \" - HORAS TOTAL: \" + element.horaTotal;\r\n    });\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  headers = new Headers([\r\n    new Header(\"Status\", \"\", false, false, \"center\", true, \"100\"),\r\n    new Header(\"Matrícula\", \"matricula\", false, false, \"left\", true, \"100\"),\r\n    new Header(\"Nome\", \"nome\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Tipo Funcionário\", \"tipo\", false, false, \"center\", true, \"150\"),\r\n    new Header(\"Turno\", \"turno\", false, false, \"left\", true, \"300\"),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  \r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/perfil\");\r\n  }\r\n\r\n  formatDate(date: string) {\r\n    if (!date) return null;\r\n\r\n    const [year, month, day] = date.split(\"-\");\r\n    return `${day}/${month}/${year}`;\r\n  }\r\n\r\n  parseDate(date: string) {\r\n    if (!date) return null;\r\n    const [day, month, year] = date.split(\"/\");\r\n    return `${year}-${month.padStart(2, \"0\")}-${day.padStart(2, \"0\")}`;\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new EspelhoPontoListaController(this);\r\n    this._controller.carregarItens();\r\n  }\r\n\r\n  @Watch(\"date\")\r\n  onDatasChanged(val: string) {\r\n    this.dateFormatted = this.formatDate(this.date);\r\n  }\r\n\r\n  @Watch(\"date2\")\r\n  onDatas2Changed(val: string) {\r\n    this.dateFormatted2 = this.formatDate(this.date2);\r\n  }\r\n}\r\n"],"sourceRoot":""}]}