 \"error\";\r\n      this.text =\r\n        this.tipo == \"O\"\r\n          ? \"Informe o código da Operação.\"\r\n          : \"Informe o código da Habilidade.\";\r\n      this.snackbar = true;\r\n      return;\r\n    }\r\n\r\n    if (\r\n      this.descricaoProcesso == \"\" ||\r\n      this.descricaoProcesso.length == 0 ||\r\n      this.descricaoProcesso == null\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text =\r\n        this.tipo == \"O\"\r\n          ? \"Informe a descrição da Operação.\"\r\n          : \"Informe a descrição da Habilidade.\";\r\n      this.snackbar = true;\r\n      return;\r\n    }\r\n\r\n    let processo = {\r\n      codigo: this.codigoProcesso,\r\n      nome: this.descricaoProcesso,\r\n      status: \"A\", //STATUS: A: ATIVO / I: INATIVO\r\n      dataCadastro: new Date(),\r\n      tipo: this.tipo,\r\n    };\r\n\r\n    this._controller!.salvarProcesso(processo);\r\n  }\r\n\r\n  deleteItemCapacidade(item: any) {\r\n    this.itemDelete = item;\r\n    this.dialogDelete = true;\r\n  }\r\n\r\n  deleteCapacidadeProdutoConfirmado() {\r\n    this.dialogDelete = false;\r\n    this._controller!.deleteProdutoCapacidadeHabilidadeItem(\r\n      this.itemDelete.id,\r\n      parseInt(this.idProduto)\r\n    );\r\n    this.itemDelete = [];\r\n    this.somaQtdeOperadores();\r\n  }\r\n\r\n  deleteCapacidadeProdutoCancelar() {\r\n    this.dialogDelete = false;\r\n    this.itemDelete = [];\r\n  }\r\n\r\n  cancelaSaveCapacidadeProduto() {\r\n    //CAPACIDADE LINHA\r\n    this.dialog = false;\r\n    this.itensCadCapacidadeProduto = [];\r\n    this.cadCapacidadeProduto = {\r\n      fase: \"\",\r\n      operacao: [],\r\n      habilidades: [],\r\n    };\r\n  }\r\n\r\n  validateCapacidadeProduto() {\r\n    let schema: ValidationSchema[] = [];\r\n\r\n    schema.push(new ValidationSchema(\"fase\", \"Fase\", \"string\", true, 30));\r\n\r\n    let result = new Validation().ValidSchema(\r\n      schema,\r\n      this.cadCapacidadeProduto\r\n    );\r\n    if (result.sucesso == false) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = result.mensagem;\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.cadCapacidadeProduto.operacao.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a Operação.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.cadCapacidadeProduto.habilidades.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a/as Habilidade(s)\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  carregaCapacidadeProduto(itemsCapacidadeProduto: any): void {\r\n    this.itensCapacidadeProduto = itemsCapacidadeProduto;\r\n    this.somaQtdeOperadores();\r\n  }\r\n\r\n  saveCapacidadeProduto() {\r\n    if (!this.validateSaveCapacidadeProduto()) {\r\n      let capacidadeProduto: any = {\r\n        fase: this.cadCapacidadeProduto.fase,\r\n        idOperacao: this.cadCapacidadeProduto.operacao.id,\r\n        dataCadastro: new Date(),\r\n        deletado: \"N\",\r\n        itens: this.itensCadCapacidadeProduto,\r\n        idProduto: parseInt(this.idProduto),\r\n      };\r\n\r\n      this._controller!.salvarCapacidadeProduto(\r\n        capacidadeProduto,\r\n        this.idProduto\r\n      );\r\n      this.somaQtdeOperadores();\r\n      this.dialog = false;\r\n    }\r\n  }\r\n\r\n  addCadHabilidadesProduto() {\r\n    if (!this.validateCapacidadeProduto()) {\r\n      this.itensCadCapacidadeProduto = [];\r\n      let cont = 0;\r\n      this.cadCapacidadeProduto.habilidades.forEach((element: any) => {\r\n        let capacidadeLinha: any = {};\r\n        cont++;\r\n        capacidadeLinha.idOperacao = this.cadCapacidadeProduto.operacao.id;\r\n        capacidadeLinha.nomeOperacao = this.cadCapacidadeProduto.operacao.nome;\r\n        capacidadeLinha.idHabilidade = element.id;\r\n        capacidadeLinha.nomeHabilidade = element.nome;\r\n        capacidadeLinha.qtdeOperador = null;\r\n        capacidadeLinha.nomeRef = \"menuRef\" + cont;\r\n        capacidadeLinha.menu = false;\r\n        capacidadeLinha.menu2 = false;\r\n        capacidadeLinha.modal = false;\r\n\r\n        this.itensCadCapacidadeProduto.push(capacidadeLinha);\r\n      });\r\n    }\r\n\r\n    this.cadCapacidadeProduto = {\r\n      fase: this.cadCapacidadeProduto.fase,\r\n      operacao: this.cadCapacidadeProduto.operacao,\r\n      habilidades: [],\r\n    };\r\n  }\r\n\r\n  addCapacidadeProduto() {\r\n    this.itensCadCapacidadeProduto = [];\r\n    this.cadCapacidadeProduto = {\r\n      fase: \"\",\r\n      operacao: [],\r\n      habilidades: [],\r\n    };\r\n    this.dialog = true;\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/produtos\");\r\n    }\r\n  }\r\n\r\n  retornaListaProdutoes() {\r\n    this.$router.push(\"/produtos\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validateSaveCapacidadeProduto() {\r\n    let retorno = false;\r\n\r\n    if (this.itensCadCapacidadeProduto.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Sem Operações e Habilidades selecionadas para salvar dados.\";\r\n      this.snackbar = true;\r\n      retorno = true;\r\n    }\r\n\r\n    this.itensCadCapacidadeProduto.forEach((element: any) => {\r\n      if (\r\n        parseInt(element.qtdeOperador) == 0 ||\r\n        parseInt(element.qtdeOperador) == null ||\r\n        parseInt(element.qtdeOperador) < 0 ||\r\n        element.qtdeOperador == \"\"\r\n      ) {\r\n        this.colorBorder = \"error\";\r\n        this.color = \"error\";\r\n        this.text =\r\n          \"O campo quantidade de operadores da habilidade \" +\r\n          element.nomeHabilidade +\r\n          \" não pode ser 0 (zero) ou inferior a 0 (zero).\";\r\n        this.snackbar = true;\r\n        retorno = true;\r\n      }\r\n    });\r\n    return retorno;\r\n  }\r\n\r\n  somaQtdeOperadores() {\r\n    this.somaOperadores = 0;\r\n\r\n    this.itensCapacidadeProduto.forEach((element: any) => {\r\n      element.produtoCapacidadeHabilidades.forEach((element2: any) => {\r\n        this.somaOperadores =\r\n          this.somaOperadores + parseInt(element2.qtdeOperadores);\r\n      });\r\n    });\r\n\r\n    this.produto.qtdeOperadorSimulado = this.somaOperadores;\r\n\r\n  }\r\n\r\n  calcularQtdOperadores(){\r\n\r\n    let horasSegundos = 3600;\r\n\r\n    let tempoProducaoPecas = this.produto.producaoHoraHora == 0 ? 0 : horasSegundos/this.produto.producaoHoraHora; \r\n\r\n    this.produto.qtdeOperador = this.produto.producaoHoraHora == 0 ? 0 : Math.ceil(this.produto.producaoHoraHora / tempoProducaoPecas);\r\n    \r\n  }\r\n\r\n  validate() {\r\n    // if (\r\n    //   this.produto.capacidadeHomemHr == 0 ||\r\n    //   this.produto.capacidadeHomemHr == null\r\n    // ) {\r\n    //   this.colorBorder = \"error\";\r\n    //   this.color = \"error\";\r\n    //   this.text =\r\n    //     \"O campo Capacidade Homem Hora do Produto está vazio ou inválido.\";\r\n    //   this.snackbar = true;\r\n    //   return true;\r\n    // }\r\n\r\n    // if (\r\n    //   this.produto.capacidadeProdutiva == 0 ||\r\n    //   this.produto.capacidadeProdutiva == null\r\n    // ) {\r\n    //   this.colorBorder = \"error\";\r\n    //   this.color = \"error\";\r\n    //   this.text =\r\n    //     \"O campo Fases do Processo do Produto está vazio ou inválido.\";\r\n    //   this.snackbar = true;\r\n    //   return true;\r\n    // }\r\n\r\n    return false;\r\n  }\r\n\r\n  saveProduto() {\r\n    if (this.validate()) {\r\n    } else {\r\n      this.produto.qtdeOperador = parseInt(\r\n        this.produto.qtdeOperador.toString()\r\n      );\r\n\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.salvar(this.produto);\r\n    }\r\n  }\r\n\r\n  updateProduto() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this.produto.qtdeOperador = parseInt(\r\n        this.produto.qtdeOperador.toString()\r\n      );\r\n      this.produto.producaoHoraHora = parseInt(\r\n        this.produto.producaoHoraHora.toString()\r\n      );\r\n\r\n      this._controller!.update(this.idProduto, this.produto);\r\n    }\r\n  }\r\n\r\n  carregaProcessos(itemsOperacao: any, itemsHabilidade: any): void {\r\n    this.itensOperacaoes = itemsOperacao;\r\n    this.itensHabilidades = itemsHabilidade;\r\n\r\n    this.dialogAddProcesso = false;\r\n    this.tipo = \"\";\r\n    this.titulo = \"\";\r\n    this.placeholderProcesso = \"\";\r\n    this.codigoProcesso = \"\";\r\n    this.descricaoProcesso = \"\";\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.idProduto = this.$route.params.id;\r\n  }\r\n\r\n  showProdutoForEditar(item: any) {\r\n    //CARREGANDO DADOS DO FUNCIONARIO\r\n    this.produto = item;\r\n    this.somaQtdeOperadores();\r\n  }\r\n\r\n  created() {\r\n    this._controller = new ProdutoFormController(this);\r\n    this._controller.carregarDtos(this.$route.params.id);\r\n    this.carregarDtos();\r\n    // this.somaQtdeOperadores();\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n"],"sourceRoot":""}]}