funcionario\",\r\n    },\r\n  ];\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n  showItens(list: any): void {\r\n    this.itens = list;\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemFuncionarioDelete = item;\r\n    this.dialogDelete = true;\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    this._controller!.deleteitem(this.itemFuncionarioDelete.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    this.$router.push(\"/funcionario/\" + item.id + \"/\" + item.matricula);\r\n  }\r\n\r\n  erpFuncionariosCarregar() {\r\n    this.loading(true);\r\n    this.itens = [];\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new FuncionarioListaController(this);\r\n    this._controller.atualizaBaseFuncionariosERP();\r\n    this._controller.carregarItens();\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  headers = new Headers([\r\n    new Header(\"Tipo\", \"tipo\", false, false, \"center\", true, \"100\"),\r\n    new Header(\"Matrícula\", \"matricula\", false, false, \"center\", true, \"100\"),\r\n    new Header(\"Nome\", \"nome\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Cargo\", \"cargo\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Setor\", \"setor\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Situação\", \"situacao\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  itens = [];\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/funcionario\");\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new FuncionarioListaController(this);\r\n\r\n    this._controller.carregarItens();\r\n  }\r\n}\r\n"],"sourceRoot":""}]}