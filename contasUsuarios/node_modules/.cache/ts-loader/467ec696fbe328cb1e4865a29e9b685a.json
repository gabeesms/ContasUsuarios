                ]\r\n                });\r\n\r\n                this._view.loading(false)\r\n                this._view.showItens(response.data)\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n    }\r\n\r\n    carregarItemEditar(idTurno: any) {\r\n\r\n        this._view.loading(true);\r\n        //CARREGA EDITAR\r\n        axios\r\n          .get(\"turno/\" + idTurno)\r\n          .then((response: any) => {\r\n            this._view.loading(false);\r\n            if (response.data) {\r\n              \r\n              this._view.carregaDadosTurnoEditar(response.data);\r\n            }\r\n          })\r\n          .catch((e: any) => {\r\n            console.log(e);\r\n            this._view.loading(false);\r\n            this._view.showMsg(e);\r\n          });\r\n    \r\n      }\r\n\r\n\r\n    deleteitem(id: number): void {\r\n        this._view.loading(true)\r\n\r\n        axios.delete('Turno/' + id).then((response: any) => {\r\n            this._view.loading(false)\r\n            if (response.data.sucesso) {\r\n                // this._view.showMsg(\"Salvo com sucesso.\")\r\n                this._view.showMsg(response.data.message)\r\n                this.carregarItens()\r\n            } else {\r\n                this._view.showMsg(response.data.message)\r\n            }\r\n        }).catch((e: any) => {\r\n            this._view.loading(false)\r\n            this._view.showMsg(e)\r\n        })\r\n    }\r\n\r\n    ativaritem(id: number): void {\r\n        this._view.loading(true)\r\n\r\n        axios.put('Turno/ativarTurno/' + id).then((response: any) => {\r\n            this._view.loading(false)\r\n            if (response.data.sucesso) {\r\n                // this._view.showMsg(\"Salvo com sucesso.\")\r\n                this._view.showMsg(response.data.message)\r\n                this.carregarItens()\r\n            } else {\r\n                this._view.showMsg(response.data.message)\r\n            }\r\n        }).catch((e: any) => {\r\n            this._view.loading(false)\r\n            this._view.showMsg(e)\r\n        })\r\n    }\r\n\r\n    salvar(turno: any): void {\r\n\r\n        this._view.loading(true);\r\n\r\n        axios\r\n            .post(\"Turno\", turno)\r\n            .then((response: any) => {\r\n                this._view.loading(false);\r\n                if (response.data.sucesso) {\r\n                    this._view.showMsg(response.data.message);\r\n                    this._view.limparCampos();\r\n                    this.carregarItens();\r\n\r\n                } else {\r\n                    this._view.showMsg(\"Não foi possível salvar os dados do Turno\");\r\n                }\r\n            })\r\n            .catch((e: any) => {\r\n                this._view.loading(false);\r\n                //this._view.showMsg(e, 0);\r\n            });\r\n\r\n    }\r\n\r\n    update(idTurno: string, turno: any): void {\r\n\r\n        this._view.loading(true);\r\n    \r\n        axios\r\n          .put(\"Turno/\" + idTurno, turno)\r\n          .then((response: any) => {\r\n            this._view.loading(false);\r\n            if (response.data.sucesso) {\r\n              this._view.showMsg(response.data.message);\r\n            } else {\r\n              this._view.showMsg(response.data.message);\r\n            }\r\n          })\r\n          .catch((e: any) => {\r\n            this._view.loading(false);\r\n            this._view.showMsg(e);\r\n          });\r\n    \r\n      }\r\n\r\n\r\n}"],"sourceRoot":""}]}