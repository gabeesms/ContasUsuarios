rovacaoFormView) {\r\n    this._view = view;\r\n  }\r\n\r\n  carregarDtos(id: string): void {\r\n\r\n    this.carregarFluxoAprovacaoTipo();\r\n    this.carregarDepartamento();\r\n    this.carregarCentroCusto();\r\n    this.carregarFuncionariosPorCentroCusto();\r\n    this._view.loading(true);\r\n\r\n\r\n    if (id != undefined) {\r\n      //CARREGA EDITAR\r\n      axios\r\n        .get(\"FluxoAprovacao/\" + id)\r\n        .then((response: any) => {\r\n          this._view.loading(false);\r\n          if (response.data) {\r\n\r\n            this._view.showFluxoAprovacaoForView(response.data);\r\n          }\r\n        })\r\n        .catch((e: any) => {\r\n          console.log(e);\r\n          this._view.loading(false);\r\n          this._view.showMsg(e, 0);\r\n        });\r\n    }\r\n  }\r\n\r\n\r\n  salvar(funcionario: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"FluxoAprovacao\", funcionario)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Funcionário\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  carregarFluxoAprovacaoTipo(): void {\r\n    axios\r\n      .get(\"FluxoAprovacaoTipo\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaFluxoAprovacaoTipo(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  carregarCentroCusto(): void {\r\n    axios\r\n      .get(\"/ConsultasBaseERPController/centrocusto\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaCentroCusto(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  carregarDepartamento(): void {\r\n    axios\r\n      .get(\"/ConsultasBaseERPController/funcionarioerpdepartamento\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaDepartamento(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n\r\n\r\n  carregarFuncionariosPorCentroCusto(): void {\r\n    axios\r\n      .get(\"/FluxoAprovacaoTipo/funcionariosporcentrocusto\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaFuncionariosPorCentroCusto(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  consultarVersaoFluxoAprovacao(id: string): void {\r\n    axios\r\n      .get(\"/FluxoAprovacao/consultaversaofluxo/\" + id)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.dadosFluxoAprovacao(response.data);\r\n        } \r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n}\r\n"],"sourceRoot":""}]}