KDataList from \"../../components/KDataList.vue\";\n\nimport { Display } from \"../../configuration/display\";\n\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\nimport { Headers } from \"../../models/headers\";\n\nimport {\n  ISaidaAntecipadaListaView,\n  ISaidaAntecipadaListaController,\n} from \"../../contracts/SaidaAntecipada/SaidaAntecipadaListaContract\";\n\nimport { SaidaAntecipadaListaController } from \"../../controllers/SaidaAntecipada/SaidaAntecipadaListaController\";\n\nimport Component from \"vue-class-component\";\n\n@Component({\n  components: {\n    KLoading,\n    KDialog,\n    KDataList,\n  },\n})\nexport default class SaidaAntecipadaLista\n  extends Vue\n  implements ISaidaAntecipadaListaView {\n  _controller?: ISaidaAntecipadaListaController;\n\n  dialogLoading = false;\n  dialogMessage = false;\n  textError = \"\";\n\n  //SNACKBAR\n  snackbar = false;\n  text = \"\";\n  vertical = true;\n  color = \"\";\n  colorBorder = \"\";\n\n  //validate form\n  valid = true;\n  lazy = false;\n\n  isSucesso = false;\n\n  search = \"\";\n\n  habilitaCampos = false;\n\n  dialogDelete = false;\n  itemFuncionarioDelete: any;\n\n  itemsFuncionario: any = [];\n\n  dialogSolicitacaoSaida = false;\n\n  matricula: any = {\n    matricula: \"\",\n    nome: \"\",\n    id: 0\n  };\n\n  saidaAntecipada = {\n    tipo: \"\",\n    motivo: \"\",\n  };\n\n  caminho = [\n    {\n      text: \"Home\",\n      disabled: false,\n      href: \"/\",\n    },\n    {\n      text: \"Saída Antecipada\",\n      disabled: false,\n      href: \"/saidaantecipada\",\n    },\n  ];\n\n  /*CR - COM RETORNO\n  SR - SEM RETORNO*/\n  itemsTipoSaidaAntecipada = [\n    \"COM RETORNO\",\"SEM RETORNO\"\n  ];\n\n  loading(visible: boolean): void {\n    this.dialogLoading = visible;\n  }\n\n  showMsg(msg: string, modo: number): void {\n    this.textError = msg;\n    if (modo == 1) {\n      this.isSucesso = true;\n    }\n    this.dialogMessage = true;\n  }\n  showItens(list: any): void {\n    this.itens = list;\n  }\n\n  fecharDialogMessage() {\n    this.dialogMessage = false;\n  }\n\n  deleteItem(item: any) {\n    this.itemFuncionarioDelete = item;\n    this.dialogDelete = true;\n    //this._controller!.deleteitem(item.id);\n  }\n\n  deleteItemConfirmado() {\n    this.dialogDelete = false;\n    //this._controller!.deleteitem(this.itemFuncionarioDelete.id);\n  }\n\n  editarItem(item: any) {\n    //this.$router.push(\"/funcionario/\" + item.id + \"/\" + item.matricula);\n  }\n\n  // erpFuncionariosCarregar() {\n  //   this.loading(true);\n  //   this.itens = [];\n  //   this.isMobile = Display.isMobile();\n  //   this._controller = new SaidaAntecipadaListaController(this);\n\n  //   //this._controller.carregarItens();\n  // }\n\n  isMobile = false;\n\n  headers = [\n    {\n      text: \"Centro de Custo\",\n      align: \"left\",\n      sortable: false,\n      width: 250,\n      value: \"setor\",\n    },\n    {\n      text: \"Matricula\",\n      align: \"center\",\n      sortable: false,\n      width: 150,\n      value: \"matricula\",\n    },\n    {\n      text: \"Nome\",\n      align: \"left\",\n      sortable: false,\n      width: 300,\n      value: \"nome\",\n    },\n    {\n      text: \"Turno\",\n      align: \"center\",\n      sortable: false,\n      width: 350,\n      value: \"turno\",\n    },\n    {\n      text: \"Tipo\",\n      align: \"center\",\n      sortable: false,\n      width: 150,\n      value: \"tipo\",\n    },\n    {\n      text: \"Status\",\n      align: \"center\",\n      sortable: false,\n      width: 150,\n      value: \"status\",\n    },\n\n    {\n      text: \"Ações\",\n      align: \"center\",\n      sortable: false,\n      width: 100,\n      value: \"acao\",\n    },\n  ];\n\n  // headers = new Headers([\n  //   new Header(\"Centro de Custo\", \"setor\", false, false, \"left\", true, \"250\"),\n  //   new Header(\"Matrícula\", \"matricula\", false, false, \"center\", true, \"150\"),\n  //   new Header(\"Nome\", \"nome\", false, false, \"left\", true, \"300\"),\n  //   new Header(\"Turno\", \"turno\", false, false, \"left\", true, \"350\"),\n  //   new Header(\"Tipo\", \"tipo\", false, false, \"center\", true, \"150\"),\n  //   new Header(\"Status\", \"status\", false, false, \"center\", true, \"150\"),\n  //   new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\n  // ]);\n\n  itens = [];\n\n  addSolicitacao() {\n    this.$router.push(\"/perfil\");\n  }\n\n  limparCampos(): void {\n    this.matricula.matricula = \"\";\n    this.matricula.nome = \"\";\n    this.saidaAntecipada.tipo = \"\";\n    this.saidaAntecipada.motivo = \"\";\n    this.matricula.id = 0;\n  }\n\n  visualizarSaidaAntecipada(item: any) {\n    this.habilitaCampos = true;\n\n    this.matricula.id = item.id;\n    this.matricula.matricula = item.matricula;\n    this.matricula.nome = item.nome\n    this.saidaAntecipada.tipo = item.tipo;\n    this.saidaAntecipada.motivo = item.motivo;\n\n    this.dialogSolicitacaoSaida = true;\n  }\n\n  cancelarSaidaAntecipada() {\n    \n    this.dialogSolicitacaoSaida = false;\n    this.limparCampos();\n  }\n\n  modalSolicitacaoSaida() {\n    this.habilitaCampos = false;\n    this.limparCampos();\n    this.dialogSolicitacaoSaida = true;\n  }\n\n  validate() {\n    if (\n      this.matricula.matricula.length == 0 ||\n      this.matricula.matricula == null ||\n      this.matricula.matricula == undefined ||\n      this.matricula.matricula == \"\"\n    ) {\n      this.colorBorder = \"error\";\n      this.color = \"error\";\n      this.text = \"Informe a matrícula.\";\n      this.snackbar = true;\n      return true;\n    }\n\n    if (this.saidaAntecipada.tipo == null || this.saidaAntecipada.tipo == \"\") {\n      this.colorBorder = \"error\";\n      this.color = \"error\";\n      this.text = \"Informe o tipo de saída antecipada.\";\n      this.snackbar = true;\n      return true;\n    }\n\n    if (\n      this.saidaAntecipada.motivo == null ||\n      this.saidaAntecipada.motivo == \"\" ||\n      this.saidaAntecipada.motivo.length == 0\n    ) {\n      this.colorBorder = \"error\";\n      this.color = \"error\";\n      this.text = \"Informe o motivo da saída antecipada.\";\n      this.snackbar = true;\n      return true;\n    }\n    return false;\n  }\n\n  saveSaidaAntecipada() {\n    if (this.validate()) {\n    } else {\n      let solicitacaoAntecipada = {\n        idFuncionario: this.matricula.matricula.id,\n        tipo: this.saidaAntecipada.tipo == 'COM RETORNO' ? 'CR' : 'SR',\n        motivo: this.saidaAntecipada.motivo,\n        status: \"AC\",\n        dataCadastro: new Date(),\n      };\n\n      // console.log(solicitacaoAntecipada);\n      // return;\n\n      this._controller!.salvar(solicitacaoAntecipada);\n\n      this.dialogSolicitacaoSaida = false;\n    }\n  }\n\n  carregaFuncionario(funcionario: any): void {\n    this.itemsFuncionario = funcionario;\n  }\n\n  changeNome(item: any){\n    this.matricula.nome = \"\",\n    this.matricula.nome = item.matricula.nome\n  }\n\n  created() {\n    this.isMobile = Display.isMobile();\n    this._controller = new SaidaAntecipadaListaController(this);\n\n    this._controller.carregarItens();\n  }\n}\n"],"sourceRoot":""}]}