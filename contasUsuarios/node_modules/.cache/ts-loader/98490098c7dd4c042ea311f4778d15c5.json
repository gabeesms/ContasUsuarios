taAlvo = element.date2;\r\n        }\r\n\r\n        if (element.habilitado == false){\r\n          element.idNivel = this.idOperadorNivelNaoHabilidtado;\r\n        }else{\r\n          element.idNivel = element.nivel.id;\r\n        }\r\n\r\n        // if (element.dataAtual != null || element.dataAlvo != null) {\r\n        //   element.dataAtual = element.date;\r\n        //   element.dataAlvo = element.date2;\r\n        //   element.idNivel = element.nivel.id;\r\n        // }\r\n        \r\n        // if (element.dataAtual == null || element.dataAlvo == null) {\r\n        //   element.dataAtual = null;\r\n        //   element.dataAlvo = null;\r\n\r\n        //   if (element.nivel == \"NÃO HABILITADO\"){\r\n        //     //não habilitado\r\n        //     console.log(\"AQUIOII\")\r\n        //     element.idNivel = this.idOperadorNivelNaoHabilidtado;\r\n        //   }else{\r\n        //     element.idNivel = element.nivel.id;\r\n        //   }\r\n\r\n        //   element.idNivel = element.nivel.id;\r\n        // }\r\n\r\n        element.nivel = null;\r\n      });\r\n\r\n      this._controller!.salvar(this.operador);\r\n      //console.log(this.operador.processos);\r\n    }\r\n  }\r\n\r\n  updateOperador() {\r\n    if (this.validate()) {\r\n    } else {\r\n      this.operador.idFuncionario = parseInt(this.funcionario.id);\r\n\r\n      this.operador.processos.forEach((element: any) => {\r\n        if (element.dataAtual != null || element.dataAlvo != null) {\r\n          (element.dataAtual = element.date),\r\n            (element.dataAlvo = element.date2);\r\n          //element.idNivel = element.nivel.id\r\n        } else {\r\n          //não habilitado\r\n          element.idNivel = this.idOperadorNivelNaoHabilidtado;\r\n          element.dataAtual = null;\r\n          element.dataAlvo = null;\r\n        }\r\n\r\n        if (element.habilitado == false) {\r\n          //não habilitado\r\n          element.idNivel = this.idOperadorNivelNaoHabilidtado;\r\n          element.dataAtual = null;\r\n          element.dataAlvo = null;\r\n        }\r\n\r\n        //verifica se o idNivel existe\r\n        if (typeof element.idNivel == \"undefined\") {\r\n          if (element.nivel == \"NÃO HABILITADO\") {\r\n            element.idNivel = this.idOperadorNivelNaoHabilidtado;\r\n          } else {\r\n            element.idNivel = element.nivel.id;\r\n          }\r\n        }\r\n\r\n        //verifica se mudou o Nivel\r\n        if (typeof element.nivel == \"object\") {\r\n          element.idNivel = element.nivel.id;\r\n        }\r\n\r\n        element.nivel = null;\r\n      });\r\n\r\n      // console.log(this.operador.processos)\r\n      // return;\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.update(this.idOperador, this.operador);\r\n    }\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.idOperador = this.$route.params.id;\r\n    this.funcionario.id = this.$route.params.idFuncionario;\r\n  }\r\n\r\n  carregaDadosFuncionario(item: any) {\r\n    this.funcionario.matricula = item.matricula;\r\n    this.funcionario.nome = item.nome;\r\n    this.funcionario.centroCusto = item.setor;\r\n  }\r\n\r\n  carregaOperadorNivel(item: any) {\r\n    this.itensNiveis = item;\r\n\r\n    // for(let i = 0; i <  this.itensNiveis.length; i++){\r\n    //    if (this.itensNiveis[i].descricao == \"NÃO HABILITADO\") {\r\n    //      this.idOperadorNivelNaoHabilidtado = this.itensNiveis[i].id;\r\n    //    }\r\n    // }\r\n    \r\n    this.itensNiveis.forEach((element: any) => {\r\n      if (element.descricao == \"NÃO HABILITADO\") {\r\n        this.idOperadorNivelNaoHabilidtado = element.id;\r\n        return;\r\n      }\r\n    });\r\n  }\r\n\r\n  carregaProcessos(items: any): void {\r\n    this.itensProcessos = items;\r\n    let cont = 0;\r\n    this.itensProcessos.forEach((element: any) => {\r\n      cont++;\r\n      element.habilitado = false;\r\n\r\n      element.nivel = \"NÃO HABILITADO\"; //this.itensNiveis;\r\n\r\n      element.dataAtual = null; //this.formatDate(new Date().toISOString().substr(0, 10));\r\n      element.date = new Date().toISOString().substr(0, 10);\r\n\r\n      element.nomeRef = \"menuRef\" + cont;\r\n      element.menu1 = false;\r\n\r\n      element.dataAlvo = null; //this.formatDate(new Date().toISOString().substr(0, 10));\r\n      element.date2 = new Date().toISOString().substr(0, 10);\r\n      element.nomeRef2 = \"menuRef2\" + cont;\r\n      element.menu2 = false;\r\n    });\r\n\r\n    this.operador.processos = this.itensProcessos;\r\n  }\r\n\r\n  showOperadorForEditar(item: any, processosNovos: any) {\r\n    //CARREGANDO DADOS DO FUNCIONARIO\r\n    this.operador.processos = item.processos;\r\n\r\n    this.operador.processos.forEach((element: any) => {\r\n      if (element.dataAtual != null || element.dataAlvo != null) {\r\n        (element.date = element.dataAtual), (element.date2 = element.dataAlvo);\r\n      }\r\n      //element.nivel = null\r\n    });\r\n\r\n    //INSERE NOVO PROCESSO QUE NÃO FOI CADASTRADO ANTERIORMENTE QUANDO O REGISTRO HAVIA SIDO INSERIDO\r\n    processosNovos.forEach((element: any) => {\r\n      let novoProcesso: any = {};\r\n      let cont = this.operador.processos.length;\r\n      if (this.verificaNovosProcessos(element.nome) == false) {\r\n        cont++;\r\n        novoProcesso.id = element.id;\r\n        novoProcesso.habilitado = false;\r\n        novoProcesso.nome = element.nome;\r\n\r\n        novoProcesso.nivel = \"NÃO HABILITADO\"; //this.itensNiveis;\r\n\r\n        novoProcesso.dataAtual = null; //this.formatDate(new Date().toISOString().substr(0, 10));\r\n        novoProcesso.date = new Date().toISOString().substr(0, 10);\r\n\r\n        novoProcesso.nomeRef = \"menuRef\" + cont;\r\n        novoProcesso.menu1 = false;\r\n\r\n        novoProcesso.dataAlvo = null; //this.formatDate(new Date().toISOString().substr(0, 10));\r\n        novoProcesso.date2 = new Date().toISOString().substr(0, 10);\r\n        novoProcesso.nomeRef2 = \"menuRef2\" + cont;\r\n        novoProcesso.menu2 = false;\r\n\r\n        this.operador.processos.push(novoProcesso);\r\n      }\r\n    });\r\n\r\n    this.funcionario.id = item.idFuncionario;\r\n    this.funcionario.nome = item.nome;\r\n    this.funcionario.matricula = item.matricula;\r\n    this.funcionario.centroCusto = item.centroCusto;\r\n  }\r\n\r\n  verificaNovosProcessos(nomeProcesso: string) {\r\n    let retorno = false;\r\n    this.operador.processos.forEach((element: any) => {\r\n      if (element.nome === nomeProcesso) {\r\n        retorno = true;\r\n      }\r\n    });\r\n\r\n    return retorno;\r\n  }\r\n\r\n  created() {\r\n    this._controller = new OperadorFormController(this);\r\n    this.carregarDtos();\r\n    this._controller.carregarDtos(\r\n      this.$route.params.id,\r\n      this.$route.params.idFuncionario\r\n    );\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n"],"sourceRoot":""}]}