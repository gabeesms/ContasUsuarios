ding = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  search = \"\";\r\n\r\n  dialogDelete = false;\r\n  itemUsuarioDelete: any;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Usuários\",\r\n      disabled: false,\r\n      href: \"/usuarios\",\r\n    },\r\n  ];\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  showItens(list: any): void {\r\n    //console.log(\"TESTE\");\r\n    this.itens = [];\r\n\r\n    this.itens = list;\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    if (item.usuarioPerfil != null) {\r\n      this.itemUsuarioDelete = item.usuarioPerfil.id;\r\n      this.dialogDelete = true;\r\n    } else {\r\n      this.showMsg(\"Usuário não possui Perfil associado.\");\r\n    }\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    this._controller!.deleteitem(this.itemUsuarioDelete);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    if (item.usuarioPerfil == null) {\r\n      this.$router.push(\"/usuario/\" + item.id + \"/\" + item.matricula);\r\n    } else {\r\n      this.$router.push(\r\n        \"/usuario/\" +\r\n          item.id +\r\n          \"/\" +\r\n          item.matricula +\r\n          \"/\" +\r\n          item.usuarioPerfil.id\r\n      );\r\n    }\r\n  }\r\n\r\n  erpFuncionariosCarregar() {\r\n    this.loading(true);\r\n    this.itens = [];\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new UsuarioListaController(this);\r\n\r\n    //this._controller.carregarItens();\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  headers = new Headers([\r\n    new Header(\"Matrícula\", \"matricula\", false, false, \"center\", true, \"100\"),\r\n    new Header(\"Nome\", \"nome\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Cargo\", \"cargo\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Situação\", \"situacao\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Tipo\", \"tipo\", false, false, \"left\", true, \"100\"),\r\n    new Header(\r\n      \"Perfil Associado\",\r\n      \"usuarioPerfil\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      true,\r\n      \"200\"\r\n    ),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  itens = [];\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/perfil\");\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new UsuarioListaController(this);\r\n\r\n    this._controller.carregarItens();\r\n  }\r\n}\r\n"],"sourceRoot":""}]}