AAyB,QAAQ,CAAC,IAAlC,EADsB,CAEtB;;AAED,iBANH,EAMK,KANL,CAMW,UAAC,CAAD,EAAW;AAClB,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;AAED,iBATH;;AAzHJ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uHAqIwC,oBArIxC;AAAA;;AAAA;AAAA;AAAA;AAAA;AAsII;AAEA,gBAAA,KAAK,CACH;AADG,iBAEF,GAFH,CAEO,yDAAyD,oBAFhE,EAGG,IAHH,CAGQ,UAAC,QAAD,EAAkB;AAGtB;AACA;AAEA,kBAAA,MAAI,CAAC,KAAL,CAAW,8BAAX,CAA0C,QAAQ,CAAC,IAAnD;AACD,iBAVH,EAUK,KAVL,CAUW,UAAC,CAAD,EAAW;AAClB,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;AAED,iBAbH;;AAxIJ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,0GAwJ2B,oBAxJ3B;AAAA;;AAAA;AAAA;AAAA;AAAA;AAyJI;AAEA,gBAAA,KAAK,CACJ,GADD,CACK,4CAA4C,oBADjD,EAGG,IAHH,CAGQ,UAAC,QAAD,EAAkB;AAGtB;AACA;AAEA,kBAAA,MAAI,CAAC,KAAL,CAAW,iBAAX,CAA6B,QAAQ,CAAC,IAAtC;AACD,iBAVH,EAUK,KAVL,CAUW,UAAC,CAAD,EAAW;AAClB,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;AAED,iBAbH;;AA3JJ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA","sourcesContent":["import {\r\n  IOrdemProducaoFormView,\r\n  IOrdemProducaoFormController,\r\n} from \"../../contracts/OrdemProducao/OrdemProducaoFormContract\";\r\nimport axios from \"../../plugins/axios\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nexport class OrdemProducaoFormController implements IOrdemProducaoFormController {\r\n  _view: IOrdemProducaoFormView;\r\n\r\n  _lista = [\r\n    {\r\n      numero: \"\",\r\n      produto: \"\",\r\n      descricao: \"\",\r\n      quantidade: \"\",\r\n      dataNecessidade: \"\",\r\n      um: \"\",\r\n      justificativa: \"\",\r\n      acao: [new HeaderAction(\"\", \"\", \"\", \"\", \"\")],\r\n    },\r\n  ];\r\n\r\n  constructor(view: IOrdemProducaoFormView) {\r\n    this._view = view;\r\n  }\r\n\r\n  carregarDtos(idOrdemProducaoLinha: string, idOrdemProducaoPlano: string): void {\r\n\r\n\r\n    this._view.loading(true);\r\n    this.carregarTurnos();\r\n    \r\n    this.carregarOperadores(idOrdemProducaoLinha);\r\n\r\n    if (idOrdemProducaoLinha != 'undefined' && idOrdemProducaoPlano == undefined) {\r\n\r\n      this.carregarOperadoresPorHabilidade(idOrdemProducaoLinha);\r\n      //CARREGA NOVO\r\n      axios\r\n        .get(\"OrdemProducaoLinha/\" + idOrdemProducaoLinha)\r\n        .then((response: any) => {\r\n          if (response.data) {\r\n\r\n            this._view.showOrdemProducaoForNovo(response.data);\r\n            //this._view.loading(false);\r\n\r\n\r\n          }\r\n        })\r\n        .catch((e: any) => {\r\n          console.log(e);\r\n          this._view.loading(false);\r\n          this._view.showMsg(e, 0);\r\n        });\r\n\r\n    }\r\n\r\n    if (idOrdemProducaoPlano != undefined) {\r\n      //CARREGA NOVO\r\n      axios\r\n        .get(\"OrdemProducaoPlano/\" + idOrdemProducaoPlano)\r\n        .then((response: any) => {\r\n          if (response.data) {\r\n\r\n            this._view.showOrdemProducaoForEditar(response.data);\r\n            //this._view.loading(false);\r\n\r\n\r\n          }\r\n        })\r\n        .catch((e: any) => {\r\n          console.log(e);\r\n          this._view.loading(false);\r\n          this._view.showMsg(e, 0);\r\n        });\r\n    }\r\n\r\n  }\r\n\r\n\r\n  salvar(ordemProducao: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"OrdemProducaoPlano\", ordemProducao)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Funcionário\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  update(idFuncionario: string, funcionario: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .put(\"FuncionarioLocal/\" + idFuncionario, funcionario)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n        } else {\r\n          this._view.showMsg(response.data.message, 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  async carregarTurnos() {\r\n    //this._view.loading(true)\r\n\r\n\r\n    axios\r\n      .get('Turno')\r\n      .then((response: any) => {\r\n        this._view.carregaTurnos(response.data)\r\n        //this._view.loading(false)\r\n\r\n      }).catch((e: any) => {\r\n        this._view.loading(false)\r\n\r\n      })\r\n  }\r\n\r\n  async carregarOperadoresPorHabilidade(idOrdemProducaoLinha: string) {\r\n    //this._view.loading(true)\r\n\r\n    axios\r\n      //.get('FuncionarioLocal/operadorOrdemProducao/' + idOrdemProducaoLinha)\r\n      .get('FuncionarioLocal/operadorOrdemProducaoPorHabilidade/' + idOrdemProducaoLinha)\r\n      .then((response: any) => {\r\n\r\n\r\n        //console.log(response.data)\r\n        //this._view.loading(false)\r\n\r\n        this._view.carregaOperadoresPorHabilidade(response.data)\r\n      }).catch((e: any) => {\r\n        this._view.loading(false)\r\n\r\n      })\r\n  }\r\n\r\n  async carregarOperadores(idOrdemProducaoLinha: string) {\r\n    //this._view.loading(true)\r\n\r\n    axios\r\n    .get('FuncionarioLocal/operadorOrdemProducao/' + idOrdemProducaoLinha)\r\n      \r\n      .then((response: any) => {\r\n\r\n\r\n        //console.log(response.data)\r\n        //this._view.loading(false)\r\n\r\n        this._view.carregaOperadores(response.data)\r\n      }).catch((e: any) => {\r\n        this._view.loading(false)\r\n\r\n      })\r\n  }\r\n\r\n\r\n}\r\n"],"sourceRoot":""}]}