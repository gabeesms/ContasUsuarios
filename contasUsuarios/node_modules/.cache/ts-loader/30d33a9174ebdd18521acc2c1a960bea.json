D;AACF;AA9EH;;AAAA;AAAA,EAAkC,GAAlC,CAAA;;AAAqB,IAAI,GAAA,OAAA,CAAA,UAAA,CAAA,CANxB,SAAS,CAAC;AACT,EAAA,UAAU,EAAE;AACV,IAAA,QAAQ,EAAR,QADU;AAEV,cAAU;AAFA;AADH,CAAD,CAMe,CAAA,EAAJ,IAAI,CAAJ;eAAA,I","sourcesContent":["\n\n\n\n\n\n\n\n\n\r\nimport Vue from \"vue\";\r\nimport KLoading from \"../components/KLoading.vue\";\r\nimport KToolbarNavigationDrawer from \"../components/KToolbarNavigationDrawer.vue\";\r\nimport Component from \"vue-class-component\";\r\nimport { Menu, MenuItem } from \"../models/menu\";\r\n\r\nimport { IHomeView, IHomeController } from \"../contracts/HomeContract\";\r\n\r\nimport { Storage } from \"../configuration/storage\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    \"k-menu\": KToolbarNavigationDrawer\r\n  }\r\n})\r\nexport default class Home extends Vue implements IHomeView {\r\n  itensMenu: MenuItem[] = [];\r\n  dialogLoading = false;\r\n\r\n  menu?: Menu;\r\n\r\n  usuario: any;\r\n\r\n  mostraErro(msg: string) {}\r\n\r\n  sair() {\r\n    Storage.salvar(\"Logado\", \"false\");\r\n    Storage.salvar(\"Session\", \"\");\r\n    Storage.salvar(\"User\", \"\");\r\n    this.$router.push(\"/Login\");\r\n  }\r\n\r\n  changePass() {\r\n    this.$router.push(\"/alteracaoSenha\");\r\n  }\r\n\r\n  createMenu(modulo: String){\r\n    this.menu = undefined;\r\n    this.itensMenu = [];\r\n\r\n    let menu: Menu|undefined = undefined;\r\n    //MODULO RECURSOS HUMANOS\r\n    if(modulo == \"RH\"){\r\n      this.itensMenu.push(\r\n        new MenuItem(\"Cadastro\", \"\", true, [\r\n          new MenuItem(\"Funcionário\", \"/funcionarios\", false),\r\n          new MenuItem(\"Categorias\", \"/categorias\", false),\r\n          new MenuItem(\"Setor\", \"/setores\", false),\r\n          new MenuItem(\"Centro de Custo\", \"/centrosCustos\", false),\r\n          new MenuItem(\"Calendário\", \"/calendario\", false)\r\n        ]),\r\n\r\n        new MenuItem(\"Acessos e Perfis\", \"\", true, [\r\n          new MenuItem(\"Perfil\", \"/perfils\", false),\r\n          new MenuItem(\"Usuários\", \"/usuarios\", false),\r\n          \r\n        ]),\r\n\r\n        new MenuItem(\"Espelho de Ponto\", \"/espelhosPonto\", false, ),\r\n        new MenuItem(\"Fluxo de Aprovacão\", \"/fluxosAprovacao\", false, ),\r\n      );\r\n\r\n      menu = new Menu(this.usuario.nome!, this.itensMenu);\r\n      menu.appBarStyle = \"background: white\"\r\n    }\r\n     //MODULO PRODUÇÃO\r\n    else{\r\n      this.itensMenu.push(\r\n\r\n        new MenuItem(\"Cadastro\", \"\", true, [\r\n          new MenuItem(\"Produto\", \"/produtos\", false),\r\n          //new MenuItem(\"Linhas\", \"/linhas\", false),\r\n          new MenuItem(\"Processos\", \"/processos\", false),\r\n          new MenuItem(\"Operadores\", \"/operadores\", false),\r\n        ]),\r\n      );\r\n\r\n      menu = new Menu(this.usuario.nome!, this.itensMenu);\r\n      menu.appBarStyle = \"background: white\"\r\n    }\r\n\r\n    this.menu = menu\r\n    this.$forceUpdate()\r\n  }\r\n\r\n  created() {\r\n    var logado = Storage.get(\"Logado\");\r\n    if (logado == \"true\") {\r\n      this.usuario = JSON.parse(Storage.get(\"Colaborador\")!!);\r\n      this.createMenu(\"RH\")\r\n    } else {\r\n      this.sair();\r\n    }\r\n  }\r\n}\r\n"],"sourceRoot":""}]}