ts/KDialog.vue\";\r\nimport KDataList from \"../../components/KDataList.vue\";\r\n\r\nimport Component from \"vue-class-component\";\r\nimport { Watch } from \"vue-property-decorator\";\r\n\r\nimport { Display } from \"../../configuration/display\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  ILinhaProducaoTransferenciaFormView,\r\n  ILinhaProducaoTransferenciaFormController,\r\n} from \"../../contracts/Linha/LinhaProducaoTransferenciaFormContract\";\r\nimport { LinhaProducaoTransferenciaFormController } from \"../../controllers/Linha/LinhaProducaoTransferenciaFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class LinhaProducaoTransferenciaForm\r\n  extends Vue\r\n  implements ILinhaProducaoTransferenciaFormView {\r\n  _controller?: ILinhaProducaoTransferenciaFormController;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"RotaAqui\",\r\n      disabled: false,\r\n      href: \"/rota\",\r\n    },\r\n  ];\r\n\r\n  headers = [\r\n    {\r\n      text: \"Linha\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"linha\",\r\n    },\r\n    {\r\n      text: \"Matricula\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"matricula\",\r\n    },\r\n    {\r\n      text: \"Nome\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"nome\",\r\n    },\r\n    {\r\n      text: \"Operação\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"operação\",\r\n    },\r\n    {\r\n      text: \"Habilidade\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"habilidade\",\r\n    },\r\n  ];\r\n\r\n  //ABAS\r\n  tab: any = null;\r\n  itemsTab: any = [];\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  itemsLinha: any = [];\r\n\r\n  itemsOperacao: any = [];\r\n\r\n  itemsOperadoresTrensferencia: any = [];\r\n  itemsOperadoresTrensferenciaFilter: any = [];\r\n\r\n  operadoresSelecionados: any = [];\r\n  linhaSelecionada: any = [];\r\n  operacaoSelecionada: any = [];\r\n\r\n  transferencia: any = {\r\n    idSolicitacaoTransferencia: 0,\r\n    idLinhaProducaoSolicitacaoTransferencia: 0,\r\n    nomeLinhaProducaoSolicitacaoTransferencia: \"\",\r\n    dadosTab: [],\r\n  };\r\n\r\n  idSolicitacaoTransferencia = \"\";\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  cliqueTab() {\r\n    console.log(this.tab);\r\n  }\r\n\r\n  changeLinha() {\r\n    this.itemsOperadoresTrensferencia.forEach((element: any) => {\r\n      if (this.linhaSelecionada.id == element.idLinhaProducao) {\r\n        element.checkbox = false;\r\n\r\n        this.itemsOperadoresTrensferenciaFilter.push(element);\r\n      }\r\n    });\r\n\r\n    console.log(this.itemsOperadoresTrensferenciaFilter);\r\n  }\r\n\r\n  addSolicitacaoTransferencia() {\r\n    console.log(this.tab);\r\n    this.transferencia.dadosTab[this.tab].itensTransferencia.push({ id: \"10\" });\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/linhascontrole\");\r\n    }\r\n  }\r\n\r\n  retornaListaLinhaControle() {\r\n    this.$router.push(\"/linhascontrole\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    if (\r\n      this.transferencia.linha == \"\" ||\r\n      this.transferencia.linha == null ||\r\n      this.transferencia.linha.lenght == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a linha atual.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.idSolicitacaoTransferencia == \"\" ||\r\n      this.idSolicitacaoTransferencia == null\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text =\r\n        \"Selecione o Produto / Nº O.P. para onde deseja realizar a transferência do operador.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.transferencia.operador == \"\" ||\r\n      this.transferencia.operador == null ||\r\n      this.transferencia.operador.lenght == 0\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o operador para solicitar a transferência.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  saveLinhaSolicitacaoTransferencia() {\r\n    if (this.validate()) {\r\n    } else {\r\n      let dadosSolicitacaiTransferencia = {\r\n        dataCadastro: new Date(),\r\n        status: \"AC\",\r\n      };\r\n\r\n      this._controller!.salvar(dadosSolicitacaiTransferencia);\r\n    }\r\n  }\r\n\r\n  updateFuncionario() {\r\n    if (!this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      // this._controller!.update(this.idFuncionario, this.funcionario);\r\n      this.retornaListaLinhaControle();\r\n    }\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.idSolicitacaoTransferencia = this.$route.params.idSolicitacaoTransferencia;\r\n  }\r\n\r\n  showBaseForTransferencia(solicitacaoTransferencia: 