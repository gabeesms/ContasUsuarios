OAAjC;AACH;AACJ,OATD,EASG,KATH,CASS,UAAC,CAAD,EAAW;AAChB,QAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;;AACA,QAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,CAAnB;AACH,OAZD;AAaH;AA5GL;AAAA;AAAA,+BA8Ge,EA9Gf,EA8GyB;AAAA;;AACjB,WAAK,KAAL,CAAW,OAAX,CAAmB,IAAnB;;AAEA,MAAA,KAAK,CAAC,GAAN,CAAU,cAAc,EAAxB,EAA4B,IAA5B,CAAiC,UAAC,QAAD,EAAkB;AAC/C,QAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;;AACA,YAAI,QAAQ,CAAC,IAAT,CAAc,OAAlB,EAA2B;AACvB;AACA,UAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,QAAQ,CAAC,IAAT,CAAc,OAAjC;;AACA,UAAA,MAAI,CAAC,aAAL;AACH,SAJD,MAIO;AACH,UAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,QAAQ,CAAC,IAAT,CAAc,OAAjC;AACH;AACJ,OATD,EASG,KATH,CASS,UAAC,CAAD,EAAW;AAChB,QAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;;AACA,QAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,CAAnB;AACH,OAZD;AAaH;AA9HL;;AAAA;AAAA","sourcesContent":["import { IProcessoListaFormView, IProcessoListaFormController } from \"../../contracts/Cadastro/ProcessoListaFormContract\";\r\nimport axios from \"../../plugins/axios\"\r\n\r\nimport { Storage } from \"../../configuration/storage\"\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nexport class ProcessoListaFormController implements IProcessoListaFormController {\r\n\r\n    _view: IProcessoListaFormView\r\n\r\n    constructor(view: IProcessoListaFormView) {\r\n        this._view = view\r\n    }\r\n\r\n    async carregarItens() {\r\n        this._view.loading(true)\r\n        let operacoes:  any = [];\r\n        await axios\r\n            .get('Processo/operacoes')\r\n            .then((response: any) => {\r\n\r\n                response.data.forEach((element: any) => {\r\n\r\n                    if (element.status == 'A') {\r\n                        element.acoes = [\r\n                            new HeaderAction(\"mdi-cancel\", \"primary\", \"Teste\", \"deleteItem\", \"200px\")\r\n                        ]\r\n                    }\r\n\r\n                    if (element.status == 'I') {\r\n                        element.acoes = [\r\n                            new HeaderAction(\"mdi-cancel\", \"error\", \"Teste\", \"editarItem\", \"200px\")\r\n                        ]\r\n                    }\r\n\r\n\r\n                });\r\n                \r\n                operacoes = response.data;\r\n                //this._view.showItens(response.data)\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n\r\n        await axios\r\n            .get('Processo/habilidades')\r\n            .then((response: any) => {\r\n\r\n                response.data.forEach((element: any) => {\r\n\r\n                    if (element.status == 'A') {\r\n                        element.acoes = [\r\n                            new HeaderAction(\"mdi-cancel\", \"primary\", \"Teste\", \"deleteItem\", \"200px\")\r\n                        ]\r\n                    }\r\n\r\n                    if (element.status == 'I') {\r\n                        element.acoes = [\r\n                            new HeaderAction(\"mdi-cancel\", \"error\", \"Teste\", \"editarItem\", \"200px\")\r\n                        ]\r\n                    }\r\n\r\n\r\n                });\r\n                this._view.limparCampos();\r\n                this._view.loading(false)\r\n                this._view.showItens(operacoes, response.data)\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n    }\r\n\r\n    salvar(processo: any): void {\r\n\r\n        this._view.loading(true);\r\n\r\n        axios\r\n            .post(\"Processo\", processo)\r\n            .then((response: any) => {\r\n                this._view.loading(false);\r\n                if (response.data.sucesso) {\r\n                    this._view.showMsg(response.data.message);\r\n                    this.carregarItens();\r\n\r\n                } else {\r\n                    this._view.showMsg(\"Não foi possível salvar os dados do Linha\");\r\n                }\r\n            })\r\n            .catch((e: any) => {\r\n                this._view.loading(false);\r\n                this._view.showMsg(e);\r\n            });\r\n\r\n    }\r\n\r\n\r\n    deleteitem(id: number): void {\r\n        this._view.loading(true)\r\n\r\n        axios.delete('Processo/' + id).then((response: any) => {\r\n            this._view.loading(false)\r\n            if (response.data.sucesso) {\r\n                // this._view.showMsg(\"Salvo com sucesso.\")\r\n                this._view.showMsg(response.data.message)\r\n                this.carregarItens();\r\n            } else {\r\n                this._view.showMsg(response.data.message)\r\n            }\r\n        }).catch((e: any) => {\r\n            this._view.loading(false)\r\n            this._view.showMsg(e)\r\n        })\r\n    }\r\n\r\n    ativaritem(id: number): void {\r\n        this._view.loading(true)\r\n\r\n        axios.put('Processo/' + id).then((response: any) => {\r\n            this._view.loading(false)\r\n            if (response.data.sucesso) {\r\n                // this._view.showMsg(\"Salvo com sucesso.\")\r\n                this._view.showMsg(response.data.message)\r\n                this.carregarItens();\r\n            } else {\r\n                this._view.showMsg(response.data.message)\r\n            }\r\n        }).catch((e: any) => {\r\n            this._view.loading(false)\r\n            this._view.showMsg(e)\r\n        })\r\n    }\r\n\r\n\r\n}"],"sourceRoot":""}]}