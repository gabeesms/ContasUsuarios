y\";\r\nimport { Storage } from \"../../configuration/storage\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nimport {\r\n  ILinhaProducaoSolicitarTransferenciaFormView,\r\n  ILinhaProducaoSolicitarTransferenciaFormController,\r\n} from \"../../contracts/Linha/LinhaProducaoSolicitarTransferenciaFormContract\";\r\nimport { LinhaProducaoSolicitarTransferenciaFormController } from \"../../controllers/Linha/LinhaProducaoSolicitarTransferenciaFormController\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class LinhaProducaoSolicitarTransferenciaForm\r\n  extends Vue\r\n  implements ILinhaProducaoSolicitarTransferenciaFormView {\r\n  _controller?: ILinhaProducaoSolicitarTransferenciaFormController;\r\n\r\n  itensTransferencia: any = [];\r\n\r\n  headers = [\r\n    {\r\n      text: \"Descrição da Habilidade\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"habilidade\",\r\n    },\r\n    {\r\n      text: \"Quantidade de Operadores Solicitada\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"qtdeOperador\",\r\n    },\r\n    {\r\n      text: \"Ações\",\r\n      align: \"center\",\r\n      filterable: true,\r\n      value: \"acao\",\r\n    },\r\n  ];\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  itemsResponsaveis: any = [];\r\n  itemsLinha: any = [];\r\n  linhaSelecionada: any = [];\r\n  responsaveisSelecionados: any = [];\r\n\r\n  itemsHabilidade: any = [];\r\n  habilidadeSelecionada: any = [];\r\n\r\n  qtdeOperadores = \"\";\r\n  disableLinha = false;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  removeHabilidade(itemSelecionado: any) {\r\n    for (const { item, index } of this.itensTransferencia.map(\r\n      (item: any, index: any) => ({\r\n        item,\r\n        index,\r\n      })\r\n    )) {\r\n      if (item.idHabilidade == itemSelecionado.idHabilidade) {\r\n        this.itensTransferencia.splice(index, 1);\r\n      }\r\n    }\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/linhascontrole\");\r\n    }\r\n  }\r\n\r\n  limparCampos() {\r\n    this.disableLinha = false;\r\n    this.linhaSelecionada = [];\r\n    this.itensTransferencia = [];\r\n    this.habilidadeSelecionada = [];\r\n    this.responsaveisSelecionados = [];\r\n    this.itemsResponsaveis = [];\r\n  }\r\n\r\n  retornaListaLinhaControle() {\r\n    this.$router.push(\"/linhascontrole\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    if (this.linhaSelecionada == \"\" || this.linhaSelecionada == null) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a linha.\";\r\n      this.snackbar = true;\r\n      this.disableLinha = false;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      this.habilidadeSelecionada == \"\" ||\r\n      this.habilidadeSelecionada == null\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a habilidade.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (\r\n      parseInt(this.qtdeOperadores) == 0 ||\r\n      this.qtdeOperadores == null ||\r\n      this.qtdeOperadores == undefined\r\n    ) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Infome a quantidade de operadores.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.itensTransferencia.length > 0) {\r\n      let retorno = false;\r\n      this.itensTransferencia.forEach((element: any) => {\r\n        if (this.habilidadeSelecionada.id == element.idHabilidade) {\r\n          this.colorBorder = \"error\";\r\n          this.color = \"error\";\r\n          this.text = \"Habilidade ja inserida à lista de solicitações.\";\r\n          this.snackbar = true;\r\n          retorno = true;\r\n        }\r\n      });\r\n      return retorno;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  saveLinhaSolicitacaoTransferencia() {\r\n    //if (this.validate()) {\r\n    //} else {\r\n      let dadosSolicitacaiTransferencia = {\r\n        idLinhaProducao: this.linhaSelecionada.id,\r\n        responsaveis: this.responsaveisSelecionados,\r\n        itensSolicitacaoTransferencia: this.itensTransferencia,\r\n        dataCadastro: new Date(),\r\n        status: \"AC\",\r\n      };\r\n\r\n      console.log(dadosSolicitacaiTransferencia)\r\n\r\n      this._controller!.salvar(dadosSolicitacaiTransferencia);\r\n   // }\r\n  }\r\n\r\n  updateFuncionario() {\r\n    if (!this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      // this._controller!.update(this.idFuncionario, this.funcionario);\r\n      this.retornaListaLinhaControle();\r\n    }\r\n  }\r\n\r\n  carregarDtos(): void {}\r\n\r\n  showBaseForEditar(item: any) {\r\n    //CARREGANDO DADOS DO FUNCIONARIO\r\n  }\r\n\r\n  changeCarregaResponsaveis() {\r\n    this.itemsResponsaveis = [];\r\n\r\n    this.itemsLinha.forEach((linha: any) => {\r\n      if (linha.id == this.linhaSelecionada.id) {\r\n        linha.linhaProducaoCategorias.forEach((categoria: any) => {\r\n          if (categoria.deletado == \"N\") {\r\n            categoria.linhaProducaoCategoriasFuncionarioLocal.forEach(\r\n              (funcionarios: any) => {\r\n                this.itemsResponsaveis.push(funcionarios.funcionarioErpDTO);\r\n              }\r\n            );\r\n          }\r\n        });\r\n      }\r\n    });\r\n\r\n    console.log(this.itemsResponsaveis);\r\n  }\r\n\r\n  //carregando todas as linhas\r\n  carregaLinha(itemLinha: any): void {\r\n    this.itemsLinha = itemLinha;\r\n    this.itemsHabilidade = [];\r\n  }\r\n\r\n  //carregando todas as Habilidades\r\n  carregaProcessoHabilidade(itemHabilidades: any): void {\r\n    this.itemsHabilidade = itemHabilidades;\r\n  }\r\n\r\n  addSolicitacaoTransferencia() {\r\n    this.disableLinha = true;\r\n    let transferencia: any = {};\r\n\r\n    transferencia = {\r\n      idHabilidade: this.habilidadeSelecionada.id,\r\n      habilidade: this.habilidadeSelecionada.nome,\r\n      qtdeOperador: parseInt(this.qtdeOperadores),\r\n    };\r\n\r\n    if (!this.validate()) {\r\n      this.itensTransferencia.push(transferencia);\r\n      this.habilidadeSelecionada = \"\";\r\n      this.qtdeOperadores = \"\";\r\n    }\r\n  }\r\n\r\n  created() {\r\n    this._controller = new LinhaProducaoSolicitarTransferenciaFormController(this);\r\n    this._controller.carregarDtos(this.$route.params.id);\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n"],"sourceRoot":""}]}