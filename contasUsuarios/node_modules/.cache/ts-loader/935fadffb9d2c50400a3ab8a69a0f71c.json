AX,CAAmB,KAAnB;;AACA,QAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,CAAnB,EAAsB,CAAtB;AACD,OAZH;AAcD;AAnMH;;AAAA;AAAA","sourcesContent":["import {\r\n  ILinhaFormView,\r\n  ILinhaFormController,\r\n} from \"../../contracts/Cadastro/LinhaFormContract\";\r\nimport axios from \"../../plugins/axios\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nexport class LinhaFormController implements ILinhaFormController {\r\n  _view: ILinhaFormView;\r\n\r\n  _lista = [\r\n    {\r\n      numero: \"\",\r\n      produto: \"\",\r\n      descricao: \"\",\r\n      quantidade: \"\",\r\n      dataNecessidade: \"\",\r\n      um: \"\",\r\n      justificativa: \"\",\r\n      acao: [new HeaderAction(\"\", \"\", \"\", \"\", \"\")],\r\n    },\r\n  ];\r\n\r\n  constructor(view: ILinhaFormView) {\r\n    this._view = view;\r\n  }\r\n\r\n  async carregarDtos(id: string) {\r\n\r\n    this._view.loading(true);\r\n    await this.carregarProcessos();\r\n    await this.carregarCapacidadeLinha();\r\n\r\n\r\n    if (id != undefined) {\r\n      //CARREGA EDITAR\r\n      axios\r\n        .get(\"Linha/\" + id)\r\n        .then((response: any) => {\r\n          this._view.loading(false);\r\n          if (response.data) {\r\n            this._view.showLinhaForEditar(response.data);\r\n          }\r\n        })\r\n        .catch((e: any) => {\r\n          console.log(e);\r\n          this._view.loading(false);\r\n          this._view.showMsg(e, 0);\r\n        });\r\n    } else {\r\n      this._view.loading(false);\r\n      \r\n    }\r\n  }\r\n\r\n  salvar(linha: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"Linha\", linha)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n          this.carregarCapacidadeLinha();\r\n\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Linha\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  salvarCapacidadeLinha(capacidadeLinha: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"LinhaCapacidade\", capacidadeLinha)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 0);\r\n          this.carregarCapacidadeLinha();\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Linha\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  update(idLinha: string, linha: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .put(\"Linha/\" + idLinha, linha)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n        } else {\r\n          this._view.showMsg(response.data.message, 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  async carregarProcessos() {\r\n    this._view.loading(true)\r\n    let operacoes: any = [];\r\n    await axios\r\n      .get('Processo/operacoes')\r\n      .then((response: any) => {\r\n\r\n        response.data.forEach((element: any) => {\r\n\r\n          if (element.status == 'A') {\r\n            element.acoes = [\r\n              new HeaderAction(\"mdi-cancel\", \"primary\", \"Teste\", \"deleteItem\", \"200px\")\r\n            ]\r\n          }\r\n\r\n          if (element.status == 'I') {\r\n            element.acoes = [\r\n              new HeaderAction(\"mdi-cancel\", \"error\", \"Teste\", \"editarItem\", \"200px\")\r\n            ]\r\n          }\r\n\r\n\r\n        });\r\n\r\n        operacoes = response.data;\r\n        //this._view.showItens(response.data)\r\n      }).catch((e: any) => {\r\n        this._view.loading(false)\r\n\r\n      })\r\n\r\n    await axios\r\n      .get('Processo/habilidades')\r\n      .then((response: any) => {\r\n\r\n        response.data.forEach((element: any) => {\r\n\r\n          if (element.status == 'A') {\r\n            element.acoes = [\r\n              new HeaderAction(\"mdi-cancel\", \"primary\", \"Teste\", \"deleteItem\", \"200px\")\r\n            ]\r\n          }\r\n\r\n          if (element.status == 'I') {\r\n            element.acoes = [\r\n              new HeaderAction(\"mdi-cancel\", \"error\", \"Teste\", \"editarItem\", \"200px\")\r\n            ]\r\n          }\r\n\r\n\r\n        });\r\n\r\n        this._view.loading(false)\r\n        this._view.carregaProcessos(operacoes, response.data)\r\n      }).catch((e: any) => {\r\n        this._view.loading(false)\r\n\r\n      })\r\n  }\r\n\r\n  carregarCapacidadeLinha(){\r\n\r\n    this._view.loading(true);\r\n\r\n    console.log(\"TESTE\")\r\n\r\n    //CARREGA EDITAR\r\n    axios\r\n      .get(\"LinhaCapacidade\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaCapacidadeLinha(response.data);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n\r\n}\r\n"],"sourceRoot":""}]}