r\n        this.dialogDelete = false;\r\n        return;\r\n      }\r\n    }\r\n  }\r\n\r\n  modalRemoverSetor(indexRemoveItem: string) {\r\n    this.indexRemoverSetor = parseInt(indexRemoveItem);\r\n    this.dialogDelete = true;\r\n  }\r\n\r\n  cancelaRemoverSetor() {\r\n    this.indexRemoverSetor = -1;\r\n    this.dialogDelete = false;\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/centrosCustos\");\r\n    }\r\n  }\r\n\r\n  retornaListaFuncionarios() {\r\n    this.$router.push(\"/centrosCustos\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    return false;\r\n  }\r\n\r\n  saveCentroCustoSetores() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.salvar(this.centroCusto);\r\n    }\r\n  }\r\n\r\n    updateCentroCustoSetores() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.update(this.centroCusto.id, this.centroCusto);\r\n    }\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.codigoCentroCusto = this.$route.params.codigo;\r\n  }\r\n\r\n  showCentroCustoForSave(item: any) {\r\n    this.centroCusto.codigo = item.codigo;\r\n    this.centroCusto.descricao = item.descricao;\r\n  }\r\n\r\n  showCentroCustoForEditar(item: any) {\r\n    this.centroCusto.id = item[0].centroCustoDTO.id;\r\n    this.centroCusto.codigo = item[0].centroCustoDTO.codigo;\r\n    this.centroCusto.descricao = item[0].centroCustoDTO.descricao;\r\n\r\n    item.forEach((element: any) => {\r\n      this.centroCusto.setores.push(element.setorDTO);\r\n    });\r\n  }\r\n\r\n  carregaSetores(item: any) {\r\n    //CARREGANDO DADOS DO FUNCIONARIO\r\n    this.itemsSetores = item;\r\n  }\r\n\r\n  validateAddSetor() {\r\n    if (this.setor.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o setor para adicionar.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    } else {\r\n      let flag = false;\r\n      this.centroCusto.setores.forEach((element: any) => {\r\n        if (element.descricao == this.setor.descricao) {\r\n          this.colorBorder = \"warning\";\r\n          this.color = \"warning\";\r\n          this.text = \"Setor já adicionado.\";\r\n          this.setor = \"\";\r\n          this.snackbar = true;\r\n          flag = true;\r\n        }\r\n      });\r\n\r\n      return flag;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/centroCusto\");\r\n  }\r\n\r\n  customFilterSetor(item: any, queryText: any, itemText: any) {\r\n    const textOne = item.sigla.toLowerCase();\r\n    const textTwo = item.descricao.toLowerCase();\r\n    const searchText = queryText.toLowerCase();\r\n\r\n    return textOne.indexOf(searchText) > -1 || textTwo.indexOf(searchText) > -1;\r\n  }\r\n\r\n  addSetor() {\r\n    if (this.validateAddSetor()) {\r\n    } else {\r\n      this.centroCusto.setores.push(this.setor);\r\n      this.setor = \"\";\r\n    }\r\n  }\r\n\r\n  created() {\r\n    this._controller = new CentroCustoFormController(this);\r\n    this._controller.carregarDtos(this.$route.params.codigo);\r\n    this.carregarDtos();\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n"],"sourceRoot":""}]}