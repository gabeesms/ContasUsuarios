;\r\n          }\r\n        })\r\n        .catch((e: any) => {\r\n          console.log(e);\r\n          this._view.loading(false);\r\n          this._view.showMsg(e, 0);\r\n        });\r\n\r\n      await this.carregarCapacidadeLinha(id);\r\n      await this.carregarRecursoTecnicoLinha(id)\r\n\r\n    } else {\r\n      this._view.loading(false);\r\n\r\n    }\r\n  }\r\n\r\n  salvar(linha: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"Linha\", linha)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          //this._view.showMsg(response.data.message, 0);\r\n          //this.carregarCapacidadeLinha();\r\n          //this.carregarRecursoTecnicoLinha();\r\n          this.carregarDtos(response.data.tRetorno.id);\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Linha\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  consultarProdutoCadastrado(idProduto: string): void {\r\n\r\n    axios\r\n      .get(\"Linha/consultaProdutoCadastrado/\" + idProduto)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        this._view.consultaProdutoCadastrado(response.data);\r\n        //console.log(retorno);\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  salvarCapacidadeLinha(capacidadeLinha: any, idLinha: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"LinhaCapacidade\", capacidadeLinha)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 0);\r\n          //this.carregarDtos(idLinha);\r\n          this.carregarCapacidadeLinha(idLinha);\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Linha\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  salvarRecursoTecnico(recursoTecnico: any, idLinha: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"LinhaRecursoTecnico\", recursoTecnico)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 0);\r\n          this.carregarDtos(idLinha);\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Linha\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  update(idLinha: string, linha: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .put(\"Linha/\" + idLinha, linha)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n        } else {\r\n          this._view.showMsg(response.data.message, 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  async carregarProcessos() {\r\n    this._view.loading(true)\r\n    let operacoes: any = [];\r\n    let habilidades: any = [];\r\n    let fases: any = [];\r\n    await axios\r\n      .get('Processo/operacoes')\r\n      .then((response: any) => {\r\n\r\n        response.data.forEach((element: any) => {\r\n\r\n          if (element.status == 'A') {\r\n            operacoes.push(element)\r\n          }\r\n\r\n        });\r\n\r\n        //operacoes = response.data;\r\n        //this._view.showItens(response.data)\r\n      }).catch((e: any) => {\r\n        this._view.loading(false)\r\n\r\n      })\r\n\r\n    await axios\r\n      .get('Processo/habilidades')\r\n      .then((response: any) => {\r\n\r\n        response.data.forEach((element: any) => {\r\n\r\n          if (element.status == 'A') {\r\n            habilidades.push(element);\r\n\r\n          }\r\n\r\n        });\r\n\r\n        //this._view.loading(false)\r\n        //this._view.carregaProcessos(operacoes, habilidades)\r\n      }).catch((e: any) => {\r\n        this._view.loading(false)\r\n\r\n      })\r\n\r\n    await axios\r\n      .get('Processo/fases')\r\n      .then((response: any) => {\r\n\r\n        response.data.forEach((element: any) => {\r\n\r\n          if (element.status == 'A') {\r\n            fases.push(element);\r\n\r\n          }\r\n\r\n        });\r\n\r\n        this._view.loading(false)\r\n        this._view.carregaProcessos(operacoes, habilidades, fases)\r\n      }).catch((e: any) => {\r\n        this._view.loading(false)\r\n\r\n      })\r\n  }\r\n\r\n  carregarCapacidadeLinha(idLinha: any) {\r\n\r\n    this._view.loading(true);\r\n    //CARREGA EDITAR\r\n    axios\r\n      .get(\"LinhaCapacidade/\" + idLinha)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaCapacidadeLinha(response.data);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  carregarRecursoTecnicoLinha(idLinha: any) {\r\n\r\n    this._view.loading(true);\r\n    //CARREGA EDITAR\r\n    axios\r\n      .get(\"LinhaRecursoTecnico/\" + idLinha)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaRecursoTecnicoLinha(response.data);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  async carregarProdutosLocal() {\r\n\r\n    this._view.loading(true);\r\n\r\n    await axios\r\n      .get(\"ProdutoLocal\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n\r\n          response.data.forEach((element: any) => {\r\n            element.dadosProduto = element.codigo.trim() + ' - ' + element.descricao.trim()\r\n          });\r\n\r\n          this._view.carregaProdutosLocal(response.data);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  deleteitem(id: number) {\r\n    this._view.loading(true)\r\n\r\n    axios.delete('Linha/' + id).then((response: any) => {\r\n      this._view.loading(false)\r\n      if (response.data.sucesso) {\r\n        // this._view.showMsg(\"Salvo com sucesso.\")\r\n        //this._view.showMsg(response.data.message, 0);\r\n        this._view.retornaListaLinhas();\r\n      }\r\n    }).catch((e: any) => {\r\n      console.log(e);\r\n      this._view.loading(false);\r\n      this._view.showMsg(e, 0);\r\n    })\r\n  }\r\n\r\n\r\n  deleteLinhaCapacidadeHabilidadeItem(id: number, idLinha: any) {\r\n    this._view.loading(true)\r\n\r\n    axios.delete('LinhaCapacidade/linhaCapacidadeHabilidade/' + id).then((response: any) => {\r\n      this._view.loading(false)\r\n      if (response.data.sucesso) {\r\n        // this._view.showMsg(\"Salvo com sucesso.\")\r\n        //this._view.showMsg(response.data.message, 0);\r\n        this.carregarCapacidadeLinha(idLinha);\r\n      }\r\n    }).catch((e: any) => {\r\n      console.log(e);\r\n      this._view.loading(false);\r\n      this._view.showMsg(e, 0);\r\n    })\r\n  }\r\n\r\n  deleteRecursoTecnicoHabilidadeItem(id: number, idLinha: any) {\r\n    this._view.loading(true)\r\n\r\n    axios.delete('LinhaRecursoTecnico/recursoTecnicoHabilidade/' + id).then((response: any) => {\r\n      this._view.loading(false)\r\n      if (response.data.sucesso) {\r\n        // this._view.showMsg(\"Salvo com sucesso.\")\r\n        //this._view.showMsg(response.data.message, 0);\r\n        this.carregarRecursoTecnicoLinha(idLinha);\r\n      }\r\n    }).catch((e: any) => {\r\n      console.log(e);\r\n      this._view.loading(false);\r\n      this._view.showMsg(e, 0);\r\n    })\r\n  }\r\n\r\n  async salvarProcesso(processo: any) {\r\n\r\n    this._view.loading(true);\r\n\r\n    await axios\r\n      .post(\"Processo\", processo)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 0);\r\n          this.carregarProcessos();\r\n\r\n        } else {\r\n          this._view.showMsg(\"Não cadastrar o novo processo.\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        //this._view.showMsg(e);\r\n      });\r\n\r\n  }\r\n\r\n\r\n\r\n}\r\n"],"sourceRoot":""}]}