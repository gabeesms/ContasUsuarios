ensTrocas = [];\r\n  itensEmprestimos: any = [];\r\n  itensParada: any = [];\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n  showItens(list: any): void {\r\n    this.itensTransferencias = [];\r\n    this.itensTrocas = [];\r\n    this.itensEmprestimos = [];\r\n    this.itensParada = [];\r\n\r\n    this.itensTransferencias = list.transferencia;\r\n    this.itensTrocas = list.troca;\r\n\r\n    list.emprestimo.forEach((element: any) => {\r\n      element.fdataInicio = moment(element.dataInicio).format(\"DD/MM/YYYY\");\r\n      element.fdataFinal = moment(element.dataFinal).format(\"DD/MM/YYYY\");\r\n      this.itensEmprestimos.push(element);\r\n    });\r\n\r\n    list.parada.forEach((element: any) => {\r\n      element.fhorario = moment(element.horario).format(\"HH:mm:ss\");\r\n      element.dadosProduto = element.codigoProduto.trim() + \" - \" + element.descricaoProduto.trim() \r\n      this.itensParada.push(element);\r\n    });\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    //this._controller!.deleteitem(this.itemFuncionarioDelete.id);\r\n  }\r\n\r\n  //APROVAR\r\n  editarItem(item: any) {\r\n\r\n    // transferencia\r\n    if (typeof item.idSolicitacaoTransferencia !== \"undefined\") {\r\n      this.$router.push(\"/LinhaProducaoTransferencia/\" + item.idSolicitacaoTransferencia);\r\n    }\r\n\r\n    // emprestimo\r\n    if (typeof item.idSolicitacaoEmprestimo !== \"undefined\") {\r\n      this.$router.push(\"/linhasolicitacaoemprestimo/\" + item.idSolicitacaoEmprestimo);\r\n    }\r\n\r\n\r\n\r\n  //   // parada\r\n  //   if (typeof item.idSolicitacaoParada !== \"undefined\") {\r\n  //     let usuario = JSON.parse(Storage.get(\"Colaborador\")!!);\r\n  //     let solicitacaoParadaAprovar = {\r\n  //       idSolicitacao: