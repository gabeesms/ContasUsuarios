r\n    }\r\n\r\n    carregarItens(): void {\r\n        this._view.loading(true)\r\n\r\n        this.carregarLinhas();\r\n\r\n\r\n        this._view.loading(false)\r\n\r\n    }\r\n\r\n    async carregarLinhas() {\r\n        this._view.loading(true)\r\n\r\n        axios\r\n            .get('Linha')\r\n            .then((response: any) => {\r\n                this._view.loading(false)\r\n                this._view.carregaLinha(response.data)\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n    }\r\n\r\n    validaChekin(frId: string): void {\r\n        this._view.loading(true)\r\n\r\n        axios.get('CheckinLinha/' + frId).then((response: any) => {\r\n            this._view.loading(false)\r\n            if (response.data) {\r\n                // this._view.showMsg(\"Salvo com sucesso.\")\r\n                this._view.validaDadosOperador(response.data)\r\n            } else {\r\n                this._view.showMsg(\"Funcionário não localizado.\")\r\n            }\r\n        }).catch((e: any) => {\r\n            this._view.loading(false)\r\n            this._view.showMsg(e)\r\n        })\r\n    }\r\n\r\n    salvar(dadosLinha: any): void {\r\n\r\n        this._view.loading(true);\r\n\r\n        axios\r\n            .post(\"LinhaCheckin\", dadosLinha)\r\n            .then((response: any) => {\r\n                this._view.loading(false);\r\n                if (response.data.sucesso) {\r\n                    this._view.showMsgSelecionaLinha(response.data.message);\r\n\r\n                } else {\r\n                    this._view.showMsg(\"Não foi possível salvar os dados de Abertura da linha para o Ckeck-in.\");\r\n                }\r\n            })\r\n            .catch((e: any) => {\r\n                this._view.loading(false);\r\n                this._view.showMsg(e);\r\n            });\r\n\r\n    }\r\n\r\n    salvarChekinOperador(checkinOperadorLinha: any): void{\r\n\r\n        this._view.loading(true);\r\n\r\n        axios\r\n            .post(\"CheckinLinha\", checkinOperadorLinha)\r\n            .then((response: any) => {\r\n                this._view.loading(false);\r\n                if (response.data.sucesso) {\r\n                    //this._view.showMsgSelecionaLinha(response.data.message);\r\n\r\n                } else {\r\n                    this._view.showMsg(\"Não foi possível salvar os dados de Abertura da linha para o Ckeck-in.\");\r\n                }\r\n            })\r\n            .catch((e: any) => {\r\n                this._view.loading(false);\r\n                this._view.showMsg(e);\r\n            });\r\n\r\n    }\r\n\r\n\r\n\r\n\r\n}"],"sourceRoot":""}]}