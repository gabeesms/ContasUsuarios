ecessidade: \"\",\r\n      um: \"\",\r\n      justificativa: \"\",\r\n      acao: [new HeaderAction(\"\", \"\", \"\", \"\", \"\")],\r\n    },\r\n  ];\r\n\r\n  constructor(view: IProdutoFormView) {\r\n    this._view = view;\r\n  }\r\n\r\n  carregarDtos(id: string): void {\r\n\r\n    this._view.loading(true);\r\n\r\n\r\n    if (id != undefined) {\r\n      //CARREGA EDITAR\r\n      axios\r\n        .get(\"ProdutoLocal/\" + id)\r\n        .then((response: any) => {\r\n          this._view.loading(false);\r\n          if (response.data) {\r\n            this._view.showProdutoForEditar(response.data);\r\n          }\r\n        })\r\n        .catch((e: any) => {\r\n          console.log(e);\r\n          this._view.loading(false);\r\n          this._view.showMsg(e, 0);\r\n        });\r\n    }else{\r\n      this._view.loading(false);\r\n    }\r\n  }\r\n\r\n\r\n  salvar(produto: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"ProdutoLocal\", produto)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Produto\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  update(idProduto: string, produto: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .put(\"ProdutoLocal/\" + idProduto, produto)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n        } else {\r\n          this._view.showMsg(response.data.message, 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n\r\n}\r\n"],"sourceRoot":""}]}