encia)\r\n        .then(async (response: any) => {\r\n          this._view.loading(false);\r\n          if (response.data) {\r\n            this._view.showBaseForTransferencia(response.data);\r\n            await this.carregarLinha();\r\n            await this.carregarOperadoresTransferencia()\r\n          }\r\n        })\r\n        .catch((e: any) => {\r\n          console.log(e);\r\n          this._view.loading(false);\r\n          this._view.showMsg(e, 0);\r\n        });\r\n    }\r\n  }\r\n\r\n\r\n  salvar(linhaSolicitacaoTransferencia: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"LinhaSolicitacaoTransferencia\", linhaSolicitacaoTransferencia)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Funcionário\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  update(idFuncionario: string, funcionario: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .put(\"FuncionarioLocal/\" + idFuncionario, funcionario)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n        } else {\r\n          this._view.showMsg(response.data.message, 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  async carregarLinha() {\r\n\r\n    this._view.loading(true);\r\n    //CARREGA EDITAR\r\n    await axios\r\n      .get(\"LinhaProducao\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaLinha(response.data);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  async carregarOperadoresTransferencia() {\r\n\r\n    this._view.loading(true);\r\n    //CARREGA EDITAR\r\n    await axios\r\n      .get(\"LinhaProducaoSolicitarTransferencia/operadoresTransferencia\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaOperadoresTransferencia(response.data);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n}\r\n"],"sourceRoot":""}]}