n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    this._controller!.deleteitem(this.itemPerfilDelete.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    this.$router.push(\"/perfil/\" + item.id);\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  headers = new Headers([\r\n    new Header(\r\n      \"Descrição do Perfil\",\r\n      \"perfil\",\r\n      false,\r\n      false,\r\n      \"left\",\r\n      true,\r\n      \"100\"\r\n    ),\r\n    new Header(\"Módulos\", \"modulo\", false, false, \"left\", true, \"200\"),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  itens = [];\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/perfil\");\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new PerfilListaController(this);\r\n\r\n    this._controller.carregarItens();\r\n  }\r\n}\r\n"],"sourceRoot":""}]}