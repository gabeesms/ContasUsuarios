];\r\n\r\n    schema.push(new ValidationSchema(\"codigo\", \"Código\", \"string\", true, 5));\r\n    schema.push(\r\n      new ValidationSchema(\"nome\", \"Nome do Processo\", \"string\", true, 30)\r\n    );\r\n\r\n    let result = new Validation().ValidSchema(schema, this.processo);\r\n    if (result.sucesso == false) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = result.mensagem;\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.processo.tipo == \"\") {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione o Tipo de Processo.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  limparCampos(): void {\r\n    this.processo.codigo = \"\";\r\n    this.processo.nome = \"\";\r\n    this.processo.tipo = \"\";\r\n  }\r\n\r\n  saveProcesso() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      \r\n      if (this.processo.tipo == \"O\") {\r\n        this.tab = 0;\r\n      }\r\n\r\n      if (this.processo.tipo == \"H\") {\r\n        this.tab = 1;\r\n      } \r\n      \r\n      if (this.processo.tipo == \"P\") {\r\n        this.tab = 2;\r\n      } \r\n      \r\n      this._controller!.salvar(this.processo);\r\n    }\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  showItens(listOperacoes: any, listHabilidade: any, listParadas: any): void {\r\n    this.itens = listHabilidade;\r\n    this.itensOperacoes = listOperacoes;\r\n    this.itensParadas = listParadas;\r\n  }\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemProcesso = item;\r\n    this.dialogDelete = true;\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  deleteItemConfirmado() {\r\n    this.dialogDelete = false;\r\n    this._controller!.deleteitem(this.itemProcesso.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    console.log(\"jhfsdikfkl\");\r\n    this.itemProcesso = item;\r\n    this.dialogAtivar = true;\r\n  }\r\n\r\n  ativarItem(item: any) {\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  ativarItemConfirmado() {\r\n    this.dialogAtivar = false;\r\n    this._controller!.ativaritem(this.itemProcesso.id);\r\n  }\r\n\r\n  isMobile = false;\r\n\r\n  headers = new Headers([\r\n    new Header(\"Código\", \"codigo\", false, false, \"center\", true, \"100\"),\r\n    new Header(\r\n      \"Descrição da Habilidade\",\r\n      \"nome\",\r\n      false,\r\n      false,\r\n      \"left\",\r\n      true,\r\n      \"200\"\r\n    ),\r\n    new Header(\"Status\", \"status\", false, false, \"left\", true, \"100\"),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  headersOperacoes = new Headers([\r\n    new Header(\"Código\", \"codigo\", false, false, \"center\", true, \"100\"),\r\n    new Header(\r\n      \"Descrição da Operação\",\r\n      \"nome\",\r\n      false,\r\n      false,\r\n      \"left\",\r\n      true,\r\n      \"200\"\r\n    ),\r\n    new Header(\"Status\", \"status\", false, false, \"left\", true, \"100\"),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  headersParadas = new Headers([\r\n    new Header(\"Código\", \"codigo\", false, false, \"center\", true, \"100\"),\r\n    new Header(\r\n      \"Descrição da Parada\",\r\n      \"nome\",\r\n      false,\r\n      false,\r\n      \"left\",\r\n      true,\r\n      \"200\"\r\n    ),\r\n    new Header(\"Status\", \"status\", false, false, \"left\", true, \"100\"),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"80\"),\r\n  ]);\r\n\r\n  itens = [];\r\n  itensOperacoes = [];\r\n  itensParadas = [];\r\n\r\n  addSolicitacao() {\r\n   // this.$router.push(\"/perfil\");\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new ProcessoListaFormController(this);\r\n\r\n    this._controller.carregarItens();\r\n  }\r\n}\r\n"],"sourceRoot":""}]}