,qBAAK,KAAL,CAAW,OAAX,CAAmB,IAAnB;;AACI,gBAAA,UAnHR,GAmH0B,EAnH1B;AAoHQ,gBAAA,KApHR,GAoHqB,EApHrB;AAAA;AAAA,uBAsHU,KAAK,CACR,GADG,CACC,gBADD,EAEH,IAFG,CAEE,UAAC,QAAD,EAAkB;AAEtB,kBAAA,QAAQ,CAAC,IAAT,CAAc,OAAd,CAAsB,UAAC,OAAD,EAAiB;AAErC,wBAAI,OAAO,CAAC,MAAR,IAAkB,GAAtB,EAA2B;AACzB,sBAAA,KAAK,CAAC,IAAN,CAAW,OAAX;AAED;AAEF,mBAPD;;AASA,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;AACD,iBAdG,EAcD,KAdC,CAcK,UAAC,CAAD,EAAW;AAClB,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;AAED,iBAjBG,CAtHV;;AAAA;AAAA;AAAA,uBAyIY,KAAK,CACV,GADK,CACD,qBADC,EAEL,IAFK,CAEA,UAAC,QAAD,EAAkB;AAEtB,kBAAA,QAAQ,CAAC,IAAT,CAAc,OAAd,CAAsB,UAAC,OAAD,EAAiB;AAErC,wBAAI,OAAO,CAAC,MAAR,IAAkB,GAAtB,EAA2B;AACzB,sBAAA,UAAU,CAAC,IAAX,CAAgB,OAAhB;AAED;AAEF,mBAPD;;AASA,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;;AACA,kBAAA,MAAI,CAAC,KAAL,CAAW,gBAAX,CAA4B,KAA5B,EAAmC,UAAnC;AACD,iBAfK,EAeH,KAfG,CAeG,UAAC,CAAD,EAAW;AAClB,kBAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;AAED,iBAlBK,CAzIZ;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA","sourcesContent":["import {\r\n  ILinhaProducaoFormView,\r\n  ILinhaProducaoFormController,\r\n} from \"../../contracts/Cadastro/LinhaProducaoFormContract\";\r\nimport axios from \"../../plugins/axios\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nexport class LinhaProducaoFormController implements ILinhaProducaoFormController {\r\n  _view: ILinhaProducaoFormView;\r\n\r\n  _lista = [\r\n    {\r\n      numero: \"\",\r\n      produto: \"\",\r\n      descricao: \"\",\r\n      quantidade: \"\",\r\n      dataNecessidade: \"\",\r\n      um: \"\",\r\n      justificativa: \"\",\r\n      acao: [new HeaderAction(\"\", \"\", \"\", \"\", \"\")],\r\n    },\r\n  ];\r\n\r\n  constructor(view: ILinhaProducaoFormView) {\r\n    this._view = view;\r\n  }\r\n\r\n  carregarDtos(id: string): void {\r\n\r\n    this._view.loading(true);\r\n    this.carregarProcessos();\r\n    this.carregarFuncionarioLocalLinhaProducao();\r\n\r\n    if (id != undefined) {\r\n\r\n      //VERIFICA SE JA EXISTE CENTRO DE CUSTO CADASTRADO\r\n      axios\r\n        .get(\"LinhaProducao/\" + id)\r\n        .then((response: any) => {\r\n          this._view.loading(false);\r\n          if (response.data) {\r\n\r\n            //this.carregarCategoriaSetores();\r\n            this._view.showLinhaProducaoForEditar(response.data);\r\n          }\r\n        })\r\n        .catch((e: any) => {\r\n          console.log(e);\r\n          this._view.loading(false);\r\n          this._view.showMsg(e, 0);\r\n        });\r\n\r\n\r\n    } else {\r\n      this._view.loading(false);\r\n    }\r\n  }\r\n\r\n  salvar(linhaProducao: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"LinhaProducao\", linhaProducao)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Funcionário\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  update(id: string, centroCusto: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .put(\"LinhaProducao/\" + id, centroCusto)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n        } else {\r\n          this._view.showMsg(response.data.message, 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  async carregarFuncionarioLocalLinhaProducao(){\r\n\r\n    this._view.loading(true);\r\n\r\n    await axios\r\n      .get(\"FuncionarioLocal/funcionarioLocalLinhaProducao\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaFuncionarioLocalLinhaProducao(response.data)\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  async carregarProcessos() {\r\n    this._view.loading(true)\r\n    let categorias: any = [];\r\n    let fases: any = [];\r\n\r\n    await axios\r\n      .get('Processo/fases')\r\n      .then((response: any) => {\r\n\r\n        response.data.forEach((element: any) => {\r\n\r\n          if (element.status == 'A') {\r\n            fases.push(element);\r\n\r\n          }\r\n\r\n        });\r\n\r\n        this._view.loading(false)\r\n      }).catch((e: any) => {\r\n        this._view.loading(false)\r\n\r\n      })\r\n\r\n      await axios\r\n      .get('Processo/categorias')\r\n      .then((response: any) => {\r\n\r\n        response.data.forEach((element: any) => {\r\n\r\n          if (element.status == 'A') {\r\n            categorias.push(element);\r\n\r\n          }\r\n\r\n        });\r\n\r\n        this._view.loading(false)\r\n        this._view.carregaProcessos(fases, categorias)\r\n      }).catch((e: any) => {\r\n        this._view.loading(false)\r\n\r\n      })\r\n  }\r\n\r\n\r\n\r\n\r\n\r\n}\r\n"],"sourceRoot":""}]}