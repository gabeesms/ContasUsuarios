AAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,KAAnB;;AACA,QAAA,MAAI,CAAC,KAAL,CAAW,OAAX,CAAmB,CAAnB;AACH,OAdL;AAgBH;AAlHL;;AAAA;AAAA","sourcesContent":["import { ICheckinLinhaView, ICheckinLinhaController } from \"../../contracts/Checkin/CheckinLinhaContract\";\r\nimport axios from \"../../plugins/axios\"\r\n\r\nimport { Storage } from \"../../configuration/storage\"\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\n\r\nexport class CheckinLinhaController implements ICheckinLinhaController {\r\n\r\n    _view: ICheckinLinhaView\r\n\r\n    constructor(view: ICheckinLinhaView) {\r\n        this._view = view\r\n    }\r\n\r\n    async carregarItens(): Promise<void> {\r\n        this._view.loading(true)\r\n\r\n        await this.carregarLinhas();\r\n\r\n        //await this.carregarLinhaProducaoProdutos();\r\n\r\n\r\n        this._view.loading(false)\r\n\r\n    }\r\n\r\n    async carregarLinhas() {\r\n        this._view.loading(true)\r\n        let linhas: any = [];\r\n        axios\r\n            .get('LinhaProducao')\r\n            .then((response: any) => {\r\n                this._view.loading(false)\r\n\r\n                response.data.forEach((element: any) => {\r\n\r\n                    if (element.deletado == \"N\") {\r\n                        linhas.push(element);\r\n                    }\r\n\r\n                });\r\n                this._view.carregaLinha(linhas)\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n    }\r\n\r\n    async carregarLinhaProducaoProdutos() {\r\n        this._view.loading(true)\r\n        axios\r\n            .get('LinhaProducao/linhaProdutos')\r\n            .then((response: any) => {\r\n                this._view.loading(false)\r\n                this._view.carregaLinhaProducaoProdutos(response.data)\r\n            }).catch((e: any) => {\r\n                this._view.loading(false)\r\n                this._view.showMsg(e)\r\n            })\r\n    }\r\n\r\n    validaChekin(frId: string): void {\r\n        this._view.loading(true)\r\n\r\n        axios.get('CheckinLinha/' + frId).then((response: any) => {\r\n            this._view.loading(false)\r\n            if (response.data) {\r\n                // this._view.showMsg(\"Salvo com sucesso.\")\r\n                this._view.validaDadosOperador(response.data)\r\n            } else {\r\n                this._view.showMsg(\"Funcionário não localizado.\")\r\n            }\r\n        }).catch((e: any) => {\r\n            this._view.loading(false)\r\n            this._view.showMsg(e)\r\n        })\r\n    }\r\n\r\n    salvar(dadosLinha: any): void {\r\n\r\n        this._view.loading(true);\r\n\r\n        axios\r\n            .post(\"LinhaCheckin\", dadosLinha)\r\n            .then((response: any) => {\r\n                this._view.loading(false);\r\n                if (response.data.sucesso) {\r\n                    this._view.retornoLinhaCheckin(response.data);\r\n\r\n                } else {\r\n                    this._view.showMsg(\"Não foi possível salvar os dados de Abertura da linha para o Ckeck-in.\");\r\n                }\r\n            })\r\n            .catch((e: any) => {\r\n                this._view.loading(false);\r\n                this._view.showMsg(e);\r\n            });\r\n\r\n    }\r\n\r\n    salvarChekinOperador(checkinOperadorLinha: any): void {\r\n\r\n        this._view.loading(true);\r\n\r\n        axios\r\n            .post(\"CheckinLinha\", checkinOperadorLinha)\r\n            .then((response: any) => {\r\n                this._view.loading(false);\r\n                if (response.data.sucesso) {\r\n                    //this._view.showMsgSelecionaLinha(response.data.message);\r\n\r\n                } else {\r\n                    this._view.showMsg(\"Não foi possível salvar os dados de Abertura da linha para o Ckeck-in.\");\r\n                }\r\n            })\r\n            .catch((e: any) => {\r\n                this._view.loading(false);\r\n                this._view.showMsg(e);\r\n            });\r\n\r\n    }\r\n\r\n\r\n\r\n\r\n}"],"sourceRoot":""}]}