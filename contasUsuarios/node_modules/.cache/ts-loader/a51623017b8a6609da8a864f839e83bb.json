rFormController,\r\n} from \"../../contracts/Cadastro/OperadorFormContract\";\r\nimport axios from \"../../plugins/axios\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nexport class OperadorFormController implements IOperadorFormController {\r\n  _view: IOperadorFormView;\r\n\r\n  _lista = [\r\n    {\r\n      numero: \"\",\r\n      produto: \"\",\r\n      descricao: \"\",\r\n      quantidade: \"\",\r\n      dataNecessidade: \"\",\r\n      um: \"\",\r\n      justificativa: \"\",\r\n      acao: [new HeaderAction(\"\", \"\", \"\", \"\", \"\")],\r\n    },\r\n  ];\r\n\r\n  constructor(view: IOperadorFormView) {\r\n    this._view = view;\r\n  }\r\n\r\n  carregarDtos(id: string, idFuncionario: string): void {\r\n\r\n    this._view.loading(true);\r\n    this.carregarOperadorNivel();\r\n  \r\n    if (id != undefined) {\r\n      //CARREGA EDITAR\r\n      axios\r\n        .get(\"operador/\" + id)\r\n        .then((response: any) => {\r\n          this._view.loading(false);\r\n          if (response.data) {\r\n            this.carregarProcessosNovos(response.data);\r\n          }\r\n        })\r\n        .catch((e: any) => {\r\n          console.log(e);\r\n          this._view.loading(false);\r\n          this._view.showMsg(e, 0);\r\n        });\r\n    } else {\r\n      this.carregarProcessosAtivos();\r\n      this.carregarFuncionario(idFuncionario);\r\n\r\n    }\r\n  }\r\n\r\n\r\n  salvar(operador: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"Operador\", operador)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Operador\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  update(idOperador: string, operador: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .put(\"Operador/\" + idOperador, operador)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n        } else {\r\n          this._view.showMsg(response.data.message, 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n\r\n  carregarProcessos(): void {\r\n    axios\r\n      .get(\"Processo\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaProcessos(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  carregarProcessosAtivos(): void {\r\n    axios\r\n      .get(\"Processo/processosHabilidadesAtivas\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaProcessos(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  carregarProcessosNovos(itemsRest: any): void {\r\n    axios\r\n      .get(\"Processo/habilidades\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.showOperadorForEditar(itemsRest,response.data);\r\n         \r\n        } \r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  \r\n\r\n  carregarFuncionario(id: string): void {\r\n    axios\r\n      .get(\"FuncionarioLocal/funcionariooperador/\" + id)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaDadosFuncionario(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  carregarOperadorNivel(): void {\r\n    axios\r\n      .get(\"OperadorNivel\")\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n          this._view.carregaOperadorNivel(response.data);\r\n        } else {\r\n          this._view.showMsg(\"Não encontrado. \", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n  }\r\n\r\n  \r\n\r\n\r\n}\r\n"],"sourceRoot":""}]}