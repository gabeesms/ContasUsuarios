log,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class PerfilForm extends Vue implements IPerfilFormView {\r\n  _controller?: IPerfilFormController;\r\n\r\n  //validate form\r\n  valid = true;\r\n  lazy = false;\r\n\r\n  dialogDelete = false;\r\n  itemPerfilDelete: any;\r\n\r\n  isMobile = false;\r\n\r\n  isSucesso = false;\r\n\r\n  optMenu = \"1\";\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  itens = [\r\n    {\r\n      modulo: \"\",\r\n      perfilPermissao: [\r\n        {\r\n          descricao: \"\",\r\n          aprovar: false,\r\n          visualizar: false,\r\n          editar: false,\r\n        },\r\n      ],\r\n    },\r\n  ];\r\n  itensRH = this.itens;\r\n  itensProd = this.itens;\r\n\r\n  perfil = {\r\n    id: \"\",\r\n    descricao: \"\",\r\n    menus: this.itens,\r\n  };\r\n\r\n  headers = new Headers([\r\n    new Header(\r\n      \"Descrição do Menu\",\r\n      \"descricao\",\r\n      false,\r\n      false,\r\n      \"left\",\r\n      false,\r\n      \"250\"\r\n    ),\r\n    new Header(\r\n      \"Opções de Acessos\",\r\n      \"perfilPermissao\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      false,\r\n      \"40\"\r\n    ),\r\n  ]);\r\n\r\n  idPerfil = \"\";\r\n  // checkboxRH = false;\r\n  // checkboxProd = falthis.itens = 1;\r\n  search = \"\";\r\n\r\n  //SNACKBAR\r\n  snackbar = false;\r\n  text = \"\";\r\n  vertical = true;\r\n  color = \"\";\r\n  colorBorder = \"\";\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Perfis\",\r\n      disabled: false,\r\n      href: \"/perfils\",\r\n    },\r\n  ];\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  deleteItem(item: any) {\r\n    this.itemPerfilDelete = item;\r\n    this.dialogDelete = true;\r\n    //this._controller!.deleteitem(item.id);\r\n  }\r\n\r\n  editarItem(item: any) {\r\n    //this.$router.push(\"/Perfil/\" + item.id + \"/\" + item.matricula);\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/perfils\");\r\n    }\r\n  }\r\n\r\n  retornaListaPerfil() {\r\n    this.$router.push(\"/perfils\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    console.log(\"TESTE MENSAGEM\");\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validate() {\r\n    if (this.perfil.descricao == \"\") {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"O campo Descrição do Perfil está vazio ou inválido.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  showPerfilForEditar(item: any): void {\r\n    this.itensRH = [];\r\n    this.itensProd = [];\r\n\r\n    this.perfil.descricao = item.descricao;\r\n\r\n    item.menus.forEach((element: any) => {\r\n      if (element.modulo == \"RH\") {\r\n        this.itensRH.push(element);\r\n      }\r\n\r\n      if (element.modulo == \"PRO\") {\r\n        this.itensProd.push(element);\r\n      }\r\n    });\r\n\r\n    //throw new Error(\"Method not implemented.\");\r\n  }\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/perfil\");\r\n  }\r\n\r\n  updatePerfil() {\r\n    if (this.validate()) {\r\n    } else {\r\n      this.perfil.menus = [];\r\n      this.perfil.id = this.idPerfil;\r\n      this.itensRH.forEach((element) => {\r\n        this.perfil.menus.push(element);\r\n      });\r\n\r\n      this.itensProd.forEach((element) => {\r\n        this.perfil.menus.push(element);\r\n      });\r\n\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.update(this.idPerfil, this.perfil);\r\n      //this.retornaListaPerfil();\r\n    }\r\n  }\r\n\r\n  savePerfil() {\r\n    if (this.validate()) {\r\n    } else {\r\n      this.perfil.menus = [];\r\n      this.itensRH.forEach((element) => {\r\n        this.perfil.menus.push(element);\r\n      });\r\n\r\n      this.itensProd.forEach((element) => {\r\n        this.perfil.menus.push(element);\r\n      });\r\n\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.salvar(this.perfil);\r\n      //this.retornaListaPerfil();\r\n    }\r\n  }\r\n\r\n  carregaMenus(item: any) {\r\n    this.itens = [];\r\n    this.itensRH = [];\r\n    this.itensProd = [];\r\n    this.itens = item;\r\n\r\n    this.itens.forEach((element: any) => {\r\n      element.perfilPermissao = {};\r\n      element.perfilPermissao.aprovar = false;\r\n      element.perfilPermissao.visualizar = false;\r\n      element.perfilPermissao.editar = false;\r\n      if (element.modulo == \"RH\") {\r\n        this.itensRH.push(element);\r\n      }\r\n      if (element.modulo == \"PRO\") {\r\n        this.itensProd.push(element);\r\n      }\r\n    });\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.idPerfil = this.$route.params.id;\r\n\r\n    if (this.$route.params.id != undefined) {\r\n    }\r\n  }\r\n\r\n  created() {\r\n    this.optMenu = \"1\";\r\n    this._controller = new PerfilFormController(this);\r\n    this.carregarDtos();\r\n    this._controller.carregarDtos(this.$route.params.id);\r\n\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n"],"sourceRoot":""}]}