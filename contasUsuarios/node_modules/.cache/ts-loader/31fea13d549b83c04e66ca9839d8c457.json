fase: this.cadCapacidadeLinha.fase,\r\n      operacao: this.cadCapacidadeLinha.operacao,\r\n      habilidades: [],\r\n    };\r\n  }\r\n\r\n  carregaProcessos(itemsOperacao: any, itemsHabilidade: any): void {\r\n    this.itensOperacaoes = itemsOperacao;\r\n    this.itensHabilidades = itemsHabilidade;\r\n  }\r\n\r\n  formatarSegundos(h: any, min: any, s: any) {\r\n    return parseFloat(h * 3600 + min * 60 + s);\r\n  }\r\n\r\n  \r\n  dataFormatSoma(s: any) {\r\n    let h = Math.floor(s / 3600);\r\n    let min = Math.floor((s - h * 3600) / 60);\r\n    let se = s - Math.floor(s / 60) * 60;\r\n\r\n    let hora = \"\";\r\n    let minuto = \"\";\r\n    let segundo = \"\";\r\n    if (h < 10) {\r\n      hora = \"0\" + h.toString();\r\n    }\r\n\r\n    if (min < 10) {\r\n      minuto = \"0\" + min.toString();\r\n    }\r\n\r\n    if (se < 10) {\r\n      segundo = \"0\" + se.toString();\r\n    }\r\n\r\n    return hora + \":\" + minuto + \":\" + segundo;\r\n  }\r\n\r\n  somaQtdeOperadoresCapacidadeProdutiva() {\r\n    // let teste1 = new Date(\r\n    //   \"Wed Oct 07 2020 00:00:50 GMT-0400 (Horário Padrão do Amazonas)\"\r\n    // );\r\n    // let teste2 = new Date(\r\n    //   \"Wed Oct 07 2020 00:01:10 GMT-0400 (Horário Padrão do Amazonas)\"\r\n    // );\r\n\r\n    // console.log(\r\n    //   this.dataFormatSoma(\r\n    //     parseFloat(\r\n    //       this.formatarSegundos(\r\n    //         teste1.getHours(),\r\n    //         teste1.getMinutes(),\r\n    //         teste1.getSeconds()\r\n    //       )\r\n    //     ) +\r\n    //       parseFloat(\r\n    //         this.formatarSegundos(\r\n    //           teste2.getHours(),\r\n    //           teste2.getMinutes(),\r\n    //           teste2.getSeconds()\r\n    //         )\r\n    //       )\r\n    //   )\r\n    // );\r\n\r\n    //console.log(this.formatarSegundos())\r\n\r\n    let somaLinhaQtdeOepradores = 0;\r\n    let somaLinhaTime = 0;\r\n\r\n    //CAPACIDADE LINHA\r\n    this.itensCapacidadeLinha.forEach((elementCapLinha: any) => {\r\n      //HABILIDADE CAPACIDADE LINHA\r\n      elementCapLinha.linhaCapacidadeHabilidades.forEach(\r\n        (elementCapLinhaHabilidade: any) => {\r\n          //SOMA QTDE OPERADORES\r\n          if (elementCapLinhaHabilidade.qtdeOperadores != \"\") {\r\n            somaLinhaQtdeOepradores += parseFloat(\r\n              elementCapLinhaHabilidade.qtdeOperadores\r\n            );\r\n          }\r\n          this.linha.qtdeOperador = somaLinhaQtdeOepradores;\r\n\r\n          if (elementCapLinhaHabilidade.time != \"\") {\r\n            let time = new Date(elementCapLinhaHabilidade.time)\r\n            console.log(elementCapLinhaHabilidade.time)\r\n\r\n\r\n            somaLinhaTime += (\r\n              this.formatarSegundos(\r\n                time.getHours(),\r\n                time.getMinutes(),\r\n                time.getSeconds()\r\n              )\r\n            );\r\n\r\n          }\r\n\r\n         console.log(somaLinhaTime)\r\n          \r\n         this.linha.capacidadeProdutiva = this.dataFormatSoma(somaLinhaTime);\r\n        }\r\n      );\r\n    });\r\n\r\n    //console.log(this.linha.capacidadeProdutiva)\r\n   this.linha.qtdeOperador = somaLinhaQtdeOepradores;\r\n    this.linha.capacidadeProdutiva = this.dataFormatSoma(somaLinhaTime);\r\n  }\r\n\r\n  carregaCapacidadeLinha(itemsCapacidadeLinha: any): void {\r\n    this.itensCapacidadeLinha = itemsCapacidadeLinha;\r\n\r\n   \r\n    //let somaLinhaQtdeOepradores = 0;\r\n    //CAPACIDADE LINHA\r\n    this.itensCapacidadeLinha.forEach((elementCapLinha: any) => {\r\n      //HABILIDADE CAPACIDADE LINHA\r\n      elementCapLinha.linhaCapacidadeHabilidades.forEach(\r\n        (elementCapLinhaHabilidade: any) => {\r\n          // elementCapLinhaHabilidade.time2 = new Date(\r\n          //   elementCapLinhaHabilidade.time\r\n          // );\r\n          elementCapLinhaHabilidade.time = new Date(\r\n            elementCapLinhaHabilidade.time\r\n          )\r\n            //.toLocaleTimeString()\r\n            //.toString();\r\n\r\n          //somaLinhaQtdeOepradores =\r\n           // somaLinhaQtdeOepradores + elementCapLinhaHabilidade.qtdeOperadores;\r\n        }\r\n      );\r\n    });\r\n\r\n    //this.linha.qtdeOperador = somaLinhaQtdeOepradores;\r\n\r\n    //console.log(this.itensCapacidadeLinha);\r\n\r\n     this.somaQtdeOperadoresCapacidadeProdutiva();\r\n\r\n  }\r\n\r\n  addCapacidadeLinha() {\r\n    this.itensCadCapacidadeLinha = [];\r\n    this.cadCapacidadeLinha = {\r\n      fase: \"\",\r\n      operacao: [],\r\n      habilidades: [],\r\n    };\r\n    this.dialog = true;\r\n  }\r\n\r\n  apenasNumeros(event: any) {\r\n    validaNumeros(event);\r\n\r\n    this.somaQtdeOperadoresCapacidadeProdutiva();\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n    if (this.isSucesso) {\r\n      this.$router.push(\"/Linhas\");\r\n    }\r\n  }\r\n\r\n  retornaListaLinhas() {\r\n    this.$router.push(\"/Linhas\");\r\n  }\r\n\r\n  showMsg(msg: string, modo: number): void {\r\n    this.textError = msg;\r\n    if (modo == 1) {\r\n      this.isSucesso = true;\r\n    }\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  validateSaveCapacidadeLinha() {\r\n    let retorno = false;\r\n\r\n    if (this.itensCadCapacidadeLinha.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Sem Operações e Habilidades selecionadas para salvar dados.\";\r\n      this.snackbar = true;\r\n      retorno = true;\r\n    }\r\n\r\n    this.itensCadCapacidadeLinha.forEach((element: any) => {\r\n      if (\r\n        parseInt(element.qtdeOperador) == 0 ||\r\n        parseInt(element.qtdeOperador) == null ||\r\n        parseInt(element.qtdeOperador) < 0 ||\r\n        element.qtdeOperador == \"\"\r\n      ) {\r\n        this.colorBorder = \"error\";\r\n        this.color = \"error\";\r\n        this.text =\r\n          \"O campo quantidade de operadores da habilidade \" +\r\n          element.nomeHabilidade +\r\n          \" não pode ser 0 (zero) ou inferior a 0 (zero).\";\r\n        this.snackbar = true;\r\n        retorno = true;\r\n      }\r\n\r\n      if (element.time == \"00:00\" || element.time == null) {\r\n        this.colorBorder = \"error\";\r\n        this.color = \"error\";\r\n        this.text =\r\n          \"O campo Time da habilidade \" +\r\n          element.nomeHabilidade +\r\n          \" não pode ser 00:00\";\r\n        this.snackbar = true;\r\n        retorno = true;\r\n      }\r\n    });\r\n    return retorno;\r\n  }\r\n\r\n  validateCapacidadeLinha() {\r\n    let schema: ValidationSchema[] = [];\r\n\r\n    schema.push(new ValidationSchema(\"fase\", \"Fase\", \"string\", true, 30));\r\n\r\n    let result = new Validation().ValidSchema(schema, this.cadCapacidadeLinha);\r\n    if (result.sucesso == false) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = result.mensagem;\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.cadCapacidadeLinha.operacao.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a Operação.\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    if (this.cadCapacidadeLinha.habilidades.length == 0) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = \"Selecione a/as Habilidade(s)\";\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  validate() {\r\n    let schema: ValidationSchema[] = [];\r\n\r\n    schema.push(new ValidationSchema(\"nome\", \"Nome\", \"string\", true, 30));\r\n    schema.push(\r\n      new ValidationSchema(\r\n        \"capacidadeOperador\",\r\n        \"Capacidade do Operador\",\r\n        \"string\",\r\n        true,\r\n        15\r\n      )\r\n    );\r\n\r\n    let result = new Validation().ValidSchema(schema, this.linha);\r\n    if (result.sucesso == false) {\r\n      this.colorBorder = \"error\";\r\n      this.color = \"error\";\r\n      this.text = result.mensagem;\r\n      this.snackbar = true;\r\n      return true;\r\n    }\r\n\r\n    return false;\r\n  }\r\n\r\n  saveLinha() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.salvar(this.linha);\r\n    }\r\n  }\r\n\r\n  updateLinha() {\r\n    if (this.validate()) {\r\n    } else {\r\n      //this.solicitacao.user = parseInt(Storage.get(\"User\")!);\r\n      this._controller!.update(this.idLinha, this.linha);\r\n    }\r\n  }\r\n\r\n  carregarDtos(): void {\r\n    this.idLinha = this.$route.params.id;\r\n  }\r\n\r\n  showLinhaForEditar(item: any) {\r\n    //CARREGANDO DADOS DO FUNCIONARIO\r\n    this.linha = item;\r\n  }\r\n\r\n  created() {\r\n    this._controller = new LinhaFormController(this);\r\n    this._controller.carregarDtos(this.$route.params.id);\r\n    this.carregarDtos();\r\n    this.isMobile = Display.isMobile();\r\n  }\r\n}\r\n"],"sourceRoot":""}]}