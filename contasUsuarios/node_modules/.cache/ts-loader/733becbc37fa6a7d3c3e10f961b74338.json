AAL,CAAW,OAAX,CAAmB,KAAnB,EADgB,CAEhB;;AACD,iBAfE,CAhPT;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA","sourcesContent":["import {\r\n  IProdutoFormView,\r\n  IProdutoFormController,\r\n} from \"../../contracts/Cadastro/ProdutoFormContract\";\r\nimport axios from \"../../plugins/axios\";\r\n\r\nimport { Header, HeaderIcon, HeaderAction } from \"../../models/header\";\r\nimport { Headers } from \"../../models/headers\";\r\n\r\nexport class ProdutoFormController implements IProdutoFormController {\r\n  _view: IProdutoFormView;\r\n\r\n  _lista = [\r\n    {\r\n      numero: \"\",\r\n      produto: \"\",\r\n      descricao: \"\",\r\n      quantidade: \"\",\r\n      dataNecessidade: \"\",\r\n      um: \"\",\r\n      justificativa: \"\",\r\n      acao: [new HeaderAction(\"\", \"\", \"\", \"\", \"\")],\r\n    },\r\n  ];\r\n\r\n  constructor(view: IProdutoFormView) {\r\n    this._view = view;\r\n  }\r\n\r\n  async carregarDtos(id: string) {\r\n\r\n    this._view.loading(true);\r\n    await this.carregarProcessos();\r\n\r\n\r\n    if (id != undefined) {\r\n      //CARREGA EDITAR\r\n      await axios\r\n        .get(\"ProdutoLocal/\" + id)\r\n        .then((response: any) => {\r\n          this._view.loading(false);\r\n          if (response.data) {\r\n            this._view.showProdutoForEditar(response.data);\r\n          }\r\n        })\r\n        .catch((e: any) => {\r\n          console.log(e);\r\n          this._view.loading(false);\r\n          this._view.showMsg(e, 0);\r\n        });\r\n\r\n      await this.carregarCapacidadeProduto(id);\r\n    } else {\r\n      this._view.loading(false);\r\n    }\r\n  }\r\n\r\n\r\n  salvar(produto: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"ProdutoLocal\", produto)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Produto\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  salvarCapacidadeLinha(capacidadeProduto: any, idProduto: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"ProdutoCapacidade\", capacidadeProduto)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 0);\r\n          this.carregarCapacidadeProduto(idProduto);\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Linha\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  update(idProduto: string, produto: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .put(\"ProdutoLocal/\" + idProduto, produto)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 1);\r\n        } else {\r\n          this._view.showMsg(response.data.message, 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  carregarCapacidadeProduto(idProduto: any) {\r\n\r\n    this._view.loading(true);\r\n    //CARREGA EDITAR\r\n    axios\r\n      .get(\"ProdutoCapacidade/\" + idProduto)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data) {\r\n\r\n\r\n\r\n          this._view.carregaCapacidadeProduto(response.data);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        console.log(e);\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  salvarCapacidadeProduto(capacidadeLinha: any, idProduto: any): void {\r\n\r\n    this._view.loading(true);\r\n\r\n    axios\r\n      .post(\"ProdutoCapacidade\", capacidadeLinha)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n          this._view.showMsg(response.data.message, 0);\r\n          this.carregarCapacidadeProduto(idProduto);\r\n        } else {\r\n          this._view.showMsg(\"Não foi possível salvar os dados do Linha\", 0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        this._view.showMsg(e, 0);\r\n      });\r\n\r\n  }\r\n\r\n  async carregarProcessos() {\r\n    this._view.loading(true)\r\n    let operacoes: any = [];\r\n    await axios\r\n      .get('Processo/operacoes')\r\n      .then((response: any) => {\r\n\r\n        response.data.forEach((element: any) => {\r\n\r\n          if (element.status == 'A') {\r\n            element.acoes = [\r\n              new HeaderAction(\"mdi-cancel\", \"primary\", \"Teste\", \"deleteItem\", \"200px\")\r\n            ]\r\n          }\r\n\r\n          if (element.status == 'I') {\r\n            element.acoes = [\r\n              new HeaderAction(\"mdi-cancel\", \"error\", \"Teste\", \"editarItem\", \"200px\")\r\n            ]\r\n          }\r\n\r\n\r\n        });\r\n\r\n        operacoes = response.data;\r\n        //this._view.showItens(response.data)\r\n      }).catch((e: any) => {\r\n        this._view.loading(false)\r\n\r\n      })\r\n\r\n    await axios\r\n      .get('Processo/habilidades')\r\n      .then((response: any) => {\r\n\r\n        response.data.forEach((element: any) => {\r\n\r\n          if (element.status == 'A') {\r\n            element.acoes = [\r\n              new HeaderAction(\"mdi-cancel\", \"primary\", \"Teste\", \"deleteItem\", \"200px\")\r\n            ]\r\n          }\r\n\r\n          if (element.status == 'I') {\r\n            element.acoes = [\r\n              new HeaderAction(\"mdi-cancel\", \"error\", \"Teste\", \"editarItem\", \"200px\")\r\n            ]\r\n          }\r\n\r\n\r\n        });\r\n\r\n        this._view.loading(false)\r\n        this._view.carregaProcessos(operacoes, response.data)\r\n      }).catch((e: any) => {\r\n        this._view.loading(false)\r\n\r\n      })\r\n  }\r\n\r\n  deleteProdutoCapacidadeHabilidadeItem(id: number, idProduto: any) {\r\n    this._view.loading(true)\r\n\r\n    axios.delete('ProdutoCapacidade/produtoCapacidadeHabilidade/' + id).then((response: any) => {\r\n      this._view.loading(false)\r\n      if (response.data.sucesso) {\r\n        // this._view.showMsg(\"Salvo com sucesso.\")\r\n        //this._view.showMsg(response.data.message, 0);\r\n        this.carregarCapacidadeProduto(idProduto);\r\n      }\r\n    }).catch((e: any) => {\r\n      console.log(e);\r\n      this._view.loading(false);\r\n      this._view.showMsg(e, 0);\r\n    })\r\n  }\r\n\r\n  async salvarProcesso(processo: any) {\r\n\r\n    this._view.loading(true);\r\n\r\n   await axios\r\n      .post(\"Processo\", processo)\r\n      .then((response: any) => {\r\n        this._view.loading(false);\r\n        if (response.data.sucesso) {\r\n         this._view.showMsg(response.data.message,0);\r\n         this.carregarProcessos();\r\n\r\n        } else {\r\n          this._view.showMsg(\"Não cadastrar o novo processo.\",0);\r\n        }\r\n      })\r\n      .catch((e: any) => {\r\n        this._view.loading(false);\r\n        //this._view.showMsg(e);\r\n      });\r\n\r\n  }\r\n\r\n\r\n\r\n\r\n}\r\n"],"sourceRoot":""}]}