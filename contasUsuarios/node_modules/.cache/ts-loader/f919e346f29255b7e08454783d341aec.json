ponent from \"vue-class-component\";\r\n\r\n@Component({\r\n  components: {\r\n    KLoading,\r\n    KDialog,\r\n    KDataList,\r\n  },\r\n})\r\nexport default class OrdemProducaoListaSimulacao\r\n  extends Vue\r\n  implements IOrdemProducaoListaSimulacaoView {\r\n  _controller?: IOrdemProducaoListaSimulacaoController;\r\n\r\n  dialogLoading = false;\r\n  dialogMessage = false;\r\n  textError = \"\";\r\n\r\n  search = \"\";\r\n\r\n  dialogAddLinha = false;\r\n\r\n  dialogConsultaStatusOP = false;\r\n  itemFuncionarioDelete: any;\r\n\r\n  isMobile = false;\r\n\r\n  itens: any = [];\r\n\r\n  headers = new Headers([\r\n     new Header(\r\n      \"Id\",\r\n      \"id\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      false,\r\n      \"80\"\r\n    ),\r\n    new Header(\"Status\", \"status\", false, false, \"center\", false, \"100\"),\r\n    new Header(\r\n      \"Nº Ordem Produção\",\r\n      \"numOrdemProducao\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      false,\r\n      \"150\"\r\n    ),\r\n   \r\n    new Header(\"Produção\", \"producao\", false, false, \"center\", false, \"100\"),\r\n    new Header(\"Produto\", \"codigo\", false, false, \"center\", false, \"80\"),\r\n    new Header(\"Linha\", \"nome\", false, false, \"center\", true, \"200\"),\r\n    new Header(\r\n      \"Operadores na Linha\",\r\n      \"quantidadeOperador\",\r\n      false,\r\n      false,\r\n      \"center\",\r\n      false,\r\n      \"100\"\r\n    ),\r\n    new Header(\"Turno\", \"nomeTurno\", false, false, \"center\", false, \"200\"),\r\n    new Header(\"Ações\", \"acoes\", true, false, \"center\", false, \"100\"),\r\n  ]);\r\n  textChecagemOrdemProducao = \"\";\r\n  itemsLinha: any = [];\r\n  linhaSelecionada: any = {};\r\n  itemSelecionado: any = {};\r\n\r\n  idOrdemProducaoPlano = 0;\r\n  idOrdemProducaoPlanoAlocada = 0;\r\n\r\n  caminho = [\r\n    {\r\n      text: \"Home\",\r\n      disabled: false,\r\n      href: \"/\",\r\n    },\r\n    {\r\n      text: \"Nova Ordem de Produção\",\r\n      disabled: false,\r\n      href: \"/ordemProducao\",\r\n    },\r\n  ];\r\n\r\n  relacionarLinha(item: any) {\r\n    this.dialogAddLinha = true;\r\n    this.itemSelecionado = item;\r\n    //console.log(this.itemSelecionado);\r\n  }\r\n\r\n  saveAddLinhaOrdemProducao() {\r\n    //console.log(this.linhaSelecionada);\r\n\r\n    //console.log(this.itemSelecionado);\r\n\r\n    let ordemProducaoLinha = {\r\n      idLinha: parseInt(this.linhaSelecionada.id),\r\n      idOrdemProducao: parseInt(this.itemSelecionado.id),\r\n      dataCadastro: new Date(),\r\n      deletado: \"N\",\r\n    };\r\n\r\n    this._controller!.salvarOrdemProducaoLinha(ordemProducaoLinha);\r\n    this.dialogAddLinha = false;\r\n  }\r\n\r\n  cancelarSaveAddLinhaOrdemProducao() {\r\n    this.dialogAddLinha = false;\r\n    this.linhaSelecionada = {};\r\n    this.itemSelecionado = {};\r\n  }\r\n\r\n  erpOrdemProducaoCarregar() {\r\n    this.loading(true);\r\n    this.itens = [];\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new OrdemProducaoListaSimulacaoController(this);\r\n    this._controller.atualizaBaseOrdemProducaoERP();\r\n    this._controller.carregarItens(this.$route.params.idOrdemProducao);\r\n  }\r\n\r\n  loading(visible: boolean): void {\r\n    this.dialogLoading = visible;\r\n  }\r\n\r\n  showMsg(msg: string): void {\r\n    this.textError = msg;\r\n    this.dialogMessage = true;\r\n  }\r\n\r\n  showItens(list: any): void {\r\n    this.linhaSelecionada = {};\r\n    this.itemSelecionado = {};\r\n    this.itens = list;\r\n\r\n    this.idOrdemProducaoPlano = 0;\r\n  }\r\n\r\n  carregaLinha(linhas: any): void {\r\n    this.itemsLinha = linhas;\r\n\r\n    this.idOrdemProducaoPlano = 0;\r\n  }\r\n\r\n  fecharDialogMessage() {\r\n    this.dialogMessage = false;\r\n  }\r\n\r\n  async verificaOPProducao(item: any) {\r\n    //this.itemFuncionarioDelete = item;\r\n    //this.dialogConsultaStatusOP = true;\r\n    this.idOrdemProducaoPlano = 0;\r\n    this.idOrdemProducaoPlano = item.id;\r\n    await this._controller!.verificarOrdemProducaoPlanoProducao(\r\n      item.idOrdemProducao,\r\n      item.id\r\n    );\r\n  }\r\n\r\n  async statusOPProducao(retorno: any) {\r\n    if (retorno.sucesso == false) {\r\n      this.textChecagemOrdemProducao = retorno.message;\r\n      this.dialogConsultaStatusOP = true;\r\n      this.idOrdemProducaoPlanoAlocada = this.concultaOrdemProducaoAlocada();\r\n    } else {\r\n      //TIPO: 1 - PRODUÇÃO / 2 - SIMULAÇÃO\r\n      await this._controller!.salvarStatusOrdemProducao(\r\n        this.idOrdemProducaoPlano.toString(),\r\n        \"1\",\r\n        this.$route.params.idOrdemProducao\r\n      );\r\n    }\r\n  }\r\n\r\n  concultaOrdemProducaoAlocada(): number {\r\n    let idOrdemProducaoPlano = 0;\r\n\r\n    for (let i = 0; i < this.itens.length; i++) {\r\n      if (this.itens[i].status == \"P\" && this.itens[i].id == this.idOrdemProducaoPlano) {\r\n        idOrdemProducaoPlano = this.itens[i].id.toString();\r\n\r\n        break;\r\n      }\r\n    }\r\n\r\n    return idOrdemProducaoPlano;\r\n  }\r\n\r\n  alterarStatusOrdemProducaoConfirmado() {\r\n    this.dialogConsultaStatusOP = false;\r\n\r\n    //desaloca OP\r\n    this._controller!.salvarStatusOrdemProducao(\r\n      this.idOrdemProducaoPlanoAlocada.toString(),\r\n      \"2\",\r\n      this.$route.params.idOrdemProducao\r\n    );\r\n\r\n    //aloca a nova op\r\n    this._controller!.salvarStatusOrdemProducao(\r\n      this.idOrdemProducaoPlano.toString(),\r\n      \"1\",\r\n      this.$route.params.idOrdemProducao\r\n    );\r\n  }\r\n\r\n  viewItem(item: any) {\r\n    this.$router.push(\r\n      \"/ordemProducao/\" + item.idOrdemProducaoLinha + \"/\" + item.id\r\n    );\r\n  }\r\n\r\n  addSolicitacao() {\r\n    this.$router.push(\"/ordemProducao\");\r\n  }\r\n\r\n  created() {\r\n    this.isMobile = Display.isMobile();\r\n    this._controller = new OrdemProducaoListaSimulacaoController(this);\r\n\r\n    this._controller.carregarItens(this.$route.params.idOrdemProducao);\r\n  }\r\n}\r\n"],"sourceRoot":""}]}